					String: sDesc
				Message Actions
					On SAM_Create
						! -------------------------------------------Selecionar-------------------------------------------
						!
						Set _sComandoPesquisa = 'ValidadeTipoCadastroS'
						Set _sIntoPesquisa = '
								:frmValidaTipoCadastro.tblParalisacao.colsSeq,
								:frmValidaTipoCadastro.tblParalisacao.colnSeq,
								:frmValidaTipoCadastro.tblParalisacao.coldDataIni,
								:frmValidaTipoCadastro.tblParalisacao.coldDataFim,
								:frmValidaTipoCadastro.tblParalisacao.coldDiaValidade,
								:frmValidaTipoCadastro.tblParalisacao.coldMesValidade,
								:frmValidaTipoCadastro.tblParalisacao.coldDataInclusao,
								:frmValidaTipoCadastro.tblParalisacao.colnUsuario,
								:frmValidaTipoCadastro.tblParalisacao.coldDataAlteracao,
								:frmValidaTipoCadastro.tblParalisacao.colnUsuarioAlteracao'
						!
						! -------------------------------------------Atualizar-------------------------------------------
						!
						Set _sComandoAtualiza = 'ValidadeTipoCadastroA
								@nTipoCadastro   		= :frmValidaTipoCadastro.tblParalisacao.colnSeq,
								@dDataIniVigencia	    	= :frmValidaTipoCadastro.tblParalisacao.coldDataIni,
								@dDataFimVigencia	= :frmValidaTipoCadastro.tblParalisacao.coldDataFim,
								@nDiaValidade		= :frmValidaTipoCadastro.tblParalisacao.coldDiaValidade,
								@nMesValidade 		= :frmValidaTipoCadastro.tblParalisacao.coldMesValidade'
						!
						! -------------------------------------------Inserir-------------------------------------------
						!
						Set _sComandoInsere = 'ValidadeTipoCadastroI
								@nTipoCadastro   		= :frmValidaTipoCadastro.tblParalisacao.colnSeq,
								@dDataIniVigencia	    	= :frmValidaTipoCadastro.tblParalisacao.coldDataIni,
								@dDataFimVigencia	= :frmValidaTipoCadastro.tblParalisacao.coldDataFim,
								@nDiaValidade		= :frmValidaTipoCadastro.tblParalisacao.coldDiaValidade,
								@nMesValidade 		= :frmValidaTipoCadastro.tblParalisacao.coldMesValidade'
						!
						! -------------------------------------------Excluir-------------------------------------------
						!
						Set _sComandoExclui = 'ValidadeTipoCadastroE			     	 	 	 	  	 	 	 	  	  	 	 	 	 	 	
								@nTipoCadastro   		= :frmValidaTipoCadastro.tblParalisacao.colnSeq,
								@dDataIniVigencia	    	= :frmValidaTipoCadastro.tblParalisacao.coldDataIni'
						!
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
						Set nSeqAnt = 0
					On MSG_PesquisaTabela
						Call SalSendClassMessage( MSG_PesquisaTabela, wParam, lParam )
					On MSG_NovaLinha
						Call SalSendClassMessage( MSG_NovaLinha , wParam, lParam )
						If fMPSPrepareProc( hSqlApp, 'dbvcen..TipoCadastroS', ':frmValidaTipoCadastro.tblParalisacao.colnSeq, :frmValidaTipoCadastro.tblParalisacao.colsSeq')
							If SqlExecute( hSqlApp )
								If SqlFetchNext ( hSqlApp, nRetorno )
									Set nIndice = 0
									While (nRetorno != FETCH_EOF )
										Set nRegiaoCod[nIndice] = colnSeq
										Set nIndice = nIndice + 1
										Call SalListAdd ( tblParalisacao.colsSeq, frmValidaTipoCadastro.tblParalisacao.colsSeq )
										Call SqlFetchNext ( hSqlApp, nRetorno )
									Set bOk = TRUE
					On UDM_AposInclusao
						Call SalSendClassMessage( UDM_AposInclusao, wParam, lParam )
						Call SalSendMsg( frmValidaTipoCadastro.tblParalisacao, MSG_PesquisaTabela, wParam, lParam )
