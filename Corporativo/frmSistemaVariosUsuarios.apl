.data INHERITPROPS
0000: 0100
.enddata
				Class Child Ref Key: 0
				Class ChildKey: 0
				Class: cQCK1DataField
				Property Template:
				Class DLL Name:
				Data
					Maximum Data Length: 8
					Data Type: Class Default
					Editable? Class Default
				Display Settings
					Window Location and Size
						Left: 1.338"
						Top: 0.313"
						Width:  1.1"
						Width Editable? Class Default
						Height: 0.25"
						Height Editable? Class Default
					Visible? Class Default
					Border? Class Default
					Justify: Left
					Format: Class Default
					Country: Class Default
					Font Name: Class Default
					Font Size: Class Default
					Font Enhancement: Class Default
					Text Color: Class Default
					Background Color: Class Default
					Input Mask: Class Default
				Message Actions
					On SAM_Create
						Set _bVisivel = TRUE
						Set _bPermiteNulos = TRUE
						Set _sNomeParaMsgs = 'Usuário'
						Set _bSempreHabilitado = FALSE
						Set _sTextoStatusBar = 'Login do usuário'
						! 06/12/2012 -- Djalma Júnior -- Habilitar Pesquisa através do F2 digitando a descrição
						! Início
						Set _bDisparaConsultaEmValidate = TRUE
						Set _bCompChaveOpcionalParaPesquisa = TRUE
						Set _sConsultaFrmOpcoesSQLOuSP = 'SP'
						Set _sConsultaFrmOpcoesColunaOuParam = '@Nome'
						Set _sConsultaFrmOpcoesCampo = 'exec UsuarioS_codnome'
						! Fim
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_KillFocus
						If Not SalIsNull( frmSistemaVariosUsuarios.dfsUsuarioOrigem )
							If frmSistemaVariosUsuarios.fRetornarNomeUsuario( frmSistemaVariosUsuarios.dfsUsuarioOrigem, 
									frmSistemaVariosUsuarios.dfsNomeOrigem)
								Call SalDisableWindow( frmSistemaVariosUsuarios.dfsUsuarioOrigem )
								Call SybPrepareProc( hSqlApp, 'exec MPS..SistemaCodigoNomeS @sUsuario = :dfsUsuarioOrigem',
										':frmSistemaVariosUsuarios.tblSistema.colnCodigo,  :frmSistemaVariosUsuarios.tblSistema.colsDescricao'  )
								If NOT  SalTblPopulate(tblSistema, hSqlApp, '', TBL_FillAll )
									Call SalWaitCursor( FALSE )
									Return FALSE
								Call SalSendClassMessage( SAM_Create, wParam, lParam )
							! Else
								Call SalMessageBox( "Usuário origem não encontrado. !!! ", 'Atenção', MB_Ok )
								! Call SalEnableWindow( frmSistemaVariosUsuarios.dfsUsuarioOrigem )
								Set dfsUsuarioOrigem = STRING_Null
								Call SalSetFocus( frmSistemaVariosUsuarios.dfsUsuarioOrigem )
								Return FALSE
