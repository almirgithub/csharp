		Message Actions
			On SAM_Create
				Set _sTextoStatusBar = 'Tela de controle de acesso por usuário para setor permitido para liberação de vistoria sem agendamento'
				Set _sNroCamposIgnorados = 'NENHUM'
				!
				Set _sCriacaoConexao = 'Default'
				Set _bCriacaoConexaoAuxiliar = TRUE
				! Set _sComandoPesquisa = 'select null'
				! Set _sListaINTOPesquisa = ' '
				Set _nEstadoInicialForm = EF_RegistroExistente
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalCenterWindow( hWndForm )
				Call SalDisableWindow( hWndBotao )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				Set frmUsuarioSetorAgendVistoria.dfsNome = sNomeUsuario
			On UDM_Atualizacao
				Return TRUE
			On SAM_Destroy
				Call SalEnableWindow( hWndBotao )
				Set hWndJanela = hWndNULL
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
