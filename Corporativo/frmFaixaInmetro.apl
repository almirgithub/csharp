				!
				! -------------------Pesquisa----------------------
				!
				Set _sComandoPesquisa = ''
				Set _sListaINTOPesquisa = ''
				!
				! -----------------------Inclusão------------------------
				!
				Set _sComandoInclusao = 'Exec dbvcen..FaixaInmetroI
							@UF 		= :frmFaixaInmetro.cmbUF,
							@Situacao 	= :frmFaixaInmetro.dfsSituacao,
							@FaixaInicial 	= :frmFaixaInmetro.dfnInicial,
							@FaixaFinal 	= :frmFaixaInmetro.dfnFinal,
							@Oic		= :frmFaixaInmetro.dfnOic'
				Set _sListaINTOInclusao = ''
				Set _bLimpaTelaAposInclusao = FALSE
				!
				! ------------------------Alteração-------------------------
				!
				Set _sComandoAtualizacao = 'Exec dbvcen..FaixaInmetroU
							@UF 		= :frmFaixaInmetro.cmbUF,
							@Situacao 	= :frmFaixaInmetro.dfsSituacao,
							@FaixaInicial 	= :frmFaixaInmetro.dfnInicial,
							@FaixaFinal 	= :frmFaixaInmetro.dfnFinal,
							@Oic		= :frmFaixaInmetro.dfnOic'
				Set _sListaINTOAtualizacao = ''
				Set _bLimpaTelaAposAtualizacao = FALSE
				!
				! -----------------------Exclusão-------------------
				!
				Set _sComandoRemocao = 'Exec dbvcen..FaixaInmetroE
						@UF 		= :frmFaixaInmetro.cmbUF,
						@FaixaInicial 	= :frmFaixaInmetro.dfnInicial,
						@FaixaFinal 	= :frmFaixaInmetro.dfnFinal,
						@Oic		= :frmFaixaInmetro.dfnOic'
				Set _sListaINTORemocao = ''
				Set _bLimpaTelaAposRemocao = TRUE
				!
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalCenterWindow ( hWndForm )
			On SAM_CreateComplete
				Call SalSetFocus( dfnInicial )
				!
				Call SalSendMsg( pbExcluir, MSG_Desabilita1, wParam, lParam)
				Set dfsSituacao = 'V'
			On MSG_Limpa
				Call SalSendClassMessage( MSG_Limpa, wParam, lParam )
				!
				Set cmbUF = 'PE'
				Call SalSetFocus( dfnInicial )
				Call SalTblReset( tblFaixaCadastrada )
				!
				Call SalSendMsg( cmbUF, MSG_Habilita1, wParam, lParam)
				Call SalSendMsg( dfnFinal, MSG_Habilita1, wParam, lParam)
				Call SalSendMsg( dfnInicial, MSG_Habilita1, wParam, lParam)
				Call SalSendMsg( pbArquivar, MSG_Habilita1, wParam, lParam)
				Call SalSendMsg( pbPesquisar, MSG_Habilita1, wParam, lParam)
			On MSG_Arquiva
				Set nLinha = -1
				If NOT SalTblFindNextRow( tblFaixaCadastrada, nLinha, ROW_Edited, 0 )
					Call frmFaixaInmetro.AtualizaEstadoForm( 4 )
				Else
					Call frmFaixaInmetro.AtualizaEstadoForm( 8 )
					Call SalSendMsg( hWndForm, MSG_MudaSemaforo, S_Vermelho, lParam )
				!
				Call SalSendClassMessage( MSG_Arquiva, wParam, lParam )
