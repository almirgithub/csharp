				Functions
				Window Variables
					Sql Handle: hSqlOrgaoAutuante
				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Permissão para Movimentação das Apreensões'
						Set _sIdUsuario = ''
						Set _sCriacaoConexao = 'Default'
						Set _bCriacaoConexaoAuxiliar = FALSE
						Set _bPesquisaCreate = TRUE
						Set _sPwdUsuario = ''
						Set _sNomeBD = ''
						Set _sComandoAtualiza = 'exec SetorOrgaoAutuanteA @nSetor = :frmSetor.dfnCodRed, @nDigito = :frmSetor.dfnDigito, @OrgaoAutuante = :tblSetorOrgaoAutuante.colnOrgaoAutuante,
								@iOrgaoOrigem = :tblSetorOrgaoAutuante.colsOrgaoOrigem'
						Set _sComandoInsere = 'exec SetorOrgaoAutuanteI @nSetor = :frmSetor.dfnCodRed, @nDigito = :frmSetor.dfnDigito, @OrgaoAutuante = :tblSetorOrgaoAutuante.colnOrgaoAutuante,
								@iOrgaoOrigem = :tblSetorOrgaoAutuante.colsOrgaoOrigem'
						Set _sComandoExclui = 'exec SetorOrgaoAutuanteE @nSetor = :frmSetor.dfnCodRed, @nDigito = :frmSetor.dfnDigito,  @OrgaoAutuante = :tblSetorOrgaoAutuante.colnOrgaoAutuante'
						Set _sComandoPesquisa = 'exec SetorOrgaoAutuanteS @nSetor = :frmSetor.dfnCodRed, @nDigito = :frmSetor.dfnDigito'
						Set _sIntoPesquisa = ':frmSetorOrgaoAutuante.tblSetorOrgaoAutuante.colnOrgaoAutuante, :frmSetorOrgaoAutuante.tblSetorOrgaoAutuante.colsDescricao, :frmSetorOrgaoAutuante.tblSetorOrgaoAutuante.colsOrgaoOrigem'
						Set _sIntoAtualiza = ''
						Set _sIntoInsere = ''
						Set _sIntoExclui = ''
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On MSG_NovaLinha
						! Elimina linhas inseridas que nao tenham sido digitadas
						! Call EliminaLinhasNulas(hWndItem)
						Set nLinha = TBL_MinRow
						While SalTblFindNextRow( frmSetorOrgaoAutuante.tblSetorOrgaoAutuante, nLinha, 0, 0 )
							Call SalTblSetContext( frmSetorOrgaoAutuante.tblSetorOrgaoAutuante, nLinha )
							If frmSetorOrgaoAutuante.tblSetorOrgaoAutuante.colnOrgaoAutuante = NUMBER_Null
								Call SalTblDeleteRow( frmSetorOrgaoAutuante.tblSetorOrgaoAutuante, nLinha, TBL_NoAdjust )
						! Posiciona na linha nova e na coluna inicial
						Call SalSendClassMessage( MSG_NovaLinha, wParam, lParam )
