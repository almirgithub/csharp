							On SAM_Create
								Set _sTextoStatusBar = 'Código do Órgão Autuante'
								Set _bPermiteNulos = FALSE
								Set _sNroCamposIgnorados = 'NENHUM'
								Set _bComponenteChave = TRUE
								Set _bVisivel = TRUE
								Set _sNomeParaMsgs = 'Órgão Autuante'
								Set _sMsgValidacao = 'Órgão Autuante já cadastrado ou digitado!'
								Set _sTipoMsgValidacao = 'ESPECIFICA'
								Call SalSendClassMessage( SAM_Create, wParam, lParam )
							On UDM_ValidaCampo
								If NOT SalIsNull( hWndItem)
									If ProcuraNJaDigitado( hWndForm, SalTblQueryColumnID( colnOrgaoAutuante ), lParam, colnOrgaoAutuante, '' )
										Return FALSE
								Return TRUE
							On WMSG_KEYDOWN
								Select Case (wParam)
									Case WKEY_F2
										Call SalModalDialog( dlgProcura, hWndForm, hWndItem, 'Exec dbvcen..SetorOrgaoAutuanteCodDescS',
												'', 'SP', NUMBER_Null, _sColunasAdicionais)
										Call SalSendMsg( hWndItem, SAM_User, lParam, wParam )
										Break
									Case WKEY_TAB
										Call SalSendMsg( hWndItem, SAM_User, lParam, wParam )
										Break
							On SAM_User
								If tblSetorOrgaoAutuante.colnOrgaoAutuante != NUMBER_Null
									If SqlConnect( tblSetorOrgaoAutuante.hSqlOrgaoAutuante )
										If fMPSPrepareProc( tblSetorOrgaoAutuante.hSqlOrgaoAutuante, 'dbvcen..OrgaoRecolhimentoDescS @Cod =:tblSetorOrgaoAutuante.colnOrgaoAutuante', ':tblSetorOrgaoAutuante.colsDescricao' )
											If SqlExecute( tblSetorOrgaoAutuante.hSqlOrgaoAutuante )
												Call SqlFetchNext( tblSetorOrgaoAutuante.hSqlOrgaoAutuante, lParam )
										If SalIsNull(tblSetorOrgaoAutuante.colsDescricao)
											Call SalMessageBox( 'Órgão inexistente!', 'Atenção', MB_IconExclamation)
											Call SalClearField(tblSetorOrgaoAutuante.colnOrgaoAutuante)
											Return FALSE
										Call SqlDisconnect( tblSetorOrgaoAutuante.hSqlOrgaoAutuante )
										Return VALIDATE_Ok
