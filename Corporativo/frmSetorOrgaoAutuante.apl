				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Arquiva o registro corrente'
						Set _sTipoComando = 'Arquiva'
						Set _sTabela = 'tblSetorOrgaoAutuante'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						Set nOrgaoCont = 0
						! Call frmSetorOrgaoAutuante.tblSetorOrgaoAutuante.EliminaLinhasNulas( frmSetorOrgaoAutuante.tblSetorOrgaoAutuante )
						If SalIsNull(tblSetorOrgaoAutuante.colsDescricao)
							Call SalMessageBox( 'Órgão inexistente!', 'Atenção', MB_IconExclamation)
							Call SalClearField(tblSetorOrgaoAutuante.colnOrgaoAutuante)
							Return FALSE
						Set nLinha = -1
						Loop
							If Not SalTblFindNextRow( tblSetorOrgaoAutuante, nLinha, 0, 0 )
								Break
							!
							Call SalTblSetContext( tblSetorOrgaoAutuante, nLinha )
							If tblSetorOrgaoAutuante.colsOrgaoOrigem = 'S'
								Set nOrgaoCont = nOrgaoCont + 1
						If nOrgaoCont > 1
							Call SalMessageBox( 'Marque apenas um Orgão de Origem!', 'Atenção', MB_IconExclamation)
						Else If nOrgaoCont < 1
							Call SalMessageBox( 'Marque o órgão de origem do setor!', 'Atenção', MB_IconExclamation)
						Else
							Call SalSendClassMessage( SAM_Click, wParam, lParam )
