						Set _sNroCamposIgnorados = 'NENHUM'
						!
						Set _sCriacaoConexao = 'Default'
						Set _bNuncaHabilitado = FALSE
						Set _bPesquisaCreate = FALSE
						Set _bSempreHabilitado = FALSE
						Set _sComandoPesquisa = 'Exec dbvcen..LogMaquinaSetorS
												@NomeMaquina = :frmLogMaquinaSetor.dfsMaquina,
												@SetorMaquina = :frmLogMaquinaSetor.dfnSetorMaquina,
												@DataInicial = :frmLogMaquinaSetor.dfdPeriodoInicial,
												@DataFinal = :frmLogMaquinaSetor.dfdPeriodoFinal'
						Set _sIntoPesquisa = '
								:frmLogMaquinaSetor.tblLogMaquinaSetor.colsMaquina,
								:frmLogMaquinaSetor.tblLogMaquinaSetor.colsSetor,
								:frmLogMaquinaSetor.tblLogMaquinaSetor.coldDataMovimentacao,
								:frmLogMaquinaSetor.tblLogMaquinaSetor.colsUsuarioMovimentacao,
								:frmLogMaquinaSetor.tblLogMaquinaSetor.colsSetorUsuario'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On UDM_AntesPesquisa
						If SalIsNull( dfdPeriodoInicial )OR SalIsNull( dfdPeriodoFinal )
							Call SalMessageBeep( 0 )
							Call SalMessageBox( 'É obrigatório informar o Período!', 'Atenção', MB_IconExclamation | MB_Ok )
							Call SalWaitCursor( FALSE )
							Call SalSetFocus( dfdPeriodoInicial )
							Call SalAbort( -1)
						Return TRUE
					On UDM_AposPesquisa
						If NOT SalTblAnyRows( tblLogMaquinaSetor, 0, 0 )
							Call SalMessageBeep( 0 )
							Call SalMessageBox( 'Nehum registro de Log encontrado!', 'Atenção', MB_IconExclamation | MB_Ok )
							Call SalWaitCursor( FALSE )
							Call SalAbort( -1)
						Call SalSendMsg( pbPesquisar, MSG_Habilita1, lParam, wParam )
						Call SalWaitCursor( FALSE )
						Return TRUE
			!
