				Message Actions
					On SAM_Create
						Set _sNroCamposIgnorados = 'NENHUM'
						Set _bVisivel = TRUE
						Set _bPermiteNulos = FALSE
						Set _bSempreHabilitado = TRUE
						Set _bComponenteChave = FALSE
						Set _sNomeParaMsgs = 'Data Inicial do Período'
						Set _sTextoStatusBar = 'Data Inicial do Período'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Validate
						If NOT SalIsNull( dfdPeriodoInicial )
							If NOT (SalIsValidDateTime( dfdPeriodoInicial ))
								Call SalMessageBeep( 0 )
								Call SalMessageBox( 'Data inválida.', 'Atenção', MB_Ok | MB_IconExclamation )
								Call SalClearField( dfdPeriodoInicial )
								Return VALIDATE_Cancel
							Else If (SalDateYear( dfdPeriodoInicial ) < 1753)
								Call SalMessageBeep( 0 )
								Call SalMessageBox( 'Ano inválido.', 'Atenção', MB_Ok | MB_IconExclamation )
								Call SalClearField( dfdPeriodoInicial )
								Return VALIDATE_Cancel
							If dfdPeriodoFinal < dfdPeriodoInicial AND NOT SalIsNull( dfdPeriodoFinal )
								Call SalMessageBeep( 0 )
								Call SalMessageBox( 'A Data Inicial deve ser menor do que a Data Final.', 'Atenção', MB_IconExclamation | MB_Ok )
								Call SalClearField( dfdPeriodoInicial )
								Return VALIDATE_Cancel
						Call SalSendClassMessage( SAM_Validate, wParam, lParam )
					On SAM_KillFocus
						Call SalSendMsg( pbPesquisar, MSG_Habilita1, lParam, wParam )
					On WMSG_KEYDOWN
						Select Case ( wParam )
							Case WKEY_TAB
								! Recupera a data do sistema (servidor)
								If SalGetDataType( hWndItem ) = DT_DateTime AND SalIsNull(hWndItem)
									Call SalWaitCursor( TRUE )
									Call fMPSPrepare( hSqlApp, 'select getdate() into :frmLogMaquinaSetor.dfdPeriodoInicial' )
									If SqlExecute( hSqlApp )
										Call SqlFetchNext( hSqlApp, nErroData )
									Call SalSetWindowText( hWndItem, SalFmtFormatDateTime( MyValue,'dd-MM-yyyy') )
									Call SalSendMsg( hWndForm, MSG_MudaSemaforo, S_Vermelho, lParam )
									Call SalSetFieldEdit( hWndItem, TRUE )
									Call SalSendMsg( hWndForm, MSG_SetaFocoProximoHabilitado, SalWindowHandleToNumber( hWndItem ), lParam )
									Call SalWaitCursor( FALSE )
								Break
							Case WKEY_D
								! Recupera a data do sistema (servidor)
								If SalGetDataType( hWndItem ) = DT_DateTime
									Call SalWaitCursor( TRUE )
									Call fMPSPrepare( hSqlApp, 'select getdate() into :frmLogMaquinaSetor.dfdPeriodoInicial' )
									If SqlExecute( hSqlApp )
										Call SqlFetchNext( hSqlApp, nErroData )
									Call SalSetWindowText( hWndItem, SalFmtFormatDateTime( MyValue,'dd-MM-yyyy') )
									Call SalSendMsg( hWndForm, MSG_MudaSemaforo, S_Vermelho, lParam )
									Call SalSetFieldEdit( hWndItem, TRUE )
									Call SalSendMsg( hWndForm, MSG_SetaFocoProximoHabilitado, SalWindowHandleToNumber( hWndItem ), lParam )
									Call SalWaitCursor( FALSE )
								Break
