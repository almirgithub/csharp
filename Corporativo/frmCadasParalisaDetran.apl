					On SAM_Create
						! ------------------------Selecionar------------------
						!
						Set _sComandoPesquisa = 'ParalisacaoDetranS'
						Set _sIntoPesquisa = '
								:frmCadasParalisaDetran.tblParalisacao.colnSeq, 
								:frmCadasParalisaDetran.tblParalisacao.coldDataIni,
								:frmCadasParalisaDetran.tblParalisacao.coldDataFim, 
								:frmCadasParalisaDetran.tblParalisacao.coldNovaData,
								:frmCadasParalisaDetran.tblParalisacao.colnUsuario, 
								:frmCadasParalisaDetran.tblParalisacao.coldDataCad,
								:frmCadasParalisaDetran.tblParalisacao.colsMotivo, 
								:frmCadasParalisaDetran.tblParalisacao.coldDataFimReal '
						!
						! ------------------------Atualizar------------------
						!
						Set _sComandoAtualiza = 'ParalisacaoDetranU
								@iParalisacaoDetran   	= :frmCadasParalisaDetran.tblParalisacao.colnSeq, 
								@dDataIni	    	= :frmCadasParalisaDetran.tblParalisacao.coldDataIni,
								@dDataFim		= :frmCadasParalisaDetran.tblParalisacao.coldDataFim,
								@dNovaData		= :frmCadasParalisaDetran.tblParalisacao.coldNovaData,
								@cUsuario		= :frmCadasParalisaDetran.tblParalisacao.colnUsuario,
								@dDataCad 		= :frmCadasParalisaDetran.tblParalisacao.coldDataCad,
								@sMotivo		= :frmCadasParalisaDetran.tblParalisacao.colsMotivo,
								@DataFimReal		= :frmCadasParalisaDetran.tblParalisacao.coldDataFimReal '
						!
						! ------------------------Inserir------------------
						!
						Set _sComandoInsere = 'ParalisacaoDetranI
								@iParalisacaoDetran   	= :frmCadasParalisaDetran.tblParalisacao.colnSeq, 
								@dDataIni	    	= :frmCadasParalisaDetran.tblParalisacao.coldDataIni,
								@dDataFim		= :frmCadasParalisaDetran.tblParalisacao.coldDataFim,
								@dNovaData		= :frmCadasParalisaDetran.tblParalisacao.coldNovaData,
								@cUsuario		= :frmCadasParalisaDetran.tblParalisacao.colnUsuario,
								@dDataCad 		= :frmCadasParalisaDetran.tblParalisacao.coldDataCad,
								@sMotivo		= :frmCadasParalisaDetran.tblParalisacao.colsMotivo,
								@DataFimReal		= :frmCadasParalisaDetran.tblParalisacao.coldDataFimReal '
						!
						! ------------------------Excluir------------------
						!
						Set _sComandoExclui = 'ParalisacaoDetranD
								@iParalisacaoDetran   	= :frmCadasParalisaDetran.tblParalisacao.colnSeq, 
								@dDataIni	    	= :frmCadasParalisaDetran.tblParalisacao.coldDataIni,
								@dDataFim		= :frmCadasParalisaDetran.tblParalisacao.coldDataFim,
								@dNovaData		= :frmCadasParalisaDetran.tblParalisacao.coldNovaData,
								@cUsuario		= :frmCadasParalisaDetran.tblParalisacao.colnUsuario,
								@dDataCad 		= :frmCadasParalisaDetran.tblParalisacao.coldDataCad'
						!
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
						Set nSeqAnt = 0
					On SAM_FetchRowDone
						If tblParalisacao.colnSeq > nSeqAnt
							Set nSeqAnt = tblParalisacao.colnSeq
						! If SalIsNull( frmCadasParalisaDetran.tblParalisacao.coldDataFimReal )
							Set frmCadasParalisaDetran.tblParalisacao.coldDataFimReal = frmCadasParalisaDetran.tblParalisacao.coldDataFim
							Return VALIDATE_Cancel
					On MSG_PesquisaTabela
						Call SalSendClassMessage( MSG_PesquisaTabela, wParam, lParam )
					On MSG_NovaLinha
						Call SalSendClassMessage( MSG_NovaLinha , wParam, lParam )
						Set tblParalisacao.colnSeq = nSeqAnt + 1
						Set nSeqAnt = nSeqAnt +1
					On UDM_AposInclusao
						Call SalSendClassMessage( UDM_AposInclusao, wParam, lParam )
						Call SalSendMsg( frmCadasParalisaDetran.tblParalisacao, MSG_PesquisaTabela, wParam, lParam )
