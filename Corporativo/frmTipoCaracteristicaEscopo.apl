				Return TRUE
			On SAM_Destroy
				Call SalEnableWindow( hWndBotao )
				Set hWndJanela = hWndNULL
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				Set dfsTipoCaracteristica = sTipoCaracteristica
				If frmTipoCaracteristicaEscopo.fObtemPermissao('frmTipoCaracteristicaEscopo') = 1
					Call SalDisableWindow( pbArquivar )
				Else If frmTipoCaracteristicaEscopo.fObtemPermissao('frmTipoCaracteristicaEscopo') = 15
					Call SalEnableWindow( pbArquivar )
