						Set _bÉDependenteLimpeza = TRUE
						Set _sCamposResponsaveisLimpeza[0] = 'dfnPais'
						Set _sCamposResponsaveisLimpeza[1] = 'cmbUF'
						Set _bÉDependente = TRUE
						Set _sCamposResponsaveis[0] = 'dfnPais'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Validate
						If NOT SalIsNull( dfnMunicipio )
							If SalIsNull( dfnPais )
								Call SalMessageBeep( 0 )
								Call SalMessageBox( 'É preciso primeiro informar o País e a UF antes de se indicar o município.', 'Atenção', MB_IconExclamation | MB_Ok )
								Call SalClearField( dfnMunicipio )
								Set nMunicipio = NUMBER_Null
								Set nDigito = NUMBER_Null
								Call SalClearField( dfsMunicipio )
								Call SalClearField( dfnBairro )
								Call SalClearField( dfsBairro )
								Return VALIDATE_Cancel
							If SalIsNull( dfsUF )
								Call SalMessageBeep( 0 )
								Call SalMessageBox( 'É preciso primeiro informar a UF antes de se indicar o município.', 'Atenção', MB_IconExclamation | MB_Ok )
								Call SalClearField( dfnMunicipio )
								Set nMunicipio = NUMBER_Null
								Set nDigito = NUMBER_Null
								Call SalClearField( dfsMunicipio )
								Call SalClearField( dfnBairro )
								Call SalClearField( dfsBairro )
								Return VALIDATE_Cancel
							Set nMunicipio = SalNumberTruncate( dfnMunicipio*0.1, 5, 0 )
							Set nDigito = SalNumberMod( dfnMunicipio, 10 )
							If nDigito != DigitoModulo11( nMunicipio )
								Call SalMessageBeep( 0 )
								Call SalMessageBox( 'Digito Invalido.', 'Atenção', MB_IconExclamation | MB_Ok )
								Call SalClearField( dfnMunicipio )
								Call SalClearField( dfsMunicipio )
								Return VALIDATE_Cancel
							Else
								Set dfnMunicipio = SalStrToNumber( '000' || SalNumberToStrX( dfnMunicipio, 0 ) )
						If NOT SalSendClassMessage( SAM_Validate, wParam, lParam )
							Return VALIDATE_Cancel
						Return VALIDATE_OkClearFlag
