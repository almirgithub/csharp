						' :frmFornecedor.dfnTelefone,  :frmFornecedor.dfnFax'
				Set _sPwdUsuario = ''
				Set _bCriacaoConexaoAuxiliar = FALSE
				Set _sCriacaoConexao = 'Default'
				Set _sNomeBD = ''
				Set _sIdUsuario = ''
				Set _sComandoPesquisa = 'exec FornecedorS  @Cod = :frmFornecedor.dfnCodigo,  @Cgc = :frmFornecedor.sCgc,  @Nome = :frmFornecedor.dfsNome'
				Set _sComandoInclusao = 'exec FornecedorI  @Cgc = :frmFornecedor.sCgc,  @Nome = :frmFornecedor.dfsNome,  @NomeSint = :frmFornecedor.dfsNomeSint,' ||
						' @Logradouro = :frmFornecedor.dfsLogradouro,  @Bairro = :frmFornecedor.dfnBairro,  @Municipio = :frmFornecedor.dfnMunicipio,' ||
						' @EMail = :frmFornecedor.dfsEMail,  @CEP = :frmFornecedor.dfnCep,  @Pais = :frmFornecedor.dfnPais,  @Telefone = :frmFornecedor.dfnTelefone,' ||
						' @FAX = :frmFornecedor.dfnFax, @Tipo = "D"'
				Set _bLimpaTelaAposInclusao = FALSE
				Set _sComandoAtualizacao = 'exec FornecedorU  @Cod = :frmFornecedor.dfnCodigo,  @Cgc = :frmFornecedor.sCgc,  @Nome = :frmFornecedor.dfsNome,' ||
						' @NomeSint = :frmFornecedor.dfsNomeSint,  @Logradouro = :frmFornecedor.dfsLogradouro,  @Bairro = :frmFornecedor.dfnBairro,' ||
						' @Municipio = :frmFornecedor.dfnMunicipio,  @EMail = :frmFornecedor.dfsEMail,  @CEP = :frmFornecedor.dfnCep,  @Pais = :frmFornecedor.dfnPais,' ||
						' @Telefone = :frmFornecedor.dfnTelefone,  @FAX = :frmFornecedor.dfnFax, @Tipo = "D"'
				Set _bLimpaTelaAposAtualizacao = FALSE
				Set _sListaINTORemocao = ''
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalCenterWindow( hWndForm )
			On MSG_Pesquisa
				Set sCgc = SalNumberToStrX( dfnCgc, 0 )
				Call SalSendClassMessage( MSG_Pesquisa, wParam, lParam )
				If nEstadoForm = EF_Vazio
					Call SalSetFocus( dfnCodigo )
				Call SalSendMsg( cmbUF, MSG_FetchRefreshDerivados, 0, 0 )
			On MSG_Limpa
				Call SalFmtSetInputMask( dfnTelefone, '999999999999' )
				Call SalFmtSetInputMask( dfnFax, '999999999999' )
				Call SalSendClassMessage( MSG_Limpa, 0, 0 )
			On SAM_Help
				Call SalWinHelp( hWndForm, 'DocSeg.HLP', HELP_Key, 0, 'Tela de cadastro de Fornecedor' )
			On MSG_Arquiva
				Set sCgc = SalNumberToStrX( dfnCgc, 0 )
				Call SalSendClassMessage( MSG_Arquiva, wParam, lParam )
			On MSG_Anterior
				Call SalSendClassMessage( MSG_Anterior, wParam, lParam )
				Call SalSendMsg( cmbUF, MSG_FetchRefreshDerivados, 0, 0 )
				Call MPSFormataTelefone()
			On MSG_Proximo
				Call SalSendClassMessage( MSG_Proximo, wParam, lParam )
				Call SalSendMsg( cmbUF, MSG_FetchRefreshDerivados, 0, 0 )
				Call MPSFormataTelefone()
			On UDM_AposAtualizacao
				Call SalDisableWindow( pbAnterior)
				Call SalDisableWindow( pbProximo)
				Return TRUE
			On UDM_AposPesquisa
				Call MPSFormataTelefone()
