				Set _sListaINTOAtualizacao = ''
				Set _sListaINTOInclusao = ':frmFornecedor.dfnCodigo'
				Set _sListaINTOPesquisa = ':frmFornecedor.dfnCodigo,  :frmFornecedor.dfnCgc,  :frmFornecedor.dfsNome,  :frmFornecedor.dfsNomeSint,' ||
						' :frmFornecedor.dfsLogradouro,  :frmFornecedor.dfnBairro,  :frmFornecedor.dfsBairro,  :frmFornecedor.dfnMunicipio,  :frmFornecedor.dfsMunicipio,' ||
