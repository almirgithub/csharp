		Functions
			Function: GerarArquivo
				Description:
				Returns
				Parameters
					String: spArquivo
				Static Variables
				Local variables
					Sql Handle: hSql
					String: sProcessado
					String: sOic
					String: sPlaca
					String: sChassi
					Number: nNumCsv
					String: sUFEmissao
					Number: nRenavam
					Date/Time: dDataEmissao
					Date/Time: dDataValidade
					Window Handle: hArquivo
					String: sArquivo
					String: sDataValidade
					String: sDataEmissao
					String: sEndereco
					String: sDiretorioConrrete
					String: sDataAtual
					!
					Number: nAno
					Number: nMes
					Number: nDia
					Number: nHora
					Number: nMinuto
					Number: nSecundo
					!
					String: sLinhaErroConvert
					Number: nTitulo
				Actions
					If SqlConnect( hSql )
						If fMPSPrepareProc( hSql, "Exec dbvcen..LogLinhaArquivoCsvS
										@Arquivo 	= :frmLogLinhaArquivoCsv.psArquivo,
										@Processado	= 'N',
										@Placa		= :frmLogLinhaArquivoCsv.psPlaca",
								':nNumCsv, :sUFEmissao, :sProcessado, :sOic, :dDataEmissao, :sPlaca, :sChassi, :nRenavam, :dDataValidade')
							If SqlExecute( hSql )
								!
								! Cria Diretorio
								If SalFileGetCurrentDirectory( sDiretorioConrrete )
									Set nDia =  SalDateDay( SalDateCurrent(  ) )
									Set nMes = SalDateMonth(SalDateCurrent(  ) )
									Set nAno = SalDateYear( SalDateCurrent(  )  )
									Set nHora = SalDateHour( SalDateCurrent(  )   )
									Set nMinuto =  SalDateMinute( SalDateCurrent(  )   )
									Set nSecundo =  SalDateSecond( SalDateCurrent(  )  )
									Set sDataAtual = 	SalNumberToStrX( nDia, 0 ) ||
													'-' ||
													SalNumberToStrX( nMes, 0 ) ||
													'-' ||
													SalNumberToStrX( nAno, 0 ) ||
													'_' ||
													SalNumberToStrX( nHora, 0 ) ||
													'_' ||
													SalNumberToStrX( nMinuto, 0 ) ||
													'_' ||
													SalNumberToStrX( nSecundo, 0 )
									Set sDiretorioConrrete = sDiretorioConrrete || '\\LogCSV\\' || sDataAtual || '\\'
									Call SalFileCreateDirectory( sDiretorioConrrete )
								!
								! Criar Arquivo
								!
								Set sEndereco = sDiretorioConrrete  || spArquivo
								!
								If SalFileOpen( hArquivo, sEndereco,  OF_Create  )
									Call SalFileClose( hArquivo )
								!
								Set nTitulo = 0
								!
								! Prenche Conteudo dop Arquivo
								!
								Call SalFileOpen( hArquivo,  sEndereco,  OF_Append  )
								While SqlFetchNext( hSql, lParam )
									If nTitulo = 0
										! Escreve Titulo
										Set sArquivo = 'Nº CSV' || '		' || 'UF Emissão'  || '	' || 'OIC' || '		' ||
												'Data Emissão' || '				' || 'Placa' || '		' || 'Chassi' || '				' ||
												 'Renavam' || '			' || 'Data Validade'
										If SalFilePutStr( hArquivo, sArquivo )
											Set nTitulo = 1
										!
									Call SalDateToStr( dDataValidade, sDataValidade )
									Call SalDateToStr( dDataEmissao, sDataEmissao )
									Set sArquivo = SalNumberToStrX( nNumCsv, 0 ) || '		' || sUFEmissao  || '		' || sOic || '		' ||
											sDataEmissao || '		' || sPlaca || '		' || sChassi || '		' || SalNumberToStrX( nRenavam, 0 ) || '		' || sDataValidade
									Call SalFilePutStr( hArquivo, sArquivo )
								Call SqlDisconnect( hSql )
								!
								! Erro Convert
								Set sArquivo = '										'
								Call SalFilePutStr( hArquivo, sArquivo )
								!
								Set sArquivo = '------------------------------------------ ERRO AO CONVERTER DADOS ------------------------------------------ '
								Call SalFilePutStr( hArquivo, sArquivo )
								!
								Set sArquivo = '										'
								Call SalFilePutStr( hArquivo, sArquivo )
								!
								If fMPSConnect( hSql )
									If fMPSPrepareProc( hSql,  ' Exec dbvcen..LogErroConvertCsvS
													@Arquivo 	= :frmLogLinhaArquivoCsv.psArquivo',
											':sLinhaErroConvert')
										If SqlExecute( hSql )
											!
											! Prenche Conteudo dop Arquivo
											!
											While SqlFetchNext( hSql, wParam )
												Set sArquivo =  sLinhaErroConvert
												Call SalFilePutStr( hArquivo, sArquivo )
											If SalFileClose( hArquivo )
												Call SalMessageBox( 'Arquivo Gerado Com Exito', 'Atenção', MB_IconExclamation | MB_Ok )
											Else
												Call SalMessageBox( 'Erro ao Gerado. Tente Novamente', 'Erro', MB_IconHand | MB_Ok )
											!
									Call SqlDisconnect( hSql )
					!
					!
