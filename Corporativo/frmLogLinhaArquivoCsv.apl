					On SAM_Create
						Set _sTextoStatusBar = 'Tabela de Log Linha Arquivo Csv'
						!
						Set _bPesquisaCreate = TRUE
						Set _sCriacaoConexao = 'TRUE'
						!
						! ----------------------------Pesquisa------------------
						!
						! Set _sComandoPesquisa = ' Exec dbvcen..LogLinhaArquivoCsvS
										@Arquivo 	= :frmLogLinhaArquivoCsv.psArquivo'
						Set _sComandoPesquisa = ' Exec dbvcen..LogLinhaArquivoCsvS
										@Arquivo 	= :frmLogLinhaArquivoCsv.psArquivo ,
										@Placa		=  :frmLogLinhaArquivoCsv.psPlaca'
						!
						Set _sIntoPesquisa = '
									:tblLogLinhaArquivoCsv.colnNumCsv,
									:tblLogLinhaArquivoCsv.colsUFEmissao,
									:tblLogLinhaArquivoCsv.colsProcessado,
									:tblLogLinhaArquivoCsv.colsOic,
									:tblLogLinhaArquivoCsv.coldDataEmissao,
									:tblLogLinhaArquivoCsv.colsPlaca,
									:tblLogLinhaArquivoCsv.colsChassi,
									:tblLogLinhaArquivoCsv.colnRenavam,
