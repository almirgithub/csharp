				Set _sComandoAtualizacao = ''
				Set _sListaINTOAtualizacao = ''
				Set _bLimpaTelaAposAtualizacao = FALSE
				! ----------------------------------------Exclusão------------------------------------
				Set _sComandoRemocao = ''
				Set _bLimpaTelaAposRemocao = TRUE
				Set _sListaINTORemocao = ''
