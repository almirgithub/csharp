		Functions
			Function: fPopularTabela
				Description: Popula a tabela com os arquivo do diretório selecionado.
				Returns
					Boolean:
				Parameters
					String: psDiretorio
				Static Variables
				Local variables
					Number: nIndex
					Number: nTamVetor
					String: sArquivo[*]
					Number: nTamanho[*]
					Date/Time: dData[*]
					Number: nAtributos[*]
				Actions
					Call SalTblReset( tblSelecionarArquivo )
					!
					Call VisDosEnumFileInfo( psDiretorio, FA_Standard, sArquivo, nTamanho, dData, nAtributos )
					Call SalArrayGetUpperBound( nTamanho, 1, nTamVetor )
					!
					Set nIndex = 0
					Loop
						If nIndex > nTamVetor
							Break
						Else
							Call SalTblInsertRow( tblSelecionarArquivo, TBL_MaxRow )
							Set tblSelecionarArquivo.colnProcessar = FALSE
							Set tblSelecionarArquivo.colsArquivos = sArquivo[nIndex]
							Set tblSelecionarArquivo.colnTamanho = nTamanho[nIndex]
							Set tblSelecionarArquivo.coldData = dData[nIndex]
							Call SalTblSetFlagsAnyRows( tblSelecionarArquivo, ROW_New, FALSE, 0,0)
						Set nIndex = nIndex + 1
			Function: fProcessarArquivo
				Description: Captura, Abre e Ler o Arquivo.
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					String: sLinha
					String: sArquivo
					Number: nLinha
					String: lsTexto
					File Handle: fhArquivo
					Sql Handle: hSql
				Actions
					Set nLinha = -1
					Loop
						Set sErroConversao = 'N'
						If NOT SalTblFindNextRow( tblSelecionarArquivo, nLinha, ROW_Edited, 0 )
							Break
						Else
							Call SalTblSetContext( tblSelecionarArquivo, nLinha )
							Set sArquivo = dfsDiretorio || tblSelecionarArquivo.colsArquivos
							!
							Call fDeletarLogArquivo( tblSelecionarArquivo.colsArquivos )
							!
							If fAbrirArquivo( fhArquivo, sArquivo, OF_Share_Exclusive | OF_Read)
								Call SalWaitCursor( TRUE )
								Loop
									If NOT fLerArquivo( fhArquivo, lsTexto, 1024 )
										Call fMontarLog( lsTexto )
										If fFecharArquivo( fhArquivo )
											If frmArquivoCsv.sArquivoErro = 'S'
												If fCopiarArquivo( sArquivo, dfsDiretorioOk )
													Call fExcluirArquivo( sArquivo )
											Else
												If fCopiarArquivo( sArquivo, dfsDiretorioErro  )
													Call fExcluirArquivo( sArquivo )
										Call SalWaitCursor( FALSE )
										Break
									Else
										Call SalSendMsg( ccProcessamento, SAM_Validate, 65000, 0)
										Call fMontarLog( lsTexto )
										Call fMontarArquivo( lsTexto, tblSelecionarArquivo.colsArquivos )
							Else
								Call SalMessageBox( ' Erro ao abrir o arquivo:' || tblSelecionarArquivo.colsArquivos , 'Erro', MB_IconStop )
								Return FALSE
			! Estou Alterando
			Function: fMontarArquivo
				Description: Monda as variáveis para execução.
				Returns
					Boolean:
				Parameters
					String: plsTexto
					String: psArquivo
				Static Variables
				Local variables
					Sql Handle: hSql
					Sql Handle: hSqlTrataErro
					Number: nPosicao
					Number: nErro
					String: sErro
					Sql Handle: hSqlErro
					Number: nTrataErro[*]
					Number: i
					Number: nQuantTrataErro
				Actions
					If plsTexto != '' and  SalStrLength( plsTexto ) > 10
						If SqlConnect( hSql )
							If fMPSPrepareProc( hSql, 'Exec dbvcen..MontarArquivoCsv
											@LinhaArquivo	= :plsTexto,
											@Arquivo		= :psArquivo', '')
								When SqlError
									Call SqlGetError( hSql, nErro, sErro )
									!
									! Popula os Codigos De Erro
									Set i = 0
									If SqlConnect( hSqlTrataErro )
										If fMPSPrepareProc( hSqlTrataErro, 'exec dbvcen..TipoErroCsvS', ':nQuantTrataErro, :nTrataErro[i]' )
											If SqlExecute( hSqlTrataErro )
												While SqlFetchNext( hSqlTrataErro, lParam )
													Set i = i + 1
									Call SqlDisconnect( hSqlTrataErro )
									!
									Set i = 0
									Loop
										If i = nQuantTrataErro
											Break
										! 45
										If nErro = nTrataErro[i]
											If fMPSConnect( hSqlErro )
												If fMPSPrepareProc( hSqlErro, 'LogErroConvertCsvI @Arquivo = :psArquivo  , @Linha = :plsTexto, @Erro = :nTrataErro[i]', '' )
													Call SqlExecute( hSqlErro )
													Call SqlCommit( hSqlErro )
												Call SqlDisconnect( hSqlErro )
											Set sErroConversao = 'S'
											Return FALSE
										Set i = i +1
									!
								Call SqlExecute( hSql )
								Call SqlCommit( hSql )
							Else
								Return FALSE
							Call SqlDisconnect( hSql )
			Function: fMontarLog
				Description:
				Returns
				Parameters
					String: plsTexto
				Static Variables
				Local variables
					Number: nLinha
					String: sArquivo
					Number: nTamanho
					Date/Time: dData
					Sql Handle: hSql
				Actions
					Set sArquivo = tblSelecionarArquivo.colsArquivos
					Set nTamanho = tblSelecionarArquivo.colnTamanho
					Set dData =  tblSelecionarArquivo.coldData
					!
					If SqlConnect( hSql )
						If fMPSPrepareProc( hSql, 'Exec dbvcen..LogArquivoCsvI
											@Arquivo		= :sArquivo,
											@Tamanho	= :nTamanho,
											@Data		= :dData,
											@ErroConversao     = :sErroConversao', ':frmArquivoCsv.sArquivoErro' )
							If SqlExecute( hSql )
								Call SqlFetchNext( hSql, lParam )
						Call SqlDisconnect( hSql )
			Function: fDeletarLogArquivo
				Description: Monda as variáveis para execução.
				Returns
					Boolean:
				Parameters
					String: psArquivo
				Static Variables
				Local variables
					Sql Handle: hSql
				Actions
					If SqlConnect( hSql )
						If fMPSPrepareProc( hSql, 'Exec dbvcen..LogArquivoCsvD
										@Arquivo		= :psArquivo', '')
							Call SqlExecute( hSql )
						Call SqlDisconnect( hSql )
			!
