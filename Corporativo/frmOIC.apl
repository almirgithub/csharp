				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Insere nova linha na tabela'
						Set _sTabela = 'tblOIC'
						Set _sTipoComando = 'TabelaNovaLinha'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						Set nLinhaAtual = TBL_MinRow
						Call SalSendClassMessage( SAM_Click, wParam, lParam )
						While SalTblFindNextRow( frmOIC.tblOIC, nLinhaAtual, 0, 0 )
							Call SalTblSetContext( frmOIC.tblOIC, nLinhaAtual )
							If SalIsNull(frmOIC.tblOIC.colnOIC) and SalIsNull(frmOIC.tblOIC.colsOIC)
								Call SalTblDeleteRow( frmOIC.tblOIC, nLinhaAtual, TBL_NoAdjust )
								Return FALSE
						Return TRUE
