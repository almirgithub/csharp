				Functions
				Window Variables
				Message Actions
					On SAM_Create
						Set _bPesquisaCreate = TRUE
						Set _sComandoPesquisa = 	'exec BairroS
											 @Municipio = :frmMunicipio.dfnCod, @cod = :frmBairro.tblBairro.colnCodigo'
						Set _sIntoPesquisa = 	':frmBairro.tblBairro.colnCodigo,
											 :frmBairro.tblBairro.colsDescricao,
											 :frmBairro.tblBairro.colsDescCorreios,
											 :frmBairro.tblBairro.colsObservacao'
						Set _sComandoAtualiza = 'exec BairroU @Cod =:frmBairro.tblBairro.colnCodigo, 
											               @Descricao = :frmBairro.tblBairro.colsDescricao, 
											               @Municipio = :frmBairro.nMunicipio, 
											               @DescCorreios = :frmBairro.tblBairro.colsDescCorreios,
											               @sObservacao = :frmBairro.tblBairro.colsObservacao' || ' '
						Set _sIntoAtualiza = ''
						Set _sComandoInsere = 'exec BairroI @Cod = :frmBairro.tblBairro.colnCodigo, 
											           @Descricao = :frmBairro.tblBairro.colsDescricao, 
											           @Municipio = :frmBairro.nMunicipio, 
											           @DescCorreios = :frmBairro.tblBairro.colsDescCorreios,
											           @sObservacao = :frmBairro.tblBairro.colsObservacao'
						Set _sIntoInsere = ''
						Set _sComandoExclui = 'exec BairroD @Cod =:frmBairro.tblBairro.colnCodigo'
						Set _sIntoExclui = ''
						Set _sTextoStatusBar = 'Bairros do município'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_RowHeaderDoubleClick
						If SalTblQueryRowFlags ( hWndForm, lParam, ROW_New )
							Call SalTblDeleteRow( hWndForm, lParam, TBL_Adjust )
						Else If frmBairro.Permissao() & H_DELETE
							Call SalTblSetRowFlags( hWndForm, lParam, ROW_MarkDeleted, TRUE )
					On SAM_RowHeaderClick
						If frmBairro.Permissao() & H_DELETE
							Call SalTblSetRowFlags( hWndForm, lParam, ROW_MarkDeleted, FALSE )
					On MSG_PesquisaTabela
						Call SalSendClassMessage( MSG_PesquisaTabela, wParam, lParam )
						! Set nMunicipio = nMunicipio * 10
						Call fMPSPrepareProc( hSql, 'exec BairroS_ultimo
												@Municipio = :frmBairro.nMunicipio',
												':frmBairro.nUltSeq' )
						Call SqlExecute( hSql )
						Call SqlFetchNext( hSql, nRetorno )
						If nUltSeq = NUMBER_Null
							Set nUltSeq = 0
						If ( frmBairro.Permissao() & H_INSERT ) or ( frmBairro.Permissao() & H_UPDATE )
							Call SalPostMsg( tblBairro, MSG_Habilita1, wParam, lParam )
							Call SalPostMsg( colnCodigo, MSG_Habilita1, wParam, lParam )
							Call SalPostMsg( colsDescricao, MSG_Habilita1, wParam, lParam )
							Call SalPostMsg( colsObservacao, MSG_Habilita1, wParam, lParam )
							Call SalPostMsg( pbArquiva, MSG_Habilita1, wParam, lParam )
					On MSG_NovaLinha
						Call SalSendClassMessage( MSG_NovaLinha, wParam, lParam )
						Set nUltSeq = nUltSeq + 1
						Call SalTblSetColumnText( tblBairro, 1,SalNumberToStrX( nUltSeq, 0 ) )
					On MSG_Arquiva
						If SalSendClassMessage( MSG_Arquiva, wParam, lParam )
							Call SalSendMsg( frmBairro, MSG_MudaSemaforo, S_Verde, lParam )
			!
