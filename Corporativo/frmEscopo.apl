		Functions
		Window Parameters
		Window Variables
		Message Actions
			On SAM_Create
				Set _sTextoStatusBar = 'Cadastro de Escopo'
				!
				Set _sCriacaoConexao = 'Default'
				Set _nEstadoInicialForm = EF_RegistroNovo
				!
				! ----------------------Pesquisa---------------------
				!
				Set _sComandoPesquisa = 'Exec EscopoS
							@Escopo		= :frmEscopo.dfnEscopo'
				Set _sListaINTOPesquisa = '
						:frmEscopo.dfnEscopo,
						:frmEscopo.dfsDecricao,
						:frmEscopo.dfsIncluiKitGNV,
						:frmEscopo.dfsRetiraKitGNV,
						:frmEscopo.dfsExigeValidadedoCSV,
						:frmEscopo.dfsAtivo'
				!
				! ----------------------Inserir---------------------
				!
				Set _sComandoInclusao = 'Exec dbvcen..EscopoI
							@Escopo		   = :frmEscopo.dfnEscopo,
							@Descricao	   = :frmEscopo.dfsDecricao,
							@iIncluiGNV	   = :frmEscopo.dfsIncluiKitGNV,
							@iRetiraGNV	   = :frmEscopo.dfsRetiraKitGNV,
							@iExigeDataValidade = :frmEscopo.dfsExigeValidadedoCSV,
							@siAtivo		   = :frmEscopo.dfsAtivo'
				Set _bLimpaTelaAposInclusao = FALSE
				!
				! ----------------------Alterar---------------------
				!
				Set _sComandoAtualizacao = 'Exec dbvcen..EscopoU
							@Escopo		  = :frmEscopo.dfnEscopo,
							@Descricao	  = :frmEscopo.dfsDecricao,
						                @iIncluiGNV	   = :frmEscopo.dfsIncluiKitGNV,
							@iRetiraGNV	   = :frmEscopo.dfsRetiraKitGNV,
							@iExigeDataValidade = :frmEscopo.dfsExigeValidadedoCSV,
							@siAtivo		   = :frmEscopo.dfsAtivo '
				Set _sListaINTOAtualizacao = ''
				Set _bLimpaTelaAposAtualizacao = FALSE
				!
				! ----------------------Excluir----------------
				!
				Set _sComandoRemocao = 'Exec dbvcen..EscopoE
							@Escopo		= :frmEscopo.dfnEscopo'
				Set _sListaINTORemocao = ''
				Set _bLimpaTelaAposRemocao = TRUE
				!
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalCenterWindow ( hWndForm )
			On SAM_CreateComplete
				Call SalSendMsg( pbArquivar, MSG_Habilita1, wParam, lParam )
				Call SalSendMsg( pbExcluir, MSG_Desabilita1, wParam, lParam )
				Call SalSendMsg( pbPesquisar, MSG_Habilita1, wParam, lParam )
			On MSG_Limpa
				Call SalSendClassMessage( MSG_Limpa, wParam, lParam )
				!
				Call SalSendMsg( pbExcluir, MSG_Desabilita1, wParam, lParam )
				Call SalPostMsg( pbPesquisar, MSG_Habilita1, wParam, lParam )
				Call SalPostMsg( pbProximo, MSG_Desabilita1, wParam, lParam )
				Call SalSendMsg( pbAnterior, MSG_Desabilita1, wParam, lParam )
			On UDM_AposPesquisa
				Call SalSendMsg( dfnEscopo, MSG_Desabilita1, wParam, lParam )
