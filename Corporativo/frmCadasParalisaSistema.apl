				Functions
				Window Variables
				Message Actions
					On SAM_Create
						! ------------------------Selecionar------------------
						!
						Set _sComandoPesquisa = 'ParalisacaoSistemaS
								@Sistema  = :frmCadasParalisaSistema.dfnSistema'
						Set _sIntoPesquisa = 
								':frmCadasParalisaSistema.tblParalisacao.colnSeq, 
								:frmCadasParalisaSistema.tblParalisacao.coldDataParalisacao,
								:frmCadasParalisaSistema.tblParalisacao.coldDataRetorno,
								:frmCadasParalisaSistema.tblParalisacao.colnUsuario,
								:frmCadasParalisaSistema.tblParalisacao.coldDataCadastro,
								:frmCadasParalisaSistema.tblParalisacao.colsMotivo'
						!
						! ------------------------Atualizar------------------
						!
						Set _sComandoAtualiza = 'ParalisacaoSistemaU
								@Sistema		= :frmCadasParalisaSistema.dfnSistema, 
								@Seq		   	= :frmCadasParalisaSistema.tblParalisacao.colnSeq,
								@DataParalisacao	    	= :frmCadasParalisaSistema.tblParalisacao.coldDataParalisacao,
								@DataRetorno		= :frmCadasParalisaSistema.tblParalisacao.coldDataRetorno,
								@Usuario		                = :frmCadasParalisaSistema.tblParalisacao.colnUsuario,
								@DataCadastro		= :frmCadasParalisaSistema.tblParalisacao.coldDataCadastro,
								@Motivo 		                = :frmCadasParalisaSistema.tblParalisacao.colsMotivo'
						!
						! ------------------------Inserir------------------
						!
						Set _sComandoInsere = 'ParalisacaoSistemaI
								@Sistema		= :frmCadasParalisaSistema.dfnSistema, 
								@Seq		   	= :frmCadasParalisaSistema.tblParalisacao.colnSeq, 
								@DataParalisacao	      	= :frmCadasParalisaSistema.tblParalisacao.coldDataParalisacao,
								@DataRetorno		= :frmCadasParalisaSistema.tblParalisacao.coldDataRetorno,
								@Usuario	                                = :frmCadasParalisaSistema.tblParalisacao.colnUsuario,
								@DataCadastro		= :frmCadasParalisaSistema.tblParalisacao.coldDataCadastro,
								@Motivo 			= :frmCadasParalisaSistema.tblParalisacao.colsMotivo'
						!
						! ------------------------Excluir------------------
						!
						Set _sComandoExclui = 'ParalisacaoSistemaD
								@Seq			= :frmCadasParalisaSistema.tblParalisacao.colnSeq'
								
						!
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
						Set nSeqAnt = 0
					On SAM_FetchRowDone
						If tblParalisacao.colnSeq > nSeqAnt
							Set nSeqAnt = tblParalisacao.colnSeq
					On MSG_PesquisaTabela
						Call SalSendClassMessage( MSG_PesquisaTabela, wParam, lParam )
					On MSG_NovaLinha
						Call SalSendClassMessage( MSG_NovaLinha , wParam, lParam )
						Set tblParalisacao.colnSeq = nSeqAnt + 1
						Set nSeqAnt = nSeqAnt +1
					On UDM_AposInclusao
						Call SalSendClassMessage( UDM_AposInclusao, wParam, lParam )
						Call SalSendMsg( frmCadasParalisaSistema.tblParalisacao, MSG_PesquisaTabela, wParam, lParam )
