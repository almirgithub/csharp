		Functions
		Window Parameters
		Window Variables
			String: sNomeEmpresa
			String: sCNPJ
			String: sCPFCNPJ
			String: sNomePessoa
			Number: nErro
			String: sDevice
			String: sDriver
			String: sPorta
			String: sDeviceDef
			String: sDriverDef
			String: sPortaDef
		Message Actions
			On SAM_Create
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalCenterWindow( hWndForm )
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo', 'Relatório de Financeiras', 'Relatorio de Financeiras' )
				Return TRUE
			On SAM_ReportFetchInit
				Call fMPSPrepareProc( hSql, 'exec RelCadastroEmpresaSocioS', ':sCNPJ, :sNomeEmpresa, :sCPFCNPJ, :sNomePessoa' )
				If NOT  SqlExecute( hSql )
					Return FALSE
				Call SqlGetResultSetCount( hSql, nErro )
				Return TRUE
			On SAM_ReportFetchNext
				Call SqlFetchNext( hSql, nErro )
				! Call SalFmtFieldToStr( dfsCGC, sCGC, TRUE )
				If nErro != FETCH_EOF
					Return 1
				Else
					Return 0
			On SAM_ReportFinish
				Call SalPrtSetDefault(sDeviceDef,sDriverDef,sPortaDef)
