			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				Call SalEnableWindow( dfsDoc )
				! Call SalEnableWindow( cmbTipoPessoa )
				Call SalFmtSetInputMask ( dfsDoc, '99999999999999' )
			On MSG_Limpa
				Call SalFmtSetInputMask ( dfsDoc, '999999999999999999' )
				Call SalSendClassMessage( MSG_Limpa, wParam, lParam )
				! Call SalEnableWindow( dfsDoc )
				! Call SalEnableWindow( cmbTipoPessoa )
			On SAM_Destroy
				If hWndEnderProprietario != hWndNULL
					Call SalSendMsg( hWndEnderProprietario, MSG_Sai, wParam, lParam )
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
			On UDM_AntesInclusao
				! If not SalIsNull(cmbTipoDocumentoProp)
					If SalIsNull(dfnNumRG)
						Call SalMessageBeep( 0 )
						Call SalMessageBox( 'O campo Número é obrigatório ao escolher um tipo de documento!', 'Atenção', MB_IconExclamation | MB_Ok )
					Else If SalIsNull(cmbOrgaoProp)
						Call SalMessageBeep( 0 )
						Call SalMessageBox( 'O campo Órgão Expedidor é obrigatório ao escolher um tipo de documento!', 'Atenção', MB_IconExclamation | MB_Ok )
					Else If SalIsNull(cmbUF)
						Call SalMessageBeep( 0 )
						Call SalMessageBox( 'O campo UF é obrigatório ao escolher um tipo de documento!', 'Atenção', MB_IconExclamation | MB_Ok )
					Return FALSE
				! Else
					Return TRUE
				Return SalSendMsg( hWndForm, UDM_AntesAtualizacao, 0, 0 )
			On UDM_AntesPesquisa
				If SalIsNull( dfsDoc )
					Call SalMessageBeep( 0 )
					Call SalMessageBox( 'O campo CPF/CNPJ é obrigatório para pesquisa!', 'Atenção', MB_IconExclamation | MB_Ok )
					Return FALSE
				Return TRUE
			On UDM_AntesAtualizacao
				If rbOculto
					Call SalMessageBeep( 0 )
					Call SalMessageBox( 'É obrigatório selecionar um tipo de documento!', 'Atenção', MB_IconExclamation | MB_Ok )
					Return FALSE
				Else
					If rbCPFProp and SalIsNull(cmbTipoDocumentoProp)
						Call SalMessageBeep( 0 )
						Call SalMessageBox( 'O campo Documento de Identificação!', 'Atenção', MB_IconExclamation | MB_Ok )
						Call SalSetFocus(cmbTipoDocumentoProp)
						Return FALSE
				If not SalIsNull(cmbTipoDocumentoProp)
					If SalIsNull(dfnNumRG)
						Call SalMessageBeep( 0 )
						Call SalMessageBox( 'O campo Número é obrigatório ao escolher um tipo de documento!', 'Atenção', MB_IconExclamation | MB_Ok )
						Call SalSetFocus(dfnNumRG)
						Return FALSE
					Else If SalIsNull(cmbOrgaoProp)
						Call SalMessageBeep( 0 )
						Call SalMessageBox( 'O campo Órgão Expedidor é obrigatório ao escolher um tipo de documento!', 'Atenção', MB_IconExclamation | MB_Ok )
						Call SalSetFocus(cmbOrgaoProp)
						Return FALSE
					Else If SalIsNull(cmbUF)
						Call SalMessageBeep( 0 )
						Call SalMessageBox( 'O campo UF é obrigatório ao escolher um tipo de documento!', 'Atenção', MB_IconExclamation | MB_Ok )
						Call SalSetFocus(cmbUF)
						Return FALSE
				Else
					If Not SalIsNull(dfnNumRG) or not SalIsNull(cmbOrgaoProp) or not SalIsNull(cmbUF)
						Call SalMessageBeep( 0 )
						Call SalMessageBox( 'Informe um tipo de documento de identificação.', 'Atenção', MB_IconExclamation | MB_Ok )
						Call SalSetFocus(dfnNumRG)
						Return FALSE
				Return TRUE
			On UDM_AposAtualizacao
				Call SalDisableWindow( pbAnterior )
				Call SalDisableWindow( pbProximo )
				Call SalMessageBox( 'Atualização Realizada com Sucesso !!!', 'Atenção', MB_IconExclamation | MB_Ok )
				Return TRUE

