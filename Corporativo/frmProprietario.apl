									Set rbCGC = TRUE
									Call SalSendMsg( rbCGC, SAM_Click, 0, 0 )
								Else
									Set rbOculto = TRUE
									Call SalSendMsg( rbOculto, SAM_Click, 0, 0 )
						Return TRUE
					On MSG_Pesquisa
						If dfnPropTipoDoc = 2
							Call SalFmtSetInputMask ( hWndItem, '99.999.999/9999-99' )
						Else If dfnPropTipoDoc = 1 OR dfnPropTipoDoc = 3 OR dfnPropTipoDoc = 4
							Call SalFmtSetInputMask ( hWndItem, '999.999.999-99' )
						Else
							Call SalFmtSetInputMask ( dfsDoc, '99999999999999' )
