				Set _sTextoStatusBar = 'Tela de cadastro Cargo do Usuário'
				!
				Set _sCriacaoConexao = 'Default'
				Set _sComandoPesquisa = 'exec CargoUsuarioS @CargoUsuario = :frmCargoUsuario.dfnCodigo'
				Set _sListaINTOPesquisa = ':frmCargoUsuario.dfnCodigo, :frmCargoUsuario.dfsDescricao'
				!
				Set _bLimpaTelaAposInclusao = FALSE
				Set _sComandoInclusao = 'exec CargoUsuarioI @Descricao = :frmCargoUsuario.dfsDescricao'
				!
				Set _bLimpaTelaAposAtualizacao = FALSE
				Set _sComandoAtualizacao = 'exec CargoUsuarioA @CargoUsuario = :frmCargoUsuario.dfnCodigo,
						@Descricao = :frmCargoUsuario.dfsDescricao'
				Set _sListaINTOAtualizacao = ''
				!
				Set _bLimpaTelaAposRemocao = TRUE
				Set _sComandoRemocao = 'exec CargoUsuarioE @CargoUsuario = :frmCargoUsuario.dfnCodigo'
				Set _sListaINTORemocao = ''
				!
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalCenterWindow ( hWndForm )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				!
				Call SalSendMsg( pbExcluir, MSG_Desabilita1, wParam, lParam )
				Call SalSendMsg( pbPesquisar, MSG_Habilita1, wParam, lParam )
				!
				! If frmTipoUsuario.fObtemPermissao('frmTipoUsuario') = 1
					Call SalDisableWindow( pbArquivar )
					Call SalDisableWindow( pbExcluir )
				! Else If frmTipoUsuario.fObtemPermissao('frmTipoUsuario') = 15
					Call SalEnableWindow( pbArquivar )
					Call SalEnableWindow( pbExcluir )

