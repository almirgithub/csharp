					On UDM_Pesquisa
						If SqlConnect( hSqlTabela )
							Call SqlSetOutMessage ( hSqlTabela, 900000 )
							If SybPrepareProc( hSqlTabela, 'exec dbvcen..EstabCredMsgR
										@Credenciado= :frmEstabCredMsg.nCredenciado,
										@Sequencial   = :frmEstabCredMsg.tblMsgs.colnSequencial',
										':sRemetente,
										:dEmissao,
										:sAssunto,
										:sEstabelecimento,
										:lsCorpoMensagemFinal' )
								If SqlExecute( hSqlTabela )
									Call SqlFetchNext( hSqlTabela, nRetornoFetch )
									While nRetornoFetch != FETCH_EOF
										Call SalTblSetContext( tblRelEstabCredMsg, nRetornoFetch )
										Call SalTblInsertRow( tblRelEstabCredMsg, TBL_MaxRow )
										Call SalDateToStr ( dEmissao, sEmissao )
										Call SalTblSetColumnText(tblRelEstabCredMsg,1, sRemetente)
										Call SalTblSetColumnText(tblRelEstabCredMsg,2, sEmissao)
										Call SalTblSetColumnText(tblRelEstabCredMsg,3, sAssunto)
										Call SalTblSetColumnText(tblRelEstabCredMsg,4, sEstabelecimento)
										Call SalTblSetColumnText(tblRelEstabCredMsg,5, lsCorpoMensagemFinal)
										Call SqlFetchNext( hSqlTabela, nRetornoFetch )
									Set nLinha = NUMBER_Null
									Call SalTblSetRowFlags( tblRelEstabCredMsg, 0 , ROW_New,  FALSE )
									While SalTblFindNextRow( tblRelEstabCredMsg, nLinha, 0, 0 )
										Call SalTblSetRowFlags( tblRelEstabCredMsg, nLinha , ROW_New,  FALSE )
									Call SqlDisconnect( hSqlTabela )
									Return TRUE
						Call fMPSDisconnect( hSqlTabela )
						Return FALSE
