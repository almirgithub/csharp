				Message Actions
					On SAM_Create
						Set _sTabela = ''
						Set _sTextoStatusBar = 'Imprime o relatório'
						Set _nPermissao = 15
						Set _sTipoComando = 'Comum'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						Set nImprimir= 0
						! --- procurar e imprimir as mensagens marcadas
						Set nLinhaAtual = TBL_MinRow
						While SalTblFindNextRow( tblMsgs, nLinhaAtual, 0, 0 )
							Call SalTblSetContext( tblMsgs, nLinhaAtual )
							If tblMsgs.colsImprimir = 'S'
								Set nImprimir= 1
								! --- 
								Call SalSendMsg( tblRelEstabCredMsg, UDM_Pesquisa, 0, 0 )
								Call SalTblSetContext( tblRelEstabCredMsg, 0 )
								! --- validar
								If tblRelEstabCredMsg.colsRemetente = STRING_Null
									! --- desmarcar a mensagem
									Set tblMsgs.colsImprimir= 'N'
								Else
									! ---- saída do relatório em Tela
									! If hWndReport  != hWndNULL
										Call SalSendMsg( hWndReport, SAM_Close, wParam, lParam)
									! Set hWndReport= SalCreateWindow( frmReportView, hWndForm )
									! Call SalSetWindowText( hWndReport, 'Mensagem do Credenciado' )
									! Call SalReportTableView( tblRelEstabCredMsg, hWndReport, 'EstabCredMsg.qrp', lParam )
									!
									! ---- saída do relatório na Impressora
									Call SalReportTablePrint( tblRelEstabCredMsg,  'EstabCredMsg.qrp',  nParametroArray, lParam )
								!
						! ---
						If nImprimir = 0
							Call SalMessageBeep( 0 )
							Call SalMessageBox( 'É preciso marcar a(s) mensagem(ns) a ser(em) impressa(s)', 'Atenção', MB_IconExclamation | MB_Ok )
						!
						Return TRUE
