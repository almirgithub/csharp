						Set _bSempreHabilitado = FALSE
						!
						Set _sComandoPesquisa = 'exec dbvcen..LacreMsgEntEstradaCredenciadoS 
											@Credenciado = :frmEstabCredMsg.dfnCredenciado'
						Set _sIntoPesquisa = ':frmEstabCredMsg.tblMsgs.colnSequencial, 
										:frmEstabCredMsg.tblMsgs.colsTitulo, 
										:frmEstabCredMsg.tblMsgs.colsMensagem'
						!
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					! On SAM_DoubleClick
						! Call SalTblSetContext( hWndForm, lParam )
						Set nSequencial = tblMsgs.colnSequencial
						Call SalSendMsg( frmEstabCredMsg, SAM_Close, 0, 0 )
			! -- #18441 [faml] -- ini
			Pushbutton: pbImprimir
				Class Child Ref Key: 0
				Class ChildKey: 0
				Class: cQCK1PushButton
				Property Template:
				Class DLL Name:
				Title: Imprimir
				Window Location and Size
					Left: 6.95"
					Top: 2.479"
					Width:  1.2"
					Width Editable? Class Default
					Height: 0.292"
