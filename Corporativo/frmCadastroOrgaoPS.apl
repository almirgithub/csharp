								If NOT SalIsNull(frmCadastroOrgaoPS.tblOrgao.colsDescricao)
									Call SalMessageBox('Orgão não encontrado','Aviso',MB_IconExclamation)
								Call SalTblDeleteRow( frmCadastroOrgaoPS.tblOrgao, nLinhaTbl, TBL_NoAdjust)
						!
						! Set _sConsultaBD = 'exec dbvcen..OrgaoPsUltimoNumeroS'
						! If fMPSConnect( hSqlOrgao )
							Call fMPSPrepareProc( hSqlOrgao, 'exec dbvcen..OrgaoPsUltimoNumeroS' ,
									':frmCadastroOrgaoPS.tblOrgao.colnCodigo')
							Call SqlExecute( hSqlOrgao )
