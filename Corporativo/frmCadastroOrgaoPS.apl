						Set nLinhaTbl = TBL_MinRow
						While SalTblFindNextRow( frmCadastroOrgaoPS.tblOrgao, nLinhaTbl, 0, 0 )
							Call SalTblSetContext( frmCadastroOrgaoPS.tblOrgao, nLinhaTbl )
