										@sCobraTaxa	= :frmCadastroOrgaoPS.tblOrgao.colsTaxa'
						!
						! --------------------------Exclusão--------------------------
						Set _sComandoExclui = 'Exec dbvcen..CadastroOrgaopsE
										@nCodigo	= :frmCadastroOrgaoPS.tblOrgao.colnCodigo'
						!
						! ------------------------Atualização-----------------------
						Set _sComandoAtualiza = 'Exec dbvcen..CadastroOrgaopsA
									@nCodigo 	= :frmCadastroOrgaoPS.tblOrgao.colnCodigo,
									@sDescricao	= :frmCadastroOrgaoPS.tblOrgao.colsDescricao,
