			!
			Number: nID
		Message Actions
			On SAM_Create
				Set _nEstadoInicialForm = 1
				Set _sTextoStatusBar = 'Tela de impressão de serviços combinados'
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalCenterWindow( hWndForm )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				Set dfsSaida = 'I'
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo', 'Relatório de Serviços Combinados', 'Relatório de Serviços Combinados' )
				Return TRUE
			On SAM_ReportFetchInit
				Call fMPSPrepareProc( hSql,'exec ServicoCombinadoS_relat @TipoServico = :frmRelServicoCombinado.dfnServico',
						':frmRelServicoCombinado.nCod, :frmRelServicoCombinado.sDescricao, :frmRelServicoCombinado.nCodSe,
						:frmRelServicoCombinado.sDescSe' )
				If NOT  SqlExecute( hSql )
					Return FALSE
				Call SqlGetResultSetCount( hSql, nErro )
				Return TRUE
			On SAM_ReportFetchNext
				Call SqlFetchNext( hSql, nErro )
				If nErro != FETCH_EOF
					Return 1
				Else
					Return 0
			On SAM_ReportFinish
				Call SalPrtSetDefault(sDeviceDef,sDriverDef,sPortaDef)
			! On SAM_Destroy
				  If hWndReportView != hWndNULL
					  Call SalReportClose( hWndReportView )
					  Call SalDestroyWindow( hWndReportView )
					  Set hWndReportView = hWndNULL
				  Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
