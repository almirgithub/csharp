						Call SalEnableWindow( pbDireitaTodos )
						Call SalEnableWindow( pbEsquerdaTodos )
						Return TRUE
					! On WMSG_KEYDOWN
						Select Case (wParam)
							Case WKEY_TAB
								If NOT SalIsNull(dfsUsuario)
									Call SalSendMsg( lbSetorDisponivel, MSG_Refresh, 0, 0 )
									Call SalSendMsg( lbSetorAssociado, MSG_Refresh, 0, 0 )
									Call SalEnableWindow( pbDireitaTodos )
									Call SalEnableWindow( pbEsquerdaTodos )
								Break 
							Case WKEY_F2
