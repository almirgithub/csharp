						Set bVetProx[nFicha] = FALSE
					Else
						Call SalSendMsg( pbProximo, MSG_Habilita1, lParam, wParam )
						Set bVetProx[nFicha] = TRUE
						Set nProximo = nProximo + 1
					If nProximo  > 0
						Call SalSendMsg( pbAnterior, MSG_Habilita1, lParam, wParam )
						Set bVetAnt[nFicha] = TRUE
					Else
						Call SalSendMsg( pbAnterior, MSG_Desabilita1, lParam, wParam )
						Set bVetAnt[nFicha] = FALSE
			Function: Anterior
				Description:
				Returns
				Parameters
					Sql Handle: phSql
				Static Variables
				Local variables
				Actions
					Call SqlFetchPrevious( phSql, lParam )
					If lParam = FETCH_EOF
						Call SalSendMsg( pbAnterior, MSG_Desabilita1, lParam, wParam )
						Set bVetAnt[nFicha] = FALSE
					Else
						Call SalSendMsg( pbAnterior, MSG_Habilita1, lParam, wParam )
