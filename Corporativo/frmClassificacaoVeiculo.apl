										Break
							On SAM_User
								If tblClassificacao.colnCarroceria != NUMBER_Null
									If SqlConnect( tblClassificacao.hSqlClassificacao )
										If fMPSPrepareProc( tblClassificacao.hSqlClassificacao, 'dbvcen..CarroceriaS_desc @Cod =:tblClassificacao.colnCarroceria', ':tblClassificacao.colsDescCarroceria' )
