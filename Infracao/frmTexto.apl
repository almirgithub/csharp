				Call SalWaitCursor( TRUE )
				Set _sCriacaoConexao = 'Default'
				Set _nEstadoInicialForm = EF_Vazio
				Set _bCriacaoConexaoAuxiliar = TRUE
				!
				! ------------------------------------Pesquisa --------------------------------------------
				!
				Set frmTexto._sComandoPesquisa = 'Exec CertidaoS
								@Certidao 	= :frmTexto.dfnCod'
				Set frmTexto._sListaINTOPesquisa =
						':frmTexto.dfnCod,
						:frmTexto.dfsNome,
						:frmTexto.dfsTipo,
						:frmTexto.dfsNomeQrp,
						:frmTexto.dfsNomeProc,
						:frmTexto.dfsImpNumVias'
				!
				! ---------------------------------Inclusão --------------------------------------------------
				!
				Set frmTexto._sComandoInclusao = 'Exec  CertidaoI
								@Codigo 		= :frmTexto.dfnCod,
								@Descricao 	= :frmTexto.dfsNome,
								@Tipo 		= :frmTexto.dfsTipo,
								@Relatorio 	= :frmTexto.dfsNomeQrp,
								@Procedure 	= :frmTexto.dfsNomeProc,
								@ImpNumVias	= :frmTexto.dfsImpNumVias'
				Set frmTexto._bLimpaTelaAposInclusao = TRUE
				!
				! ----------------------------------- Alteração -------------------------------------------------
				!
				Set frmTexto._sComandoAtualizacao = 'Exec CertidaoU
								@Codigo 		= :frmTexto.dfnCod,
								@Descricao 	= :frmTexto.dfsNome,
								@Tipo 		= :frmTexto.dfsTipo,
								@Relatorio 	= :frmTexto.dfsNomeQrp,
								@Procedure 	= :frmTexto.dfsNomeProc,
								@ImpNumVias	= :frmTexto.dfsImpNumVias'
				Set frmTexto._bLimpaTelaAposAtualizacao = TRUE
				!
				! ---------- Excluão -----------------------------------------------------------------------------------------
				!
				Set frmTexto._sComandoRemocao = 'Exec CertidaoD
								@Codigo		=:frmTexto.dfnCod'
				Set frmTexto._bLimpaTelaAposRemocao = TRUE
				!
