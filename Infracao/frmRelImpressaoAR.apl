.head 8 -  Set nTotMoedaIPVA = nTotMoedaIPVA + nDMoeda
.head 4 +  ! Else 
.head 5 +  If NOT ( nDDebAnt = nDDebito and nDParcela = 99 and nDParcAnt != 99 and nDExeAnt = nDExe or sDGrupo = 'MULTAS COM RECURSO/SUSPENSIVO'  )
.head 6 -  Set nTotReal = nTotReal + nDReal
.head 6 -  Set nTotMoeda = nTotMoeda + nDMoeda
.head 5 +  If ( nDParcela = 99 and  sDGrupo != 'MULTAS COM RECURSO/SUSPENSIVO' )
.head 6 -  Set nTotRealUnica = nTotRealUnica + nDReal
.head 6 -  Set nTotMoedaUnica = nTotMoedaUnica + nDMoeda
