		Functions
			Function: fExportarArquivo
				Description: Gerar o rel. em arquivop do tipo .txt
				Returns
				Parameters
				Static Variables
				Local variables
					FunctionalVar: oArquivo
						Class: cArquivo
					Number: nLinha
				Actions
					Call oArquivo.IniciaLinha(  )
					While SalTblFindNextRow( tblRNFRepasse, nLinha, 0, 0 )
						Call SalTblSetContext( tblRNFRepasse, nLinha )
						If tblRNFRepasse.colsOrgaoAutuante != STRING_Null
							Call oArquivo.NovaLinha(  )
							Call oArquivo.AddStr( tblRNFRepasse.colsOrgaoAutuante, 'Orgao Autuante' )
							Call oArquivo.AddStr( tblRNFRepasse.colsAgenteEquip, 'Agente Equipe' )
							Call oArquivo.AddStr( tblRNFRepasse.colsMunicipio, 'Municipio' )
							Call oArquivo.AddStr( tblRNFRepasse.colsSerie, 'Série' )
							Call oArquivo.AddNumber( tblRNFRepasse.colnAuto, 0, 'Auto' )
							Call oArquivo.AddNumber( tblRNFRepasse.colnInfracao, 0, 'Infração' )
							Call oArquivo.AddStr( tblRNFRepasse.colsCompetencia, 'Competencia' )
							Call oArquivo.AddStr( tblRNFRepasse.colsCategoria, 'Categoria' )
							Call oArquivo.AddStr( tblRNFRepasse.colsOrgaoReceberdor, 'Orgão Recebedor' )
							Call oArquivo.AddNumber( tblRNFRepasse.colnValorPago, 2, 'Valor Pago' )
							Call oArquivo.AddNumber( tblRNFRepasse.colnValorRepasse, 2, 'Valor Repasse' )
