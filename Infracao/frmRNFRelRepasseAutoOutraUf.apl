				Font Enhancement: Class Default
				Picture File Name:
				Picture Transparent Color: Class Default
				Image Style: Class Default
				Text Color: Class Default
				Background Color: Class Default
				Message Actions
					On SAM_Create
						Set _sNroCamposIgnorados = 'NENHUM'
						Set _sTextoStatusBar = 'Imprime o relatório'
						Set _sTipoComando = 'Comum'
						Set _bValidaCamposNulos = TRUE
						Set _nPermissao = EF_PesquisaHabilitada | EF_RegistroExistente | EF_RegistroNovo  | EF_Vazio
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						If SalIsNull( dfnRNFOrgaoAutuante )
							Call SalMessageBox( 'Informe o Órgão Autuante!', 'Atenção', MB_IconExclamation | MB_Ok )
							Call SalSetFocus( dfnRNFOrgaoAutuante )
							Set dfsRNFAgenteEquip= STRING_Null
							Return FALSE
						If SalDateYear( dfdRNFVigencia ) < 1900 or SalDateMonth( dfdRNFVigencia ) < 1 or SalDateMonth( dfdRNFVigencia ) > 12
							Call SalMessageBeep( 0 )
							Call SalMessageBox( 'Vigência inválida!', 'Atenção!', MB_Ok |MB_IconExclamation )
							Call SalSetFocus( dfdRNFVigencia )
							Return FALSE
						! -- pedir confirmação
						If SalIsNull( dfnRNFMunicipio ) and dfnRNFOrgaoAutuante = 117200 !-- DER
							If SalMessageBox( 'Com esses dados, o relatório será bastante demorado. Confirma a emissão?', 'Confirmação', MB_IconQuestion|MB_YesNo ) = IDNO
								Call SalSetFocus( dfnRNFOrgaoAutuante )
								Return FALSE
						! If not SalIsNull( dfnRNFAgenteEquip ) and SalIsNull( dfnRNFOrgaoAutuante )
							Call SalMessageBox( 'É preciso informar também o orgão autuante, sempre que informar um agente equipamento!', 'Atenção', MB_IconExclamation | MB_Ok )
							Call SalSetFocus( dfnRNFOrgaoAutuante )
							Set dfnRNFAgenteEquip= NUMBER_Null
							Set dfsRNFAgenteEquip= STRING_Null
							Return FALSE
						!
						Set frmRNFRelRepasseAutoOutraUf.tblRNFRepasse.nPermissao = H_SELECT
						Call SalSendMsg( frmRNFRelRepasseAutoOutraUf.tblRNFRepasse, MSG_PesquisaTabela, lParam, wParam )
						!
						If dfsSaida = 'T'
							If hWndReport  != hWndNULL
								Call SalSendMsg( hWndReport, SAM_Close, wParam, lParam)
							Set hWndReport= SalCreateWindow( frmReportView, hWndForm )
							Call SalSetWindowText( hWndReport,'Repasse RENAINF Outras UF - Autos de Infração' )
							Call SalReportTableView( frmRNFRelRepasseAutoOutraUf.tblRNFRepasse, hWndReport,  'RNFRelRepasseAutoOutraUf.qrp', lParam )
							! Call SalReportTableCreate( 'RNFRelRepasseAutoOutraUf.qrp', frmRNFRelRepasseAutoOutraUf.tblRNFRepasse, lParam )
						Else If dfsSaida = 'I'
							Call SalReportTablePrint( frmRNFRelRepasseAutoOutraUf.tblRNFRepasse,  'Repasse RENAINF Outras UF - Autos de Infração', nParametroArray, lParam )
						Else If dfsSaida = 'A'
							Call fExportarArquivo()
