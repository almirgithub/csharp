				! Extrai o CPF/CGC do nome
				Set sCPFCGC = SalStrLeftX( sNome, 14 )
				Set sNome = SalStrRightX( sNome, SalStrLength( sNome ) - 15 )
				! Faz formatação de CGC / CPF
				Call SalFmtSetInputMask ( dfsCGCCPF, '' )
				If DetranCGC( sCPFCGC )
					Call SalFmtSetInputMask ( dfsCGCCPF, '99.999.999/9999-99' )
				Else If DetranCPF( ( sCPFCGC ) )
					Call SalFmtSetInputMask ( dfsCGCCPF, '999.999.999-99' )
				Set dfsCGCCPF = sCPFCGC
				Set sCPFCGC = dfsCGCCPF
				! Converte número do código de barras
				Call fCodigoBarra( sNumBarra, sNumBarra )
				Return TRUE
			On SAM_ReportFinish
				Call SqlPrepareAndExecute( hSql, 'cancel' )
				Call SalSendClassMessage( SAM_ReportFinish, wParam, lParam )
				Call SqlImmediate( 'select null' )
			On SAM_Destroy
				Set hWndJanela = hWndNULL
				Call SalFmtKeepMask( FALSE )
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
