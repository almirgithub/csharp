				Message Actions
					On SAM_Create
						Set _sNroCamposIgnorados = 'NENHUM'
						Set _sTextoStatusBar = 'Imprime o relatório na saída selecionada'
						Set _sTipoComando = 'Comum'
						Set _bValidaCamposNulos = TRUE
						Set _nPermissao = EF_PesquisaHabilitada | EF_RegistroExistente | EF_RegistroNovo  | EF_Vazio
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						! --------------------------------- validações
						If SalIsNull( dfnMunicipio )
							Call SalMessageBeep(MB_IconExclamation)
							Call SalMessageBox('Informe o município!', 'Atenção', MB_IconExclamation | MB_Ok)
							Call SalSetFocus(dfnMunicipio)
							Return FALSE
						!
						If SalIsNull(dfdVigenciaIni)
							Call SalMessageBeep( MB_IconExclamation )
							Call SalMessageBox( 'Informe a vigência inicial (mês/ano)!', 'Atenção', MB_IconExclamation | MB_Ok )
							Call SalSetFocus(dfdVigenciaIni)
							Return FALSE
						!
						If SalIsNull(dfdVigenciaFim)
							Call SalMessageBeep( MB_IconExclamation )
							Call SalMessageBox( 'Informe a vigência final (mês/ano)!', 'Atenção', MB_IconExclamation | MB_Ok )
							Call SalSetFocus(dfdVigenciaFim)
							Return FALSE
						!
						If dfdVigenciaIni > dfdVigenciaFim
							Call SalMessageBeep( MB_IconExclamation )
							Call SalMessageBox( 'Período inválido! Vigência inicial MAIOR que final!', 'Atenção', MB_IconExclamation | MB_Ok )
							Call SalSetFocus(dfdVigenciaFim)
							Return FALSE
						! --- ano e mês do período de vigência
						Call SalDateToStr( dfdVigenciaIni, vsVigenciaIni )
						Set nAno1= SalStrToNumber( SalStrMidX( vsVigenciaIni, 0, 4 )  )
						Set nMes1= SalStrToNumber( SalStrMidX( vsVigenciaIni, 5, 2 )  )
						Call SalDateToStr( dfdVigenciaFim, vsVigenciaFim )
						Set nAno2= SalStrToNumber( SalStrMidX( vsVigenciaFim, 0, 4 )  )
						Set nMes2= SalStrToNumber( SalStrMidX( vsVigenciaFim, 5, 2 )  )
						Call SalDateToStr( dfdVigenciaIni, vsVigencia )
						If nAno1 < 2004
							Call SalMessageBeep( MB_IconExclamation )
							Call SalMessageBox( 'Período inválido! Informe vigência a partir de 01/2004', 'Atenção', MB_IconExclamation | MB_Ok )
							Call SalSetFocus(dfdVigenciaFim)
							Return FALSE
						!
						If ( nAno1 != nAno2 ) or ( ( nMes2 - nMes1 ) > 3 )
							Set nResposta= SalMessageBox ( 'Esse período de vigência pode ser muito demorado. Confirma?', 'Confirmação', MB_IconQuestion | MB_YesNo )
							If nResposta = IDNO
								Call SalSetFocus(dfdVigenciaFim)
								Return FALSE
						!
						! --------------------------------------- setar variáveis do relatório
						Set vsVigencia= SalStrMidX(vsVigenciaIni,5,3) || SalStrMidX(vsVigenciaIni,0,4)
						If dfdVigenciaFim != dfdVigenciaIni 
							Set vsVigenciaFim= SalStrMidX(vsVigenciaFim,5,3) || SalStrMidX(vsVigenciaFim,0,4)
							Set vsVigencia= 'de ' || vsVigencia || ' até ' || vsVigenciaFim
						!
						! -------------------- exibir o relatório
						If rbImpressora
							Call SalReportPrint( hWndForm, 'RelRepInfraMun.qrp' ,
										'vnValorInfracao,
										vnQuantidade,
										sGlobalSecretaria,
										vdVigencia,
										vnValorFunset,
										vnValorTaxaAdm,
										vnValorDetran,
										vnValorMunicipio
										SqlUser,
										vsMunicipio,
										vsVigencia,	
										vsInfracaoLeve' ,
										'nVlrInfracao,
										nQtde,
										sReportHeader,
										dVigencia,
										nVlrFunset,
										nVlrTaxaAdm,
										nVlrDetran,
										nVlrMunicipio
										sUser,
										sMunicipio,
										sVigencia,
										sInfracaoLeve' ,
										1,RPT_PrintAll,0,0,nErro)
						Else
							! ------ tela
							Set hWndReportView= SalCreateWindow( frmReportView, hWndForm )
							Call SalSetWindowText( hWndReportView,  'Resumo de Infrações do Município - Repasse' )
							!
							Call SalReportView( hWndForm, hWndReportView, 'RelRepInfraMun.qrp' ,
										'vnValorInfracao,
										vnQuantidade,
										sGlobalSecretaria,
										vdVigencia,
										vnValorFunset,
										vnValorTaxaAdm,
										vnValorDetran,
										vnValorMunicipio
										SqlUser,
										vsMunicipio,
										vsVigencia,	
										vsInfracaoLeve' ,
										'nVlrInfracao,
										nQtde,
										sReportHeader,
										dVigencia,
										nVlrFunset,
										nVlrTaxaAdm,
										nVlrDetran,
										nVlrMunicipio
										sUser,
										sMunicipio,
										sVigencia,
										sInfracaoLeve' ,
										nErro )
