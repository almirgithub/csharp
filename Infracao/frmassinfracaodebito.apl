			On SAM_Create
				Set _nEstadoInicialForm = EF_Vazio
				Set _sTextoStatusBar = 'Tela de associação de tipos de débitos à infração de um determinado órgão autuante'
				! Variáveis para conexão
				Set _bCriacaoConexaoAuxiliar = TRUE
				Set _sCriacaoConexao = 'Default'
				Set _sNomeBD = ''
				Set _sIdUsuario = ''
				Set _sPwdUsuario = ''
				! Variáveis para as procedure's
				Set _bLimpaTelaAposAtualizacao = FALSE
				Set _bLimpaTelaAposInclusao = FALSE
				Set _bLimpaTelaAposRemocao = TRUE
				Set _sComandoAtualizacao = 'exec tmpInfracaoTipoDebitoU @OrgaoAutuante = :frmAssInfracaoDebito.dfnOrgaoAutuante, @TipoDebito = :frmAssInfracaoDebito.dfnTipoDebito,@AgenteEquip=:frmAssInfracaoDebito.dfnAgenteEquip'
				Set _sComandoInclusao = 'exec tmpInfracaoTipoDebitoU @OrgaoAutuante=:frmAssInfracaoDebito.dfnOrgaoAutuante,@TipoDebito=:frmAssInfracaoDebito.dfnTipoDebito,@AgenteEquip=:frmAssInfracaoDebito.dfnAgenteEquip'
				! Set _sComandoInclusao='exec DebitoInfracaoI @OrgaoAutuante=:frmAssInfracaoDebito.dfnOrgaoAutuante,@TipoDebito=:frmAssInfracaoDebito.dfnTipoDebito,@AgenteEquip=:frmAssInfracaoDebito.dfnAgenteEquip'
				! Set _sComandoPesquisa = 'exec TipoDebitoS_coddesc_setor @Setor = :nGlobalSetor'
				Set _sComandoPesquisa = 'select  null,null'
				Set _sComandoRemocao = 'exec DebitoInfracaoD @OrgaoAutuante = :frmAssInfracaoDebito.dfnOrgaoAutuante,@AgenteEquip=:frmAssInfracaoDebito.dfnAgenteEquip'
				! Variáveis para o retorno
				Set _sListaINTOAtualizacao = ''
				Set _sListaINTOInclusao = ''
				! Set _sListaINTOPesquisa = ':frmAssInfracaoDebito.dfnTipoDebito, :frmAssInfracaoDebito.dfsTipoDebito'
				Set _sListaINTOPesquisa = ':frmAssInfracaoDebito.nTipoDebito,:frmAssInfracaoDebito.sTipoDebito'
				Set _sListaINTORemocao = ''
				Call SalCenterWindow( hWndForm )
				Set sSetorUsuarioN = SalStrMidX(SalNumberToStrX(nGlobalSetor,0),0,SalStrLength( SalNumberToStrX(nGlobalSetor,0) )-1)
				Set nSetorUsuario = SalStrToNumber(sSetorUsuarioN)
				! Set nSetorUsuario = nGlobalSetor/10
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
			On UDM_AposAtualizacao
				! Call SalDisableWindow( pbAnterior )
				! Call SalDisableWindow( pbProximo )
				Return SalSendClassMessage( UDM_AposAtualizacao, wParam, lParam )


