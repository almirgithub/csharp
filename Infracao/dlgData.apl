				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Data do vencimento que corresponde à data inicial do intervalo'
						Set _sNomeParaMsgs = 'Data de vencimento inicial'
						Set _sTipoMsgNulo = 'PADRAO'
						Set _bVisivel = TRUE
						! Variáveis para indentificar o Componente Chave
						Set _bComponenteChave = TRUE
						Set _bPermiteNulos = TRUE
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Validate
						If NOT SalIsNull( dfdData )
							If NOT (SalIsValidDateTime( dfdData ))
								Call SalMessageBeep( 0 )
								Call SalMessageBox( 'Data inválida.', 'Atenção', MB_Ok | MB_IconExclamation )
								Call SalClearField( dfdData )
								Return VALIDATE_Cancel
							Else If SalDateYear( dfdData ) < 1753
								Call SalMessageBeep( 0 )
								Call SalMessageBox( 'Ano inválido.', 'Atenção', MB_Ok | MB_IconExclamation )
								Call SalClearField( dfdData )
								Return VALIDATE_Cancel
						Call SalSendClassMessage( SAM_Validate, wParam, lParam )
