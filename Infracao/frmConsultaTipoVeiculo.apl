				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Escolha o Município'
						Set _sNomeParaMsgs = 'Município'
						Set _sTipoMsgNulo = 'PADRAO'
						Set _bVisivel = TRUE
						Set _sRefresh = 'Create'
						Set _sNomeCampoCodigo = 'sMes'
						Set _sConsultaBD = 'exec dbvcen..InfracaoS_cod_desc'
						Set _bComponenteChave = TRUE
						Set _bCompChaveGeradoPeloSistema = TRUE
						Set _bCompChaveOpcionalParaPesquisa = TRUE
						Set _bDisparaConsultaEmValidate = FALSE
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						Call SalDateToStr(SalDateConstruct( SalDateYear( SalDateCurrent(  ) ), SalDateMonth( SalDateCurrent(  ) ), SalDateDay( SalDateCurrent(  ) ), 0, 0, 0 ),strData)
						Set strData = SalStrMidX(strData,0,10)
						If fMPSPrepareProc ( hSqlApp, 'Exec dbinfracao..InfracaoPontuacaoS @DescInfracao = :cmbInfracao', ':frmConsultaTipoVeiculo.dfsGravidade,:frmConsultaTipoVeiculo.dfnPontos,
								:frmConsultaTipoVeiculo.dfnValorEmUFIR,:frmConsultaTipoVeiculo.dfnValorEmReal')
							Call SqlExecute( hSqlApp )
							Call SqlFetchNext( hSqlApp, lParam )
						! Set strComando = 'select g.Descricao,g.Pontuacao,vg.Valor, vg.Valor*1.0641
								 from ' || DATABASE_INFRACAO || '..Infracao i, ' || DATABASE_INFRACAO ||
								'..GrupoInfracao g, '  || DATABASE_INFRACAO || '..ValorGrupo vg
								 where i.Descricao = :cmbInfracao and  i.Grupo = g.Cod and i.Grupo = vg.Grupo
								 and vg.Vigencia = (select Max(Vigencia)
											from dbinfracao..ValorGrupo vg1
											where vg1.Grupo = vg.Grupo
											and Vigencia <= "' || strData || '")'
						! Call fMPSPrepareProc( hSqlApp, strComando,
								':frmConsultaTipoVeiculo.dfsGravidade,:frmConsultaTipoVeiculo.dfnPontos,
								:frmConsultaTipoVeiculo.dfnValorEmUFIR,:frmConsultaTipoVeiculo.dfnValorEmReal')
						! If NOT SqlExecute( hSqlApp )
							Call SalWaitCursor( FALSE )
							Call SalClearField( hWndItem )
							! Call SalClearField( colnOrgao )
							Return VALIDATE_Cancel
						! Call SqlFetchNext( hSqlApp, nRetorno)
						Return SalSendClassMessage( SAM_Click, wParam, lParam )
