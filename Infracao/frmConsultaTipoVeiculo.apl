				Set _bLimpaTelaAposAtualizacao = FALSE
				Set _bLimpaTelaAposInclusao = FALSE
				Set _bLimpaTelaAposRemocao = TRUE
				! Set _sComandoPesquisa = 'exec dbdesenv..EstatS_MaisCometidas'
				! Set _sListaINTOPesquisa = ':frmConsultaTipoVeiculo.tblMultas.colDescricao,:frmConsultaTipoVeiculo.tblMultas.colQuantidade'
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalCenterWindow( hWndForm )
			On UDM_Pesquisa
				Call SalSendMsg( tblMultas, MSG_PesquisaTabela, 0, 0)
				! Call fObtemTotais()
				Return TRUE
			On UDM_AposPesquisa
				Call SalSendClassMessage( UDM_AposPesquisa, wParam, lParam )
				Call SalDisableWindow(rbTipo)
				Call SalDisableWindow(rbPlaca)
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				! Call HabilitacaoNome( 'pbAuto', mdiInfracao.MenuHabilitado( 'frmConsultaTipoVeiculo' ) )
				Call SalEnableWindow(cmbInfracao)
			On SAM_Destroy
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo',
						'Relatório de Multas Mais Cometidas', 'Relatório de Multas Mais Cometidas' )
				! Set sMes = frmConsultaTipoVeiculo.cmbInfracao
				! Set sAno =  frmConsultaTipoVeiculo.cmbAno
				! Call SalReportSetStringVar( hWndReport, 'Ano', frmConsultaTipoVeiculo.sAno )
				! Call SalReportSetStringVar( hWndReport, 'Mes', frmConsultaTipoVeiculo.sMes)
				Return TRUE
			On SAM_ReportFetchInit
				Call SqlImmediate( 'select getdate() into :frmConsultaTipoVeiculo.dCorrente' )
				Set nRow= 0
				Call SalTblSetContext( tblMultas, 0 )
			On SAM_ReportFetchNext
				If SalTblFetchRow(tblMultas,nRow)!=TBL_NoMoreRows
					Set nRow = nRow + 1
					Return TRUE
				Else
					Return FALSE
			! On SAM_ReportFinish
				Call SalReportSetStringVar( hWndReport, 'Ano', frmConsultaTipoVeiculo.cmbAno )
				Call SalReportSetStringVar( hWndReport, 'Mes', frmConsultaTipoVeiculo.cmbInfracao)

