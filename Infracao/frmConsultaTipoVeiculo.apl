						Set _bPesquisaCreate = FALSE
						Set _bSempreHabilitado = TRUE
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_FetchRowDone
						If SalStrScan( colDescricao,'Total Geral:')!=-1
							Call SalTblSetContext( tblMultas, lParam )
						If tblMultas.colDescricao != sTipoAnterior
							Set sTipoAnterior = tblMultas.colDescricao
						Else
							Set tblMultas.colDescricao = ""
						Call SalSendClassMessage( SAM_FetchRowDone, wParam, lParam )
					On SAM_ReportNotify
						If lParam = RPT_BeforePageHeader
							Call SqlImmediate( 'select getdate() into :frmConsultaTipoVeiculo.dCorrente' )
							Call SalReportSetStringVar( hWndReport, 'Usuario', SqlUser )
							Call SalReportSetStringVar( hWndReport, 'Ano', cmbAno )
							Call SalReportSetStringVar( hWndReport, 'Municipio', cmbMunicipio )
							Call SalReportSetStringVar( hWndReport, 'Infracao', cmbInfracao )
							Call SalReportSetDateTimeVar( hWndReport, 'DataCorrente', dCorrente )
					! On SAM_FetchRowDone
						 Set dfTotalQtde = tblMultas.colQuantidade+dfTotalQtde
						 Set dfTotalValorEmUFIR = tblMultas.colValorEmUFIR+dfTotalValorEmUFIR
						 Set dfTotalValorEmREAL = tblMultas.colValorEmREAL+dfTotalValorEmREAL
