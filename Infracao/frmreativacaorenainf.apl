				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Número da placa do veículo'
						Set _sNroCamposIgnorados = 'NENHUM'
						!
						Set _bNuncaHabilitado = TRUE
						Set _bPermiteNulos = TRUE
						Set _bSempreHabilitado = FALSE
						!
						Set _bVisivel = TRUE
						Set _sNomeParaMsgs = 'Placa'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Validate
						Call SalSendClassMessage( SAM_Validate, wParam, lParam )
						If NOT SalIsNull( dfsPlaca )
							If NOT PlacaValida( dfsPlaca )
								Call SalMessageBeep( 0 )
								Call SalMessageBox( 'Placa inválida', 'Atenção', MB_IconAsterisk | MB_Ok )
								Call SalClearField( hWndItem )
								Return VALIDATE_Cancel
							Call SalClearField( dfsChassi )
							If NOT SalIsNull( dfsUFPlaca )
								If not SalSendMsg( dfsPlaca, SAM_User, 0, 0 )
									Call SalMessageBeep( 0 )
									Call SalMessageBox( 'Veículo não cadastrado!', 'Atenção', MB_IconAsterisk | MB_Ok )
									Call SalClearField( hWndItem )
									Call SalSetFocus( hWndItem )
							If SalStrLength( dfsPlaca ) = 7
								Call SalClearField( cmbUFPlaca)
								Call SalClearField( dfsUFPlaca)
								Call SalPostMsg( dfnGrupoMarca, SAM_User, 0, 0)
							! Else
								    If SalIsNull( cmbUFPlaca)
									    Set dfsUFPlaca = sGlobalUF
									    Set cmbUFPlaca = sGlobalUF
									    Call SalSetFieldEdit( cmbUFPlaca, TRUE )
								    Else
									    Call SalSendMsg( dfsPlaca, SAM_User, 0, 0 )
						Return VALIDATE_Ok
					! ---- SAM_User => Realiza a pesquisa do veículo -----
					On SAM_User
						Call LimparVeiculo( )
						Set dfsUFPlaca = cmbUFPlaca
						Call SalWaitCursor( TRUE )
						If NOT fMPSPrepareProc( hSqlApp, 'exec VeiculoS_auto @Placa = :frmReativacaoRENAINF.dfsPlaca,
								@UFPlaca = :frmReativacaoRENAINF.dfsUFPlaca, @Chassi = :frmReativacaoRENAINF.dfsChassi',
								':frmReativacaoRENAINF.dfnVeiculo, :frmReativacaoRENAINF.dfsPlaca,
								:frmReativacaoRENAINF.dfsUFPlaca, :frmReativacaoRENAINF.nGrupoMarca, :frmReativacaoRENAINF.dfnMarcaVeiculo,
								:frmReativacaoRENAINF.dfsMarcaModelo, :nEspecieVeic, :sEspecie, :nCategoriaVeic, :sCategoria,
								:frmReativacaoRENAINF.sPlacaAnt, :frmReativacaoRENAINF.sUFPlacaAnt, :frmReativacaoRENAINF.nTipoVeic, :frmReativacaoRENAINF.nFoiPlacaAnt, :frmReativacaoRENAINF.dfsChassi' )
							Call SalWaitCursor( FALSE )
							Return FALSE
						If NOT SqlExecute( hSqlApp )
							Call SalWaitCursor( FALSE )
							Call LimparVeiculo( )
							Return FALSE
						Call SqlFetchNext( hSqlApp, nTmp )
						Call SalWaitCursor( FALSE )
						If sGlobalUF = dfsUFPlaca and dfnVeiculo = NUMBER_Null
							Return FALSE
						If nTmp != FETCH_Ok
							Call LimparVeiculo( )
							Return FALSE
						Call SalSendMsg( cmbUFPlaca, MSG_FetchRefreshDerivados, 0, 0 )
						Return TRUE
