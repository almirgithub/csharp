							   Call SalListAdd( cmbSituacao, 'CANCELADO')
							   Set nIndex = SalListSelectString( cmbSituacao , 0, 'CANCELADO' )
							   Call SalListSetSelect( cmbSituacao, SalListSelectString( cmbSituacao , 0, 'CANCELADO' ) )
							Call SalListAdd( frmReativacaoRENAINF.tblInfra.colsSituacao, 'ABERTO')
							Call SalListAdd( frmReativacaoRENAINF.tblInfra.colsSituacao, 'CANCELADO')
