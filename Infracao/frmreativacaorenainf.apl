						  Call SalPostMsg( cbRetencao, MSG_Habilita1, 0, 0)
						  Call SalPostMsg( cbApreensaoCNH, MSG_Habilita1, 0, 0)
						  Call SalPostMsg( dfsCondutor, MSG_Habilita1, 0, 0)
						  Call SalPostMsg( dfnCNHCondutor, MSG_Habilita1, 0, 0)
						  Call SalPostMsg( cmbUFCondutor, MSG_Habilita1, 0, 0)
						  Call SalPostMsg( mlObservacao, MSG_Habilita1, 0, 0)
						  Call SalPostMsg( pbNovaLinha, MSG_Habilita1, 0, 0)
						Call SalPostMsg( frmReativacaoRENAINF.tblInfra.colsGerar, MSG_Habilita1, 0, 0)
						Call SalPostMsg( frmReativacaoRENAINF.tblInfra.colnCodigo, MSG_Habilita1, 0, 0)
						Call SalPostMsg( frmReativacaoRENAINF.tblInfra.colnPermitido, MSG_Habilita1, 0, 0)
						Call SalPostMsg( frmReativacaoRENAINF.tblInfra.colnAferido, MSG_Habilita1, 0, 0)
						Call SalPostMsg( frmReativacaoRENAINF.tblInfra.colsInstrum, MSG_Habilita1, 0, 0)
					  Else
						  Call SalPostMsg( dfsPlaca, MSG_Desabilita1, 0, 0)
						  Call SalPostMsg( cmbUFPlaca, MSG_Desabilita1, 0, 0)
						  Call SalPostMsg( dfnGrupoMarca, MSG_Desabilita1, 0, 0)
