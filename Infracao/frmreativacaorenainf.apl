						  Call SalDisableWindow( pbArquivar )
						  Call SalDisableWindow( pbPesquisar )
						  Call SalDisableWindow( pbLimpar )
						  Call SalDisableWindow( pbNovaLinha )
					If sExisteDebito = 'S'
						! Set dfnNovoLote = dfnLote
						Call SalPostMsg( dfsPlaca, MSG_Desabilita1, 0, 0)
						Call SalPostMsg( cmbUFPlaca, MSG_Desabilita1, 0, 0)
						Call SalPostMsg( dfsChassi, MSG_Desabilita1, 0, 0)
						Call SalSendMsgToChildren( frmReativacaoRENAINF.tblInfra, MSG_Desabilita1, 0, 0)
						Call SalPostMsg( pbNovaLinha, MSG_Desabilita1, 0, 0)
						! Habilita CNH para informar condutor
						If sConsulta ='N'
							! Call SalPostMsg( dfnCNHCondutor, MSG_Habilita1, 0, 0)
							! Call SalPostMsg( cmbUFCondutor, MSG_Habilita1, 0, 0)
							Call SalSendMsg( pbArquivar, MSG_Habilita1, 0, 0)
							Call SalPostMsg( dfsCondutor, MSG_Desabilita1, 0, 0)
							Call SalPostMsg( mlObservacao, MSG_Desabilita1, 0, 0)
					Else If bPodeAlterar
						Call SalPostMsg( dfsPlaca, MSG_Desabilita1, 0, 0)
						Call SalPostMsg( cmbUFPlaca, MSG_Desabilita1, 0, 0)
						Call SalPostMsg( dfsChassi, MSG_Desabilita1, 0, 0)
						! Call SalPostMsg( dfnNovoOA, MSG_Habilita1, 0, 0)
						! Call SalPostMsg( frmReativacaoRENAINF.tblInfra, MSG_Habilita1, 0, 0)
						Call SalSendMsgToChildren( frmReativacaoRENAINF.tblInfra, MSG_Habilita1, 0, 0)
						Call SalPostMsg( pbNovaLinha, MSG_Desabilita1, 0, 0)
		Window Parameters
			String: sConsulta
		Window Variables
			Date/Time: dInfracao
			String: sMarcaAux
			String: sPlacaAnt
			String: sUFPlacaAnt
			String: sSetorApreensao
			String: sExisteDebito
			Number: nTipoVeic
			Number: nApreensao
			Date/Time: dDataApreensao
			Number: nEspecieVeic
			Number: nCategoriaVeic
			Number: nFoiPlacaAnt
			Number: nGrupoMarca
			Number: nMaxSeq
			Number: nTmp
			String: sEspecie
			String: sCategoria
			Window Handle: hWndBotaoPai
