						Description:
						Returns
						Parameters
						Static Variables
						Local variables
							Number: nLinha
						Actions
							Set nLinha = TBL_MinRow
							While SalTblFindNextRow( frmReativacaoRENAINF.tblInfra, nLinha, 0, 0 )
								Call SalTblSetContext( frmReativacaoRENAINF.tblInfra, nLinha )
								Set colsSitI = colsSit
							Return TRUE
					Function: fTransmiteAlteracao
						Description:
						Returns
						Parameters
						Static Variables
						Local variables
							Number: nRow
							String: sCondicionalidade
						Actions
							Set nRow = 0
							Call SalTblSetContext( tblInfra, nRow )
							Loop
								If SalTblFetchRow( tblInfra, nRow ) = TBL_NoMoreRows
									Break
								Call SalTblSetRowFlags( tblInfra, ROW_Edited, nRow, TRUE )
								Call fTransmiteRENAINF(dfsSerie,dfnOrgaoAutuante,dfnNumero,
										tblInfra.colnCodigoCTB,416,'EV01',hSqlApp,'','','','',8,1,'3',0)
								Set nRow = nRow + 1
							Return TRUE
