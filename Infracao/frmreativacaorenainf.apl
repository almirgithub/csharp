				Contents
					Column: colnCodigo
						Class Child Ref Key: 0
						Class ChildKey: 0
						Class: cQCK2Column
						Property Template:
						Class DLL Name:
						Title: Código
						Visible? Class Default
						Editable? Class Default
						Maximum Data Length: Class Default
						Data Type: Number
						Justify: Right
						Width:  0.75"
						Width Editable? Class Default
						Format: Class Default
						Country: Class Default
						Input Mask: Class Default
						Cell Options
							Cell Type? Class Default
							Multiline Cell? Class Default
							Cell DropDownList
								Sorted? Class Default
								Vertical Scroll? Class Default
								Auto Drop Down? Class Default
								Allow Text Editing? Class Default
							Cell CheckBox
								Check Value:
								Uncheck Value:
								Ignore Case? Class Default
						List Values
						Message Actions
							On SAM_Create
								Set _sTextoStatusBar = 'Código da infração cometida'
								Set _sNroCamposIgnorados = 'NENHUM'
								!
								Set _bNuncaHabilitado = FALSE
								Set _bPermiteNulos = FALSE
								Set _bSempreHabilitado = FALSE
								Set _bComponenteChave = TRUE
								!
								Set _bVisivel = TRUE
								Set _sNomeParaMsgs = 'Código infração'
								! Dados para a pesquisa de F2
								Set _sConsultaFrmOpcoesSQLOuSP = 'SP'
								Set _sConsultaFrmOpcoesCampo = 'exec InfracaoS_coddesc'
								Call SalSendClassMessage( SAM_Create, wParam, lParam )
							On SAM_User
								Set nTmp = TBL_MinRow
								Call SalTblFindNextRow( frmReativacaoRENAINF.tblInfra, nTmp, ROW_New, 0 )
								Call SalTblSetFocusCell( frmReativacaoRENAINF.tblInfra, nTmp, frmReativacaoRENAINF.tblInfra.colnCodigo, 0, -1 )
							On SAM_Validate
								If NOT SalIsNull( hWndItem )
									If ProcuraNJaDigitado( hWndForm, SalTblQueryColumnID( hWndItem ), lParam, colnCodigo, 'Infração já digitada.' )
										Return VALIDATE_OkClearFlag
									! Adilson
									If NOT SalIsNull( hWndItem )
										Set nTmp = DigitoModulo11( SalNumberTruncate( MyValue/10, 10, 0 ))
										If nTmp != SalNumberMod( MyValue, 10 )
											Call SalMessageBeep( MB_IconExclamation )
											Call SalMessageBox( 'Infração não cadastrada', 'Atenção', MB_IconExclamation | MB_Ok )
											Call SalClearField( hWndItem )
											Return VALIDATE_Cancel
											! Call SalMessageBeep( MB_IconExclamation )
											! Call SalMessageBox( 'Dígito inválido', 'Atenção', MB_IconExclamation | MB_Ok )
											! Call SalClearField( hWndItem )
										! Retorno = FETCH_EOF
									! Else
										Return VALIDATE_OkClearFlag
									Set nInfracaoCTB = fInfracaoNovoCodigo(frmReativacaoRENAINF.tblInfra.colnCodigo)
									Set frmReativacaoRENAINF.tblInfra.colnCodigoCTB = nInfracaoCTB
									! Fim Adilson
									If fMPSPrepareProc( hSqlApp, 'exec InfracaoS_resumo @Cod = :frmReativacaoRENAINF.tblInfra.colnCodigoCTB,
											@TipoVeiculo = :frmReativacaoRENAINF.nTipoVeic, @EspecieVeiculo = :frmReativacaoRENAINF.nEspecieVeic, @CategoriaVeiculo = :frmReativacaoRENAINF.nCategoriaVeic',
											':frmReativacaoRENAINF.tblInfra.colsDescricao, :frmReativacaoRENAINF.tblInfra.sApreensao, :frmReativacaoRENAINF.tblInfra.sRetencao,
											:frmReativacaoRENAINF.tblInfra.sRemocao, :frmReativacaoRENAINF.tblInfra.sApreensaoCNH, :frmReativacaoRENAINF.tblInfra.nETipo,
											:frmReativacaoRENAINF.tblInfra.nEEsp, :frmReativacaoRENAINF.tblInfra.nECat, :frmReativacaoRENAINF.tblInfra.nGrupoInfra,
											:frmReativacaoRENAINF.tblInfra.sCLA, :frmReativacaoRENAINF.tblInfra.sCRV, :frmReativacaoRENAINF.tblInfra.sOutros, :frmReativacaoRENAINF.tblInfra.sSituacao,
											:frmReativacaoRENAINF.tblInfra.sCompetencia' )
										If SqlExecute( hSqlApp )
											Call SqlFetchNext( hSqlApp, nTmp )
											If nTmp = FETCH_Ok
												If sApreensao = 'S' AND NOT cbApreensao
													Set cbApreensao = TRUE
													Call SalSendMsg( cbApreensao, SAM_Click, 0, 0 )
												If sRetencao = 'S' AND NOT cbRetencao
													Set cbRetencao = TRUE
													Call SalSendMsg( cbRetencao, SAM_Click, 0, 0 )
												If sRemocao = 'S' AND NOT cbRemocao
													Set cbRemocao = TRUE
													Call SalSendMsg( cbRemocao, SAM_Click, 0, 0 )
												If sApreensaoCNH = 'S' AND NOT cbApreensaoCNH
													Set cbApreensaoCNH = TRUE
													Call SalSendMsg( cbApreensaoCNH, SAM_Click, 0, 0 )
												If sCLA = 'S' AND NOT cbCLA
													Set cbCLA = TRUE
													Call SalSendMsg( cbCLA, SAM_Click, 0, 0 )
												If sCRV = 'S' AND NOT cbCRV
													Set cbCRV = TRUE
													Call SalSendMsg( cbCRV, SAM_Click, 0, 0 )
												If sOutros = 'S' AND NOT cbOutros
													Set cbOutros = TRUE
													Call SalSendMsg( cbOutros, SAM_Click, 0, 0 )
												! TRATAMENTO PARA INFRAÇÕES DO CÓDIGO NOVO x ANTIGO
												! If dfdAuto > SalStrToDate( '1998-01-22' )  AND frmReativacaoRENAINF.tblInfra.nGrupoInfra < 10
													  Call SalMessageBeep( 0 )
													  Call SalMessageBox( 'Infração do Código Antigo - Data de Cometimento Inválida!', 'Atenção', MB_IconAsterisk | MB_Ok )
													  Call SalTblDeleteRow( hWndForm, lParam, wParam )
													  Return VALIDATE_Cancel
												! If dfdAuto < SalStrToDate( '1998-01-22' ) AND frmReativacaoRENAINF.tblInfra.nGrupoInfra > 10
													  Call SalMessageBeep( 0 )
													  Call SalMessageBox( 'Infração do Código Novo - Data de Cometimento Inválida!', 'Atenção', MB_IconAsterisk | MB_Ok )
													  Call SalTblDeleteRow( hWndForm, lParam, wParam )
													  Return VALIDATE_Cancel
												If sSituacao = 'I'
													Call SalMessageBeep( 0 )
													Call SalMessageBox( 'Infração do Código Antigo - Infração Inválida!', 'Atenção', MB_IconAsterisk | MB_Ok )
													Call SalTblDeleteRow( hWndForm, lParam, wParam )
													Return VALIDATE_Cancel
												Call SalTblSetContext( frmReativacaoRENAINF.tblInfra, lParam )
												If nETipo AND NOT SalIsNull( dfnVeiculo )
													Call SalMessageBeep( 0 )
													Call SalMessageBox( 'Esta infração não é válida para este tipo de veículo!', 'Atenção', MB_IconAsterisk | MB_Ok )
													Call SalClearField( hWndItem )
													Call SalClearField( colsDescricao )
													Return VALIDATE_Cancel
												If nEEsp AND NOT SalIsNull( dfnVeiculo )
													Call SalMessageBeep( 0 )
													Call SalMessageBox( 'Esta infração não é válida para esta espécie de veículo!', 'Atenção', MB_IconAsterisk | MB_Ok )
													Call SalClearField( hWndItem )
													Call SalClearField( colsDescricao )
													Return VALIDATE_Cancel
												If nECat AND NOT SalIsNull( dfnVeiculo )
													Call SalMessageBeep( 0 )
													Call SalMessageBox( 'Esta infração não é válida para esta categoria de veículo!', 'Atenção', MB_IconAsterisk | MB_Ok )
													Call SalClearField( hWndItem )
													Call SalClearField( colsDescricao )
													Return VALIDATE_Cancel
												Return VALIDATE_OkClearFlag
											Else If nTmp = FETCH_EOF
												Call SalMessageBeep( MB_IconExclamation )
												Call SalMessageBox( 'Infração não cadastrada', 'Atenção', MB_IconExclamation | MB_Ok )
												Call SalClearField( hWndItem )
												Return VALIDATE_Cancel
									Call SalClearField( hWndItem)
									Return VALIDATE_Cancel
								Return VALIDATE_OkClearFlag
					Column: colnCodigoCTB
