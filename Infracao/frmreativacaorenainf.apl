								@Serie = :frmReativacaoRENAINF.dfsSerie, @Auto = :frmReativacaoRENAINF.dfnNumero, @OrgaoAutuante = :frmReativacaoRENAINF.dfnOrgaoAutuante'
						Set _sIntoPesquisa = ':frmReativacaoRENAINF.tblInfra.colnCodigoCTB, :frmReativacaoRENAINF.tblInfra.colsDescricao,
								:frmReativacaoRENAINF.tblInfra.colnPermitido, :frmReativacaoRENAINF.tblInfra.colnAferido,
								:frmReativacaoRENAINF.tblInfra.colnInstrum, :frmReativacaoRENAINF.tblInfra.colsInstrum, :frmReativacaoRENAINF.tblInfra.colsSitI'
						!
						Set _sComandoInsere = 'exec dbinfracao..InfracaoAutoI
								@Serie = :frmReativacaoRENAINF.dfsSerie, @Auto = :frmReativacaoRENAINF.dfnNumero, @OrgaoAutuante = :frmReativacaoRENAINF.dfnOrgaoAutuante,
								@Infracao = :frmReativacaoRENAINF.tblInfra.colnCodigoCTB, @Permitido = :frmReativacaoRENAINF.tblInfra.colnPermitido,
								@Aferido = :frmReativacaoRENAINF.tblInfra.colnAferido, @Instrumento = :frmReativacaoRENAINF.tblInfra.colnInstrum,
								@Acerto = "S", @GerarNotificacao = :frmReativacaoRENAINF.dfsGerar,
								@ApreensaoCNH = :frmReativacaoRENAINF.dfsApreensaoCNH,
								@Veiculo = :frmReativacaoRENAINF.dfnVeiculo, @Placa = :frmReativacaoRENAINF.dfsPlaca, @UFPlaca = :frmReativacaoRENAINF.dfsUFPlaca,
								@GrupoMarcaVeiculo = :frmReativacaoRENAINF.dfnGrupoMarca, @DataInfracao = :frmReativacaoRENAINF.dInfracao,
								@TipoVeic=:frmReativacaoRENAINF.nTipoVeic, @EspVeic=:frmReativacaoRENAINF.nEspecieVeic, @CatVeic=:frmReativacaoRENAINF.nCategoriaVeic,@GrupoApp = :frmReativacaoRENAINF.dfnGrupoMarca'
						Set _sIntoInsere = ''
						!
						Set _sComandoAtualiza = 'exec dbinfracao..InfracaoAutoU
								@Serie = :frmReativacaoRENAINF.dfsSerie, @Auto = :frmReativacaoRENAINF.dfnNumero,
								@OrgaoAutuante = :frmReativacaoRENAINF.dfnOrgaoAutuante,
								@Infracao = :frmReativacaoRENAINF.tblInfra.colnCodigoCTB,
								@Permitido = :frmReativacaoRENAINF.tblInfra.colnPermitido,
								@Aferido = :frmReativacaoRENAINF.tblInfra.colnAferido,
								@Instrumento = :frmReativacaoRENAINF.tblInfra.colnInstrum,
								@GerarNotificacao = :frmReativacaoRENAINF.tblInfra.colsGerar,
								@Veiculo = :frmReativacaoRENAINF.dfnVeiculo, @Situacao = :frmReativacaoRENAINF.tblInfra.colsSit, @FoiPlacaAnt=:frmReativacaoRENAINF.nFoiPlacaAnt,
								@TipoVeic=:frmReativacaoRENAINF.nTipoVeic, @EspVeic=:frmReativacaoRENAINF.nEspecieVeic, @CatVeic=:frmReativacaoRENAINF.nCategoriaVeic'
						Set _sIntoAtualiza = ''
