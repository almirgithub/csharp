						Call SalPostMsg( frmReativacaoRENAINF.tblInfra.colnAferido, MSG_Desabilita1, 0, 0)
						Call SalPostMsg( frmReativacaoRENAINF.tblInfra.colsInstrum, MSG_Desabilita1, 0, 0)
						! If dfsSituacao = 'B'
							   Call SalListAdd( cmbSituacao, 'BAIXADO')
							   Call SalListAdd( cmbSituacao, 'ABERTO')
							   Set nIndex = SalListSelectString( cmbSituacao , 0, 'BAIXADO' )
