						  Call SalPostMsg( cbRetencao, MSG_Desabilita1, 0, 0)
						  Call SalPostMsg( cbApreensaoCNH, MSG_Desabilita1, 0, 0)
						  Call SalPostMsg( dfsCondutor, MSG_Desabilita1, 0, 0)
						  Call SalPostMsg( dfnCNHCondutor, MSG_Desabilita1, 0, 0)
						  Call SalPostMsg( cmbUFCondutor, MSG_Desabilita1, 0, 0)
