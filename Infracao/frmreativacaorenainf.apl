			String: sComplLocal
			!
			Number: nReturn
			String: sUFCNH
		Message Actions
			! Observações
				      Autor
					     Celso - 09/01/97
			On SAM_Create
				Set _nEstadoInicialForm = EF_Vazio
				Set _sTextoStatusBar = 'Tela para correção dos autos de infração e sua situação'
				! Variáveis para conexão
				Set _bCriacaoConexaoAuxiliar = TRUE
				Set _sCriacaoConexao = 'Default'
				! Variáveis para as procedure's
				Set _bLimpaTelaAposAtualizacao = TRUE
				Set _bLimpaTelaAposInclusao = TRUE
				Set _bLimpaTelaAposRemocao = TRUE
				Set _sComandoAtualizacao = 'exec dbinfracao..AutoU_acerto @Serie = :frmReativacaoRENAINF.dfsSerie, @Cod = :frmReativacaoRENAINF.dfnNumero,
						@Lote = :frmReativacaoRENAINF.dfnLote, @Placa = :frmReativacaoRENAINF.dfsPlaca, @UFPlaca = :frmReativacaoRENAINF.dfsUFPlaca, @Chassi = :frmReativacaoRENAINF.dfsChassi,
						@CNH = :frmReativacaoRENAINF.dfnCNHCondutor, @UFCNH = :frmReativacaoRENAINF.dfsUFCondutor, @Condutor = :frmReativacaoRENAINF.dfsCondutor,
						@Municipio = :frmReativacaoRENAINF.dfnMunicipio, @MarcaVeiculo = :frmReativacaoRENAINF.dfnMarcaVeiculo, @Local = :frmReativacaoRENAINF.dfsLocal,
						@DataInfracao = :frmReativacaoRENAINF.dInfracao, @Agente = :frmReativacaoRENAINF.dfnAgente, @Setor = :nGlobalSetor, @Situacao = :frmReativacaoRENAINF.dfsSituacao,
						@Veiculo = :frmReativacaoRENAINF.dfnVeiculo, @GrupoMarcaVeiculo = :frmReativacaoRENAINF.dfnGrupoMarca, @ApreensaoCNH = :frmReativacaoRENAINF.dfsApreensaoCNH,
						@ApreensaoVeiculo = :frmReativacaoRENAINF.dfsApreensao, @RetencaoVeiculo = :frmReativacaoRENAINF.dfsRetencao, @RemocaoVeiculo = :frmReativacaoRENAINF.dfsRemocao,
						@Observacao = :frmReativacaoRENAINF.mlObservacao, @Placant=:frmReativacaoRENAINF.sPlacaAnt,  @UFPlacaAnt=:frmReativacaoRENAINF.sUFPlacaAnt,
						@TipoVeic=:frmReativacaoRENAINF.nTipoVeic, @EspVeic=:frmReativacaoRENAINF.nEspecieVeic, @CatVeic=:frmReativacaoRENAINF.nCategoriaVeic, @FoiPlacaAnt=:frmReativacaoRENAINF.nFoiPlacaAnt,
						@SetorApreensao=:frmReativacaoRENAINF.sSetorApreensao, @Apreensao=:frmReativacaoRENAINF.nApreensao, @DataApreensao=:frmReativacaoRENAINF.dDataApreensao,
						@NovoLote=:frmReativacaoRENAINF.dfnNovoLote, @NovoOA=:frmReativacaoRENAINF.dfnNovoOA,
						@ApreensaoCLA = :frmReativacaoRENAINF.dfsCLA, @ApreensaoCRV = :frmReativacaoRENAINF.dfsCRV, @ApreensaoOutros = :frmReativacaoRENAINF.dfsOutros,
						@OrgaoAutuante=:frmReativacaoRENAINF.dfnOrgaoAutuante,@NovoA=:frmReativacaoRENAINF.dfnNovoA,@TipoMov="M"'
				Set _sComandoPesquisa = 'exec dbinfracao..AutoS_ReativaRENAINF @Lote = :frmReativacaoRENAINF.dfnLote, @Serie = :frmReativacaoRENAINF.dfsSerie,
						@Cod = :frmReativacaoRENAINF.dfnNumero, @OrgaoAutuante = :frmReativacaoRENAINF.dfnOrgaoAutuante'
				! Variáveis para o retorno
				Set _sListaINTOPesquisa = ':frmReativacaoRENAINF.dfnLote, :frmReativacaoRENAINF.dfsSerie, :frmReativacaoRENAINF.dfnNumero, :frmReativacaoRENAINF.dfsOrgao,
						:frmReativacaoRENAINF.dfsSituacao, :frmReativacaoRENAINF.dfsSituacao, :frmReativacaoRENAINF.dfnVeiculo, :frmReativacaoRENAINF.dfsPlaca, :frmReativacaoRENAINF.dfsUFPlaca,
						:frmReativacaoRENAINF.cmbUFPlaca, :frmReativacaoRENAINF.dfnGrupoMarca, :frmReativacaoRENAINF.dfnMarcaVeiculo, :frmReativacaoRENAINF.dfsMarcaModelo,
						:frmReativacaoRENAINF.dfsLocal, :frmReativacaoRENAINF.dInfracao, :frmReativacaoRENAINF.dfnMunicipio, :frmReativacaoRENAINF.dfsMunicipio, :frmReativacaoRENAINF.dfnAgente,
						:frmReativacaoRENAINF.dfsApreensao, :frmReativacaoRENAINF.dfsRetencao, :frmReativacaoRENAINF.dfsRemocao, :frmReativacaoRENAINF.dfsApreensaoCNH,
						:frmReativacaoRENAINF.dfsCondutor,:frmReativacaoRENAINF.dfnCNHCondutor, :frmReativacaoRENAINF.dfsUFCondutor, :frmReativacaoRENAINF.cmbUFCondutor,:frmReativacaoRENAINF.mlObservacao,
						:frmReativacaoRENAINF.nGrupoMarca, :frmReativacaoRENAINF.sPlacaAnt, :frmReativacaoRENAINF.sUFPlacaAnt, :frmReativacaoRENAINF.nTipoVeic, :frmReativacaoRENAINF.nEspecieVeic, :frmReativacaoRENAINF.nCategoriaVeic,
						:frmReativacaoRENAINF.sSetorApreensao, :frmReativacaoRENAINF.nApreensao, :frmReativacaoRENAINF.dDataApreensao, :frmReativacaoRENAINF.sExisteDebito,
						:frmReativacaoRENAINF.dfsChassi, :frmReativacaoRENAINF.dfsCLA, :frmReativacaoRENAINF.dfsCRV, :frmReativacaoRENAINF.dfsOutros, :frmReativacaoRENAINF.dfsCategoria, :frmReativacaoRENAINF.bPodeAlterar,
						:frmReativacaoRENAINF.dfnOrgaoAutuante,:frmReativacaoRENAINF.dfnAgenteEquip, :frmReativacaoRENAINF.dfsAutoRENAINF'
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalDisableWindow( mdipbAcertoAuto )
				Call SalCenterWindow( hWndForm )
				Set sSetorUsuarioN = SalStrMidX(SalNumberToStrX(nGlobalSetor,0),0,SalStrLength( SalNumberToStrX(nGlobalSetor,0) )-1)
				Set nSetorUsuario = SalStrToNumber(sSetorUsuarioN)
				! Set nSetorUsuario = nGlobalSetor/10
			On SAM_CreateComplete
				Call SalDisableWindow( pbNovaLinha )
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				Call SalWaitCursor( TRUE )
				If NOT fMPSPrepareProc( hSqlApp, 'exec ParametroGeralS_not',
						':frmReativacaoRENAINF.nAutoNotificacao' )
					Call SalWaitCursor( FALSE )
					Return FALSE
				If NOT SqlExecute( hSqlApp )
					Call SalWaitCursor( FALSE )
					Return FALSE
				Call SalWaitCursor( FALSE )
				Call SqlFetchNext( hSqlApp, nAux )
			On SAM_Destroy
				Set hWndAcertoAuto = hWndNULL
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
				Call SalSendMsg( hWndBotaoPai, MSG_Habilita1, 0, 0)
				Call SalEnableWindow( mdipbAcertoAuto )
			On UDM_Pesquisa
				If NOT SalSendClassMessage( UDM_Pesquisa, wParam, lParam )
					Return FALSE
				Return SalPostMsg( frmReativacaoRENAINF.tblInfra, MSG_PesquisaTabela, wParam, lParam )
			On MSG_Anterior
				Call SalSendClassMessage( MSG_Anterior, wParam, lParam )
				If SalIsNull( dfnVeiculo)
					Set sMarcaAux = dfsMarcaModelo
				Set dfdAuto = dInfracao
				Set dfdHora = dInfracao
				! Verificando a permissão do orgão no Setor
				! Call fMPSPrepareProc( hSqlApp,'Exec dbinfracao..AutoS_verSetor @OrgaoAutuante = :dfnOrgaoAutuante,
						@Serie = :dfsSerie, @Cod = :dfnNumero' , ':nAux' )
				! If NOT SqlExecute( hSqlApp )
					Call SalSendMsg( pbNovaLinha, MSG_Desabilita1, 0, 0 )
					Call SalSendMsg( pbArquivar, MSG_Desabilita1, 0, 0 )
					Return VALIDATE_Cancel
				! Else
				Call SalPostMsg( pbArquivar, MSG_Habilita1, 0, 0 )
				Return SalSendMsg( frmReativacaoRENAINF.tblInfra, MSG_PesquisaTabela, wParam, lParam )
			On MSG_Proximo
				Call SalSendClassMessage( MSG_Proximo, wParam, lParam )
				If SalIsNull( dfnVeiculo)
					Set sMarcaAux = dfsMarcaModelo
				Set dfdAuto = dInfracao
				Set dfdHora = dInfracao
				! Verificando a permissão do orgão no Setor
				! Call fMPSPrepareProc( hSqlApp,'Exec dbinfracao..AutoS_verSetor  @OrgaoAutuante = :dfnOrgaoAutuante, @Serie = :dfsSerie, @Cod = :dfnNumero' , ':nAux' )
				! If NOT SqlExecute( hSqlApp )
					Call SalSendMsg( pbNovaLinha, MSG_Desabilita1, 0, 0 )
					Call SalSendMsg( pbArquivar, MSG_Desabilita1, 0, 0 )
					Return VALIDATE_Cancel
				! Else
				Call SalPostMsg( pbArquivar, MSG_Habilita1, 0, 0 )
				Return SalSendMsg( frmReativacaoRENAINF.tblInfra, MSG_PesquisaTabela, wParam, lParam )
			On UDM_AntesAtualizacao
				! If SalIsNull( dfnVeiculo ) AND SalIsNull( dfsPlaca )
					  Call SalMessageBeep( MB_IconExclamation )
					  Call SalMessageBox( 'O veículo é obrigatório.', 'Atenção', MB_IconExclamation | MB_Ok )
					  Call SalSetFocus( dfsPlaca )
					  Return FALSE
				If SalStrLength( dfsPlaca ) = 6 AND SalIsNull( dfsUFPlaca)
					Call SalMessageBeep( MB_IconExclamation )
					Call SalMessageBox( 'É preciso informar a UF da placa de 6 (seis) dígitos', 'Atenção', MB_IconExclamation | MB_Ok )
					Return FALSE
				Set nTmp = TBL_MaxRow
				If SalTblFindPrevRow( frmReativacaoRENAINF.tblInfra, nTmp, 0, ROW_MarkDeleted )
					If nTmp >= nAutoNotificacao
						Call SalMessageBeep( MB_IconExclamation )
						Call SalMessageBox( 'Não é permitido inserir mais do que ' || SalNumberToStrX( nAutoNotificacao, 0) ||
								' infrações em um auto, este valor é definido nos parâmetros gerais do sistema', 'Atenção', MB_IconExclamation | MB_Ok )
						Return FALSE
				Else
					Call SalMessageBeep( MB_IconExclamation )
					Call SalMessageBox( 'É obrigatório informar pelo menos uma infração', 'Atenção', MB_IconExclamation | MB_Ok )
					Return FALSE
				Set bAlteraTabela = FALSE
				! Necessario para poder consistir infracao/tipoveiculo
				Set sLocal =  SalStrLeftX( frmReativacaoRENAINF.dfsLocal, 50)
				Set sComplLocal =  SalStrMidX( frmReativacaoRENAINF.dfsLocal, 50,50)
				Return SalSendMsg( frmReativacaoRENAINF.tblInfra, MSG_Arquiva, 0, 0 )
			On UDM_AposPesquisa
				! If bNaoExec
					  Set bNaoExec = FALSE
					  Return TRUE
				If nEstadoForm = EF_RegistroNovo
					Call SalMessageBeep( MB_IconExclamation )
					Call SalMessageBox( 'Auto não cadastrado', 'Atenção', MB_IconExclamation | MB_Ok )
					Return FALSE
				! Call AtualizaSituacao( )
				Set dfdAuto = SalDateConstruct( SalDateYear( dInfracao ), SalDateMonth( dInfracao ), SalDateDay( dInfracao ), 0, 0, 0 )
				Set dfdHora = dInfracao
				Call SalEnableWindow( pbNovaLinha )
				If SalIsNull( dfnVeiculo)
					Set sMarcaAux = dfsMarcaModelo
				! Verificando a permissão do orgão no Setor
				! Call fMPSPrepareProc( hSqlApp,'Exec dbinfracao..AutoS_verSetor  @OrgaoAutuante = :dfnOrgaoAutuante, @Serie = :dfsSerie, @Cod = :dfnNumero' , ':nAux' )
				! If NOT SqlExecute( hSqlApp )
					Call SalSendMsg( pbNovaLinha, MSG_Desabilita1, 0, 0 )
					Call SalSendMsg( pbArquivar, MSG_Desabilita1, 0, 0 )
					Return VALIDATE_Cancel
				! Verificando se a cidade esta associada com o Setor.
				If NOT SalPostMsg( dfnMunicipio, UDM_ValidaCampo, 0, 0 )
					Return VALIDATE_Cancel
				Return TRUE
			On UDM_AposAtualizacao
				Call SalDisableWindow( pbAnterior )
				Call SalDisableWindow( pbProximo )
				If NOT SalIsNull( dfnNovoOA  ) and NOT SalIsNull( dfnNovoLote  ) and NOT SalIsNull(dfnNovoA)
					Set dfnLote = dfnNovoLote
				! Call SalPostMsg( pbPesquisar, SAM_Click, 0, 0 )
				Set bAlteraTabela = TRUE
				Call SalSendMsg( frmReativacaoRENAINF.tblInfra, MSG_Arquiva, wParam, lParam )
				Set bAlteraTabela = FALSE
				! If cbNotificacao
					   Set dfsSituacao = 'N'
				Return SalSendClassMessage( UDM_AposAtualizacao, wParam, lParam )

