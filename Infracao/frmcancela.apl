		Message Actions
			! Observações
				     Autor
					    Celso - 09/01/97
			On SAM_Create
				Set _sTextoStatusBar = 'Tela para cancelamento de autos de infração'
				Set _nEstadoInicialForm = EF_Vazio
				Set _bLimpaTelaAposAtualizacao = FALSE
				Set _bLimpaTelaAposInclusao = FALSE
				Set _sComandoPesquisa = 'exec AutoS_existe_placa @Placa = :frmCancela.dfsPlaca, @UFPlaca = :frmCancela.dfsUFPlaca'
				Set _sListaINTOPesquisa = ':frmCancela.dfnVeiculo, :frmCancela.dfsPlaca, :frmCancela.dfsUFPlaca'
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalCenterWindow( hWndForm )
				Call SalDisableWindow( mdipbCancelaAuto )
			On SAM_Destroy
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
				Call SalEnableWindow( mdipbCancelaAuto )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				Set sSetorUsuarioN = SalStrMidX(SalNumberToStrX(nGlobalSetor,0),0,SalStrLength( SalNumberToStrX(nGlobalSetor,0) )-1)
				Set nSetorUsuario = SalStrToNumber(sSetorUsuarioN)
				Call SalSetFocus( dfsPlaca)
			On UDM_Atualizacao
				Return SalSendMsg( tblCancela, MSG_Arquiva, wParam, lParam )
			On UDM_AposAtualizacao
				Call SalPostMsg( dfnProcessoRecurso, MSG_Desabilita1, 0, 0 )
				! Call SalPostMsg( tblCancela, MSG_Desabilita1, 0, 0 )
				Call SalSendMsgToChildren( tblCancela, MSG_Desabilita1, wParam, lParam )
				Call SalPostMsg( pbArquivar, MSG_Desabilita1, 0, 0 )
				Call SalPostMsg( pbNovaLinha, MSG_Desabilita1, 0, 0 )
				Return TRUE
			On UDM_AposPesquisa
				Call SalSetFocus( dfnProcessoRecurso)
				Return TRUE
