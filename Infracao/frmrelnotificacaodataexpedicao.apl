				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Imprime o relatório'
						Set _nPermissao = 15
						Set _sTipoComando = 'Comum'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						! Validações
						If SalIsNull( dfnOrgao)
							Call SalMessageBeep( MB_IconExclamation )
							Call SalMessageBox( 'O órgão autuante é obrigatório', 'Atenção', MB_IconExclamation | MB_Ok )
							Call SalSetFocus( dfnOrgao )
							Return FALSE
						If NOT SalIsNull( dfdInicial) and NOT SalIsNull( dfdFinal)
							If dfdInicial > dfdFinal
								Call SalMessageBeep( MB_IconExclamation )
								Call SalMessageBox( 'A data de entrada inicial deve ser menor do que a data de entrada final', 'Atenção', MB_IconExclamation | MB_Ok )
								Call SalSetFocus( dfdInicial )
								Return FALSE
						! Placa,
								NumeroNotAut as Controle,
								DataEmissao,
								ARCorreio,
								DataEntregaCorreios,
								DataExpedicao,
								DataPostagem,
								DataEntregaAoProp,
								isnull((select 'Notificação Autuação' where a.TipoDocumento = 89),'Notificação Penalidade') as Tipo
						!
						! Insere Dados na Tabela Temporária
						!
						! select 	#tmpCorreios.Placa, #tmpCorreios.NumeroNotAut,  
										#tmpCorreios.DataEmissao, #tmpCorreios.ARCorreio,  
										#tmpCorreios.DataEntregaCorreios, #tmpCorreios.DataExpedicao,  
										#tmpCorreios.DataPostagem, #tmpCorreios.DataEntregaAoProp,  
										#tmpCorreios.Tipo,
										#tmpCorreios.Dias,
										#tmpCorreios.DataRecebimentoCorr, 
										#tmpCorreios.DataRecebimentoFise,
										#tmpCorreios.Auto 
						If hWndReport != hWndNULL
							Call SalSendMsg( hWndReport, SAM_Close, 0,0)
						If dfsOpcao = 'A'
							If dfsSaida = 'I'
								Call SalReportPrint( hWndForm, 'NotifDat.qrp',
										'sGlobalSecretaria, 			sGlobalDetran, 		nGlobalSetor, 		sGlobalSetor,
										SqlUser, 				dfnOrgao, 		dfsOrgao,			dfdInicial,
										dfdFinal,
										
										sPlaca,
										nNumeroNotAut,
										dDataEmissao,
										sARCorreio,
										dDataEntregaCorreios,
										dDataExpedicao,
										dDataPostagem,
										dDataEntregaAoProp,
										sTipo,
										nDias,
										dDataRecebCorr,
										dDataRecebFise,
										sAuto',
										
										'Secretaria, 			Detran, 			Setor, 			NomeSetor,
										Usuario, 				OrgaoSel, 		DesOrgaoSel,		EntradaI,
										EntradaF,
												 
										Placa,
										NumeroNotAut,
										DataEmissao,
										ARCorreio,
										DataEntregaCorreios,
										DataExpedicao,
										DataPostagem,
										DataEntregaAoProp,
										Tipo,
										Dias,
										DataRecebCorreios,
										DataRecebFisepe,
										Auto', 1,RPT_PrintAll,0,0, wParam )
							Else
								Set hWndReport = SalCreateWindow( frmReportView, hWndForm )
								Call SalSetWindowText( hWndReport, 'Relatório de datas não atualizadas' )
								Call SalReportView( hWndForm, hWndReport, 'NotifDat.qrp',
										'sGlobalSecretaria, 			sGlobalDetran, 		nGlobalSetor, 		sGlobalSetor,
										SqlUser, 				dfnOrgao, 		dfsOrgao,			dfdInicial,
										dfdFinal,
										
										sPlaca,
										nNumeroNotAut,
										dDataEmissao,
										sARCorreio,
										dDataEntregaCorreios,
										dDataExpedicao,
										dDataPostagem,
										dDataEntregaAoProp,
										sTipo,
										nDias,
										dDataRecebCorr,
										dDataRecebFise,
										sAuto',
										
										'Secretaria, 			Detran, 			Setor, 			NomeSetor,
										Usuario, 				OrgaoSel, 		DesOrgaoSel,		EntradaI,
										EntradaF,
												 
										Placa,
										NumeroNotAut,
										DataEmissao,
										ARCorreio,
										DataEntregaCorreios,
										DataExpedicao,
										DataPostagem,
										DataEntregaAoProp,
										Tipo,
										Dias,
										DataRecebCorreios,
										DataRecebFisepe,
										Auto', wParam )
						Else
							If dfsSaida = 'I'
								Call SalReportPrint( hWndForm, 'NotifDats.qrp',
										'sGlobalSecretaria, 			sGlobalDetran, 		nGlobalSetor, 		sGlobalSetor,
										SqlUser, 				dfnOrgao, 		dfsOrgao,			dfdInicial,
										dfdFinal,
										
										sDataEmissao,
										nQtde',
										
										'Secretaria, 			Detran, 			Setor, 			NomeSetor,
										Usuario, 				OrgaoSel, 		DesOrgaoSel,		EntradaI,
										EntradaF,
												 
										DataEmissao,
										Qtde', 1,RPT_PrintAll,0,0, wParam )
							Else
								Set hWndReport = SalCreateWindow( frmReportView, hWndForm )
								Call SalSetWindowText( hWndReport, 'Relatório de datas não atualizadas' )
								Call SalReportView( hWndForm, hWndReport, 'NotifDats.qrp',
										'sGlobalSecretaria, 			sGlobalDetran, 		nGlobalSetor, 		sGlobalSetor,
										SqlUser, 				dfnOrgao, 		dfsOrgao,			dfdInicial,
										dfdFinal,
										
