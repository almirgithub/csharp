			Date/Time: dDataExpedicao
			Date/Time: dDataPostagem
			Date/Time: dDataEntregaAoProp
			String: sTipo
			Number: nDias
			Number: nQtde
			String: sDataEmissao
			String: sAuto
			Date/Time: dDataRecebCorr
			Date/Time: dDataRecebFise
			!
		Message Actions
			! Observações
				    Autor: Celso - 09/10/1997
			On SAM_Create
				Set _sTextoStatusBar = 'Tela de relatório de autos inconsistentes'
				Set _nEstadoInicialForm = EF_RegistroExistente
				Call SalCenterWindow( hWndForm )
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
			!
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				Set sSetorUsuarioN = SalStrMidX(SalNumberToStrX(nGlobalSetor,0),0,SalStrLength( SalNumberToStrX(nGlobalSetor,0) )-1)
				Set nSetorUsuario = SalStrToNumber(sSetorUsuarioN)
			!
			On SAM_Destroy
				If hWndReport != hWndNULL
					Call SalSendMsg( hWndReport, SAM_Close, 0,0)
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
			!
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo',
						'Relatório de autos inconsistentes', 'Relatório de autos inconsistentes' )
				Return TRUE
			On SAM_ReportFetchInit
				If fMPSConnect( hSql )
					If dfsOpcao	=	'A'
						If fMPSPrepareProc( hSql, 'exec dbinfracao..NotificacaoDatasCorreios
								@OrgaoAutuante 	= :frmRelNotificacaoDataExpedicao.dfnOrgao,
								@DataInicial 	= :frmRelNotificacaoDataExpedicao.dfdInicial,
								@DataFinal 	= :frmRelNotificacaoDataExpedicao.dfdFinal,
								@Opcao                  =:frmRelNotificacaoDataExpedicao.dfsOpcao',
								
								':frmRelNotificacaoDataExpedicao.sPlaca,
								:frmRelNotificacaoDataExpedicao.nNumeroNotAut,
								:frmRelNotificacaoDataExpedicao.dDataEmissao,
								:frmRelNotificacaoDataExpedicao.sARCorreio,
								:frmRelNotificacaoDataExpedicao.dDataEntregaCorreios,
								:frmRelNotificacaoDataExpedicao.dDataExpedicao,
								:frmRelNotificacaoDataExpedicao.dDataPostagem,
								:frmRelNotificacaoDataExpedicao.dDataEntregaAoProp,
								:frmRelNotificacaoDataExpedicao.sTipo,
								:frmRelNotificacaoDataExpedicao.nDias,
								:frmRelNotificacaoDataExpedicao.dDataRecebCorr,
								:frmRelNotificacaoDataExpedicao.dDataRecebFise,
								:frmRelNotificacaoDataExpedicao.sAuto' )
					Else
						If fMPSPrepareProc( hSql, 'exec dbinfracao..NotificacaoDatasCorreios
								@OrgaoAutuante 	= :frmRelNotificacaoDataExpedicao.dfnOrgao,
								@DataInicial 	= :frmRelNotificacaoDataExpedicao.dfdInicial,
								@DataFinal 	= :frmRelNotificacaoDataExpedicao.dfdFinal,
								@Opcao                  =:frmRelNotificacaoDataExpedicao.dfsOpcao',
								
								':frmRelNotificacaoDataExpedicao.sDataEmissao,
								:frmRelNotificacaoDataExpedicao.nQtde' )
					Return SqlExecute( hSql )
				!
				! sPlaca,
						nNumeroNotAut,
						dDataEmissao,
						sARCorreio,
						dDataEntregaCorreios,
						dDataExpedicao,
						dDataPostagem,
						dDataEntregaAoProp,
						sTipo',
			On SAM_ReportFetchNext
				If SqlFetchNext( hSql, lParam )
					Return TRUE
				Else
					Call fMPSDisconnect( hSql )
					Return FALSE
			On SAM_ReportFinish
				Call SalPrtSetDefault(sDeviceDef,sDriverDef,sPortaDef)
				Return TRUE
