		Message Actions
			On SAM_Create
				Call SalCenterWindow( hWndForm )
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Set sComandoArq = '
						exec	Prot..GerarArquivoBordero
							@DataInicial	= :frmGerarArqCheque.dfdInicial,
							@DataFinal	= :frmGerarArqCheque.dfdFinal,
							@OrgaoCompetencia =  :frmGerarArqCheque.dfOrgao,
							@Grupo   	=  :frmGerarArqCheque.dfGrupo,
							@Arquivo		= :frmGerarArqCheque.sNomArq,
							@Exercicio	= :frmGerarArqCheque.dfnExercicio,
							@ExercicioAtual 	= :frmGerarArqCheque.dfsTipoArq,
							@Diretorio	= :frmGerarArqCheque.dfsDiretorio'
				Set sIntoArq = ':frmGerarArqCheque.sString'
				Set sComandoImp = '
						exec	Prot..ArqBorderoS_Rel
							@Arquivo		= :frmGerarArqCheque.dfsArquivo,
							@Exercicio	= :frmGerarArqCheque.dfnExercicio  '
				Set sIntoImp= '
							:frmGerarArqCheque.dCriacao,
							:frmGerarArqCheque.sFinanceiro,
							:frmGerarArqCheque.sProcesso,
							:frmGerarArqCheque.sNome,
							:frmGerarArqCheque.sCgcCpf,
							:frmGerarArqCheque.nValorRest,
							:frmGerarArqCheque.sForma,
							:frmGerarArqCheque.sConta'
				Set sComandoCop = '
						exec	Prot..Copiar_ArqBordero
							@Arquivo	=	:frmGerarArqCheque.dfsArquivo,
							@NomeArquivo=     :frmGerarArqCheque.sArquivoCopiado,
							@Exercicio=	:frmGerarArqCehque.dfnExercicio '
				Set sIntoCop= ':frmGerarArqCheque.sArquivoCopiado'
							
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, 0, 0 )
				Set dfsDiretorio =   'C:\\BANDEPE\\'
				Set dfsDiretorioDest =   'D:\\BANDEPE\\'
				Set dfsTipoArq = 'S'
				Call SalSendMsg( tblArquivos, MSG_PesquisaTabela, wParam, lParam )
				Call SalDisableWindow( pbRelacao )
			On MSG_GravarArquivoTexto
				If fMPSPrepareProc( hSqlApp, sComandoArq, sIntoArq )
					If SqlExecute( hSqlApp )
						If SqlFetchNext( hSqlApp, nFetch )
							If nFetch = FETCH_Ok
								If SalFileOpen( fArquivo,  dfsDiretorio || sNomArq, OF_Create|OF_ReadWrite)
									While nFetch = FETCH_Ok
										Set sString = sString || SalStrRepeatX( ' ', 240 - SalStrLength( sString))
										Call SalFilePutStr( fArquivo, sString)
										Call SqlFetchNext( hSqlApp, nFetch )
									Call SalFileClose( fArquivo )
								Else
									Call SalMessageBox( 'Não foi possivel abrir o arquivo' || sNomArq || ' !','Aviso', MB_Ok | MB_IconHand)
									Return FALSE
					Else
						Return FALSE
				Return TRUE
			On MSG_CopiarArquivoTexto
				If NOT fMPSPrepareProc( hSqlApp, sComandoCop, sIntoCop )
					Call SalWaitCursor( FALSE )
					Return FALSE
				If NOT SqlExecute( hSqlApp )
					Call SalWaitCursor( FALSE )
					Return FALSE
				Return TRUE
				! Call SqlFetchNext( hSqlApp, nRetorno )
				! Call SalWaitCursor( TRUE )
			On SAM_ReportNotify
				Set hWndReportNotify = SalNumberToWindowHandle (wParam )
				If lParam = RPT_BeforePageHeader
					Call SalReportSetStringVar( hWndReportNotify, 'Des_Sec', sGlobalSecretaria )
					Call SalReportSetStringVar( hWndReportNotify, 'Des_Org', sGlobalDetran )
					Call SalReportSetNumberVar( hWndReportNotify, 'Cod_Set', nGlobalSetor )
					Call SalReportSetStringVar( hWndReportNotify, 'Des_Set', sGlobalSetor )
					Call SalReportSetStringVar( hWndReportNotify, 'Cod_User', SqlUser )
					Call SalReportSetStringVar( hWndReportNotify, 'Nom_Arq', dfsArquivo )
					Call SalReportSetStringVar( hWndReportNotify, 'ExercicioPG', SalNumberToStrX(dfnExercicio,0))
					Call SalReportSetStringVar( hWndReportNotify, 'Des_Arq', sDesArq )
					Call SalReportSetDateTimeVar( hWndReportNotify,'Dat_Ini', dfdInicial )
					Call SalReportSetDateTimeVar( hWndReportNotify,'Dat_Fin', dfdFinal )
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo', 'Relatório de Restituição', 'Relatório de de Restituição' )
				Return TRUE
			On SAM_ReportFetchInit
				Call fMPSPrepareProc( hSql,sComandoImp,sIntoImp )
				Return SqlExecute( hSql )
			On SAM_ReportFetchNext
				Call SqlFetchNext( hSql, nErro )
				If nErro != FETCH_EOF
					Return TRUE
				Else
					Return FALSE
			On SAM_ReportFinish
				Call SalPrtSetDefault(sDeviceDef,sDriverDef,sPortaDef)
				Call SqlGetResultSetCount( hSql, nCount )
				If nCount > 0
					Call SqlFetchRow( hSql, nCount,nCount)
			On SAM_Destroy
				If hWndReport != hWndNULL
					Call SalReportClose( hWndReport )
					Call SalDestroyWindow( hWndReport )
					Set hWndReport = hWndNULL
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
