				Message Actions
					On SAM_Create
						Call SalTblSetTableFlags( hWndItem, TBL_Flag_SingleSelection, TRUE )
						Call SalTblSetTableFlags( hWndItem, TBL_Flag_MovableCols, FALSE )
						Set _sTextoStatusBar = 'Nome dos Arquivos reconhecidos pelo Sistema, com suas Datas de criação'
						Set _bSempreHabilitado = TRUE
						Set _sCriacaoConexao = 'FALSE'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On MSG_Limpa
						Call SalTblSetFlagsAnyRows( hWndItem, ROW_Selected, FALSE, 0, 0 )
					On MSG_PesquisaTabela
						Call SalWaitCursor( TRUE )
						Call SalTblReset( tblArquivos )
						! Set hSqlApp = hSqlApp
						Select Case SalListQuerySelection( cmbsParametros )
							Case 0
								Call SybPrepareProc( hSqlApp, 'exec Prot..ArqBorderoS_Orgao  @Ultimos = 	1, @OrgaoCompetencia=:frmGerarArqCheque.dfOrgao,@Grupo=:frmGerarArqCheque.dfGrupo ',
										':tblArquivos.colsDiretorio ,:tblArquivos.colsArquivo, :tblArquivos.colnExercicio, :tblArquivos.colsProcessado, :tblArquivos.coldCriacao,
										:tblArquivos.coldDataInicial, :tblArquivos.coldDataFinal, :tblArquivos.colsArquivoCopiado, :tblArquivos.colsExerc')
								Break
							Case 1
								Call SybPrepareProc( hSqlApp, 'exec Prot..ArqBorderoS_Orgao @Ultimos = 	0, @OrgaoCompetencia=:frmGerarArqCheque.dfOrgao,@Grupo=:frmGerarArqCheque.dfGrupo ',
										':tblArquivos.colsDiretorio ,:tblArquivos.colsArquivo, :tblArquivos.colnExercicio, :tblArquivos.colsProcessado, :tblArquivos.coldCriacao,
										:tblArquivos.coldDataInicial, :tblArquivos.coldDataFinal, :tblArquivos.colsArquivoCopiado,:tblArquivos.colsExerc')
								Break
							Default
								Call SalMessageBeep( MB_IconExclamation )
								Call SalMessageBox( 'Opção inválida', 'Atenção', MB_IconExclamation | MB_Ok )
								Return FALSE
						If SalListQuerySelection( cmbsParametros ) >= 0
							Call SalTblPopulate( tblArquivos, hSqlApp, '', TBL_FillAll)
						Call SalWaitCursor( FALSE )
						Return TRUE
					On SAM_Click
						Call SalTblGetColumnText( hWndItem, SalTblQueryColumnID(colsArquivo), dfsArquivo )
						Call SalTblGetColumnText( hWndItem, SalTblQueryColumnID(colnExercicio), tblArquivos.sExercicioArq )
						Call SalTblGetColumnText( hWndItem, SalTblQueryColumnID(colsExerc), tblArquivos.sExerc )
						Set dfnExercicio = SalStrToNumber( tblArquivos.sExercicioArq )
						If tblArquivos.sExerc = 'A'
							Call SalSendMsg( rbAtual, SAM_Click, wParam, lParam )
							Set rbAtual = TRUE
						Else
							Call SalSendMsg( rbAnterior, SAM_Click, wParam, lParam )
							Set rbAnterior = TRUE
						Call SalTblGetColumnText( hWndItem, SalTblQueryColumnID(coldDataInicial), sDatIni )
						Set dfdInicial = SalStrToDate( sDatIni )
						Call SalTblGetColumnText( hWndItem, SalTblQueryColumnID(coldDataFinal), sDatFin )
						Set dfdFinal  = SalStrToDate( sDatFin )
						! Call SalDisableWindow( pbNovo )
						Call SalEnableWindow( pbRelacao )
					On SAM_DoubleClick
						Call SalSendMsg( hWndItem, SAM_Click, wParam, lParam )
					On SAM_RowHeaderClick
						Call SalSendMsg( hWndItem, SAM_Click, wParam, lParam )
					On SAM_RowHeaderDoubleClick
						Call SalSendMsg( hWndItem, SAM_Click, wParam, lParam )
