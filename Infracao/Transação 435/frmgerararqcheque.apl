				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Código do órgão de competência'
						Set _sNomeParaMsgs = 'Órgão de Competência'
						Set _sTipoMsgNulo = 'PADRAO'
						Set _bVisivel = TRUE
						Set _sConsultaFrmOpcoesSQLOuSP = 'SQL'
						! Set _sConsultaFrmOpcoesCampo = 'select convert( varchar( 10), Cod), DescricaoSintetica from ' || DATABASE_INFRACAO || '..OrgaoAutuante'
						Set _sConsultaFrmOpcoesCampo = '
								SELECT 	DISTINCT
									CONVERT(VARCHAR(10), gr.OrgaoCompetencia),
									OA.Descricao
								FROM 	dbinfracao..OrgaoAutuante OA,
									Prot..GrupoRecurso gr
								WHERE	 gr.Restituicao = "S" and gr.OrgaoCompetencia = OA.Cod '
						! Variáveis para indentificar o Componente Chave
						Set _bComponenteChave = TRUE
						Set _bCompChaveGeradoPeloSistema = TRUE
						Set _bCompChaveOpcionalParaPesquisa = TRUE
						Set _bDisparaConsultaEmValidate = FALSE
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_KillFocus
						Call SalWaitCursor( TRUE )
						Call SalSendMsg( tblArquivos, MSG_PesquisaTabela, 0, 0 )
						Call SalWaitCursor( FALSE )
