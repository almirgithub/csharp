			Function: ArquivoCorreto
				Description:
				Returns
				Parameters
					String: sArquivo
				Static Variables
				Local variables
					File Handle: hArquivo
				Actions
					If NOT SalFileOpen( hArquivo, sArquivo, OF_Read | OF_Share_Exclusive)
						Call SalWaitCursor( FALSE )
						Call SalFileClose( hArquivo )
						Call SalMessageBeep( MB_IconStop )
						Call SalMessageBox( 'Não foi possível abrir arquivo: ' || sArquivo||' verifique se caminho está correto',
								'Atenção', MB_Ok|MB_IconStop )
						Return FALSE
					! Lê os dados do Header do Arquivo.
					Set nCaracLido =  SalFileRead( hArquivo, lsHeader, nTamanhoLinha + 2 )
					Call SalFileClose( hArquivo )
					If nCaracLido < nTamanhoLinha
						Call SalMessageBeep( MB_IconExclamation )
						Call SalMessageBox( 'Arquivo informado não é um ' || sTipoArquivo, 'Atenção', MB_IconExclamation | MB_Ok )
						Return FALSE
					If NOT HeaderCorreto(  )
						Return FALSE
					Return TRUE
			Function: ReProcessar
				Description:
				Returns
				Parameters
					String: sUsu
					Date/Time: dCri
				Static Variables
				Local variables
					String: sAux
				Actions
					Call SalSendMsg( dfmContador, SAM_Validate, 0, 0 )
					If SalIsNull( dfnOrgao)
						Call SalMessageBeep( MB_IconExclamation )
						Call SalMessageBox( 'O órgão autuante é obrigatório', 'Aviso', MB_Ok | MB_IconAsterisk )
						Call SalSetFocus( dfnOrgao )
						Return FALSE
					If SalIsNull( dfsErro)
						Call SalMessageBeep( MB_IconExclamation )
						Call SalMessageBox( 'O nome do arquivo de erros é obrigatório', 'Aviso', MB_Ok | MB_IconAsterisk )
						Call SalSetFocus( dfsErro )
						Return FALSE
					Call SalClearField( dfsArquivo)
					Set sUsuario = sUsu
					Set dCriacao = dCri
					Set sOcorrencia = ''
					If SalListQueryCount( lbMsg ) > 1
						Call SalListAdd( lbMsg, SalStrRepeatX( '_', 70 ) )
					If NOT Processar( )
						Call SalPostMsg( pbLimpar, SAM_Click, 0, 0)
						Return FALSE
					If NOT GeraArqErros(  dCriacao, dfsErro)
						Return FALSE
					Call SalSendMsg( dfmContador, SAM_Validate, 65000, 0 )
					Call SalMessageBeep( MB_IconExclamation )
					Call SalDateToStr( dCriacao, sAux )
					If NOT bErros
						Call SalMessageBox( 'O arquivo que foi lido em '  || sAux ||   ' pelo usuário ' || sUsuario || ', foi re-processado', 'Aviso', MB_Ok | MB_IconExclamation )
					Else
						If SalMessageBox( 'Arquivo que foi lido em '  || sAux ||   ' pelo usuário ' || sUsuario || ', foi re-processado e gerado arquivo de erros. Deseja abrir a tela para impressão dos erros ?', 'Aviso', MB_YesNo | MB_IconExclamation ) = IDYES
							If hWndRelMulta = hWndNULL
								Set hWndRelMulta= SalCreateWindow( frmRelMulta, hWndMDI )
							Else
								Call SalPostMsg( hWndRelMulta, MSG_Limpa, 0, 0)
								Call SalBringWindowToTop( hWndRelMulta )
							Call frmRelMulta.SelecionaArquivo( dCriacao)
					Return TRUE
