				Message Actions
					On SAM_Create
						Set _sNroCamposIgnorados = 'NENHUM'
						Set _sTextoStatusBar = 'Fazer a entrada do arquivo de multas'
						Set _sTipoComando = 'Comum'
						Set _bValidaCamposNulos = TRUE
						Set _nPermissao = EF_RegistroExistente | EF_RegistroNovo
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						If SalIsNull( dfnOrgao)
							Call SalMessageBeep( MB_IconExclamation )
							Call SalMessageBox( 'O órgão autuante é obrigatório', 'Aviso', MB_Ok | MB_IconAsterisk )
							Call SalSetFocus( dfnOrgao )
							Return FALSE
						If SalIsNull( dfsArquivo)
							Call SalMessageBeep( MB_IconExclamation )
							Call SalMessageBox( 'Nome do arquivo a ser lido é obrigatório', 'Aviso', MB_Ok | MB_IconAsterisk )
							Call SalSetFocus( dfsArquivo )
							Call SalSendMsg( pbProcessar, MSG_Desabilita1, 0, 0 )
							! Call SalSendMsg( pbListagem, MSG_Desabilita1, 0, 0 )
							Return FALSE
						If SalIsNull( dfsErro)
							Call SalMessageBeep( MB_IconExclamation )
							Call SalMessageBox( 'O nome do arquivo de erros é obrigatório', 'Aviso', MB_Ok | MB_IconAsterisk )
							Call SalSetFocus( dfsErro )
							Return FALSE
						Set bErros = FALSE
						Call SalWaitCursor( TRUE )
						Call SalFileClose( hArquivo )
						Call SalSendMsg( dfmContador, SAM_Validate, 0, 0 )
						If SalListQueryCount( lbMsg ) > 1
							Call SalListAdd( lbMsg, SalStrRepeatX( '_', 70 ) )
							Call SalListSetSelect( lbMsg, SalListQuerySelection( lbMsg )+ 1 )
						! Verifica se o Arquivo já foi processado. Deleta dados se já houve tentativa de processamento.
						Call AntesVerificacao( )
						Set hSqlErro = hSqlApp
						Call SybPrepareProc( hSqlApp, sProcedureCriacao,
								':frmArqMulta.sUsuario, :frmArqMulta.dCriacao, :frmArqMulta.sProcessado')
						If NOT SqlExecute( hSqlApp )
							Call SalWaitCursor( FALSE )
							Return FALSE
						Call SqlFetchNext( hSqlApp, nAux )
						If sProcessado != 'N' and sProcessado != 'S'
							Call SalListAdd( lbMsg, 'Transferindo o ' || sTipoArquivo || ': ' || dfsArquivo )
							Call SalListAdd( lbMsg, 'Horário de início:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss' ) )
							Call SalListSetSelect( lbMsg, SalListQuerySelection( lbMsg )+ 2 )
							If NOT SalFileOpen( hArquivo, dfsArquivo, OF_Read | OF_Share_Exclusive)
								Call SalListAdd( lbMsg, ' ' )
								Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
								Call SalWaitCursor( FALSE )
								Call SalMessageBeep( MB_IconStop )
								Call SalMessageBox( 'Não foi possível abrir arquivo: '||dfsArquivo||' verifique se caminho está correto, ou se esta sendo usado por alguém',
										'Atenção', MB_Ok|MB_IconStop )
								Call SalSetFocus( dfsArquivo )
								Call SalSendMsg( pbProcessar, MSG_Desabilita1, 0, 0 )
								Return FALSE
							Set nCaracLido =  SalFileRead( hArquivo, lsHeader, nTamanhoLinha + 2 )
							Set nRegLido = 0
							If hWndDetalhe != hWndNULL
								Call SalSendMsg( hWndDetalhe, SAM_Close, 0, 0 )
							Set hWndDetalhe = AbreDetalhe( )
							If hWndDetalhe = hWndNULL
								Call SalSendMsg( dfmContador, SAM_Validate, 0, 0 )
								Call SalFileClose( hArquivo )
								Call SalListAdd( lbMsg, 'Horário do erro:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss'))
								Call SalListAdd( lbMsg, ' ' )
								Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
								Call SalWaitCursor( FALSE )
								Return FALSE
							Set nCaracLido =  SalFileRead( hArquivo, lsRegistro, nTamanhoLinha + 2 )
							Call SalStatusSetText( hWndForm, 'Lendo dados do ' || sTipoArquivo )
							Call SalUpdateWindow( hWndForm)
							! Lê os Registro do Tipo Detalhe
							While ContinuaLeitura( ) AND nCaracLido = nTamanhoLinha + 2
								Call SalSendMsg( hWndDetalhe, SAM_User, 65520, SalHStringToNumber( lsRegistro ) )
								Set nRegLido = nRegLido + 1
								Set lsRegistro = ''
								Set nCaracLido =  SalFileRead( hArquivo, lsRegistro, nTamanhoLinha + 2 )
								If SalNumberMod( nRegLido, 5) = 0
									Call SalSendMsg( dfmContador, SAM_Validate, nRegLido, 0)
									Call SalStatusSetText( hWndForm, 'Transferindo os dados para uma tabela temporária... Número do registro: ' || SalNumberToStrX( nRegLido, 0 ) ||
											'; de um total de : ' || SalNumberToStrX( dfnArqTamanho / nTamanhoLinha , 0) || ' registros')
									If SalNumberMod(nRegLido, 50) = 0
										Call SalUpdateWindow( hWndMDI)
										If NOT SalSendMsg( hWndDetalhe, MSG_Arquiva, 0, 0)
											Call SalWaitCursor( FALSE )
											Call SalListAdd( lbMsg, 'Horário do erro:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss'))
											Call SalListAdd( lbMsg, ' ' )
											Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
											Call SalFileClose( hArquivo )
											Call SalPostMsg( pbLimpar, SAM_Click, 0, 0)
											Return FALSE
							Call SalFileClose( hArquivo )
							If nCaracLido < nTamanhoLinha
								Call SalWaitCursor( FALSE )
								Call SalMessageBeep( MB_IconQuestion )
								If SalMessageBox( 'Erro no final do arquivo. Trailer de tamanho ' || SalNumberToStrX( nCaracLido, 0 ) ||
										' bytes. Sendo que deveria ser de tamanho de ' ||  SalNumberToStrX( nTamanhoLinha, 0 ) ||  ', deseja continuar ?', 'Aviso', MB_YesNo | MB_IconQuestion ) = IDNO
									Call SalListAdd( lbMsg, ' ' )
									Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
									Return FALSE
							Call SalStatusSetText( hWndForm, 'Transferindo os dados para uma tabela temporária' )
							If bManutencao
								Call SalMessageBeep( MB_IconQuestion )
								If SalMessageBox( 'Os dados foram lidos. Deseja arquivar os últimos registros ?', 'Atenção', MB_YesNo | MB_IconQuestion | MB_DefButton2)  =  IDNO
									Call SalWaitCursor( FALSE )
									Return FALSE
							! Adiciona dados finais do arquivo
							If NOT SalSendMsg( hWndDetalhe, MSG_Arquiva, 0, 0)
								Call SalWaitCursor( FALSE )
								Call SalListAdd( lbMsg, 'Horário do erro:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss'))
								Call SalListAdd( lbMsg, ' ' )
								Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
								Return FALSE
							Call SalSendMsg( hWndDetalhe, SAM_Close, 0, 0)
							Call SalSendMsg( dfmContador, SAM_Validate, nRegLido, 0)
							! Lê o Trailer.
							If NOT TrailerCorreto( )
								Call SalWaitCursor( FALSE )
								Call SalListAdd( lbMsg, 'Horário do erro:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss'))
								Call SalListAdd( lbMsg, ' ' )
								Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
								Return FALSE
							Set nAux = NumeroRegistros( )
							! If nRegLido  != NumeroRegistros( )
								    Call SalMessageBeep( MB_IconQuestion )
								    If SalMessageBox( 'Foram lido ' || SalNumberToStrX( nRegLido , 0) || ' registros, só que o arquivo informou um total de '
										||SalNumberToStrX( nAux, 0) || ' registros. Deseja continuar com o processamento ?',
										'Atenção', MB_YesNo | MB_IconQuestion | MB_DefButton2)  =  IDNO
									    Call SalWaitCursor( FALSE )
									    Return FALSE
							If nRegLido + 2  != NumeroRegistros( )
								Call SalMessageBeep( MB_IconQuestion )
								If SalMessageBox( 'Foram lido ' || SalNumberToStrX( nRegLido + 2, 0) || ' registros, só que o arquivo informou um total de '
										||SalNumberToStrX( nAux, 0) || ' registros. Deseja continuar com o processamento ?',
										'Atenção', MB_YesNo | MB_IconQuestion | MB_DefButton2)  =  IDNO
									Call SalWaitCursor( FALSE )
									Call SalListAdd( lbMsg, ' ' )
									Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
									Return FALSE
							If NOT FimLeitura( )
								Call SalWaitCursor( FALSE )
								Call SalListAdd( lbMsg, 'Horário do erro:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss'))
								Call SalListAdd( lbMsg, ' ' )
								Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
								Return FALSE
							Call SalListAdd( lbMsg, 'Horário de término:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss' ) )
							Call SalListAdd( lbMsg, ' ' )
							Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
						Else
							Set nRegLido = dfnArqTamanho / nTamanhoLinha
						If bManutencao
							Call SalMessageBeep( MB_IconQuestion )
							If SalMessageBox( 'Os dados foram lidos. Deseja continuar com o processamento ?', 'Atenção', MB_YesNo | MB_IconQuestion | MB_DefButton2)  =  IDNO
								Call SalWaitCursor( FALSE )
								Return FALSE
						! Processamento dos dados
						Call SalUpdateWindow( hWndForm )
						If NOT Processar( )
							Call SalWaitCursor( FALSE )
							Call SalPostMsg( pbLimpar, SAM_Click, 0, 0)
							Return FALSE
						If NOT bManutencao
							If hWndHeader  != hWndNULL
								Call SalSendMsg( tblMultaHeader, SAM_Close, 0, 0)
							If hWndDetalhe != hWndNULL
								Call SalSendMsg( hWndDetalhe, SAM_Close, 0, 0)
							If hWndTrailer   != hWndNULL
								Call SalSendMsg( hWndTrailer, SAM_Close, 0, 0)
						Call SalMessageBeep( MB_Ok )
						Call SalWaitCursor( FALSE )
						Call SalSendMsg( hWndItem, MSG_Desabilita1, 0, 0 )
						Call SalSendMsg( cmbsParametros, SAM_Click, 0, 0)
						If NOT GeraArqErros( dCriacao, dfsErro )
							Call SalListAdd( lbMsg, ' ' )
							Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
							Return FALSE
						Call SalSendMsg( dfmContador, SAM_Validate, 65000, 0 )
						Call SalMessageBeep( MB_IconExclamation )
						If NOT bErros
							Call SalMessageBox( 'Arquivo lido e processado', 'Aviso', MB_Ok | MB_IconExclamation )
						Else
							If SalMessageBox( 'Arquivo lido e processado e gerado arquivo de erros. Deseja abrir a tela para impressão dos erros ?', 'Aviso', MB_YesNo | MB_IconExclamation ) = IDYES
								If hWndRelMulta = hWndNULL
									Set hWndRelMulta= SalCreateWindow( frmRelMulta, hWndMDI )
								Else
									Call SalPostMsg( hWndRelMulta, MSG_Limpa, 0, 0)
									Call SalBringWindowToTop( hWndRelMulta )
								Call frmRelMulta.SelecionaArquivo( dCriacao)
						Return TRUE
