				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Caminho e nome do ' || sTipoArquivo || ' a ser lido - Tecle <<?>> para selecionar o arquivo'
						Set _sNomeParaMsgs = 'Arquivo'
						Set _bVisivel = TRUE
						Set _bPermiteNulos = FALSE
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_SetFocus
						Call SalFileClose( hArquivo )
					On SAM_AnyEdit
						! Abre dialog box para seleção de arquivo
						If SalStrRightX( dfsArquivo, 1) = '?'
							Call SalClearField( hWndItem )
							If SalDlgOpenFile( frmArqMulta, 'Seleção do ' || sTipoArquivo, saOpcaoSelecao,8, nIndice, sArquivo, dfsArquivo )
								Call SalSendMsg( hWndItem, SAM_Validate, 0, 0 )
					On SAM_Validate
						If hWndHeader != hWndNULL
							Call SalSendMsg( hWndHeader, SAM_Close, 0, 0)
						If hWndDetalhe != hWndNULL
							Call SalSendMsg( hWndDetalhe, SAM_Close, 0, 0)
						Call SalClearField( dfnArqTamanho )
						Call SalSendMsg( dfmContador, SAM_Validate, 0, 0 )
						If dfsArquivo != ''
							If NOT ArquivoCorreto(  dfsArquivo )
								Call SalSendMsg( pbProcessar, MSG_Desabilita1, 0, 0 )
								Return VALIDATE_Cancel
							Call SalSendMsg( dfnArqTamanho, SAM_Validate, 0, 0 )
							Call SalSendMsg( pbProcessar, MSG_Habilita1, 0, 0 )
							If SalIsNull( dfsErro)
								Set dfsErro = NomeDiretorio( dfsArquivo) || 'Erro.TXT'
								Call SalSetFieldEdit( dfsErro, TRUE)
						Else
							Call SalFileClose( hArquivo )
							Call SalSendMsg( pbProcessar, MSG_Desabilita1, 0, 0 )
						Return VALIDATE_OkClearFlag
