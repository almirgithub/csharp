			! Variáveis para controle da manutenção
			Boolean: bManutencao
			Number: nValorManutencao
		Message Actions
			! Observação
				    Autor
					    Celso - 13/05/97
			On SAM_Create
				Set bManutencao = FALSE
				Call SalCenterWindow( hWndForm )
				! Inicializa valores para o listbox do dialogbox Seleciona Arquivo
				Set saOpcaoSelecao [0] = 'Todos os Arquivos   (*.*)'
				Set saOpcaoSelecao [1] = '*.*'
				Set saOpcaoSelecao [2] = 'Arquivos Textos      (*.txt)'
				Set saOpcaoSelecao [3] = '*.txt'
				Set nIndice = 2
				Set _sTextoStatusBar = 'Tela para leitura do arquivo de entrada de multas'
				Set _nEstadoInicialForm = EF_RegistroExistente
				Set _bLimpaTelaAposAtualizacao = TRUE
				Set _bLimpaTelaAposInclusao = FALSE
				Set _bLimpaTelaAposRemocao = TRUE
				! Variáveis para definição do arquivo
				Set nTamanhoLinha =  250
				Set sTipoArquivo = 'arquivo de entrada de multas'
				Set sProcedureCriacao = 'exec ArqMultaS_existe_insere @Diretorio = :frmArqMulta.sDiretorio, @Arquivo = :frmArqMulta.sArquivo'
				Set sProcedureVerificacao = 'exec ArqMultaD_inconsistentes'
				Set sProcedureProcessar = 'exec HisMultaI_processar @Orgao = :frmArqMulta.dfnOrgao,
						@Usuario = :frmArqMulta.sUsuario, @DataCriacao = :frmArqMulta.dCriacao,
						@NumRegistros = :frmArqMulta.nRegLido, @Header = :frmArqMulta.lsRegistro'
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, 0, 0 )
				Call SalPostMsg( pbLimpar, SAM_Click, 0, 0 )
			On SAM_Destroy
				Call SalFileClose( hArquivo )
				If hWndHeader != hWndNULL
					Call SalSendMsg( hWndHeader, SAM_Close, 0, 0)
				If hWndDetalhe != hWndNULL
					Call SalSendMsg( hWndDetalhe, SAM_Close, 0, 0)
				If hWndTrailer != hWndNULL
					Call SalSendMsg( hWndTrailer, SAM_Close, 0, 0)
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
			On MSG_Limpa
				If hWndDetalhe != hWndNULL
					Call SalSendMsg( hWndDetalhe, SAM_Close, 0, 0)
				Set nAux = SalListQuerySelection( cmbsParametros )
				Call SalSendClassMessage( MSG_Limpa, wParam, lParam)
				Call SalListSetSelect( cmbsParametros, nAux )
				If nAux = 0
					Call SalTblReset( tblArquivos )
				Call SalSendMsg( dfmContador, SAM_Validate, 0, 0 )
				Set sProcessado = ''
				Set hSqlErro = hSql
				Call SybPrepareProc( hSql, sProcedureVerificacao,
						':frmArqMulta.sUsuario, :frmArqMulta.dCriacao, :frmArqMulta.sProcessado,
						:frmArqMulta.dfnOrgao, :frmArqMulta.dfsOrgao, :frmArqMulta.dfsErro')
				If NOT SqlExecute( hSql )
					Return FALSE
				Call SqlFetchNext( hSql, nAux )
				If sProcessado = 'N'
					Call SalMessageBeep( MB_IconExclamation )
					Call SalDateToStr( dCriacao, sAux )
					If SalMessageBox( 'Encontrado um arquivo lido em '  || sAux ||   ' pelo usuário ' || sUsuario || ', mas não foi processado completamente, deseja processá-lo novamente ?' ,
							'Atenção', MB_IconExclamation | MB_YesNo ) = IDNO
						Return FALSE
					Set dfsErro = dfsErro || 'ERRO.TXT'
					Call ReProcessar( sUsuario, dCriacao )
