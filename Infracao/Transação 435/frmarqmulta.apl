						Return tblTrailerMulta.NumeroRegistros( )
					Return FALSE
			Function: GeraArqErros
				Description:
				Returns
				Parameters
					Date/Time: dDataCriacao
					String: sArquivo
				Static Variables
				Local variables
					Number: nRegistros
					String: sHeader
					String: sRegistro1
					String: sRegistro2
					File Handle: hArquivo
					Boolean: bFimLeitura
				Actions
					If bErros
						Call SalWaitCursor( TRUE )
						Call SalStatusSetText( hWndForm, 'Gerando arquivos de erros' )
						Call SalListAdd( lbMsg, 'Criando arquivo de erros: ' || dfsErro )
						Call SalListAdd( lbMsg, 'Horário de início:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss' ) )
						Call SalListSetSelect( lbMsg, SalListQuerySelection( lbMsg )+ 2 )
						Set hSqlErro = hSqlApp
						Call SybPrepareProc( hSqlApp, 'ArqMultaS_header @DataCriacao = :dDataCriacao',
								':sHeader')
						If NOT SqlExecute( hSqlApp )
							Call SalListAdd( lbMsg, 'Horário do erro:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss'))
							Call SalListAdd( lbMsg, ' ' )
							Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
							Call SalWaitCursor( FALSE )
							Call SalMessageBeep( MB_IconAsterisk)
							Call SalMessageBox( 'Não foi criado corretamente o arquivo de erros', 'Atenção', MB_IconAsterisk | MB_Ok)
							Return FALSE
						Call SqlFetchNext( hSqlApp, nProximo )
						If nProximo != FETCH_Ok
							Call SalListAdd( lbMsg, 'Horário do erro:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss'))
							Call SalListAdd( lbMsg, ' ' )
							Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
							Call SalWaitCursor( FALSE )
							Call SalMessageBeep( MB_IconAsterisk)
							Call SalMessageBox( 'Não foi criado corretamente o arquivo de erros', 'Atenção', MB_IconAsterisk | MB_Ok)
							Return FALSE
						If NOT SalFileOpen( hArquivo, sArquivo, OF_Write | OF_Create | OF_Prompt )
							Call SalListAdd( lbMsg, 'Horário do erro:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss'))
							Call SalListAdd( lbMsg, ' ' )
							Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
							Call SalWaitCursor( FALSE )
							Call SalMessageBeep( MB_IconStop )
							Call SalMessageBox( 'Não foi possível abrir arquivo de erros: '|| sArquivo ||' verifique se caminho está correto, ou se esta sendo usado por alguém',
									'Atenção', MB_Ok|MB_IconStop )
							Call SalSetFocus( dfsErro)
							Return FALSE
						Call SalFilePutStr( hArquivo, sHeader )
						Call SalWaitCursor( FALSE )
						Set hSqlErro = hSqlApp
						Call SybPrepareProc( hSqlApp, 'exec HisMultaS_arq_erros @DataCriacao = :dDataCriacao',
								':sRegistro1, :sRegistro2')
						If NOT SqlExecute( hSqlApp )
							Call SalListAdd( lbMsg, 'Horário do erro:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss'))
							Call SalListAdd( lbMsg, ' ' )
							Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
							Call SalWaitCursor( FALSE )
							Call SalMessageBeep( MB_IconAsterisk)
							Call SalMessageBox( 'Não foi criado corretamente o arquivo de erros', 'Atenção', MB_IconAsterisk | MB_Ok)
							Return FALSE
						Set bFimLeitura = FALSE
						If NOT  SqlFetchNext( hSqlApp, nProximo )
							Set bFimLeitura = TRUE
						Set nRegistros = 2
						Loop
							Set nRegistros = nRegistros + 1
							Call SalSendMsg( dfmContador, SAM_Validate, nRegLido + nRegistros, 0)
							If NOT bFimLeitura
								Call SalFilePutStr( hArquivo, sRegistro1 || sRegistro2)
								If NOT  SqlFetchNext( hSqlApp, nProximo )
									Set bFimLeitura = TRUE
									If SalNumberMod( nRegistros, 131 ) = 0
										Break
							Else
								Call SalFilePutStr( hArquivo, SalStrRepeatX( ' ', 250 ) )
								If SalNumberMod( nRegistros, 131 ) = 0
									Break
						Call SalFilePutStr( hArquivo, '90' || SalStrRightX( SalNumberToStrX( 1000000000 + nRegistros, 0 ), 9 )
								|| SalStrRepeatX( ' ' , 239 ) )
						Call SalFileClose( hArquivo )
						If VisFileGetSize( sArquivo ) <= 0
							Call SalFileOpen( hArquivo, sArquivo, OF_Delete )
						Call SalListAdd( lbMsg, 'Horário de término:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss' ) )
						Call SalListAdd( lbMsg, 'Registros gerados:	' || SalNumberToStrX( nRegistros, 0 ) )
						Call SalListAdd( lbMsg, ' ' )
						Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
					Return TRUE
			Function: Processar
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Call SalListAdd( lbMsg, 'Processando o ' || sTipoArquivo || ' criado em ' || SalFmtFormatDateTime( dCriacao, 'dd-MM-yyyy hhhh:mm:ss')
							|| ' pelo usuário ' || sUsuario )
					Call SalListAdd( lbMsg, 'Horário de início:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss'))
					Call SalListSetSelect( lbMsg, SalListQuerySelection( lbMsg )+ 2 )
					Call SalStatusSetText( hWndForm, 'Processando os dados indicados no arquivo' )
					Set hSqlErro = hSqlApp
					Call SybPrepareProc( hSqlApp, sProcedureProcessar, ':frmArqMulta.bErros')
					If NOT SqlExecute( hSqlApp )
						Call SalWaitCursor( FALSE )
						Call SalListAdd( lbMsg, 'Horário do erro:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss'))
						Call SalListAdd( lbMsg, ' ' )
						Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
						Call SalMessageBeep( MB_IconAsterisk)
						Call SalMessageBox( 'Não foi processado corretamente o ' || sTipoArquivo || '. Tente processá-lo novamente', 'Atenção', MB_IconAsterisk | MB_Ok )
						Return FALSE
					Call SqlFetchNext( hSqlApp, nProximo )
					If nProximo != FETCH_Ok
						Call SalListAdd( lbMsg, 'Horário do erro:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss'))
						Call SalListAdd( lbMsg, ' ' )
						Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
						Call SalMessageBeep( MB_IconAsterisk)
						Call SalMessageBox( 'Não foi processado corretamente o ' || sTipoArquivo, 'Atenção', MB_IconAsterisk | MB_Ok )
						Call SalWaitCursor( FALSE )
						Return FALSE
					Call SalListAdd( lbMsg, 'Horário de término:	' || SalFmtFormatDateTime( SalDateCurrent(  ), 'dd-MM-yyyy hhhh:mm:ss' ) )
					Call SalListAdd( lbMsg, ' ' )
					Call SalListSetSelect( lbMsg, SalListSelectString( lbMsg, SalListQuerySelection( lbMsg ), ' ' ) )
					Return TRUE
			Function: ContinuaLeitura
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Return SalStrLeftX( lsRegistro, 2) = '10'
			Function: FimLeitura
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Call SalStatusSetText( hWndForm, 'Atualizando dados do header' )
					Set hSqlErro = hSqlApp
					Call SybPrepareProc( hSqlApp, 'exec ArqMultaU_header @Usuario = :frmArqMulta.sUsuario, @DataOcorrencia  = :frmArqMulta.sOcorrencia,
							@DataCriacao = :frmArqMulta.dCriacao, @Header = :frmArqMulta.lsHeader, @Orgao = :frmArqMulta.dfnOrgao', ':bErros')
					If NOT SqlExecute( hSqlApp )
						Call SalWaitCursor( FALSE )
						Return FALSE
					Return TRUE
		Window Parameters
		Window Variables
			Number: nTamanhoLinha
			String: sTipoArquivo
			String: sProcedureCriacao
			String: sProcedureProcessar
			String: sProcedureVerificacao
			! ____________
			String: sOcorrencia
			Boolean: bErros
			String: sUsuario
			Date/Time: dCriacao
			String: sProcessado
			Number: nSequencial
			Number: nCaracLido
			Number: nRegLido
			Long String: lsHeader
			Long String: lsRegistro
			Window Handle: hWndHeader
			Window Handle: hWndDetalhe
			Window Handle: hWndTrailer
			! Handle do arquivo
			File Handle: hArquivo
			! Variaveis para  dialog box seleção de arquivo
			String: saOpcaoSelecao [8]
			Number: nIndice
			! Nome do Arquivo (Sem o Caminho).
			String: sArquivo
			String: sDiretorio
			! Variáveis 'padrão'
			Number: nProximo
