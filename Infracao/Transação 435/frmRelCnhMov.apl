				Message Actions
					On SAM_Create
						Set _sNroCamposIgnorados = 'NENHUM'
						Set _sTextoStatusBar = 'Imprime o relatório'
						Set _sTipoComando = 'Comum'
						Set _bValidaCamposNulos = TRUE
						Set _nPermissao = EF_PesquisaHabilitada | EF_RegistroExistente | EF_RegistroNovo  | EF_Vazio
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						Set nErro = 0
						Call SqlImmediate( 'select getdate() into :dDataImp' )
						! Set sNomeQRP = 'RelCNHMovPeriodo.qrp'
						If SalIsNull( dfdInicial )
							Call SalMessageBeep( 0 )
							Call SalMessageBox( 'Data inicial deve ser escolhida !', 'Atenção', MB_Ok | MB_IconExclamation )
							Return FALSE
						If SalIsNull( dfdFinal )
							Call SalMessageBeep( 0 )
							Call SalMessageBox( 'Data final deve ser escolhida !', 'Atenção', MB_Ok | MB_IconExclamation )
							Return FALSE
						! Mostra o relatório
						If rbImpressora
							Call SalReportPrint( hWndForm, 'RelCNHMovPeriodo.qrp',  '
									sGlobalSecretaria, 			
									sGlobalDetran,
									nGlobalSetor,			
