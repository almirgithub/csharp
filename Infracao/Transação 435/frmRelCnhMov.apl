		Functions
		Window Parameters
		Window Variables
			Number: nDia
			Number: nResultado
			! Variáveis 'padrão' de relatório
			Boolean: bAux
			Boolean: bAbortaImpressao
			String: sStored
			Number: nErro
			String: sDevice
			String: sDriver
			String: sPorta
			String: sDeviceDef
			String: sDriverDef
			String: sPortaDef
			Window Handle: hWndReportView
			Number: nAux
			Date/Time: dData
			Window Handle: hWndRel
			Window Handle: hWndRelInf
			Date/Time: dDataImp
			Number: nCont
			String: sPlaca
			String: sOrgaoAutuante
			String: sSerie
			String: sCondutor
			String: sOficio
			String: sTesteAlcoolemia
			Number: nAuto
			Number: nDigito
			Number: nInfracao
			Number: nDesdobramento
			Number: nCNH
			Number: nProtocolo
			Date/Time: dDataOficio
			String: sObservacao
		Message Actions
			On SAM_Create
				Set _sTextoStatusBar = 'Tela de relatório de repasses.'
				Set _nEstadoInicialForm = EF_Vazio
				Set _bLimpaTelaAposAtualizacao = TRUE
				Set _bLimpaTelaAposInclusao = TRUE
				Call SalCenterWindow( hWndForm )
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
			! On SAM_ReportNotify
				If lParam = RPT_BeforePageHeader
					Call SalReportSetNumberVar( SalNumberToWindowHandle ( wParam ), 'nResultado', nResultado )
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo',
						'Relatórios das impressões de CNH´s movimentadas/protocoladas', 'por Período' )
				Return TRUE
			On SAM_ReportFetchInit
				Call SalWaitCursor( TRUE )
				! Call SalReportSetNumberVar( SalNumberToWindowHandle ( wParam ), 'nResultado', nResultado )
				!
				If fMPSPrepareProc( hSqlApp, 'exec dbinfracao..RelCNHRecolhidaS
								@DataInicial		= :frmRelCnhMov.dfdInicial,
								@DataFinal 		= :frmRelCnhMov.dfdFinal',
								':nProtocolo,
								:sPlaca,
								:sOrgaoAutuante,
								:sSerie,
								:nAuto,
								:nDigito,
								:nInfracao,
								:nDesdobramento,
								:nCNH,
								:sCondutor,
								:sOficio,
								:dDataOficio,
								:sObservacao')
					If NOT SqlExecute( hSqlApp )
						Call SalWaitCursor( FALSE )
						Return FALSE
				!
				Call SalWaitCursor( FALSE )
				!
				Return SqlGetResultSetCount( hSqlApp, nErro )
				If nErro = 0
					Call SalMessageBox('Não existe registros para essa pesquisa', 'Atenção', MB_IconExclamation | MB_Ok)
					Return FALSE
			On SAM_ReportFetchNext
				Call SqlFetchNext( hSqlApp, nErro )
				If nErro != FETCH_EOF
					Return TRUE
				Else
					Return FALSE
			On SAM_ReportFinish
				Call SalPrtSetDefault(sDeviceDef,sDriverDef,sPortaDef)


