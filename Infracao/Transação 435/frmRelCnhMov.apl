									sSerie,
									nAuto, 
									nDigito, 
									nInfracao, 
									nDesdobramento, 
									nCNH, 
									sCondutor, 
									sOficio, 
									dDataOficio,
									sObservacao',
									1,RPT_PrintAll,0,0, nErro )
						Else
							Set hWndReportView= SalCreateWindow( frmReportView, hWndForm )
							Call SalSetWindowText( hWndReportView,  'Relatórios das impressões de CNH´s movimentadas/protocoladas por período ' )
							Call SalReportView( hWndForm, hWndReportView, 'RelCNHMovPeriodo.qrp', '
									sGlobalSecretaria, 			
									sGlobalDetran,
									nGlobalSetor,			
									sGlobalSetor,
									SqlUser,
									frmRelCnhMov.dDataImp,
									frmRelCnhMov.dfdInicial,
