			On SAM_ReportNotify
				If lParam = RPT_BeforePageHeader
					Call SalReportSetNumberVar( SalNumberToWindowHandle ( wParam ), 'nResultado', nResultado )
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo',
						'Analítico de Infrações RENAINF (Outras UFs)', 'Analítico de Infrações RENAINF (Outras UFs)' )
				!
				Call SqlImmediate( 'select getdate() into :frmRNFRelDemonstrativoOutraUf.dCorrente' )
				!
				Return TRUE
			On SAM_ReportFetchInit
				! Call SalWaitCursor( TRUE )
				!
				Set nResultado = 0
				Call SalReportSetNumberVar( SalNumberToWindowHandle ( wParam ), 'nResultado', nResultado )
				If fMPSPrepareProc( hSql, 'Exec dbarrecadacao..RNFRelDemonstrativoOutraUf
							@Vigencia 	= :frmRNFRelDemonstrativoOutraUf.dfdVigencia
						,	@Municipio 	= :frmRNFRelDemonstrativoOutraUf.dfnMunicipio
						,	@OrgaoAutuante	= :frmRNFRelDemonstrativoOutraUf.dfnOrgaoAutuante',
							 ':frmRNFRelDemonstrativoOutraUf.MunicipioD
						,	:frmRNFRelDemonstrativoOutraUf.OrgaoAutuanteD
						,	:frmRNFRelDemonstrativoOutraUf.Serie
						,	:frmRNFRelDemonstrativoOutraUf.Auto
						,	:frmRNFRelDemonstrativoOutraUf.Infracao
						,	:frmRNFRelDemonstrativoOutraUf.CompetenciaD
						,	:frmRNFRelDemonstrativoOutraUf.AutoRENAINF
						,	:frmRNFRelDemonstrativoOutraUf.ValorLiquido
						,	:frmRNFRelDemonstrativoOutraUf.ValorMunicipio
						,	:frmRNFRelDemonstrativoOutraUf.ValorDetran' )
					If NOT SqlExecute( hSql )
						Return FALSE
					Return SqlGetResultSetCount( hSql, nErro )
				!
				! Call SalWaitCursor( FALSE )
				!
			On SAM_ReportFetchNext
				Call SalWaitCursor( FALSE )
				If SqlFetchNext( hSql, nErro )
					If bAbortaImpressao
						Return FALSE
						!
					Return TRUE
			On SAM_ReportFinish
				Return TRUE



