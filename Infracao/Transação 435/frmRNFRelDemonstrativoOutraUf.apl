			On SAM_Create
				Set _sTextoStatusBar = 'Tela do Relatório Analítico de Infrações RENAINF (Outras UFs)'
				Set _nEstadoInicialForm = EF_Vazio
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
