										, 		frmRNFRelDemonstrativoOutraUf.Auto
										, 		frmRNFRelDemonstrativoOutraUf.Infracao
										, 		frmRNFRelDemonstrativoOutraUf.CompetenciaD
										, 		frmRNFRelDemonstrativoOutraUf.AutoRENAINF
										,		frmRNFRelDemonstrativoOutraUf.ValorLiquido
										,		frmRNFRelDemonstrativoOutraUf.ValorMunicipio
										,		frmRNFRelDemonstrativoOutraUf.ValorDetran',
												'Secretaria,Detran,Setor, NomeSetor, Usuario
										,		DataCorrente
										,		Vigencia
										, 		MunicipioD
										,		OrgaoAutuanteD
										,		Serie
										,		Auto
										,		Infracao
										,		CompetenciaD
										,		AutoRENAINF
										,		ValorLiquido
										,		ValorMunicipio
										,		ValorDetran', 
												1,RPT_PrintAll,0,0, nErro )
										! 
							If dfsSaida = 'T'
								Set hWndReportView= SalCreateWindow( frmReportView, hWndForm )
								Call SalSetWindowText( hWndReportView, 'Demonstrativo de Val. RENAINF Arrecadados - Outras UFs' )
								! ----------------------------------------------------------- largura, altura
								Call SalSetWindowSize( hWndReportView,15,8 )
								Call SalReportView( hWndForm, hWndReportView,'RNFRelDemonstrativoOutraUf.qrp',  
												'sGlobalSecretaria, sGlobalDetran, nGlobalSetor, sGlobalSetor, SqlUser
										, 		frmRNFRelDemonstrativoOutraUf.dCorrente
										, 		frmRNFRelDemonstrativoOutraUf.dfdVigencia
