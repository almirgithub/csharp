		Window Parameters
			Window Handle: hWndBotaoPai
			Number: pnSetor
			Number: pnProcesso
		Window Variables
			Number: nSituacao
			Number: nRetorno
			Boolean: bFinalNulo
			Boolean: bFilaTransacao
			Date/Time: dInicial
			String: sProcessoCancelar
			String: sProcessoLiberar
			String: sProcessoVoltar
		Message Actions
			On SAM_Create
				Set _sTextoStatusBar = 'Tela de alteração de processo'
				Set _sNroCamposIgnorados = 'NENHUM'
				Set _sCriacaoConexao = 'Default'
				Set _nEstadoInicialForm = 1
				! Set _nEstadoInicialForm = EF_Vazio
				!
				Set _sComandoPesquisa = 'exec ProcessoS_alteracao2 @Placa = :frmAlteracaoProcesso.dfsPlaca,
						@Renavam = :frmAlteracaoProcesso.dfnRenavam, @Chassi = :frmAlteracaoProcesso.dfsChassi,
						@SetorProcesso = :frmAlteracaoProcesso.dfnSetor, @Processo =:frmAlteracaoProcesso.dfnProcesso'
				Set _sListaINTOPesquisa = ':frmAlteracaoProcesso.dfsPlaca, :frmAlteracaoProcesso.dfnRenavam,
						:frmAlteracaoProcesso.dfsChassi, :frmAlteracaoProcesso.dfnSetor, :frmAlteracaoProcesso.dfnProcesso,
						:frmAlteracaoProcesso.dfnSituacao, :frmAlteracaoProcesso.dfsSituacao, :frmAlteracaoProcesso.dfdData,
						:frmAlteracaoProcesso.dfdProInicial, :frmAlteracaoProcesso.dfsUsuario,
						:frmAlteracaoProcesso.bFinalNulo, :frmAlteracaoProcesso.bFilaTransacao,
						:frmAlteracaoProcesso.sProcessoCancelar, :frmAlteracaoProcesso.sProcessoLiberar,
						:frmAlteracaoProcesso.sProcessoVoltar'
				Set _sComandoAtualizacao = 'exec ProcessoU_alteracao2 @SetorN = :frmAlteracaoProcesso.dfnSetor,
						@Processo = :frmAlteracaoProcesso.dfnProcesso, @Opcao = :frmAlteracaoProcesso.dfsOpcao,
						@ProUsuario = :frmAlteracaoProcesso.dfsUsuario, @ProDataInicial = :frmAlteracaoProcesso.dfdProInicial,
						@DataInicial = :frmAlteracaoProcesso.dInicial, @Observacao = :frmAlteracaoProcesso.mlsObservacao'
				Set _sListaINTOAtualizacao = ''
				Set _bLimpaTelaAposAtualizacao = FALSE
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalCenterWindow( hWndForm )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				Call SalSendMsg( hWndBotaoPai, MSG_Desabilita1, 0, 0)
				If hWndBotaoPai != hWndNULL
					Set dfnSetor = pnSetor
					Set dfnProcesso = pnProcesso
					Call SalSendMsg( hWndForm, MSG_Pesquisa, 0, 0 )
					Call SalDisableWindow( pbLimpar )
			On UDM_AntesAtualizacao
				If sProcessoCancelar= 'N' and sProcessoLiberar = 'N' and sProcessoVoltar = 'N'
					Call SalMessageBeep( MB_IconExclamation )
					Call SalMessageBox( 'Usuário sem nenhuma permissao.', 'Atenção', MB_IconExclamation | MB_Ok )
					Return FALSE
				If rbCancelar and SalIsNull( mlsObservacao )
					Call SalMessageBeep( MB_IconExclamation )
					Call SalMessageBox( 'É obrigatório informar o motivo de cancelamento.', 'Atenção', MB_IconExclamation | MB_Ok )
					Call SalSetFocus( mlsObservacao )
					Return FALSE
				If (rbCancelar)
					If (bFinalNulo)
						Call SalMessageBeep( MB_IconExclamation )
						Call SalMessageBox( 'É necessário liberar antes de cancelar o processo.', 'Atenção', MB_IconExclamation | MB_Ok )
						Call SalSetFocus( mlsObservacao )
						Return FALSE
				Return TRUE
			On UDM_AposAtualizacao
				! - Envia transação 250 para cancelamento de ordem de emplacamento
				If fVerificarEnvioTrans250(frmAlteracaoProcesso.dfnSetor, frmAlteracaoProcesso.dfnProcesso, 'C')
					! - Faz o processo de envio para cancelamento na BIN
					Call BINEnviaTransacaoPlacaEstampa(frmAlteracaoProcesso.dfsPlaca, 250, frmAlteracaoProcesso.dfnSetor, frmAlteracaoProcesso.dfnProcesso, 2)
				!
				Call MudaObservacaoApos( )
				Call SalSendMsg( pbArquivar, MSG_Desabilita1, 0, 0 )
				Call SalSendMsg( rbLiberar, MSG_Desabilita1, 0, 0 )
				Call SalSendMsg( rbVoltar, MSG_Desabilita1, 0, 0 )
				Call SalSendMsg( rbCancelar, MSG_Desabilita1, 0, 0 )
				Call SalSendMsg( mlsObservacao, MSG_Desabilita1, 0, 0 )
				Return TRUE
			On UDM_AntesPesquisa
				If SalIsNull( dfsPlaca ) and SalIsNull( dfnRenavam ) and SalIsNull( dfsChassi ) and SalIsNull( dfnSetor )
					Return FALSE
				Return TRUE
			On UDM_AposPesquisa
				Call SalSendMsg( lbServico, MSG_Refresh, 0, 0 )
				If NOT HabilitaOpcao( )
					Call SalMessageBeep( MB_IconExclamation )
					Call SalMessageBox( 'Não é possível alterar o processo pois o mesmo esta ' || dfsSituacao, 'Atenção', MB_IconExclamation | MB_Ok )
					Return FALSE
				Set dInicial = SalDateCurrent(  )
				Set nEstadoSemaforo = S_Vermelho
				If sProcessoCancelar= 'N' and sProcessoLiberar = 'N' and sProcessoVoltar = 'N'
					Call SalPostMsg( pbArquivar, MSG_Desabilita1, 0, 0 )
					Return FALSE
				Set rbInvisivel = TRUE
				Return TRUE
			On SAM_Destroy
				Call SalSendMsg( hWndBotaoPai, MSG_Habilita1, 0, 0)
				If pnSetor != NUMBER_Null
					Call SalSendMsg( frmConsultaHistVeiculo.pbPesquisar, SAM_Click, wParam, lParam )
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )


