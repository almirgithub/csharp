			Number: nTabInfracao
		Message Actions
			On SAM_Create
				Set _nEstadoInicialForm = 1
				Set _sTextoStatusBar = 'Tela para cadastro de grupo de infrações'
				Set _sCriacaoConexao = 'Default'
				!
				Set _sComandoPesquisa = 'exec GrupoInfracaoS @Cod = :frmGrupoInfracao.dfnCodigo'
				Set _sListaINTOPesquisa = ':frmGrupoInfracao.dfnCodigo,  :frmGrupoInfracao.dfsDescricao,
						:frmGrupoInfracao.mlObservacao'
				!
				Set _bLimpaTelaAposInclusao = FALSE
				Set _sComandoInclusao = 'exec GrupoInfracaoI @Cod = :frmGrupoInfracao.dfnCodigo,
						@Descricao = :frmGrupoInfracao.dfsDescricao,@Observacao = :frmGrupoInfracao.mlObservacao'
				Set _sListaINTOInclusao = ''
				!
				Set _bLimpaTelaAposAtualizacao = FALSE
				Set _sComandoAtualizacao = 'exec GrupoInfracaoU @Cod = :frmGrupoInfracao.dfnCodigo,
						@Descricao = :frmGrupoInfracao.dfsDescricao,@Observacao = :frmGrupoInfracao.mlObservacao'
				Set _sListaINTOAtualizacao = ''
				!
				Set _bLimpaTelaAposRemocao = TRUE
				Set _sComandoRemocao = 'exec GrupoInfracaoD @Cod = :frmGrupoInfracao.dfnCodigo'
				Set _sListaINTORemocao = ''
				!
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalCenterWindow ( hWndForm )
			On MSG_Limpa
				Call SalSendClassMessage( MSG_Limpa, wParam, lParam )
				Set nTabValor = 0
				Set nTabInfracao = 0
				Call SWTabsBringToTop( picTabs, 0, TRUE )
				Call SalSetFocus( dfnCodigo )
			On UDM_AposAtualizacao
				Call SalDisableWindow( pbAnterior )
				Call SalDisableWindow( pbProximo )
				Return SalSendClassMessage( UDM_AposAtualizacao, wParam, lParam )





