		Message Actions
			On SAM_CreateComplete
				Call SqlImmediate( 'select getdate() into :frmRelCaracVeiculo.dDataImp' )
				Call SalFmtKeepMask( TRUE )
				Set _sStored = 'exec VeiculoS_carac @Cod = :frmRelCaracVeiculo.nVeiculo'
				Set _sNomeQRP = 'caracvei.QRP'
				Set _sTituloRelatório = 'Características do Veículo'
				Set _sPrimeiroRetorno = ':frmRelCaracVeiculo.sChassi, :frmRelCaracVeiculo.nRenavam,
						:frmRelCaracVeiculo.sPlaca, :frmRelCaracVeiculo.sUF, :frmRelCaracVeiculo.sChassiRegr,
						:frmRelCaracVeiculo.sChassiDB, :frmRelCaracVeiculo.nAnoModelo,
						:frmRelCaracVeiculo.nAnoFabricacao, :frmRelCaracVeiculo.sTipo, :frmRelCaracVeiculo.sEspecie,
						:frmRelCaracVeiculo.sCombustivel, :frmRelCaracVeiculo.sMarca,
						:frmRelCaracVeiculo.sComplCategoria, :frmRelCaracVeiculo.sCategoria, :frmRelCaracVeiculo.sCor,
						:frmRelCaracVeiculo.sCarroceria, :frmRelCaracVeiculo.sOnibusMarca,
						:frmRelCaracVeiculo.nOnibusAnoFabric, :frmRelCaracVeiculo.nOnibusAnoModelo,
						:frmRelCaracVeiculo.nPotencia, :frmRelCaracVeiculo.nCilindrada, :frmRelCaracVeiculo.nCapPassageiro,
						:frmRelCaracVeiculo.nCapCarga, :frmRelCaracVeiculo.nCMT, :frmRelCaracVeiculo.nRTB,
						:frmRelCaracVeiculo.nNumEixos, :frmRelCaracVeiculo.sFabrPropria, :frmRelCaracVeiculo.sFabric,
						:frmRelCaracVeiculo.sNumCaixa, :frmRelCaracVeiculo.sNumCarroceria, :frmRelCaracVeiculo.sNumEixoAux,
						:frmRelCaracVeiculo.sNumEixoTras, :frmRelCaracVeiculo.sNumMotor, :frmRelCaracVeiculo.sOficioATP,
						:frmRelCaracVeiculo.nAutorizacaoAluguel, :frmRelCaracVeiculo.sSituacaoVeiculo,
						:frmRelCaracVeiculo.nPBT'
				Set _sVarSQLWindows = 'sGlobalSecretaria,sGlobalDetran,nGlobalSetor, sGlobalSetor, frmRelCaracVeiculo.dDataImp, SqlUser,
						frmRelCaracVeiculo.sChassi, frmRelCaracVeiculo.nRenavam, frmRelCaracVeiculo.sPlaca,
						frmRelCaracVeiculo.sUF, frmRelCaracVeiculo.sChassiRegr, frmRelCaracVeiculo.sChassiDB,
						frmRelCaracVeiculo.nAnoModelo, frmRelCaracVeiculo.nAnoFabricacao, frmRelCaracVeiculo.sTipo,
						frmRelCaracVeiculo.sEspecie, frmRelCaracVeiculo.sCombustivel, frmRelCaracVeiculo.sMarca,
						frmRelCaracVeiculo.sComplCategoria, frmRelCaracVeiculo.sCategoria, frmRelCaracVeiculo.sCor,
						frmRelCaracVeiculo.sCarroceria, frmRelCaracVeiculo.sOnibusMarca, frmRelCaracVeiculo.nOnibusAnoFabric,
						frmRelCaracVeiculo.nOnibusAnoModelo, frmRelCaracVeiculo.nPotencia, frmRelCaracVeiculo.nCilindrada,
						frmRelCaracVeiculo.nCapPassageiro, frmRelCaracVeiculo.nCapCarga, frmRelCaracVeiculo.nCMT,
						frmRelCaracVeiculo.nRTB, frmRelCaracVeiculo.nNumEixos, frmRelCaracVeiculo.sFabrPropria,
						frmRelCaracVeiculo.sFabric, frmRelCaracVeiculo.sNumCaixa, frmRelCaracVeiculo.sNumCarroceria,
						frmRelCaracVeiculo.sNumEixoAux, frmRelCaracVeiculo.sNumEixoTras, frmRelCaracVeiculo.sNumMotor,
						frmRelCaracVeiculo.sOficioATP, frmRelCaracVeiculo.nAutorizacaoAluguel, frmRelCaracVeiculo.sSituacaoVeiculo,
						frmRelCaracVeiculo.nPBT'
				Set _sVarQRP = 'Secretaria,Detran,Setor, NomeSetor, DataImp, Usuario, Chassi, Renavam, Placa, UF, ChassiRegr, ChassiDB,
						AnoModelo, AnoFabricacao, Tipo, Especie, Combustivel, Marca, ComplCategoria, Categoria, Cor, Carroceria,
						OnibusMarca, OnibusAnoFabric, OnibusAnoModelo, Potencia, Cilindrada, CapPassageiro, CapCarga, CMT,
						RTB, NumEixos, FabrPropria, Fabric, NumCaixa, NumCarroceria, NumEixoAux, NumEixoTras,  NumMotor, OficioATP,
						AutorizacaoAluguel, SituacaoVeiculo, PBT'
				Call SalSendMsg( pbImprimir, SAM_Click, nParCopias, 0)
			On SAM_Destroy
				Set hWndJanela = hWndNULL
				Call SalFmtKeepMask( FALSE )
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
