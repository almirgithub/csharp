			! Function: fCriaTable
				Description: 
				Returns 
					Boolean: 
				Parameters 
					String: sColunas
				Static Variables 
				Local variables 
					String: sSelect
					Boolean: bExists
				Actions 
					! When SqlError
						Call SalMessageBox( "Erro durante a criação da tabela", "Erro", MB_IconExclamation )
						Return FALSE
					If sLimparInicio = 'S'
						If fExistsTable()
							Set sCreate = "Delete from  " || dfNomeDaTabela
							If Not SqlPrepareAndExecute( hSql, sCreate  )
								Return FALSE
						Else 
							Set sCreate = "Create Table " || dfNomeDaTabela || " (" || sColunas  || ")"
							If Not SqlPrepareAndExecute( hSql, sCreate  )
								Return FALSE
					Else 
						If Not fExistsTable()
							Set sCreate = "Create Table " || dfNomeDaTabela || " (" || sColunas  || ")"
							If Not SqlPrepareAndExecute( hSql, sCreate  )
								Return FALSE
					Return TRUE
			!
			! Function: fExistsTable
				Description: 
				Returns 
					Boolean: 
				Parameters 
				Static Variables 
				Local variables 
					String: sSelect
					Boolean: bExists
					Boolean: bOk
				Actions 
					Set sSelect = "Select name  From sysobjects Where name = '" || dfNomeDaTabela || "'
							and type = 'U' "
					Set bOk  = SqlExists( sSelect, bExists )
					If bExists
						Return TRUE
					Return FALSE
			!
			Function: fInseriTableSertel
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Set strComando = 'Exec dbinfracao..RegistroTipoArqS @TipoArquivo = "Atualiz. Data de Expedição"'
					! Set bOk = fMPSPrepareProc(hSqlApp,strComando ,':sCampo, :sTipoCampo, :nPosicaoInic,
							                            :nTamanho,:nCasasDecimais')
					Set bOk = fMPSPrepareProc(hSql,strComando ,':sCampo, :sTipoCampo, :nPosicaoInic,
							                            :nTamanho,:nCasasDecimais,:sTabela,:sStorageProcedure,:sLimparInicio')
					Set bOk = SqlExecute(hSql)
					If Not bOk
						Return FALSE
					Set bOk =  SqlFetchNext(hSql,nInd)
					If Not bOk
						Call SalTblReset( tblColunas )
						Return FALSE
					Set nRow = 0
					Call SalTblReset( tblColunas )
					Set nTamanhoRegistro = 0
					While nInd != FETCH_EOF
						Call SalTblInsertRow( tblColunas, nRow )
						Set tblColunas.colNome = sCampo
						Set tblColunas.colPosicaoInicial = nPosicaoInic
						Set tblColunas.colTamanho = nTamanho
						Set tblColunas.colTipoCaracter = sTipoCampo
						Set tblColunas.colCasasDecimais = nCasasDecimais
						Set tblColunas.colPosicaoFinal= nPosicaoInic + nTamanho - 1
						Set nRow = nRow + 1
						Set nTamanhoRegistro=nTamanhoRegistro+nTamanho
						Call SqlFetchNext(hSql,nInd)
					Call SalTblSetFlagsAnyRows ( tblColunas, ROW_New, FALSE, 0, 0 )
					Set dfNomeDaTabela = sTabela
					Set dfStorageProcedure = sStorageProcedure
