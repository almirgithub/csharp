			Function: fPopularTabela
				Description: Popula a tabela com os arquivo do diretório selecionado.
				Returns
					Boolean:
				Parameters
					String: psDiretorio
				Static Variables
				Local variables
					Number: nIndex
					Number: nTamVetor
					String: sArquivo[*]
					Number: nTamanho[*]
					Date/Time: dData[*]
					Number: nAtributos[*]
				Actions
					Call SalTblReset( tblSelecionarArquivo )
					!
					Call VisDosEnumFileInfo( psDiretorio, FA_Standard, sArquivo, nTamanho, dData, nAtributos )
					Call SalArrayGetUpperBound( nTamanho, 1, nTamVetor )
					!
					Set nIndex = 0
					Loop
						If nIndex > nTamVetor
							Break
						Else
							Call SalTblInsertRow( tblSelecionarArquivo, TBL_MaxRow )
							Set tblSelecionarArquivo.colnProcessar = 	TRUE
							Set tblSelecionarArquivo.colsArquivos = sArquivo[nIndex]
							Set tblSelecionarArquivo.colnTamanho = nTamanho[nIndex]
							Set tblSelecionarArquivo.coldData = dData[nIndex]
							Call SalTblSetFlagsAnyRows( tblSelecionarArquivo, ROW_New, FALSE, 0,0)
							Call SalTblSetFlagsAnyRows( tblSelecionarArquivo, ROW_Edited, TRUE, 0,0)
							! Call SalSendMsg( tblSelecionarArquivo.colnProcessar, SAM_Click, wParam, lParam )
						Set nIndex = nIndex + 1
					! Set nRow = 0
					!
					! Call SalTblSetContext(tblSelecionarArquivo,nRow)
					! While SalTblFetchRow( tblSelecionarArquivo,nRow) = FETCH_EOF
						Call SalTblSetContext(tblSelecionarArquivo,nRow)
						Set tblSelecionarArquivo.colnProcessar = TRUE
						!
						! Call SalSendMsg( tblSelecionarArquivo.colnProcessar, SAM_Click, wParam, lParam )
						Call SalTblSetFlagsAnyRows( tblSelecionarArquivo, ROW_Edited, TRUE, 0,0)
						Set nRow = nRow + 1
			Function: fGravaRegistrosNoBanco
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					String: sPosicaoInicial[*]
					String: sPosicaoFinal[*]
					Number: nIndice
					Number: numRead
					String: sValues
					String: sValor
					Boolean: bUltimoCampo
					String: sInsertAnt
					String: sFirstChar
					Number: nReturn
				Actions
					If sLimparInicio = 'S'
						Set sCreate = "Delete from dbinfracao.. " || dfNomeDaTabela
						If Not SqlPrepareAndExecute( hSql, sCreate  )
							Return FALSE
					!
					Set bCancela = FALSE
					Set sValues = ''
					Set sInsertAnt = sInsert
					Loop
						Call SalFileSeek( hFile, 0, FILE_SeekCurrent )
						Set numRead = SalFileRead(hFile,strTexto,nTamanhoRegistro+2)
						If (SalStrMidX(strTexto,5,2) = 'PY' or SalStrMidX(strTexto,5,2) = "DP") and SalStrMidX(strTexto,0,2)='00'
							Set strTexto = SalStrMidX(strTexto,0,37)||SalStrRepeatX(' ',116)||SalStrMidX(strTexto,142,75)
						While SalStrScan(strTexto,'"') > 0
							Set strTexto = SalStrReplaceX( strTexto, SalStrScan(strTexto,'"'), 1, ' ' )
						While SalStrScan(strTexto,':') > 0
							Set strTexto = SalStrReplaceX( strTexto, SalStrScan(strTexto,':'), 1, ' ' )
						!
						If numRead < nTamanhoRegistro+2
							Call SalFileClose( hFile )
							Break
						!
						Set nIndice = 1
						Set sValues = ''
						!
						! If dfRegistros > 10
							Break
						! If SalStrMidX( strTexto, 44, 4 )>  '1998' or cmbTipoArquivo ='Atualiz. Data de Expedição'
						Loop
							Set sValor = SalStrMidX( strTexto, vPosicaoInicial[nIndice]-1, vTamanho[nIndice] )
							If SalStrTrimX(vTipoCaracter[nIndice]) = 'Numérico'
								Set nReturn =  SalStrScan( sValor, ' ' )
								If nReturn != -1
									Call SalStrRepeat(  '0',  vTamanho[nIndice], sValor )
								Set sValor = SalStrMidX(sValor,0, vTamanho[nIndice]-vCasasDecimais[nIndice]) ||
										                    "." || SalStrRightX(sValor,vCasasDecimais[nIndice])
								Set bUltimoCampo = TRUE
								Set sValues = sValues || sValor || ","
							Else
								If cmbTipoArquivo = 'Atualiz. Data de Expedição'
									If nIndice != 5
										Set sValues = sValues || "'" ||  sValor || "',"
									Else
										Set sValues = sValues || "' ',"
								Else
									Set sValues = sValues || '"' ||  sValor || '",'
								Set bUltimoCampo = FALSE
							Set nIndice = nIndice + 1
							If nIndice > nNumeroColunas
								Break
						!
						Set sInsert = sInsertAnt || SalStrLeftX( sValues, SalStrGetBufferLength(sValues)-2) || ")"
						If Not SqlPrepare( hSql, sInsert )
							Call fGravaErro(strTexto)
						Else
							If Not SqlExecute( hSql )
								Call SalMessageBox( "Erro durante a inserção de linhas na tabela", "ERRO", MB_IconStop )
								Call fGravaErro(strTexto)
								Return FALSE
						!
						If bCancela
							Return FALSE
					If dfStorageProcedure != STRING_Null
						Set strComando = 'Exec dbinfracao..' || dfStorageProcedure || '  @Arquivo = :dfArquivo,@Aplicativo = "I"'
						Set bOk = fMPSPrepareProc(hSql,strComando ,':strErro')
						Set bOk = SqlExecute(hSql)
						Call fGravaErrosDaHisMulta()
						If Not bOk
							Return FALSE
						! Set bOk =  SqlFetchNext(hSqlApp,nInd)
					Return TRUE
			!
			Function: fCarregaVetor
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					Number: nRow
					String: sColunas
					Boolean: bOk
					String: sTipoCaracter
				Actions
					Call SalTblSetContext( tblColunas, 0 )
					Set sInsert = "Insert Into dbinfracao.." || dfNomeDaTabela || " ( "
					Set nRow = 0
					Set nNumeroColunas =  0
					Set nIndice = 0
					Set sColunas = ''
					!
					Loop
						Set bOk = SalTblSetContext( tblColunas, nRow )
						If bOk = FALSE
							Break
						!
						Set nIndice = nIndice + 1
						Set vColuna[nIndice] = tblColunas.colNome
						Set vPosicaoInicial[nIndice] = tblColunas.colPosicaoInicial
						Set vTamanho[nIndice] = tblColunas.colTamanho
						Set vTipoCaracter[nIndice] = tblColunas.colTipoCaracter
						Set vCasasDecimais[nIndice] = tblColunas.colCasasDecimais
						Set sInsert = sInsert || tblColunas.colNome || ", "
						Set nRow = nRow + 1
						Set nNumeroColunas = nNumeroColunas + 1
					!
					Set sInsert = SalStrLeftX( sInsert, SalStrGetBufferLength(sInsert)-3)
					Set sInsert = sInsert || ") Values ( "
					Return TRUE
			!
