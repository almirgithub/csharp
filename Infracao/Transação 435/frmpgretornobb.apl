					On SAM_Create
						Call SalTblSetTableFlags( hWndItem, TBL_Flag_SingleSelection, TRUE )
						Call SalTblSetTableFlags( hWndItem, TBL_Flag_MovableCols, FALSE )
						Set _sTextoStatusBar = 'Nome dos Arquivos reconhecidos pelo Sistema, com suas Datas de criação'
						Set _bSempreHabilitado = TRUE
						Set _sCriacaoConexao = 'FALSE'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On MSG_Limpa
						Call SalTblSetFlagsAnyRows( hWndItem, ROW_Selected, FALSE, 0, 0 )
					On MSG_PesquisaTabela
						Call SalWaitCursor( TRUE )
						Call SalTblReset( tblArquivos )
						Set hSqlApp = hSqlApp
						Select Case SalListQuerySelection( cmbsParametros )
							Case 0
								Call SybPrepareProc( hSqlApp, 'exec Prot..PG_RetornoS  @Ultimos = 	1 ',
										':tblArquivos.colsArquivo, :tblArquivos.colnExercicio, :tblArquivos.coldCriacao')
								Break
							Case 1
								Call SybPrepareProc( hSqlApp, 'exec Prot..PG_RetornoS @Ultimos = 	0 ',
										':tblArquivos.colsArquivo, :tblArquivos.colnExercicio, :tblArquivos.coldCriacao')
								Break
							Default
								Call SalMessageBeep( MB_IconExclamation )
								Call SalMessageBox( 'Opção inválida', 'Atenção', MB_IconExclamation | MB_Ok )
								Return FALSE
						If SalListQuerySelection( cmbsParametros ) >= 0
							Call SalTblPopulate( tblArquivos, hSqlApp, '', TBL_FillAll)
						Call SalWaitCursor( FALSE )
						Return TRUE
					On SAM_Click
						Call SalTblGetColumnText( hWndItem, SalTblQueryColumnID(colsArquivo), dfArquivo )
						Call SalTblGetColumnText( hWndItem, SalTblQueryColumnID(colnExercicio), sExercicioRet )
						Set dfnExercicio = SalStrToNumber( sExercicioRet )
						Call SalEnableWindow( pbRelacao )
						Call SalEnableWindow( pbInconsistencia )
					On SAM_DoubleClick
						Call SalSendMsg( hWndItem, SAM_Click, wParam, lParam )
					On SAM_RowHeaderClick
						Call SalSendMsg( hWndItem, SAM_Click, wParam, lParam )
					On SAM_RowHeaderDoubleClick
						Call SalSendMsg( hWndItem, SAM_Click, wParam, lParam )
					On WMSG_KEYDOWN
					On MSG_NovaLinha
					On MSG_RemoveLinha
