							String: sCondicionalidade
						Actions 
							Set nRow = 0
							Call SalTblSetContext( tblInf, nRow )
							Loop 
								If SalTblFetchRow( tblInf, nRow ) = TBL_NoMoreRows
									Break 
								If tblInf.colsAutoRENAINF != STRING_Null  and colbEfeitoSuspensivo
									If not cbJudicial and  (tblInf.colsUFOrgao  != 'PE' and tblInf.colsUFOrgao != 'DF')
										Set colsEfeitoSuspensivo = ''
										Call SalMessageBeep( MB_IconExclamation )
										Call SalMessageBox( 'Para autos registrados por outras UFs, o efeito suspensivo so deve ser colocado por ordem judicial', 'Atenção', MB_IconExclamation | MB_Ok )
										Set colbEfeitoSuspensivo = FALSE
										Return FALSE
								Set nRow = nRow + 1
							Return TRUE
				Window Variables
					Boolean: bRetorno
