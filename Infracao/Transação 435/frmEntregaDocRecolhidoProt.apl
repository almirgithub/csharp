				Functions
					! Function: fTransmiteAlteracao
						Description: 
						Returns 
						Parameters 
						Static Variables 
						Local variables 
							Number: nRow
							String: sCondicionalidade
						Actions 
							Set nRow = 0
							Call SalTblSetContext( tblInf, nRow )
							Loop 
								If SalTblFetchRow( tblInf, nRow ) = TBL_NoMoreRows
									Break 
								If tblInf.colsAutoRENAINF != STRING_Null  and colbEfeitoSuspensivo
									If cbJudicial
										Call SalTblSetRowFlags( tblInf, ROW_Edited, nRow, TRUE )
										Call fTransmiteRENAINF(tblInf.colsSerie,tblInf.colnOrgaoAutuante,tblInf.colnAuto,
												tblInf.colnInfracao,416,'EV01',hSqlApp,tblInf.colsAutoRENAINF,'','','',6,1,'4',0)
									Else If (tblInf.colsUFOrgao  != 'PE' and tblInf.colsUFOrgao != 'DF')
										Set colsEfeitoSuspensivo = ''
										Call SalMessageBeep( MB_IconExclamation )
										Call SalMessageBox( 'Para autos registrados por outras UFs, o efeito suspensivo so deve ser colocado por ordem judicial', 'Atenção', MB_IconExclamation | MB_Ok )
										Set colbEfeitoSuspensivo = FALSE
										Return FALSE
									Else 
										Call SalTblSetRowFlags( tblInf, ROW_Edited, nRow, TRUE )
										Call fTransmiteRENAINF(tblInf.colsSerie,tblInf.colnOrgaoAutuante,tblInf.colnAuto,
												tblInf.colnInfracao,416,'EV01',hSqlApp,tblInf.colsAutoRENAINF,'','','',6,1,'3',0)
								Set nRow = nRow + 1
							Return TRUE
