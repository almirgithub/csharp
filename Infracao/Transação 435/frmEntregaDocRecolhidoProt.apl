						Set _bSempreHabilitado = FALSE
						Set _sCriacaoConexao = 'FALSE'
						Set nProtocolo = NUMBER_Null
						! Procedure's
						Set _sComandoPesquisa = 'exec dbinfracao..AutoCNHRecolhidaS
								@OrgaoAutuante=:frmEntregaDocRecolhidoProt.dfnOrgao,
								@DataInicial=:frmEntregaDocRecolhidoProt.dfdInicial,
								@DataFinal = :frmEntregaDocRecolhidoProt.dfdFinal,
								@Protocolo = :frmEntregaDocRecolhidoProt.nProtocolo,
								@Oficio = :frmEntregaDocRecolhidoProt.dfsOficio'
						Set _sIntoPesquisa = '
								:frmEntregaDocRecolhidoProt.tblInf.colsPlaca,
								:frmEntregaDocRecolhidoProt.tblInf.colsOrgaoAutuante,
								:frmEntregaDocRecolhidoProt.tblInf.colsSerie,
								:frmEntregaDocRecolhidoProt.tblInf.colnAuto,
								:frmEntregaDocRecolhidoProt.tblInf.colnDigito,
								:frmEntregaDocRecolhidoProt.tblInf.colnInfracao,
								:frmEntregaDocRecolhidoProt.tblInf.colnDesdobramento,
								:frmEntregaDocRecolhidoProt.tblInf.colnCNHAuto,
								:frmEntregaDocRecolhidoProt.tblInf.colsCondutorAuto,
								:frmEntregaDocRecolhidoProt.tblInf.colsOficio,
								:frmEntregaDocRecolhidoProt.tblInf.coldDataOficio,
								:frmEntregaDocRecolhidoProt.tblInf.colsAutoSituacao,
								:frmEntregaDocRecolhidoProt.tblInf.colnOrgaoA'
						!
						Set _sComandoAtualiza = 'exec Prot..ComunicadoI_CNH 
								@Protocolo=:frmEntregaDocRecolhidoProt.nProtocolo,
								@OrgaoAutuante = :frmEntregaDocRecolhidoProt.tblInf.colnOrgaoA,
								@Serie =  :frmEntregaDocRecolhidoProt.tblInf.colsSerie, 
								@Auto = :frmEntregaDocRecolhidoProt.tblInf.colnAuto,
								@Infracao =  :frmEntregaDocRecolhidoProt.tblInf.colnInfracao,
								@Desdobramento = :frmEntregaDocRecolhidoProt.tblInf.colnDesdobramento,
								@TesteAlcoolemia = :frmEntregaDocRecolhidoProt.tblInf.colbTesteAlcoolemia,
								@CNH  = :frmEntregaDocRecolhidoProt.tblInf.colnCNHAuto,
								@Condutor = :frmEntregaDocRecolhidoProt.tblInf.colsCondutorAuto'
						!
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_RowHeaderClick
						Call SalTblSetRowFlags( hWndForm, lParam,  ROW_Edited, NOT SalTblQueryRowFlags( hWndForm, lParam, ROW_Edited ) )
					On SAM_CornerDoubleClick
						Call SalTblSetFlagsAnyRows( hWndForm, ROW_Edited, NOT SalTblAnyRows( hWndForm, ROW_Edited, 0 ), 0, 0 )
					On UDM_AposAtualizacao
						Call SalSendMsgToChildren( tblInf, MSG_Desabilita1, wParam, lParam )
						If hWndImpProtEntrega != hWndNULL
							Call SalSendMsg( hWndImpProtEntrega, SAM_Close, 0, 0 )
						If SalMessageBox( 'Deseja imprimir o protocolo de entrega?', 'Atenção', MB_IconQuestion | MB_YesNo  ) = IDYES
							Set hWndImpProtEntrega = SalCreateWindow( frmRelProtEntrega, hWndMDI, hWndImpProtEntrega, 2 )
						Call SalSendMsg(frmEntregaDocRecolhidoProt, MSG_Limpa, wParam, lParam )
						Set nProtocolo = NUMBER_Null
						Return TRUE
					On MSG_Arquiva
						Call SalWaitCursor( TRUE )
						Set bMsgOk = TRUE
						If bHabilitado
							Call SalWaitCursor( TRUE )
							If fMPSPrepareProc( hSqlApp,'exec Prot..ProtocoloI_CNH
									@Protocolo=:frmEntregaDocRecolhidoProt.nProtocolo,
									@Observacao= :frmEntregaDocRecolhidoProt.mlfColConteudo',
									':frmEntregaDocRecolhidoProt.nProtocolo')
								If SqlExecute( hSqlApp )
									Set bRetorno = TRUE
									! car 22/08/96 - se houver uma lista into na atualizacao faca o fetch dos valores
									If not SqlFetchNext ( hSqlApp, nRetVal )
										Set bRetorno =  FALSE
							Call SalWaitCursor( FALSE )
							!
							! Elimina linhas inseridas que nao tenham sido digitadas
							Call EliminaLinhasNulas(hWndItem)
							! Insere os dados no banco de dados
							! If _sComandoInsere != '' and (nPermissao & H_INSERT)
								Set bMsgOk = SalSendMsg( hWndItem, UDM_AntesInclusao, wParam, lParam )
								! Executa a insercao
								If bMsgOk
									If fMPSPrepareProc(  hSqlApp, _sComandoInsere, _sIntoInsere )
										If not  SalTblDoInserts( hWndForm, hSqlApp, TRUE )
											Set bMsgOk = FALSE
									Else 
										Set bMsgOk = FALSE
									If _sIntoInsere != ''
										If not SqlFetchNext( hSqlApp, nRetVal )
											Set bMsgOk = FALSE
								! Envia a mensagem de após insercao
								If bMsgOk
									Set bMsgOk = SalSendMsg( hWndItem, UDM_AposInclusao, wParam, lParam )
							! Atualiza os dados no banco de dados
							If _sComandoAtualiza != '' and (nPermissao & H_UPDATE) and bMsgOk
								! Set bMsgOk = SalSendMsg( hWndItem, UDM_AntesAtualizacao, wParam, lParam )
								! Executa a atualizacao
								If bMsgOk
									If fMPSPrepareProc(  hSqlApp, _sComandoAtualiza, _sIntoAtualiza )
										If not SalTblDoUpdates( hWndForm, hSqlApp, TRUE )
											Set bMsgOk = FALSE
									Else
										Set bMsgOk = FALSE
									If _sIntoAtualiza != ''
										If not SqlFetchNext( hSqlApp, nRetVal  )
											Set bMsgOk = FALSE
								! Manda a mensagem de apos atualizacao
								If bMsgOk
									Set bMsgOk = SalSendMsg( hWndItem, UDM_AposAtualizacao, wParam, lParam )
							! Exclui os dados no banco de dados
							! If _sComandoExclui != '' and (nPermissao & H_DELETE) and bMsgOk
								Set bMsgOk = SalSendMsg( hWndItem, UDM_AntesRemocao, wParam, lParam )
								! Executa as remocoes
								If bMsgOk
									If fMPSPrepareProc(  hSqlApp, _sComandoExclui, _sIntoExclui )
										If not SalTblDoDeletes( hWndForm, hSqlApp, ROW_MarkDeleted )
											Set bMsgOk = FALSE
									Else 
										Set bMsgOk = FALSE
									If _sIntoExclui != ''
										If not SqlFetchNext( hSqlApp, nRetVal  )
											Set bMsgOk = FALSE
								! Envia a mensagem de apos remocao
								If bMsgOk
									Call SalSendMsg( hWndItem, UDM_AposRemocao, wParam, lParam )
						Call SalWaitCursor( FALSE )
						Return bMsgOk
					On UDM_AposPesquisa
						If Not SalTblSetContext( tblInf,0 )
							If NOT SalIsNull(dfnOrgao) and NOT SalIsNull(dfdInicial) and  NOT SalIsNull(dfdFinal)
								Call MsgErro('Registro não encontrado!')
								Return FALSE
						Else
							Call SalEnableWindow(pbArquivar)
							Call SalEnableWindow(mlfColConteudo)
							Return TRUE
					! On UDM_AntesPesquisa
						If SalIsNull(dfnOrgao) or  SalIsNull(dfdInicial) or SalIsNull(dfdFinal)
							Call MsgErro('Para consulta é obrigatório informar Orgão Autuante, Data Inicial e Final.')
							Return FALSE
						Return TRUE
