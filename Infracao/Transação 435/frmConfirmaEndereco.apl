						!
						Set _sTipoComando = 'Comum'
						Set _nPermissao = 15
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						If NOT MPSPrepareProc( hSqlApp, 'exec dbvcen..ConfirmaEnderecoCepVerificar
								@Placa = :frmConfirmaEndereco.dfsPlaca,
								@Cep = :frmConfirmaEndereco.dfnPropCEP,
								@Tela = "S"',
								':frmConfirmaEndereco.sCepCorreto,
								:frmConfirmaEndereco.sMunicipioVeiculo,
								:frmConfirmaEndereco.sConfirmacao' )
							Call SalWaitCursor( FALSE )
							Return FALSE
						If NOT SqlExecute( hSqlApp )
							Call SalWaitCursor( FALSE )
							! Call ErroForm( 0 )
							Return FALSE
						Call SqlFetchNext( hSqlApp, nRetorno )
						If nRetorno != FETCH_Ok
							Call SalWaitCursor( FALSE )
							Return FALSE
						If nRetorno = FETCH_Ok and frmConfirmaEndereco.sCepCorreto = 'S'
							Call SalMessageBox( 'Confirmação de endereço realizada com sucesso!','Mensagem',MB_IconExclamation|MB_Ok)
							Set frmGuiaLicenciamento.sTelaCepConfirmacao = 'S'
							Call SalPostMsg( hWndForm, MSG_Sai, wParam, lParam )
						If nRetorno = FETCH_Ok and frmConfirmaEndereco.sCepCorreto = 'N' 
							Set sMensagem = 'Atenção! O CEP informado diverge do cadastrado no DETRAN/PE! Realize o serviço de atualização de endereço se for o mesmo município, caso contrário, agende a Vistoria.'
							Set frmGuiaLicenciamento.sTelaCepConfirmacao = 'N'
							Call SalMessageBox( sMensagem,'Mensagem',MB_IconStop|MB_Ok)
						Call SalSendMsg( frmGuiaLicenciamento.pbPesquisar, SAM_Click, 0, 0 )
						Call SalSendMsg( frmGuiaLicenciamento.tblDebito, SAM_User, 0, 0 )
