					Background Color: Class Default
					Input Mask: 99999-9
				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Município do endereço do proprietário do veículo'
						Set _sConsultaFrmOpcoesCampo = 'MunicipioSetorS_munic @Setor = :nGlobalSetor'
						Set _sConsultaFrmOpcoesColunaOuParam = '@Descricao'
						Set _sConsultaFrmOpcoesSQLOuSP = 'SP'
						Set _sNroCamposIgnorados = 'NENHUM'
						Set _bPermiteNulos = FALSE
						Set _sNomeParaMsgs = 'Município do Proprietário'
						Set _bNuncaHabilitado = TRUE
						Set _bSempreHabilitado = FALSE
						Set _bVisivel = FALSE
						Set _sMsgValidacao = 'Dígito inválido!'
						Set _sTipoMsgValidacao = 'ESPECIFICA'
						Set _bNaoTemDependentes = TRUE
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On UDM_ValidaCampo
						If NOT SalIsNull( hWndItem )
							If SalNumberMod( dfnPropMunicipio, 10 ) !=
									DigitoModulo11( SalNumberTruncate( dfnPropMunicipio/10, 8, 0 ) )
								Set _sMsgValidacao = 'Dígito inválido!'
								Call SalClearField( dfsPropMunicipio )
								Return FALSE
							If fMPSConnect (hSql)
								If MPSPrepareProc( hSql, 'exec MunicipioS_cep @Cod = :frmConsultaCEPEndereco.dfnPropMunicipio',
										':frmConsultaCEPEndereco.dfnPropCEP' )
									If SqlExecute( hSql )
										Call SqlFetchNext( hSql, nRetorno )
								Call fMPSDisconnect (hSql)
						!
						Set bDependentesOK = TRUE
						Call SalSendMsg( dfsPropMunicipio, MSG_RefreshDependentes, SalWindowHandleToNumber( hWndItem ), lParam )
						If NOT bDependentesOK AND NOT SalIsNull( hWndItem )
							Set _sMsgValidacao = 'Valor inexistente no banco de dados!'
							Return FALSE
						!
						Call SalSendMsg( dfnPropBairro, MSG_Limpa, 0, 0 )
						Return TRUE
					On MSG_Limpa
						Call SalSendClassMessage( MSG_Limpa, wParam, lParam )
						Call SalSendMsg( dfsPropMunicipio, MSG_Limpa, 0, 0 )
						Call SalSendMsg( dfnPropBairro, MSG_Limpa, 0, 0 )
					On MSG_TestaNulo2
						Return ( NOT _bPermiteNulos AND SalIsNull( hWndItem ) AND SalIsWindowEnabled( hWndItem ) )
