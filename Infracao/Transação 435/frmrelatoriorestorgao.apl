			On SAM_Create
				Set _sTextoStatusBar = 'Relatório de restituicao de infração por órgão autuante'
				!
				Set sComandoImp = 'Exec	Prot..RestituicaoS_Rel_Orgao
							@DataInicial		= :frmRelatorioRestOrgao.dfdInicial ,
							@DataFinal		= :frmRelatorioRestOrgao.dfdFinal,
							@OrgaoCompetencia 	= :frmRelatorioRestOrgao.dfnOrgao,
							@Grupo   		= :frmRelatorioRestOrgao.dfnGrupo '
				!
				Set sIntoImp= '
						:frmRelatorioRestOrgao.sCodOrgao,
						:frmRelatorioRestOrgao.sDesOrgao,
						:frmRelatorioRestOrgao.sProcesso,
						:frmRelatorioRestOrgao.sOrgaoAutuante,
						:frmRelatorioRestOrgao.sSerie,
						:frmRelatorioRestOrgao.sAuto,
						:frmRelatorioRestOrgao.sDigitoAuto,
						:frmRelatorioRestOrgao.sInfracao,
						:frmRelatorioRestOrgao.sDesdobramento,
						:frmRelatorioRestOrgao.nValorRest,
						:frmRelatorioRestOrgao.sUsuario,
						:frmRelatorioRestOrgao.dData,
						:frmRelatorioRestOrgao.nAgenteEquip,
						:frmRelatorioRestOrgao.sNomeRest,
						:frmRelatorioRestOrgao.sCPF,
						:frmRelatorioRestOrgao.sLogradouro,
						:frmRelatorioRestOrgao.sNumero,
						:frmRelatorioRestOrgao.sComplemento,
						:frmRelatorioRestOrgao.sBairro,
						:frmRelatorioRestOrgao.nCEP,
						:frmRelatorioRestOrgao.sMunicipio,
						:frmRelatorioRestOrgao.sUF,
						:frmRelatorioRestOrgao.sPossuiConta'
				! Set sVariavel = ':sCodOrgao, :sDesOrgao,:sProcesso,:sSerie,:sAuto,:sDigitoAuto,:sInfracao,:nValorRest'
				!
				Set _nEstadoInicialForm = 1
				Call SalCenterWindow( hWndForm )
				If hWndObjeto != hWndNULL
					Call SalDisableWindow( hWndObjeto )
				!
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				Set dfsSaida = 'I'
			On SAM_ReportNotify
				Set hWndReport = SalNumberToWindowHandle ( wParam )
				If lParam = RPT_BeforePageHeader
					Call SalReportSetStringVar( hWndReport, 'Des_Sec', sGlobalSecretaria )
					Call SalReportSetStringVar( hWndReport, 'Des_Org', sGlobalDetran )
					Call SalReportSetNumberVar( hWndReport, 'Cod_Set', nGlobalSetor )
					Call SalReportSetStringVar( hWndReport, 'Des_Set', sGlobalSetor )
					Call SalReportSetStringVar( hWndReport, 'Cod_User', SqlUser )
					Call SalReportSetDateTimeVar( hWndReport, 'Dat_Ini', dfdInicial )
					Call SalReportSetDateTimeVar( hWndReport, 'Dat_Fin', dfdFinal )
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo', 'Relatório de Restituição', 'Relatório de de Restituição' )
				Return TRUE
			On SAM_ReportFetchInit
				Call SybPrepareProc( hSql,sComandoImp,sIntoImp )
				Return SqlExecute( hSql )
			On SAM_ReportFetchNext
				Call SqlFetchNext( hSql, nErro )
				If nErro != FETCH_EOF
					Return 1
				Else
					Return 0
			On SAM_ReportFinish
				Call SalPrtSetDefault(sDeviceDef,sDriverDef,sPortaDef)
				Call SqlGetResultSetCount( hSql, nCount )
				If nCount > 0
					Call SqlFetchRow( hSql, nCount,nCount)
			On SAM_Destroy
				If hWndReportView != hWndNULL
					Call SalReportClose( hWndReportView )
					Call SalDestroyWindow( hWndReportView )
					Set hWndReportView = hWndNULL
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )


