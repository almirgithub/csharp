					On SAM_Click
						If SalIsNull( dfdInicial)
							Call SalMessageBeep( 0 )
							Call SalMessageBox( 'Data Inicial é Obrigatória.', 'Atenção', MB_IconExclamation | MB_Ok )
							Call SalSetFocus( dfdInicial )
							Return FALSE
						If SalIsNull( dfdFinal)
							Call SalMessageBeep( 0 )
							Call SalMessageBox( 'Data Final é Obrigatória.', 'Atenção', MB_IconExclamation | MB_Ok )
							Call SalSetFocus( dfdFinal )
							Return FALSE
						If dfdFinal < dfdInicial AND NOT SalIsNull( dfdFinal )
							Call SalMessageBeep( 0 )
							Call SalMessageBox( 'Data Inicial deve ser menor do que a data Final.', 'Atenção', MB_IconExclamation | MB_Ok )
							Return FALSE
						If SalIsNull( dfnOrgao )
							Call SalMessageBeep( 0 )
							Call SalMessageBox( 'Órgão de Competência deve ser informado', 'Atenção', MB_IconExclamation | MB_Ok )
							Return FALSE
						If SalIsNull( dfnGrupo )
							Call SalMessageBeep( 0 )
							Call SalMessageBox( 'Grupo de Restituição deve ser informado', 'Atenção', MB_IconExclamation | MB_Ok )
							Return FALSE
						Call SalWaitCursor( TRUE )
						Set sVariavel = ":sCodOrgao,:sDesOrgao,:sProcesso,:sOrgaoAutuante,:sSerie,:sAuto,:sDigitoAuto,:sInfracao,:sDesdobramento,:nValorRest,:sUsuario,:dData,:sNomeRest,:sCPF,:sLogradouro,:sNumero,:sComplemento,:sBairro,:nCEP,:sMunicipio,:sUF,:sPossuiConta"
						Set sImput = 'Cod_Orgao,Des_Orgao,Processo,OrgaoAutuante,Serie,Auto,DigitoAuto,Infracao,Desdobramento,Val_Rest,Usuario,DataCad,NomeRest,CPF,Logradouro,Numero,Complemento,Bairro,CEP,Municipio,UF,PossuiConta'
						If dfsSaida='I'
							Call SalReportPrint( hWndForm, 'rest_org.qrp', sVariavel , sImput, 1,RPT_PrintAll, 0, 0, nErro )
						Else
							If hWndReport != hWndNULL
								Call SalSendMsg( hWndReportView, SAM_Close, 0, 0)
							Set hWndReport= SalCreateWindow( frmReportView, hWndForm )
							Call SalSetWindowText( hWndReport, 'Relatório de restituição' )
							Call SalReportView( hWndForm, hWndReport,'rest_org.qrp', sVariavel, sImput, nErro )
						Call SalWaitCursor( FALSE )
