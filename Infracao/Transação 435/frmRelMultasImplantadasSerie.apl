		Functions
			Function: fExportarArquivo
				Description: Gera arquivi txt a partir do preenchimento da tabela.
				Returns
				Parameters
				Static Variables
				Local variables
					FunctionalVar: oArquivo
						Class: cArquivo
					Number: nLinha
					!
					! String: aSerie[ * ]
					! String: sSerieAux
					! String: aDados[ *, 10 ]
					! String: sDescInfrAux
					!
					! Number: nIndSerie
					! Number: nIndDados
					! Number: nTotal
					Number: nTotalInfr
					Number: nTotalSerie1
					Number: nTotalSerie2
					Number: nTotalSerie3
					Number: nTotalSerie4
					Number: nTotalSerie5
					Number: nTotalSerie6
					Number: nTotalSerie7
					Number: nTotalSerie8
					Number: nTotalSerie9
					Number: nTotalSerie10
				Actions
					! -- cabeçalho 1a. linha
					Call oArquivo.NovaLinha(  )
					Call oArquivo.AddStr( '' , 'nulo' )
					Call oArquivo.AddStr( 'Série / Contrato' , 'Serie/Contrato' )
					Call oArquivo.AddStr( '' , 'nulo' )
					Call oArquivo.AddStr( '' , 'nulo' )
					Call oArquivo.AddStr( '' , 'nulo' )
					Call oArquivo.AddStr( '' , 'nulo' )
					Call oArquivo.AddStr( '' , 'nulo' )
					Call oArquivo.AddStr( '' , 'nulo' )
					Call oArquivo.AddStr( '' , 'nulo' )
					Call oArquivo.AddStr( '' , 'nulo' )
					Call oArquivo.AddStr( '' , 'nulo' )
					Call oArquivo.AddStr( '' , 'nulo' )
					! -- cabeçalho 2a. linha
					Call oArquivo.NovaLinha(  )
					Call oArquivo.AddStr( 'Infração' , 'Infração' )
					Call oArquivo.AddStr( tblMultasImplantadas.colsSerie1, 'Serie1' )
					Call oArquivo.AddStr( tblMultasImplantadas.colsSerie2, 'Serie2' )
					Call oArquivo.AddStr( tblMultasImplantadas.colsSerie3, 'Serie3' )
					Call oArquivo.AddStr( tblMultasImplantadas.colsSerie4, 'Serie4' )
					Call oArquivo.AddStr( tblMultasImplantadas.colsSerie5, 'Serie5' )
					Call oArquivo.AddStr( tblMultasImplantadas.colsSerie6, 'Serie6' )
					Call oArquivo.AddStr( tblMultasImplantadas.colsSerie7, 'Serie7' )
					Call oArquivo.AddStr( tblMultasImplantadas.colsSerie8, 'Serie8' )
					Call oArquivo.AddStr( tblMultasImplantadas.colsSerie9, 'Serie9' )
					Call oArquivo.AddStr( tblMultasImplantadas.colsSerie10, 'Serie10' )
					Call oArquivo.AddStr( 'Total' , 'Total' )
					!
					Set nTotalSerie1= NUMBER_Null
					Set nTotalSerie2= NUMBER_Null
					Set nTotalSerie3= NUMBER_Null
					Set nTotalSerie4= NUMBER_Null
					Set nTotalSerie5= NUMBER_Null
					Set nTotalSerie6= NUMBER_Null
					Set nTotalSerie7= NUMBER_Null
					Set nTotalSerie8= NUMBER_Null
					Set nTotalSerie9= NUMBER_Null
					Set nTotalSerie10= NUMBER_Null
					! -- detalhe
					Set nLinha= TBL_MinRow
					While SalTblFindNextRow( tblMultasImplantadas, nLinha, 0, 0 )
						Call oArquivo.NovaLinha(  )
						Call SalTblSetContext( tblMultasImplantadas, nLinha )
						!
						Set nTotalSerie1= nTotalSerie1 +  tblMultasImplantadas.colnQtde1 
						Set nTotalSerie2= nTotalSerie2 +  tblMultasImplantadas.colnQtde2 
						Set nTotalSerie3= nTotalSerie3 +  tblMultasImplantadas.colnQtde3 
						Set nTotalSerie4= nTotalSerie4 +  tblMultasImplantadas.colnQtde4 
						Set nTotalSerie5= nTotalSerie5 +  tblMultasImplantadas.colnQtde5 
						Set nTotalSerie6= nTotalSerie6 +  tblMultasImplantadas.colnQtde6 
						Set nTotalSerie7= nTotalSerie7 +  tblMultasImplantadas.colnQtde7 
						Set nTotalSerie8= nTotalSerie8 +  tblMultasImplantadas.colnQtde8 
						Set nTotalSerie9= nTotalSerie9 +  tblMultasImplantadas.colnQtde9 
						Set nTotalSerie10= nTotalSerie10 +  tblMultasImplantadas.colnQtde10
						Set nTotalInfr= nTotalInfr + tblMultasImplantadas.colnQtde1 + tblMultasImplantadas.colnQtde2 + tblMultasImplantadas.colnQtde3 
								+ tblMultasImplantadas.colnQtde4 +  tblMultasImplantadas.colnQtde5 + tblMultasImplantadas.colnQtde6 + tblMultasImplantadas.colnQtde7 
								+ tblMultasImplantadas.colnQtde8 + tblMultasImplantadas.colnQtde9 + tblMultasImplantadas.colnQtde10 
						!
						Call oArquivo.AddStr( tblMultasImplantadas.colsDescInfr, 'Infração' )
						Call oArquivo.AddNumber( tblMultasImplantadas.colnQtde1, 0, 'Qtde1' )
						Call oArquivo.AddNumber( tblMultasImplantadas.colnQtde2, 0, 'Qtde2' )
						Call oArquivo.AddNumber( tblMultasImplantadas.colnQtde3, 0, 'Qtde3' )
						Call oArquivo.AddNumber( tblMultasImplantadas.colnQtde4, 0, 'Qtde4' )
						Call oArquivo.AddNumber( tblMultasImplantadas.colnQtde5, 0, 'Qtde5' )
						Call oArquivo.AddNumber( tblMultasImplantadas.colnQtde6, 0, 'Qtde6' )
						Call oArquivo.AddNumber( tblMultasImplantadas.colnQtde7, 0, 'Qtde7' )
						Call oArquivo.AddNumber( tblMultasImplantadas.colnQtde8, 0, 'Qtde8' )
						Call oArquivo.AddNumber( tblMultasImplantadas.colnQtde9, 0, 'Qtde9' )
						Call oArquivo.AddNumber( tblMultasImplantadas.colnQtde10, 0, 'Qtde10' )
						Call oArquivo.AddNumber( nTotalInfr, 0, 'nTotalInfr' )
						!
						Set nTotalInfr= 0
						!
					! --- rodapé
					Call oArquivo.NovaLinha(  )
					Call oArquivo.AddStr( 'Total' , 'Total' )
					Call oArquivo.AddNumber( nTotalSerie1, 0, 'nTotalSerie1' )
					Call oArquivo.AddNumber( nTotalSerie2, 0, 'nTotalSerie2' )
					Call oArquivo.AddNumber( nTotalSerie3, 0, 'nTotalSerie3' )
					Call oArquivo.AddNumber( nTotalSerie4, 0, 'nTotalSerie4' )
					Call oArquivo.AddNumber( nTotalSerie5, 0, 'nTotalSerie5' )
					Call oArquivo.AddNumber( nTotalSerie6, 0, 'nTotalSerie6' )
					Call oArquivo.AddNumber( nTotalSerie7, 0, 'nTotalSerie7' )
					Call oArquivo.AddNumber( nTotalSerie8, 0, 'nTotalSerie8' )
					Call oArquivo.AddNumber( nTotalSerie9, 0, 'nTotalSerie9' )
					Call oArquivo.AddNumber( nTotalSerie10, 0, 'nTotalSerie10' )
					Call oArquivo.AddNumber( nTotalSerie1 + nTotalSerie2 + nTotalSerie3 + nTotalSerie4 + nTotalSerie5
							+ nTotalSerie6 + nTotalSerie7 + nTotalSerie8 + nTotalSerie9 + nTotalSerie10, 0, 'nTotalSerie' )
					Call oArquivo.GerarArquivo( 'MultasImplantadasPorSerie' )
					!
					Return TRUE
