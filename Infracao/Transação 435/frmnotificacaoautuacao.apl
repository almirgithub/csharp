							:frmNotificacaoAutuacao.sRegistro4,
							:frmNotificacaoAutuacao.sRegistro5, :frmNotificacaoAutuacao.sRegistro6,
							:frmNotificacaoAutuacao.sRegistro7, :frmNotificacaoAutuacao.sRegistro8,:frmNotificacaoAutuacao.sRegistro9')
					!
					! 26/11/2004
					!
					!
					Call SybPrepareProc( hSqlApp, 'exec dbinfracao..RNFNotificacaoAutuacao @TipoNotificacao = :cmbTipoNotificacao,
							@TipoArq = :frmNotificacaoAutuacao.tblTiposArquivos.colnTipoArquivo,
							@NotificacaoI = :dNotificacaoI, @NotificacaoF = :dNotificacaoF',
							':frmNotificacaoAutuacao.nTipoArquivo,:frmNotificacaoAutuacao.sArquivo,:frmNotificacaoAutuacao.nQtdeRegistros,:frmNotificacaoAutuacao.sRegistro1, :frmNotificacaoAutuacao.sRegistro2,
							:frmNotificacaoAutuacao.sRegistro3,
							:frmNotificacaoAutuacao.sRegistro4,
							:frmNotificacaoAutuacao.sRegistro5, :frmNotificacaoAutuacao.sRegistro6,
							:frmNotificacaoAutuacao.sRegistro7, :frmNotificacaoAutuacao.sRegistro8,:frmNotificacaoAutuacao.sRegistro9')
					If NOT SqlExecute( hSqlApp )
						Call SalWaitCursor( FALSE )
						Return FALSE
					! 26/11/2004
					Call SqlGetResultSetCount ( hSqlApp, nTotalRegistros)
					Set nTotalRegistros = nTotalRegistros + 2
					Call SqlFetchNext( hSqlApp, nProximo )
					If nTotalRegistros <= 2
						! Call SalWaitCursor( FALSE )
						! Call SalMessageBeep( 0 )
						! Call SalMessageBox( 'Não há notificações de multa a serem enviadas', 'Atenção', MB_IconExclamation | MB_Ok )
						Return FALSE
					! TERMINA 26/11/2004
					If NOT  MontaArquivo( )
						Call SalWaitCursor( FALSE )
						Return TRUE
					! Fecha arquivo
					Call SalWaitCursor( FALSE )
					Call SalSendMsg( dfmContador, SAM_Validate, 65000, 0 )
					Call SalWaitCursor( FALSE )
					Return TRUE
			Function: MontaArquivo
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					Boolean: bFimLeitura
				Actions
					Set nRegistros = 0
					Set nRow = 0
					Set bFimLeitura = FALSE
					! Call SqlFetchNext( hSqlApp, nProximo )
					If nProximo = FETCH_EOF
						Call SalMessageBox( 'Não há notificações de multa a serem enviadas', 'Atenção', MB_IconExclamation | MB_Ok )
						Call SalWaitCursor( FALSE )
						Return FALSE
					Loop
						Call SalSendMsg( dfmContador, SAM_Validate, nRegistros, 0)
						Set nRegistros = nRegistros + 1
						If NOT bFimLeitura
							If nTipoArquivo != nTipoArquivoAnt
								If nTipoArquivoAnt != 0
									Call SalFileClose( hArquivo )
								Call SalTblInsertRow( tblArquivos, nRow )
								Set tblArquivos.colsArquivo = sArquivo
								Set tblArquivos.colnQtdeRegistros = nQtdeRegistros
								Set nRow = nRow + 1
								Set nTipoArquivoAnt = nTipoArquivo
								If SalFileOpen( hArquivo, sArquivo, OF_Read | OF_Share_Exclusive)
									Call SalFileClose( hArquivo )
									Call SalFileCopy( sArquivo, SalStrLeftX( sArquivo,  SalStrLength( sArquivo ) - 3 ) || 'BCK', FALSE)
								If NOT SalFileOpen( hArquivo, sArquivo, OF_Write | OF_Create  )
									Call SalMessageBox( 'Não foi possível abrir Arquivo: '||sArquivo||' verifique se caminho está correto, ou se há espaço na unidade de disco ou ainda se existe permissão para a gravação do arquivo',
											                                     'Atenção', MB_Ok|MB_IconStop )
									Return FALSE
							! Call SalFilePutStr( hArquivo,
									sRegistro1||SalStrRepeatX(' ',255-SalStrLength(sRegistro1)) || sRegistro2 ||SalStrRepeatX(' ',256-SalStrLength(sRegistro2))   || sRegistro3 ||SalStrRepeatX(' ',249-SalStrLength(sRegistro3))
									|| sRegistro4 || SalStrRepeatX(' ',249-SalStrLength(sRegistro4)) || sRegistro5 || SalStrRepeatX(' ',249-SalStrLength(sRegistro5)) || sRegistro6 ||SalStrRepeatX(' ',249-SalStrLength(sRegistro6)) || sRegistro7 ||SalStrRepeatX(' ',249-SalStrLength(sRegistro7)) ||
									sRegistro8 || SalStrRepeatX(' ',57-SalStrLength(sRegistro8)) || sRegistro9 || SalStrRepeatX(' ',22-SalStrLength(sRegistro9)))
							Call fGravaArquivoNotificacao(hArquivo,sRegistro1,sRegistro2,sRegistro3,sRegistro4,sRegistro5,sRegistro6,sRegistro7,sRegistro8,sRegistro9)
							If NOT  SqlFetchNext( hSqlApp, nProximo )
								Set bFimLeitura = TRUE
								Break
						Else
							Call SalFilePutStr( hArquivo, SalStrRepeatX( ' ', 790 ) )
							If SalNumberMod( nRegistros, 30 ) = 0
								Break
					Call SalWaitCursor( FALSE )
					! adilson 25/07/2005
					Call SalFileClose( hArquivo )
					Return TRUE
			Function: GeraArquivoRENAINF
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					! começa a verificação do Arquivo...
					Set nTipoArquivoAnt = 0
					! Seleciona dados para o arquivo
					Set sArquivo = NomeArquivo( sArquivo)
					Set sCaminho = NomeDiretorio( sArquivo)
					!
					Set _sComandoPesquisa = 'exec dbinfracao..RNFGeraArquivoTransacaoS @Transacao = 412'
					! -
					!
					Call SybPrepareProc( hSqlApp,_sComandoPesquisa, ':frmNotificacaoAutuacao.sArquivo,:frmNotificacaoAutuacao.sRegistro1,:frmNotificacaoAutuacao.sRegistro2,:frmNotificacaoAutuacao.sRegistro8')
					If NOT SqlExecute( hSqlApp )
						Call SalWaitCursor( FALSE )
						Return FALSE
					! 26/11/2004
					Call SqlFetchNext( hSqlApp, nProximo )
					If nProximo != FETCH_EOF
						Set nRetomando =  SalFileOpen( hArquivo, sArquivo,OF_Write|OF_Create )
						!
					If NOT  MontaArquivoRENAINF( )
						Call SalWaitCursor( FALSE )
						Return TRUE
					! Fecha arquivo
					Return TRUE
			Function: MontaArquivoRENAINF
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					Boolean: bFimLeitura
				Actions
					Set bFimLeitura = FALSE
					! Call SqlFetchNext( hSqlApp, nProximo )
					If nProximo = FETCH_EOF
						Call SalMessageBox( 'Não há Registros para transmitir', 'Atenção', MB_IconExclamation | MB_Ok )
						Call SalWaitCursor( FALSE )
						Return FALSE
					Loop
						If NOT bFimLeitura
							Call SalFilePutStr( hArquivo,frmNotificacaoAutuacao.sRegistro1||frmNotificacaoAutuacao.sRegistro2||frmNotificacaoAutuacao.sRegistro8)
							If NOT  SqlFetchNext( hSqlApp, nProximo )
								Set bFimLeitura = TRUE
								Break
					Call SalFileClose( hArquivo )
					Return TRUE
		Window Parameters
		Window Variables
			Number: nRegistros
			Number: nTotalRegistros
			Number: nTipoArquivoAnt
			Number: nTipoArquivo
			! Variaveis que conterao os dados do Arquivo de Remessa
			String: sHeader
			String: sUsuario
			Date/Time: dCriacao
			Long String: sRegistro1
			Long String: sRegistro2
			Long String: sRegistro3
			Long String: sRegistro4
			Long String: sRegistro5
			String: sRegistro6
			String: sRegistro7
			String: sRegistro8
			String: sRegistro9
			!
			Number: nRetomando
			Number: nProximo
			Number: nAux
			! Handle do Arquivo de Remessa
			File Handle: hArquivo
			String: sArquivo
			String: sCaminho
			String: sTipo
			Number: nCod
			String: sDescricao
			Number: nQtdeRegistros
			! 26/11/2004 - foi tirado da função MontaArquivo() e jogado aqui
			Number: nRow
			Number: nRowTipoArquivos
		Message Actions
			! Comentários
				    Autor
					    Celso - 04/06/97
			On SAM_Create
				Set _sTextoStatusBar = 'Tela para geração do arquivo das notificações de multa para a FISEPE'
				Call SqlSetInMessage( hSqlApp, 40000 )
				Set _nEstadoInicialForm = EF_RegistroExistente
				Set _bLimpaTelaAposAtualizacao = FALSE
				Set _bLimpaTelaAposInclusao = TRUE
				Set _bLimpaTelaAposRemocao = TRUE
				Call SalCenterWindow( hWndForm )
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalHideWindow( picSemaforo )
				! Call SalDisableWindow( mdipbGeraNotif )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, 0, 0 )
				Call SalDisableWindow( pbArquivar )
				Call SalPostMsg( tblArquivos, SAM_CornerDoubleClick, 0 ,0)
				Call SalStrSetBufferLength( sRegistro1, 255 )
				Call SalStrSetBufferLength( sRegistro3, 255 )
				Call SalStrSetBufferLength( sRegistro4, 255 )
				Call SalStrSetBufferLength( sRegistro5, 255 )
				Call SalStrSetBufferLength( sRegistro6, 255 )
				Call SalStrSetBufferLength( sRegistro7, 255 )
				Call SalSetMaxDataLength( hArquivo, 1400 )
				Call SalSendMsg( tblNot,MSG_PesquisaTabela, 0, 0 )
			On MSG_Limpa
				Call SalSendClassMessage( MSG_Limpa, wParam, lParam )
				Call SalWaitCursor( TRUE )
				Set nRetomando = 0
				! Pegar as descrições e verificar se foi gerado corretamente ou não.
				Call fMPSPrepareProc( hSql, 'exec ArqNotMultaNCS_ultimo',
						':frmNotificacaoAutuacao.dfsArquivo, :frmNotificacaoAutuacao.dfsUltimo, :frmNotificacaoAutuacao.dfdTermino,
						:frmNotificacaoAutuacao.sUsuario, :frmNotificacaoAutuacao.dCriacao' )
				If NOT SqlExecute( hSql )
					Call SalWaitCursor( FALSE )
					Return FALSE
				If NOT  SqlFetchNext( hSql, nProximo )
					Call SalWaitCursor( FALSE )
					Return FALSE
				Call fMPSPrepareProc( hSql, 'exec ArqNotMultaNCS_retoma',
						':frmNotificacaoAutuacao.dfnTipoArq, :frmNotificacaoAutuacao.cmbTipoArquivo, :frmNotificacaoAutuacao.dfsArquivo, :frmNotificacaoAutuacao.dCriacao,
						:frmNotificacaoAutuacao.dfdNotI, :frmNotificacaoAutuacao.dfdNotF, :frmNotificacaoAutuacao.nRetomando' )
				If NOT SqlExecute( hSql )
					Call SalWaitCursor( FALSE )
					Return FALSE
				If NOT  SqlFetchNext( hSql, nProximo )
					Call SalWaitCursor( FALSE )
					Return FALSE
				Call SalWaitCursor( FALSE )
				! Call SalSendMsg(cmbTipoArquivo , MSG_FetchRefreshDerivados, 0, 0)
				If dCriacao != DATETIME_Null
					If SalMessageBox( 'Houve interrupção  do último arquivo de notificações. Deseja retomar o processo agora?',
							'Atenção', MB_YesNo ) = IDNO
						Call SalSendMsg( hWndForm, SAM_Close, 0, 0)
					Else If NOT GeraArquivo(  )
						Return FALSE
					Return FALSE
				Call SalSendMsg( dfmContador, SAM_Validate, 0, 0 )
				Return TRUE
			On SAM_Destroy
				Call SalFileClose( hArquivo )
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
				! Call SalEnableWindow( mdipbGeraNotif )
			On MSG_Arquiva
				! 26/11/2004
				Set nRowTipoArquivos = 0
				Set nRow = 0
				Call SalTblSetContext( frmNotificacaoAutuacao.tblTiposArquivos, nRowTipoArquivos )
				While SalTblFetchRow( frmNotificacaoAutuacao.tblTiposArquivos, nRowTipoArquivos ) != TBL_NoMoreRows
					Call GeraArquivo(  )
					Set nRowTipoArquivos = nRowTipoArquivos + 1
				! 26/11/2004
				If SalTblSetContext( tblArquivos, 0 ) = FALSE
					Call SalWaitCursor( FALSE )
					Call SalMessageBeep( 0 )
					Call SalMessageBox( 'Não há notificações de multa a serem enviadas', 'Atenção', MB_IconExclamation | MB_Ok )
					Return FALSE
				Else
					Set dfsMensagem = 'Aguarde gerando arquivo para transmissão pelo QWARE'
					Call GeraArquivoRENAINF()
					Set dfsMensagem = 'Fim da geração dos arquivos de Notificações e do SERPRO -  QWARE'
				Call SalDisableWindow( pbArquivar )
				! 26/11/2004
				Call SalWaitCursor( FALSE )
				Return TRUE



