				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Veículos recuperados pela pesquisa'
						Set _sCriacaoConexao = 'Default'
						Set _bSempreHabilitado = TRUE
						! Set _bPesquisaCreate = TRUE
						Set _sComandoPesquisa = 'exec dbvcen..BINConsultaSISCSVS 
										@CodConsulta = :dlgBINConsAutSISCSV.nCodConsulta,
										@NumSeqTransacao = :dlgBINConsAutSISCSV.nSeqTransacao,
										@TipoTransacao = "RT03",
										@TipoDaConsulta = "A" '
						Set _sIntoPesquisa = '
								:dlgBINConsAutSISCSV.tblKey.colsNumSisCsv,
								:dlgBINConsAutSISCSV.tblKey.colsChassi,
								:dlgBINConsAutSISCSV.tblKey.colsPlaca, 
								:dlgBINConsAutSISCSV.tblKey.colnRenavam,
								:dlgBINConsAutSISCSV.tblKey.colsiSituacaoAutorizacao,
								:dlgBINConsAutSISCSV.tblKey.colsTipoAutorizacao,
								:dlgBINConsAutSISCSV.tblKey.colsSistemaConsumidor,
								:dlgBINConsAutSISCSV.tblKey.coldDataEmissaoSisCsv'
						!
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
						Set nPermissao = H_SELECT
					On SAM_DoubleClick
						Call SalSendMsg( pbSeleciona, SAM_Click, 0, 0 )
					On SAM_RowHeaderClick
					On SAM_RowHeaderDoubleClick
