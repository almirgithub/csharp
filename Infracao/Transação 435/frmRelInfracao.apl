						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsApreencaoCHN, 'Apreende CNH' )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsApreencaoVeic, 'Apreende Veiculo' )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsRetencaoVeiculo, 'Retém Veiculo' )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsRemoveVeiculo, 'Remove Veiculo' )
						Call oArquivo.AddNumber( frmRelInfracao.tblInfracao.colnValor,0, 'Valor' )
						Call oArquivo.AddNumber( frmRelInfracao.tblInfracao.colnPontuacao,0, 'Qtd. Pontos' )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsApreensaoCLA, 'Apreende CLA' )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsApreensaoCRV, 'Apreende CRV')
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsApreensaoOutros, 'Apreende Outros' )
						Call oArquivo.AddNumber( frmRelInfracao.tblInfracao.colnResRecolhimentoDoc,0, 'ResRecolhimentoDoc' )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsRecolhimentoCNH, 'RecolhimentoCNH' )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsRecolhimentoCrvCrlv, 'RecolhimentoCrvCrlv' )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsSuspDireitoDirigir, 'Susp. Direito Dirigir' )
						While SqlFetchNext( hSql, lParam )
							Call oArquivo.NovaLinha(  )
							Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsInfracao, 'Infração' )
							Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsDescricao, 'Descrição' )
							Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsDescricaoCompl, 'Complemento Desc.' )
