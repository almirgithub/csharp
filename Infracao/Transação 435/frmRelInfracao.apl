						Set _sTextoStatusBar = 'Inicia a impressão'
						Set _sTabela = ''
						Set _nPermissao = 15
						Set _sTipoComando = 'Comum'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						! - - - - - -
						Set nErro = 0
						! - - - - - -
						Set sNomeQRP = 'cadastroinfracao.qrp'
						! - - - Valida se há dados na tabela
						If NOT SalTblAnyRows( tblInfracao, 0, 0)
							Call SalMessageBox('Não há dados para impressão. Faça a pesquisa antes de imprimir!', 'Atenção!', MB_IconExclamation)
							Call SalMessageBeep( -1 )
							Call SalSetFocus( pbPesquisa )
							Return FALSE
						! - - Valida se existe o qrp na pasta
						Call ValidarQRP(sNomeQRP,nErro)
						! - - Valida se o qrp já esta aberto
						If hWndReportView != hWndNULL
							Call SalReportClose( hWndReportView )
						! - - - - - -
						If dfsSaida = 'I'
							Set nParamArray[RPT_PrintParamOptions] = RPT_PrintDraft
