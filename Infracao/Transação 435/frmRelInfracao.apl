				Functions
				Window Variables
					Number: nAux
				Message Actions
					On SAM_Create
						Set _bPesquisaCreate = TRUE
						! - -
						Set _sCriacaoConexao = 'FALSE'
						Set _bNuncaHabilitado = FALSE
						Set _bSempreHabilitado = FALSE
						! - -
						Set _sComandoPesquisa = 'exec dbinfracao..InfracaoR @iSituacao = :frmRelInfracao.dfsSituacao'
						! - -
						Set _sIntoPesquisa = '
								:frmRelInfracao.tblInfracao.colsInfracao,
								:frmRelInfracao.tblInfracao.colsDescricao,
								:frmRelInfracao.tblInfracao.colsDescricaoCompl,
								:frmRelInfracao.tblInfracao.colsAmparoLegal,
								:frmRelInfracao.tblInfracao.colsCompetencia,
								:frmRelInfracao.tblInfracao.colsInfrator,
								:frmRelInfracao.tblInfracao.colsSituacao,
								:frmRelInfracao.tblInfracao.colsApreencaoCHN,
								:frmRelInfracao.tblInfracao.colsApreencaoVeic,
								:frmRelInfracao.tblInfracao.colsRetencaoVeiculo,
								:frmRelInfracao.tblInfracao.colsRemoveVeiculo,
								:frmRelInfracao.tblInfracao.colnValor,
								:frmRelInfracao.tblInfracao.colnPontuacao,
								:frmRelInfracao.tblInfracao.colsApreensaoCLA,
								:frmRelInfracao.tblInfracao.colsApreensaoCRV,
								:frmRelInfracao.tblInfracao.colsApreensaoOutros,
								:frmRelInfracao.tblInfracao.colnResRecolhimentoDoc,
								:frmRelInfracao.tblInfracao.colsRecolhimentoCNH,
								:frmRelInfracao.tblInfracao.colsRecolhimentoCrvCrlv,
								:frmRelInfracao.tblInfracao.colsSuspDireitoDirigir
								'
						! - -
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On UDM_AntesPesquisa
						! - - Limpa a tabela
						Call SalTblReset( tblInfracao )
						! - - Obriga a especificar o orgão
						If dfsSituacao = 'A'
							Set sCabSituacao = 'Ativas'
						Else If dfsSituacao = 'T'
							Set sCabSituacao = 'Todas'
						Else If dfsSituacao = 'I'
							Set sCabSituacao = 'Inativas'
						Return TRUE
					! - - -  Cabeçalho > Setar as variaveis no INPUT VARIABLES
					On SAM_ReportNotify
						Set hWndReportView = SalNumberToWindowHandle( wParam )
						If lParam = RPT_BeforePageHeader
							Call SalReportSetStringVar( hWndReportView, 'Secretaria', sGlobalSecretaria )
							Call SalReportSetStringVar( hWndReportView, 'Detran', sGlobalDetran )
							Call SalReportSetNumberVar( hWndReportView, 'CodSetor', nGlobalSetor )
							Call SalReportSetStringVar( hWndReportView, 'NomeSetor', sGlobalSetor )
							Call SalReportSetStringVar( hWndReportView, 'Usuario', SqlUser )
							Call SalReportSetStringVar( hWndReportView, 'CabSituacao', sCabSituacao )
