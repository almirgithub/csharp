			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo', 
								           'Relatório de Cadastro de Infração', 'Relatório de Cadastro de Infração' )
				! - - Saída arquivo
				If dfsSaida = 'A'
					Set oArquivo = new cArquivo
					Call oArquivo.setSeparador( ';' )
					Call oArquivo.IniciaLinha(  )
				Return TRUE
			On SAM_ReportFetchInit
				! - - Se a saída for arquivo
				If dfsSaida = 'A'
					Call fMPSPrepareProc( hSql, 'exec dbinfracao..InfracaoR @iSituacao = :frmRelInfracao.dfsSituacao',
							'
							:frmRelInfracao.tblInfracao.colsInfracao,
							:frmRelInfracao.tblInfracao.colsDescricao,
							:frmRelInfracao.tblInfracao.colsDescricaoCompl,
							:frmRelInfracao.tblInfracao.colsAmparoLegal,
							:frmRelInfracao.tblInfracao.colsCompetencia,
							:frmRelInfracao.tblInfracao.colsInfrator,
							:frmRelInfracao.tblInfracao.colsSituacao,
							:frmRelInfracao.tblInfracao.colsApreencaoCHN,
							:frmRelInfracao.tblInfracao.colsApreencaoVeic,
