							:frmRelInfracao.tblInfracao.colsApreensaoCRV,
							:frmRelInfracao.tblInfracao.colsApreensaoOutros,
							:frmRelInfracao.tblInfracao.colnResRecolhimentoDoc,
							:frmRelInfracao.tblInfracao.colsRecolhimentoCNH,
							:frmRelInfracao.tblInfracao.colsRecolhimentoCrvCrlv,
							:frmRelInfracao.tblInfracao.colsSuspDireitoDirigir
							'		)
					If SqlExecute( hSql )
						! - - Faz a geração do arquivo
						Call oArquivo.IniciaLinha(  )
						Call oArquivo.setSeparador( ';' )
						Call oArquivo.NovaLinha(  )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsInfracao, 'Infração' )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsDescricao, 'Descrição' )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsDescricaoCompl, 'Complemento Desc.' )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsAmparoLegal, 'Amparo Legal' )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsCompetencia, 'Competência' )
						Call oArquivo.AddStr( frmRelInfracao.tblInfracao.colsInfrator, 'Infrator' )
