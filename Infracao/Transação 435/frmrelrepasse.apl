			Number: nErro
			Number: nQuantidade
			Number: nErroPrin
			String: sDevice
			String: sDriver
			String: sPorta
			String: sDeviceDef
			String: sDriverDef
			String: sPortaDef
			Window Handle: hWndReportView
			Number: nAux
			Date/Time: dData
			String: sBaixaManual
			String: sBaixaBancaria
			Window Handle: hWndRel
			Window Handle: hWndRelInf
			Number: nPorcenCompetencia
			Number: nValorTotal
			Number: nTipoDebito
			Number: nOrgaoRecebedor
			Number: nBanco
			Number: nCodMunicipio
			Number: nDigMunicipio
			Number: nOrgaoAutuante
			Number: nInfracao
			String: sTipoDebito
			String: sMunicipio
			Number: nDigito
			Number: nMunicipio
			String: sInfracao
			String: sOrgaoRecebedor
			String: sOrgaoAutuante
			String: sCompetencia
			String: sBanco
			String: sRetornoPesquisa
			String: sBancario
			String: sManual
			Number: nSpid
			String: sTipoRepasse
			String: sTituloInfracao
		Message Actions
			On SAM_Create
				Set _sTextoStatusBar = 'Tela de relatório de repasses.'
				Set _nEstadoInicialForm = EF_Vazio
				Set _bLimpaTelaAposAtualizacao = TRUE
				Set _bLimpaTelaAposInclusao = TRUE
				Call SalCenterWindow( hWndForm )
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
			On SAM_ReportNotify
				If lParam = RPT_BeforePageHeader
					Call SalReportSetNumberVar( SalNumberToWindowHandle ( wParam ), 'nResultado', nResultado )
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo',
						'Relatório de Repasses', 'por Infração ' )
				Call SqlImmediate( 'select getdate() into :frmRelRepasse.dCorrente' )
				Return TRUE
			On SAM_ReportFetchInit
				Set nResultado = 0
				Set nErroPrin = 0
				Call SalWaitCursor( TRUE )
				If nMunicipio = 0
					Set nMunicipio = NUMBER_Null
				Call SalReportSetNumberVar( SalNumberToWindowHandle ( wParam ), 'nResultado', nResultado )
				!
				If fMPSPrepareProc( hSql, 'exec RelRepasseInfracao
								@Banco		= :frmRelRepasse.dfnBanco,
								@Opcao 		= :frmRelRepasse.dfnOpcao,
								@Vigencia 	= :frmRelRepasse.dfdVigencia,
								@TipoDebito 	=  :frmRelRepasse.dfnTipoDebito,
								@BaixaManual 	= "N",
								@BaixaBancaria 	= "S",
								@Listar 		= :frmRelRepasse.dfsListar,
								@Municipio 	= :frmRelRepasse.nMunicipio',
								sRetornoPesquisa)
					If NOT SqlExecute( hSql )
						Set nErroPrin = 1
						Call SalWaitCursor( FALSE )
						Return FALSE
				!
				Call SalWaitCursor( FALSE )
				!
				Return SqlGetResultSetCount( hSql, nErro )
			On SAM_ReportFetchNext
				Call SalWaitCursor( FALSE )
				Call SqlFetchNext( hSql, nErro )
				!
				If bAbortaImpressao
					Return FALSE
				!
				If sCompetencia = 'E'
					Set sCompetencia = 'Estadual'
				Else If sCompetencia = 'A'
					Set sCompetencia = 'Ambas'
				Else If sCompetencia = 'M'
					Set sCompetencia = 'Municipal'
				!
				If sTipoRepasse = 'P'
					Set sTipoRepasse = 'Percentual'
				Else If sTipoRepasse = 'V'
					Set sTipoRepasse = 'Valor Fixo'
				Else If sTipoRepasse = 'S'
					Set sTipoRepasse = 'Saldo'
				!
				If nErro != FETCH_EOF
					Return TRUE
				Else
					Return FALSE
			On SAM_ReportFinish
				Call SalPrtSetDefault(sDeviceDef,sDriverDef,sPortaDef)
				If nErroPrin = 0
					Set hWndRelInf = SalCreateWindow( frmRelRepasseInf, hWndMDI, hWndRelInf, sBaixaBancaria,
							sBaixaManual, dfsSaida, dfdVigencia, nSpid, rbImpressora, dfsListar, sTituloInfracao)
				Call SqlPrepareAndExecute( hSql, 'cancel' )

