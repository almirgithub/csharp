						If NOT SalIsNull( hWndItem )
							Set nAux = SerieDigitoModulo11( dfsSerie, SalNumberTruncate( MyValue/10, 10, 0 ))
							If nAux >= 0 AND nAux != SalNumberMod( MyValue, 10 )
								If nAux != -1
									Call SalMessageBeep( MB_IconExclamation )
									Call SalMessageBox( 'Dígito inválido', 'Atenção', MB_IconExclamation | MB_Ok )
								Call SalClearField( hWndItem )
								Call SalAbort( -1)
							Set nCodSemDig = SalNumberTruncate( MyValue/10, 10, 0 )
						Return TRUE
			!
