			On SAM_Create
				Set _sTextoStatusBar = 'Tela de transferencia de auto DETRAN/DE - Por implantação indevida'
				Set _nEstadoInicialForm = EF_Vazio
				Set _bLimpaTelaAposRemocao = FALSE
				Set _sComandoRemocao = ''
				Set _sListaINTORemocao = ''
				! ======================== PESQUISA ===========================================
				Set _sComandoPesquisa = 'exec dbinfracao..TransferenciaAutoS @OrgaoAutuante = :frmTransferenciaAuto.dfnOrgaoAutuador, 
						@Serie = :frmTransferenciaAuto.dfsSerie, @Auto = :frmTransferenciaAuto.dfnAuto'
				Set _sListaINTOPesquisa = ':frmTransferenciaAuto.dfsPlaca, :frmTransferenciaAuto.dfsUFPlaca, :frmTransferenciaAuto.dfdDataHora,
						:frmTransferenciaAuto.dfnMunicipio, :frmTransferenciaAuto.dfsMunicipio, :frmTransferenciaAuto.dfsLocal'
				! ======================== INCLUSAO ===========================================
				Set _bLimpaTelaAposInclusao = FALSE
				Set _sComandoInclusao = 'exec dbinfracao..TransferenciaAutoI @OrgaoAutuanteDE = :frmTransferenciaAuto.dfnOrgaoAutuador, 
						@OrgaoAutuantePara = :frmTransferenciaAuto.dfnOrgaoAutuante, @Serie = :frmTransferenciaAuto.dfsSerie, 
						@Auto = :frmTransferenciaAuto.dfnAuto, @Lote = :frmTransferenciaAuto.dfnLote, @AgenteEquip = :frmTransferenciaAuto.dfnAgenteEquipe'
				Set _sListaINTOInclusao = ''
				! ======================== ATUALIZACAO ===========================================
				Set _bLimpaTelaAposAtualizacao = FALSE
				Set _sComandoAtualizacao = 'exec dbinfracao..TransferenciaAutoI @OrgaoAutuanteDE = :frmTransferenciaAuto.dfnOrgaoAutuador, 
						@OrgaoAutuantePara = :frmTransferenciaAuto.dfnOrgaoAutuante, @Serie = :frmTransferenciaAuto.dfsSerie, 
						@Auto = :frmTransferenciaAuto.dfnAuto, @Lote = :frmTransferenciaAuto.dfnLote, @AgenteEquip = :frmTransferenciaAuto.dfnAgenteEquipe'
				Set _sListaINTOAtualizacao = ''
				! =============================================================================
				Call SalCenterWindow( hWndForm )
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				Call SalEnableWindow( frmTransferenciaAuto.pbPesquisar )
				Call SalEnableWindow( frmTransferenciaAuto.dfnOrgaoAutuador )
				Call SalEnableWindow( frmTransferenciaAuto.dfsSerie )
				Call SalEnableWindow( frmTransferenciaAuto.dfnAuto )
				Call SalDisableWindow( frmTransferenciaAuto.dfsPlaca )
				Call SalDisableWindow( frmTransferenciaAuto.dfsUFPlaca )
				Call SalDisableWindow( frmTransferenciaAuto.dfdDataHora )
				Call SalDisableWindow( frmTransferenciaAuto.dfnMunicipio )
				Call SalDisableWindow( frmTransferenciaAuto.dfsMunicipio )
				Call SalDisableWindow( frmTransferenciaAuto.dfsLocal )
				Call SalDisableWindow( frmTransferenciaAuto.dfnOrgaoAutuante )
				Call SalDisableWindow( frmTransferenciaAuto.dfnAgenteEquipe )
				Call SalDisableWindow( frmTransferenciaAuto.dfnLote )
			On UDM_AposPesquisa
				Call SalSendClassMessage( UDM_AposPesquisa, wParam, lParam )
				Call SalDisableWindow( frmTransferenciaAuto.dfnOrgaoAutuador )
				Call SalDisableWindow( frmTransferenciaAuto.dfsSerie )
				Call SalDisableWindow( frmTransferenciaAuto.dfnAuto )
				Call SalDisableWindow( frmTransferenciaAuto.dfsPlaca )
				Call SalDisableWindow( frmTransferenciaAuto.dfsUFPlaca )
				Call SalDisableWindow( frmTransferenciaAuto.dfdDataHora )
				Call SalDisableWindow( frmTransferenciaAuto.dfnMunicipio )
				Call SalDisableWindow( frmTransferenciaAuto.dfsMunicipio )
				Call SalDisableWindow( frmTransferenciaAuto.dfsLocal )
				Call SalEnableWindow( frmTransferenciaAuto.dfnOrgaoAutuante )
				Call SalEnableWindow( frmTransferenciaAuto.dfnAgenteEquipe )
				Call SalEnableWindow( frmTransferenciaAuto.dfnLote )
			On UDM_AposInclusao
				Call SalSendClassMessage( UDM_AposInclusao, wParam, lParam )
				! Call SalEnableWindow( frmTransferenciaAuto.dfnOrgaoAutuador )
				! Call SalEnableWindow( frmTransferenciaAuto.dfsSerie )
				! Call SalEnableWindow( frmTransferenciaAuto.dfnAuto )
				Call SalDisableWindow( frmTransferenciaAuto.dfsPlaca )
				Call SalDisableWindow( frmTransferenciaAuto.dfsUFPlaca )
				Call SalDisableWindow( frmTransferenciaAuto.dfdDataHora )
				Call SalDisableWindow( frmTransferenciaAuto.dfnMunicipio )
				Call SalDisableWindow( frmTransferenciaAuto.dfsMunicipio )
				Call SalDisableWindow( frmTransferenciaAuto.dfsLocal )
				Call SalDisableWindow( frmTransferenciaAuto.dfnOrgaoAutuante )
				Call SalDisableWindow( frmTransferenciaAuto.dfnAgenteEquipe )
				Call SalDisableWindow( frmTransferenciaAuto.dfnLote )
				Call SalDisableWindow( frmTransferenciaAuto.pbArquivar )
				Call SalMessageBox( "Auto transferido com sucesso", "Mensagem", MB_IconExclamation )
			On UDM_AposAtualizacao
				Call SalSendClassMessage( UDM_AposAtualizacao, wParam, lParam )
				! Call SalEnableWindow( frmTransferenciaAuto.dfnOrgaoAutuador )
				! Call SalEnableWindow( frmTransferenciaAuto.dfsSerie )
				! Call SalEnableWindow( frmTransferenciaAuto.dfnAuto )
				Call SalDisableWindow( frmTransferenciaAuto.dfsPlaca )
				Call SalDisableWindow( frmTransferenciaAuto.dfsUFPlaca )
				Call SalDisableWindow( frmTransferenciaAuto.dfdDataHora )
				Call SalDisableWindow( frmTransferenciaAuto.dfnMunicipio )
				Call SalDisableWindow( frmTransferenciaAuto.dfsMunicipio )
				Call SalDisableWindow( frmTransferenciaAuto.dfsLocal )
				Call SalDisableWindow( frmTransferenciaAuto.dfnOrgaoAutuante )
				Call SalDisableWindow( frmTransferenciaAuto.dfnAgenteEquipe )
				Call SalDisableWindow( frmTransferenciaAuto.dfnLote )
				Call SalDisableWindow( frmTransferenciaAuto.pbArquivar )
				Call SalMessageBox( "Auto transferido com sucesso", "Mensagem", MB_IconExclamation )
			On MSG_Pesquisa
				Call SalSendClassMessage( MSG_Pesquisa, wParam, lParam )
			On MSG_Limpa
				Call SalSendClassMessage( MSG_Limpa, wParam, lParam )
