				If NOT SqlExecute( hSqlApp )
					Call SalWaitCursor( FALSE )
					! Call LimparVeiculo( )
					! Return FALSE
				Call SqlFetchNext( hSqlApp, nReturn )
				! Set nSetorUsuario = nGlobalSetor/10
				Set _sComandoInclusao = 'select null'
			On MSG_Pesquisa
				Set rbDefault = TRUE
				Set rbPGU = FALSE
				Set rbCNH = FALSE
				Return SalSendMsg( tblAuto,MSG_PesquisaTabela, wParam, lParam )
			On UDM_AposAtualizacao
				! Set bAlteraTabela = TRUE
				Call SalSendMsg( frmIndCondutorProcessoEspeciais.tblAuto, MSG_Arquiva, wParam, lParam )
				! Set bAlteraTabela = FALSE
				! If cbNotificacao
					   Set dfsSituacao = 'N'
				! If cmbUFPlaca !='PE'
					Call fTransmiteRENAINF(frmConsistencia.dfsSerie,dfnOrgaoAutuante,dfnNumero,
							tblInfra.colnCodigoCTB,411,'EV01',hSqlApp,'','')
				Return SalSendMsg( tblAuto, UDM_AposAtualizacao, wParam, lParam )
				! Return SalSendClassMessage( UDM_AposAtualizacao, wParam, lParam )
			On UDM_Inclusao
				Set rbDefault = TRUE
				Call SalSendMsg( tblAuto, MSG_Arquiva, wParam, lParam )
				Return SalSendMsg( tblAuto, UDM_AposAtualizacao, wParam, lParam )

		
		

