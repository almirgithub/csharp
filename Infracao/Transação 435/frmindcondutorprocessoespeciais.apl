						Set _bNuncaHabilitado = FALSE
						Set _sNomeParaMsgs = 'CNH condutor'
						Set _sMsgValidacao = 'Valor inválido!'
						Set _sTipoMsgValidacao = 'ESPECIFICA'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					! On UDM_ValidaCampo
						! Set cmbUFCondutor = sGlobalUF
						Call SybPrepareProc( hSqlApp, 'exec dbinfracao..RpcLerCondutorCnh @nRegistro = :frmIndCondutorProcessoEspeciais.dfnCNHCondutor',
								                                ':frmIndCondutorProcessoEspeciais.dfsCondutor,:frmIndCondutorProcessoEspeciais.sUFCNH' )
						Call SqlExecute( hSqlApp )
						Call SqlFetchNext( hSqlApp, frmIndCondutorProcessoEspeciais.nReturn )
						If nReturn = FETCH_EOF
							Call SalMessageBox('Registro não encontrado','CNH', MB_IconStop )
							Call SalListSelectString( cmbUFCondutor, 0, 'PE' )
							Set dfsCondutor = ''
						Else 
							Call SalListSelectString( cmbUFCondutor, 0, sUFCNH )
						Call SalSendMsg( pbArquivar, MSG_Habilita1, wParam, lParam )
						Return TRUE
						! Call SalSetFieldEdit( cmbUFCondutor, TRUE )
