				Functions
					Function: fTransmiteIndicaCondutor
						Description:
						Returns
						Parameters
						Static Variables
						Local variables
							Number: nRow
							String: sCondicionalidade
						Actions
							! começa a verificação do Arquivo...
							! Call fTransmiteRENAINF(tblInf.colsSerie,tblInf.colnOrgaoAutuante,tblInf.colnAuto,
									tblInf.colnCodigoCTB,416,'EV01',hSqlApp,'','','','',7,1)
							Set nRow = 0
							Call SalTblSetContext( tblAuto, nRow )
							Loop
								If SalTblFetchRow( tblAuto, nRow ) = TBL_NoMoreRows
									Break
								Call SalTblSetRowFlags( tblAuto, ROW_Edited, nRow, TRUE )
								! Call fTransmiteRENAINF(tblAutos.colsSerie,tblAutos.colnOrgaoAutuante,
										tblAutos.colnAuto,
										tblAutos.colnInfracao,412,'EV01',hSqlApp,'','')
								! Call fTransmiteRENAINF(tblInf.colsSerie,tblReenvioRENAINF.colnOrgaoAutuante,tblReenvioRENAINF.colnAuto,
										tblReenvioRENAINF.colnInfracao,tblReenvioRENAINF.colnCodTransacao,'EV01',hSqlApp,'','',sCondicionalidade,'',0)
								! If tblAuto.colsAutoRENAINF != STRING_Null
									Call fTransmiteRENAINF(tblAuto.colsSerie,tblAuto.colnOrgaoAutuante,tblAuto.colnNumero,
											tblAuto.colnInfracao,418,'EV01',hSqlApp,'','','','',7,1,'3',0)
								If tblAuto.colsDataImpl>= dDataInicioRENAINFTotal and  dDataInicioRENAINFTotal != DATETIME_Null
									If tblAuto.colsAutoRENAINF != STRING_Null
										Call fTransmiteRENAINF(tblAuto.colsSerie,tblAuto.colnOrgaoAutuante,tblAuto.colnNumero,
												0,418,'EV01',hSqlApp,'','','','',7,1,'3',0)
