					On SAM_Create
						Set _sTextoStatusBar = 'CNH do condutor que cometeu cometeu a(s) infração(ões)'
						Set _sNroCamposIgnorados = 'NENHUM'
						Set _bPermiteNulos = TRUE
						Set _bVisivel = TRUE
