				Functions
					Function: fPopulaMotCanc
						Description:
						Returns
						Parameters
						Static Variables
						Local variables
						Actions
							If NOT SalIsNull( hWndItem)
								Call SalWaitCursor(TRUE )
								Call fMPSPrepareProc( hSqlApp, 'exec MotivoCancelamentoS_rest @Descricao  = :colsMotCancelamento',
										':frmCancMulta.tblInf.colnMotCancelamento,:frmCancMulta.tblInf.colsRestituir')
								If NOT SqlExecute( hSqlApp )
									Call SalWaitCursor( FALSE )
									Call SalClearField( hWndItem )
									Return VALIDATE_Cancel
								Call SqlFetchNext( hSqlApp, nRetorno)
								Call SalWaitCursor( FALSE )
								If nRetorno = FETCH_EOF
									Call SalMessageBeep( MB_IconExclamation  )
									Call SalMessageBox( 'Motivo de cancelamento não cadastrado', 'Atenção', MB_IconExclamation | MB_Ok )
									Call SalClearField( hWndItem )
									Return VALIDATE_Cancel
								If colsRestituir = 'S'  and colsPago = 'S'
									Set colbRestituir = 1
									Set colsRestituir = 'C/Restituição'
								Else If colsRestituir = 'S' and colsPago = 'N'
									Set colsMotCancelamento = STRING_Null
									Set colnMotCancelamento =  NUMBER_Null
									Set colsRestituir = ''
									Call SalMessageBeep( MB_IconExclamation )
									Call SalMessageBox( 'Motivo: ' || sMotivoCancelamento || '
											Não é permitido restituição para auto que não estejam pagos' , 'Atenção', MB_IconExclamation | MB_Ok )
									Set colbRestituir = FALSE
									Set nLinhaAtual = TBL_MaxRow
									Set bMsgRestituicao = TRUE
									Return VALIDATE_Cancel
								Else
									Set colsRestituir = 'S/Restituição' 
									Set colbRestituir = 0
								Set bMsgRestituicao = FALSE
								Return VALIDATE_OkClearFlag
					Function: fTransmiteCancelamento
						Description:
						Returns
						Parameters
						Static Variables
						Local variables
							Number: nRow
							String: sCondicionalidade
						Actions
							! começa a verificação do Arquivo...
							Set nRow = 0
							Call SalTblSetContext( tblInf, nRow )
							Loop
								If SalTblFetchRow( tblInf, nRow ) = TBL_NoMoreRows
									Break
								Call SalTblSetRowFlags( tblInf, ROW_Edited, nRow, TRUE )
								If tblInf.colnMotCancelamento != NUMBER_Null and tblInf.colsAutoRENAINF != STRING_Null and tblInf.colsIndicaDefesa != 'S'
									Call fTransmiteRENAINF(tblInf.colsSerie,tblInf.colnOrgaoAutuante,tblInf.colnAuto,
											tblInf.colnCodigoCTB,416,'EV01',hSqlApp,'','','','',7,1,'3',0)
								Set nRow = nRow + 1
							Return TRUE
					Function: fCopiaMotivoCancelamento
						Description:
						Returns
						Parameters
							String: MotivoCancelamento
							Number: CodMotivoCancelamento
						Static Variables
						Local variables
							Number: nRow
							String: sCondicionalidade
						Actions
							Call fPopulaMotCanc()
							While SalTblFindNextRow( frmCancMulta.tblInf, nLinhaAtual, 0, 0 )
								Call SalTblSetContext( tblInf, nLinhaAtual )
								Call SalTblSetRowFlags(frmCancMulta.tblInf, nLinhaAtual, ROW_Edited, TRUE)
								Set frmCancMulta.tblInf.colsMotCancelamento =  MotivoCancelamento
								! Set frmCancMulta.tblInf.colnMotCancelamento =  CodMotivoCancelamento
								Call SalTblSetRowFlags(frmCancMulta.tblInf, nLinhaAtual, ROW_Edited, TRUE)
								Call fPopulaMotCanc()
							If bMsgRestituicao = FALSE
								Call SalSetFocus(pbArquivar)
							Return TRUE
