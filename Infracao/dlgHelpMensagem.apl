				Property Template:
				Class DLL Name:
				Title: &Pesquisar
				Window Location and Size
					Left: 0.133"
					Top: 4.667"
					Width:  0.95"
					Width Editable? Yes
					Height: 0.25"
					Height Editable? Yes
				Visible? No
				Keyboard Accelerator: (none)
				Font Name: Default
				Font Size: Default
				Font Enhancement: Default
				Picture File Name:
				Picture Transparent Color: None
				Image Style: Single
				Text Color: Default
				Background Color: Default
				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Pesquisa os dados'
						Set _sTipoComando = 'Comum'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						Call SalStrSetBufferLength( sTexto, 100000 )
						Call SalWaitCursor( FALSE )
						If fMPSConnect (hSql)
							Call SqlSetInMessage( hSql, 100000 )
							If NOT fMPSPrepareProc( hSql, 'exec dbvcen..HelpMensagemS  @Codigo = :dlgHelpMensagem.pCodigo,@Sistema = :dlgHelpMensagem.pCodigoSistema',
									':dlgHelpMensagem.sMensagem,:dlgHelpMensagem.sTexto' )
								Call fMPSDisconnect (hSql)
								Return FALSE
							If SqlExecute( hSql )
								Call SqlFetchNext( hSql, nRetorno )
							If nRetorno != FETCH_EOF
								While nRetorno != FETCH_EOF
									Call SalSetWindowText( mlTexto, sTexto )
									Call SqlFetchNext( hSql, nRetorno )
							Call fMPSDisconnect (hSql)
