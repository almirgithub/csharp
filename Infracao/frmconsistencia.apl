				Set nTmp = TBL_MaxRow
				If SalTblFindPrevRow( frmConsistencia.tblInfra, nTmp, 0, ROW_MarkDeleted )
					If nTmp >= nAutoNotificacao
						Call SalMessageBeep( MB_IconExclamation )
						Call SalMessageBox( 'Não é permitido inserir mais do que ' || SalNumberToStrX( nAutoNotificacao, 0) ||
								' infrações em um auto, este valor é definido nos parâmetros gerais do sistema', 'Atenção', MB_IconExclamation | MB_Ok )
						Return FALSE
				Else
					Call SalMessageBeep( MB_IconExclamation )
					Call SalMessageBox( 'É obrigatório informar pelo menos uma infração', 'Atenção', MB_IconExclamation | MB_Ok )
					Return FALSE
				Set bAlteraTabela = FALSE
				! Necessario para poder consistir infracao/tipoveiculo
				Set sLocal =  SalStrLeftX( frmConsistencia.dfsLocal, 50)
				Set sComplLocal =  SalStrMidX( frmConsistencia.dfsLocal, 50,50)
				Return SalSendMsg( frmConsistencia.tblInfra, MSG_Arquiva, 0, 0 )
			On UDM_AposPesquisa
				! If bNaoExec
					  Set bNaoExec = FALSE
					  Return TRUE
				If nEstadoForm = EF_RegistroNovo
					Call SalMessageBeep( MB_IconExclamation )
					Call SalMessageBox( 'Auto não cadastrado', 'Atenção', MB_IconExclamation | MB_Ok )
					Return FALSE
				! Call AtualizaSituacao( )
				Set dfdAuto = SalDateConstruct( SalDateYear( dInfracao ), SalDateMonth( dInfracao ), SalDateDay( dInfracao ), 0, 0, 0 )
				Set dfdHora = dInfracao
				Call SalEnableWindow( pbNovaLinha )
				If SalIsNull( dfnVeiculo)
					Set sMarcaAux = dfsMarcaModelo
				! Verificando a permissão do orgão no Setor
				! Call fMPSPrepareProc( hSqlApp,'Exec dbinfracao..AutoS_verSetor  @OrgaoAutuante = :dfnOrgaoAutuante, @Serie = :dfsSerie, @Cod = :dfnNumero' , ':nAux' )
				! If NOT SqlExecute( hSqlApp )
					Call SalSendMsg( pbNovaLinha, MSG_Desabilita1, 0, 0 )
					Call SalSendMsg( pbArquivar, MSG_Desabilita1, 0, 0 )
					Return VALIDATE_Cancel
				! Verificando se a cidade esta associada com o Setor.
				If NOT SalPostMsg( dfnMunicipio, UDM_ValidaCampo, 0, 0 )
					Return VALIDATE_Cancel
				Return TRUE
			On UDM_AposAtualizacao
				! Call SalDisableWindow( pbAnterior )
				! Call SalDisableWindow( pbProximo )
				If NOT SalIsNull( dfnNovoOA  ) and NOT SalIsNull( dfnNovoLote  ) and NOT SalIsNull(dfnNovoA)
					Set dfnLote = dfnNovoLote
				Call SalPostMsg( pbPesquisar, SAM_Click, 0, 0 )
				Set bAlteraTabela = TRUE
				Call SalSendMsg( frmConsistencia.tblInfra, MSG_Arquiva, wParam, lParam )
				Set bAlteraTabela = FALSE
				! If cbNotificacao
					   Set dfsSituacao = 'N'
				If dfsAutoRENAINF != STRING_Null
					Call fTransmiteRENAINF(frmConsistencia.dfsSerie,dfnOrgaoAutuante,dfnNumero,
							tblInfra.colnCodigoCTB,420,'EV01',hSqlApp,'','','','2',0,0,'',0)
				! If cmbUFPlaca !='PE'
				Call fTransmiteRENAINF(frmConsistencia.dfsSerie,dfnOrgaoAutuante,dfnNumero,
						tblInfra.colnCodigoCTB,411,'EV01',hSqlApp,'','','','',0,0,'',0)
				Return SalSendClassMessage( UDM_AposAtualizacao, wParam, lParam )







