				Message Actions 
					On SAM_Create
						Set _sTextoStatusBar = 'Infrações do autos'
						Set _sNroCamposIgnorados = 'NENHUM'
						!
						Set _sCriacaoConexao = 'Default'
						!
						Set _bNuncaHabilitado = FALSE
						Set _bPesquisaCreate = FALSE
						Set _bSempreHabilitado = FALSE
						!
						Set _sComandoPesquisa = 'exec dbinfracao..InfracaoAutoS
								@Serie = :frmConsistencia.dfsSerie, @Auto = :frmConsistencia.dfnNumero, @OrgaoAutuante = :frmConsistencia.dfnOrgaoAutuante'
						! frmConsistencia
						Set _sIntoPesquisa = '
								:frmConsistencia.tblInfra.colnCodigoCTB,
								:frmConsistencia.tblInfra.colsDescricao,
								:frmConsistencia.tblInfra.colnPermitido,
								:frmConsistencia.tblInfra.colnAferido,
								:frmConsistencia.tblInfra.colnInstrum,
								:frmConsistencia.tblInfra.colsInstrum,
								:frmConsistencia.tblInfra.colsSitI,
								:frmConsistencia.tblInfra.colsUnidadeMedida,
								:frmConsistencia.tblInfra.colnConsiderada'
						!
						Set _sComandoInsere = 'exec dbinfracao..InfracaoAutoI
								@Serie = :frmConsistencia.dfsSerie, @Auto = :frmConsistencia.dfnNumero, @OrgaoAutuante = :frmConsistencia.dfnOrgaoAutuante,
								@Infracao = :frmConsistencia.tblInfra.colnCodigoCTB, @Permitido = :frmConsistencia.tblInfra.colnPermitido,
								@Aferido = :frmConsistencia.tblInfra.colnAferido, @Instrumento = :frmConsistencia.tblInfra.colnInstrum,
								@Acerto = "S", @GerarNotificacao = :frmConsistencia.dfsGerar,
								@ApreensaoCNH = :frmConsistencia.dfsApreensaoCNH,
								@Veiculo = :frmConsistencia.dfnVeiculo, @Placa = :frmConsistencia.dfsPlaca, @UFPlaca = :frmConsistencia.dfsUFPlaca,
								@GrupoMarcaVeiculo = :frmConsistencia.dfnGrupoMarca, @DataInfracao = :frmConsistencia.dInfracao,
								@TipoVeic=:frmConsistencia.nTipoVeic, @EspVeic=:frmConsistencia.nEspecieVeic, @CatVeic=:frmConsistencia.nCategoriaVeic,
								@GrupoApp = :frmConsistencia.dfnGrupoMarca, @UnidadeMedida = :frmConsistencia.tblInfra.colsUnidadeMedida,
								@Assinado = :frmConsistencia.dfsAssinado,
								@Considerada = :frmConsistencia.tblInfra.colnConsiderada,
								@Chassi	        = :frmConsistencia.dfsChassi'
						Set _sIntoInsere = ''
						!
						Set _sComandoAtualiza = 'exec dbinfracao..InfracaoAutoU
								@Serie = :frmConsistencia.dfsSerie, @Auto = :frmConsistencia.dfnNumero,
								@OrgaoAutuante = :frmConsistencia.dfnOrgaoAutuante,
								@Infracao = :frmConsistencia.tblInfra.colnCodigoCTB,
								@Permitido = :frmConsistencia.tblInfra.colnPermitido,
								@Aferido = :frmConsistencia.tblInfra.colnAferido,
								@Instrumento = :frmConsistencia.tblInfra.colnInstrum,
								@GerarNotificacao = :frmConsistencia.tblInfra.colsGerar,
								@Veiculo = :frmConsistencia.dfnVeiculo, @Situacao = :frmConsistencia.tblInfra.colsSit, @FoiPlacaAnt=:frmConsistencia.nFoiPlacaAnt,
								@TipoVeic=:frmConsistencia.nTipoVeic, @EspVeic=:frmConsistencia.nEspecieVeic, @CatVeic=:frmConsistencia.nCategoriaVeic,
								@UnidadeMedida = :frmConsistencia.tblInfra.colsUnidadeMedida,@Considerada = :frmConsistencia.tblInfra.colnConsiderada'
						Set _sIntoAtualiza = ''
						!
						Set _sComandoExclui = 'exec dbinfracao..InfracaoAutoD
								@Serie = :frmConsistencia.dfsSerie, @Auto = :frmConsistencia.dfnNumero,
								@OrgaoAutuante = :frmConsistencia.dfnOrgaoAutuante,
								@Infracao = :frmConsistencia.tblInfra.colnCodigoCTB'
						Set _sIntoExclui = ''
						!
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
						Set nMaxSeq = 0
					On SAM_FetchRowDone
						Set colsSit = colsSitI
						Call SalTblSetContext( hWndForm, lParam )
						If colsSit = 'A'
							Set colsSituacao = 'ABERTO'
						Else If colsSit = 'L'
							Set colsSituacao = 'LIQUIDADO'
						Else If colsSit = 'N'
							Set colsSituacao = 'NOTIFICADO'
						Else If colsSit = 'B'
							Set colsSituacao = 'BAIXADO'
						Else If colsSit = 'P'
							Set colsSituacao = 'PAGO'
						Else If colsSit = 'C'
							Set colsSituacao = 'CANCELADO'
						Else If colsSit = 'E'
							Set colsSituacao = 'SUSPENSO'
						Else If colsSit = 'I'
							Set colsSituacao = 'INCONSISTENTE'
						Else If colsSit = 'D'
							Set colsSituacao = 'EM TRAMITE'
						Set frmConsistencia.tblInfra.colnCodigo=  fInfracaoInseriDigito(frmConsistencia.tblInfra.colnCodigoCTB)
					On SAM_DropDown
						Call SalListClear( frmConsistencia.tblInfra.colsSituacao )
						If colsSitI = 'A' OR colsSitI = 'I'
							Call SalListAdd( frmConsistencia.tblInfra.colsSituacao, 'ABERTO')
							Call SalListAdd( frmConsistencia.tblInfra.colsSituacao, 'INCONSISTENTE')
						Else 
							If colsSitI = 'B'
								Call SalListAdd( frmConsistencia.tblInfra.colsSituacao, 'BAIXADO')
								Call SalListAdd( frmConsistencia.tblInfra.colsSituacao, 'ABERTO')
							Else If colsSitI = 'C'
								Call SalListAdd( frmConsistencia.tblInfra.colsSituacao, 'ABERTO')
								Call SalListAdd( frmConsistencia.tblInfra.colsSituacao, 'CANCELADO')
							Else If colsSitI = 'L'
								Call SalListAdd( frmConsistencia.tblInfra.colsSituacao, 'LIQUIDADO')
							Else If colsSitI = 'N'
								Call SalListAdd( frmConsistencia.tblInfra.colsSituacao, 'NOTIFICADO')
								Call SalListAdd( frmConsistencia.tblInfra.colsSituacao, 'ABERTO')
							Else If colsSitI = 'P'
								Call SalListAdd( frmConsistencia.tblInfra.colsSituacao, 'PAGO')
								Call SalListAdd( frmConsistencia.tblInfra.colsSituacao, 'ABERTO')
							Else 
								Call SalListAdd( frmConsistencia.tblInfra.colsSituacao, 'ABERTO')
								Set colsSitI = 'A'
						Call SalTblSetContext( hWndForm, lParam )
						Call SalListSetSelect( frmConsistencia.tblInfra.colsSituacao, SalListSelectString( frmConsistencia.tblInfra.colsSituacao , 0, colsSit ) )
					On MSG_NovaLinha
						If SalIsWindowEnabled( pbNovaLinha )
							Set nTmp = TBL_MaxRow
							If SalTblFindPrevRow( hWndForm, nTmp, 0, ROW_MarkDeleted )
								If nTmp + 1 >= nAutoNotificacao
									Call SalMessageBeep( MB_IconExclamation )
									Call SalMessageBox( 'Não é permitido inserir mais do que ' || SalNumberToStrX( nAutoNotificacao, 0) ||
											' infrações em um auto, este valor é definido nos parâmetros gerais do sistema', 'Atenção', MB_IconExclamation | MB_Ok )
									Return FALSE
							If nPermissao & H_INSERT
								Call SalSendMsg( hWndItem, MSG_MudaEstadoForm, EF_RegistroNovo, lParam )
								Set nTmp = SalTblInsertRow( hWndItem, TBL_MaxRow )
								Call SalTblSetFocusCell( hWndItem, nTmp, colnCodigo, 0, -1)
								Call SalListAdd( frmConsistencia.tblInfra.colsSituacao, 'ABERTO')
								Set colsSitI = 'A'
								Set colsSit = 'A'
								Call SalListSetSelect( frmConsistencia.tblInfra.colsSituacao, SalListSelectString( frmConsistencia.tblInfra.colsSituacao , 0, colsSit ) )
								Call SalSendMsg(hWndForm, MSG_MudaSemaforo, S_Vermelho, lParam)
							Set nMaxSeq = nMaxSeq + 1
							Set colnSequencia = nMaxSeq
					On MSG_RemoveLinha
						If SalIsWindowEnabled( pbNovaLinha )
							Call SalSendClassMessage( MSG_RemoveLinha, wParam, lParam )
					On UDM_AposPesquisa
						Call HabilitaAlteracao( )
						Return TRUE
					On MSG_Arquiva
						If bAlteraTabela
							If NOT SalSendClassMessage( MSG_Arquiva, wParam, lParam )
								Return FALSE
							Return AtualizaSituacaoInicial( )
						Else 
							Return TRUE
					On UDM_AposAtualizacao
						If fMPSPrepareProc( hSqlApp, 'exec dbinfracao..InfracaoAuto_Competencia @Serie = :frmConsistencia.dfsSerie,
								@OrgaoAutuante =:frmConsistencia.dfnOrgaoAutuante, @Auto =:frmConsistencia.dfnNumero', '' )
							Call SqlExecute( hSqlApp )
						Return TRUE
			!
