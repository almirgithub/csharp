							Call SalWaitCursor( TRUE )
							If fMPSPrepareProc( hSqlConsistencia, _sComandoAtualizacao, _sListaINTOAtualizacao )
								If SqlExecute( hSqlConsistencia )
									Set bRetorno = TRUE
									! car 22/08/96 - se houver uma lista into na atualizacao faca o fetch dos valores
									If SalStrTrimX(_sListaINTOAtualizacao) != ''
										If not SqlFetchNext ( hSqlConsistencia, nRetVal )
											Set bRetorno =  FALSE
							Call SalWaitCursor( FALSE )
						! Call SqlCommit(hSqlConsistencia)
						Call fMPSDisconnect(hSqlConsistencia)
					Return bRetorno
