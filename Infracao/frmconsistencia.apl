			Pushbutton: pbArquivar
				Class Child Ref Key: 0
				Class ChildKey: 0
				Class: cQCK1PushButton
				Property Template:
				Class DLL Name:
				Title: &Arquivar
				Window Location and Size
					Left: 6.938"
					Top: 5.313"
					Width:  Class Default
					Width Editable? Class Default
					Height: Class Default
					Height Editable? Class Default
				Visible? Class Default
				Keyboard Accelerator: Class Default
				Font Name: Class Default
				Font Size: Class Default
				Font Enhancement: Class Default
				Picture File Name:
				Picture Transparent Color: Class Default
				Image Style: Class Default
				Text Color: Class Default
				Background Color: Class Default
				Message Actions
					On SAM_Create
						Set _sNroCamposIgnorados = 'NENHUM'
						Set _sTextoStatusBar = 'Arquiva o registro corrente'
						Set _sTipoComando = 'Arquiva'
						Set _bValidaCamposNulos = TRUE
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						! If dfdAuto < dGlobalHoje - nGlobalPzDigAuto
							  Call SalMessageBeep( 0 )
							  Call SalMessageBox( 'Data Inferior ao limite previsto para implantação!', 'Atenção', MB_IconStop | MB_Ok )
							  Return FALSE
						!
						! Foi colocado esse teste para não permitir lançar dous autos com a situação ABERTO
						Set nLinha = TBL_MinRow
						Set nContaLinha = 0
						While SalTblFindNextRow( frmConsistencia.tblInfra, nLinha, 0, 0 )
							Call SalTblSetContext( frmConsistencia.tblInfra, nLinha )
							If SalStrUpperX( tblInfra.colsSituacao ) = "ABERTO"
								Set nContaLinha = nContaLinha + 1
						If nContaLinha > 1
							Call SalMessageBox( "Não é permitido cadastrar dois autos com situação ABERTO" ,"Atenção", MB_Ok | MB_IconInformation)
							Return FALSE
						!
						Call SalTblSetContext(tblInfra,0)
						Set nLinha = 0
						While SalTblFetchRow( tblInfra, nLinha ) != TBL_NoMoreRows
							If tblInfra.colnConsiderada = NUMBER_Null
								Set tblInfra.colnConsiderada = 0
							If tblInfra.colnBafDesc = STRING_Null
								Set tblInfra.colnBafDesc = 'N'
							If tblInfra.colnRecusa = STRING_Null
								Set tblInfra.colnRecusa = 'N'
							If tblInfra.colnAferido = NUMBER_Null
								Set tblInfra.colnAferido = 0
							If tblInfra.colnPermitido = NUMBER_Null
								Set tblInfra.colnPermitido =0
							If tblInfra.colnCodigo = 5169
								If (tblInfra.colnRecusa = "S"  or tblInfra.colnBafDesc = "S" ) and (tblInfra.colnAferido != 0 or tblInfra.colnPermitido != 0 or tblInfra.colnConsiderada != 0 )
									Call SalMessageBox( 'Infração de Alcoolemia(5169) COM Recusa ou Bafômetro descartável, Valores NÃO DEVEM ser informados!' ,"Atenção", MB_Ok )
									Return FALSE
								If (tblInfra.colnRecusa = "N"  and  tblInfra.colnBafDesc = "N" ) and (tblInfra.colnAferido = 0 or  tblInfra.colnConsiderada = 0 )
									Call SalMessageBox( 'Infração de Alcoolemia(5169) SEM Recusa e SEM Bafômetro Descartável,  Valores DEVEM ser informados!'   ,"Atenção", MB_Ok )
									Return FALSE
								! If (tblInfra.colnRecusa = "N"  and  tblInfra.colnBafDesc = "N") and (tblInfra.colnAferido = 0 or tblInfra.colnPermitido = 0 or tblInfra.colnConsiderada = 0 )
									Call SalMessageBox( 'Infração de Alcoolemia(5169) SEM Recusa e SEM Bafômetro Descartável,  Valores DEVEM ser informados!'   ,"Atenção", MB_Ok )
									Return FALSE
								If (tblInfra.colnRecusa = "S"  and  tblInfra.colnBafDesc = "S")
									Call SalMessageBox( "Indique apenas uma opção: HOUVE RECUSA ou usou o BAFÔMETRO FOI DESCARTÁVEL" ,"Atenção", MB_Ok )
									Return FALSE
								Call SalTblSetContext(tblMedicaoAlcoolemia,0)
								Set nLinhaAlcoolemia = 0
								If (tblInfra.colnAferido >300 or tblInfra.colnPermitido >300 or tblInfra.colnConsiderada >300 )
									Call SalMessageBox( 'Valores para Infração de Alcoolemia(5169), NÃO pode passar de 300'     ,"Atenção", MB_Ok )
									Return FALSE
								While SalTblFetchRow( tblMedicaoAlcoolemia, nLinhaAlcoolemia ) != TBL_NoMoreRows
									If ((tblInfra.colnAferido / 100)  >= tblMedicaoAlcoolemia.colnMedicaoInicial and (tblInfra.colnAferido / 100) <= tblMedicaoAlcoolemia.colnMedicaoFinal  )
										Set nValorConsiderado = tblInfra.colnConsiderada /100  
										Set nValorAferido =  (tblInfra.colnAferido / 100 ) - tblMedicaoAlcoolemia.colnErroMaximo
										! If nValorConsiderado != nValorAferido
											Call SalMessageBox( 'Infração de Alcoolemia(5169) a medição considerada está divergindo com a tabela  do INMETRO! Corrija pela tela de Consistencia!A medição considerada deveria ser: ' || SalNumberToStrX( ((tblInfra.colnAferido / 100) -
													tblMedicaoAlcoolemia.colnErroMaximo), 2) ,"Atenção", MB_Ok )
										If (tblInfra.colnConsiderada/100 != ((tblInfra.colnAferido / 100) - tblMedicaoAlcoolemia.colnErroMaximo))
											Call SalMessageBox( 'Infração de Alcoolemia(5169) a medição considerada está divergindo com a tabela  do INMETRO! Corrija pela tela de Consistencia!A medição considerada deveria ser: ' || SalNumberToStrX( ((tblInfra.colnAferido / 100) -
													tblMedicaoAlcoolemia.colnErroMaximo), 2) ,"Atenção", MB_Ok )
									Set nLinhaAlcoolemia = nLinhaAlcoolemia + 1
							Else
								! If (tblInfra.colnRecusa = "S"  or tblInfra.colnBafDesc = "S" )
									Call SalMessageBox( 'Infração  não é de Alcoolemia(5169) Recusa ou Bafômetro descartável NÃO DEVEM ser MARCADOS!' ,"Atenção", MB_Ok )
									Return FALSE
								If (tblInfra.colnAferido != 0 or tblInfra.colnPermitido != 0 or tblInfra.colnConsiderada != 0 )
									Call SalMessageBox( 'Infração  não é de Alcoolemia(5169) , Permitido, Aferido e Considerada NÃO DEVEM ser INFORMADOS!' ,"Atenção", MB_Ok )
									Return FALSE
							Set nLinha = nLinha + 1
						Call SalSendClassMessage( SAM_Click, wParam, lParam )
