								Set _bComponenteChave = TRUE
								!
								Set _bVisivel = TRUE
								Set _sNomeParaMsgs = 'Código infração'
								! Dados para a pesquisa de F2
								Set _sConsultaFrmOpcoesSQLOuSP = 'SP'
								Set _sConsultaFrmOpcoesCampo = 'exec InfracaoS_coddesc'
								Call SalSendClassMessage( SAM_Create, wParam, lParam )
							On SAM_User
								Set nTmp = TBL_MinRow
								Call SalTblFindNextRow( frmConsistencia.tblInfra, nTmp, ROW_New, 0 )
								Call SalTblSetFocusCell( frmConsistencia.tblInfra, nTmp, frmConsistencia.tblInfra.colnCodigo, 0, -1 )
							On SAM_Validate
								If NOT SalIsNull( hWndItem )
									If ProcuraNJaDigitado( hWndForm, SalTblQueryColumnID( hWndItem ), lParam, colnCodigo, 'Infração já digitada.' )
										Return VALIDATE_OkClearFlag
									! Adilson
									If NOT SalIsNull( hWndItem )
										Set nTmp = DigitoModulo11( SalNumberTruncate( MyValue/10, 10, 0 ))
										If nTmp != SalNumberMod( MyValue, 10 )
											Call SalMessageBeep( MB_IconExclamation )
											Call SalMessageBox( 'Infração não cadastrada', 'Atenção', MB_IconExclamation | MB_Ok )
											Call SalClearField( hWndItem )
											Return VALIDATE_Cancel
											! Call SalMessageBeep( MB_IconExclamation )
											! Call SalMessageBox( 'Dígito inválido', 'Atenção', MB_IconExclamation | MB_Ok )
											! Call SalClearField( hWndItem )
										! Retorno = FETCH_EOF
									! Else
										Return VALIDATE_OkClearFlag
									Set nInfracaoCTB = fInfracaoNovoCodigo(frmConsistencia.tblInfra.colnCodigo)
									Set frmConsistencia.tblInfra.colnCodigoCTB = nInfracaoCTB
									! Fim Adilson
									If fMPSPrepareProc( hSqlApp, 'exec InfracaoS_resumo @Cod = :frmConsistencia.tblInfra.colnCodigoCTB,
											@TipoVeiculo = :frmConsistencia.nTipoVeic, @EspecieVeiculo = :frmConsistencia.nEspecieVeic, @CategoriaVeiculo = :frmConsistencia.nCategoriaVeic',
