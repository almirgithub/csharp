		Message Actions
			! Observações
				  Autor: Roberto T. - 13/08/1999
			On SAM_CreateComplete
				Call SalFmtKeepMask( TRUE )
				! If sTipo = 'P'
					  Set _sStored = 'exec DebitoS_veiculo_relat @Veiculo = :frmPreAtendimento.dfnCodVeiculo,
							@Chassi = :frmPreAtendimento.dfsChassi, @Renavam = :frmPreAtendimento.dfnRenavam'
				! Else If sTipo = 'C'
					  Set _sStored = 'exec DebitoS_veiculo_relat @Veiculo = :frmConsultaVeiculo.tblResultado.colnCod'
				! Else
					  Set _sStored = 'exec DebitoS_veiculo_relat @Veiculo = :frmAtendimento.dfnCodVeiculo,
							@Chassi = :frmAtendimento.dfsChassi, @Renavam = :frmAtendimento.dfnRenavam'
				Set _sStored = 'exec RelRepasse_Resumo @Listar = :frmRelRepasse2.sListar'
				Set _sTituloRelatório = 'Resumo Relatório de Repasse'
				If sListar = 'T'
					Set _sPrimeiroRetorno =':frmRelRepasse2.sBancoD, :frmRelRepasse2.sTipoDebitoD,:frmRelRepasse2.sMunicipioD,
							:frmRelRepasse2.sOrgaoAutuanteD, :frmRelRepasse2.sOrgaoRecebedorD,
							:frmRelRepasse2.nRepasse'
					Set _sNomeQRP = 'repas_5r.qrp'
					Set _sVarSQLWindows = 'sGlobalSecretaria, sGlobalDetran, nGlobalSetor,
							sGlobalSetor, :frmRelRepasse2.dDataCorrente, SqlUser, :frmRelRepasse2.sBancoD, :frmRelRepasse2.sTipoDebitoD,
							:frmRelRepasse2.sMunicipioD, :frmRelRepasse2.sOrgaoAutuanteD,
							:frmRelRepasse2.sOrgaoRecebedorD, :frmRelRepasse2.nRepasse, :frmRelRepasse2.sBaixaManual,
							:frmRelRepasse2.sBaixaBancaria, :frmRelRepasse2.dVigencia'
					Set _sVarQRP = 'Secretaria, Detran, Setor, NomeSetor, DataCorrente, Usuario, Banco, TipoDebito,
							Municipio, OrgaoAutuante, OrgaoReceb, ValorTotal, Manual, Bancaria, Vigencia'
				Else
					Set _sPrimeiroRetorno =':frmRelRepasse2.sBancoD,:frmRelRepasse2.sTipoDebitoD,
							:frmRelRepasse2.sMunicipioD,:frmRelRepasse2.sOrgaoAutuanteD,
							:frmRelRepasse2.sOrgaoRecebedorD, :frmRelRepasse2.nRepasse'
					Set _sNomeQRP = 'repas_6r.qrp'
					Set _sVarSQLWindows = 'sGlobalSecretaria, sGlobalDetran, nGlobalSetor,
							sGlobalSetor, :frmRelRepasse2.dDataCorrente, SqlUser, :frmRelRepasse2.sBancoD,
							:frmRelRepasse2.sTipoDebitoD, :frmRelRepasse2.sMunicipioD, :frmRelRepasse2.sOrgaoAutuanteD,
							:frmRelRepasse2.sOrgaoRecebedorD, :frmRelRepasse2.nRepasse, :frmRelRepasse2.sBaixaManual,
							:frmRelRepasse2.sBaixaBancaria, :frmRelRepasse2.dVigencia'
					Set _sVarQRP = 'Secretaria, Detran, Setor, NomeSetor, DataCorrente, Usuario, Banco, TipoDebito,
							Municipio, OrgaoAutuante, OrgaoReceb, ValorTotal, Manual, Bancaria, Vigencia'
				Call SalSendMsg( pbImprimir, SAM_Click, nParCopias, 0)
			On SAM_Destroy
				Set hWndJanela = hWndNULL
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
			On SAM_ReportStart
				Call SqlImmediate( 'select getdate() into :dDataCorrente' )
				Call SalSendClassMessage( SAM_ReportStart, wParam, lParam)
			! On UDM_AposPesquisa
				! If sTipoPessoa = 'J'
					  Call SalFmtSetInputMask ( dfsCpfCgc, '99.999.999/9999-99' )
				! Else If sTipoPessoa = 'F'
					  Call SalFmtSetInputMask ( dfsCpfCgc, '999.999.999-99' )
				! If colnDePar = 99
					   Set colsDePar = 'ÚNICA'
				! Else
					   Set nDParcela = SalNumberToStrX( nDParcela, 0 )
				  If NOT ( nDDebAnt = nDDebito and nDParcela = 99 and nDParcAnt != 99 and nDExeAnt = nDExe )
					  Set nTotReal = nTotReal + nDReal
					  Set nTotMoeda = nTotMoeda + nDMoeda
				  If nDParcela = 99
					  Set nTotRealUnica = nTotRealUnica + nDReal
					  Set nTotMoedaUnica = nTotMoedaUnica + nDMoeda
				  Set nDDebAnt = nDDebito
				  Set nDExeAnt = nDExe
				  Set nDParcAnt = nDParcela
				  Return TRUE
			! On SAM_ReportFetchInit
				  Set nTotReal = 0
				  Set nTotMoeda = 0
				  Set nTotRealUnica = 0
				  Set nTotMoedaUnica = 0
				  Call SalSendClassMessage( SAM_ReportFetchInit, wParam, lParam )
			On SAM_ReportFinish
				Call SqlPrepareAndExecute( hSql, 'cancel' )
				Call SalSendClassMessage( SAM_ReportFinish, wParam, lParam)
