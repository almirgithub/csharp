		Message Actions
			On SAM_Create
				Call SalWaitCursor( TRUE )
				Call SalCenterWindow( hWndForm )
				Set _sTextoStatusBar = 'Restituição de débitos'
				Set _bLimpaTelaAposInclusao = TRUE
				Set _bLimpaTelaAposAtualizacao = TRUE
				Set _sNroCamposIgnorados = 'NENHUM'
				Set _nEstadoInicialForm = EF_Vazio
				! Set _sCriacaoConexao = 'FALSE'
				Set _sCriacaoConexao = 'Default'
				Set sSiglaUser = SqlUser
				Set _bCriacaoConexaoAuxiliar = TRUE
				!
				!
				If hWndObjeto != hWndNULL
					Call SalDisableWindow( hWndObjeto )
				!
				Set _sComandoPesquisa = '
						exec Prot..RestituicaoS 	@Prot = :frmRestituicaoDebito.dfProcesso, 	@Serie = :frmRestituicaoDebito.dfSerie ,
									@Auto = :frmRestituicaoDebito.dfAuto , 	@Infracao = :frmRestituicaoDebito.dfnInfracao,
									@OrgaoAutuante=:frmRestituicaoDebito.dfnOrgaoAutuante,@Desdobramento = :frmRestituicaoDebito.dfnDesdobramento'
				!
				Set _sComandoAtualizacao = '
						exec Prot..RestituicaoU 	@Prot =  :frmRestituicaoDebito.dfProcesso ,	@Serie = :frmRestituicaoDebito.dfSerie,
									@Auto =  :frmRestituicaoDebito.dfAuto, 		@Infracao = :frmRestituicaoDebito.dfnInfracao,
									@Valor = :frmRestituicaoDebito.dfValorRest,	@Moeda = :frmRestituicaoDebito.dfMoeda,
									@User =  :frmRestituicaoDebito.sSiglaUser,
									@OrgaoAutuante = :frmRestituicaoDebito.dfnOrgaoAutuante,
									@Desdobramento = :frmRestituicaoDebito.dfnDesdobramento'
				!
				Set _sComandoInclusao = '
						exec Prot..RestituicaoI 	@Prot =  :frmRestituicaoDebito.dfProcesso , 	@Serie = :frmRestituicaoDebito.dfSerie,
									@Auto =  :frmRestituicaoDebito.dfAuto,	 	@Infracao = :frmRestituicaoDebito.dfnInfracao,
									@Valor = :frmRestituicaoDebito.dfValorRest, 	@Moeda = :frmRestituicaoDebito.dfMoeda,
									@User =  :frmRestituicaoDebito.sSiglaUser , 	@PagtoExercicio = :frmRestituicaoDebito.dfExercicio,
									@OrgaoAutuante = :frmRestituicaoDebito.dfnOrgaoAutuante,
									@Desdobramento = :frmRestituicaoDebito.dfnDesdobramento'
				!
				Set sComandoProc2 = '
						exec Prot..RestituicaoS_NovoValor  	@Prot = :frmRestituicaoDebito.dfProcesso, 	@Serie = :frmRestituicaoDebito.dfSerie ,
										@Auto = :frmRestituicaoDebito.dfAuto , 	@Infracao = :frmRestituicaoDebito.dfnInfracao,
										@OrgaoAutuante = :frmRestituicaoDebito.dfnOrgaoAutuante,
										@Desdobramento = :frmRestituicaoDebito.dfnDesdobramento'
				!
				Set _sListaINTOPesquisa = '
									:frmRestituicaoDebito.dfPlaca, :frmRestituicaoDebito.dfChassi, :frmRestituicaoDebito.dfRenavam,
									:frmRestituicaoDebito.dfnOrgaoAutuante, :frmRestituicaoDebito.dfsOrgaoAutuante,
									:frmRestituicaoDebito.dfSerie, :frmRestituicaoDebito.dfAuto, :frmRestituicaoDebito.dfnInfracao,
									:frmRestituicaoDebito.dfnDesdobramento,
									:frmRestituicaoDebito.dfValorPag, :frmRestituicaoDebito.dfExercicio, :frmRestituicaoDebito.dfMoeda'
				!
				Set _sListaINTOAtualizacao = ''
				!
				Set _sListaINTOInclusao = ''
				!
				Set sIntoProc2 = '		:frmRestituicaoDebito.dfValorPag, :frmRestituicaoDebito.dfExercicio, :frmRestituicaoDebito.dfMoeda'
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				Set nSqlRestituicao = SalSendMsg( hWndForm, MSG_ObtemSqlHandleAux, 0, 0 )
				Set hSqlRestituicao = SalNumberToWindowHandle( nSqlRestituicao )
				Set sSetorUsuarioN = SalStrMidX(SalNumberToStrX(nGlobalSetor,0),0,SalStrLength( SalNumberToStrX(nGlobalSetor,0) )-1)
				Set nSetorUsuario = SalStrToNumber(sSetorUsuarioN)
				If nProcesso != NUMBER_Null
					Set dfProcesso = nProcesso
					Set dfSerie = sSerie
					Set dfAuto = nAuto
					Set dfnInfracao = nInfracao
					Set dfnOrgaoAutuante=nOrgaoAutuante
					Call SalPostMsg( pbPesquisar, SAM_Click, wParam, lParam )
				Call SalDisableWindow( mdipbRestituicao )
				Call SalWaitCursor( FALSE )
			On SAM_Destroy
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
				If hWndObjeto != hWndNULL
					Call SalEnableWindow( hWndObjeto )
					Call SalPostMsg( hWndObjeto, SAM_User, 0, 0 )
				Call SalEnableWindow( mdipbRestituicao )
			!
			On MSG_Anterior
				Call SalSendClassMessage( MSG_Anterior, wParam, lParam )
				Call SalSendMsg( hWndForm, UDM_AposPesquisa, 0, 0 )
			On MSG_Proximo
				Call SalSendClassMessage( MSG_Proximo, wParam, lParam )
				Call SalSendMsg( hWndForm, UDM_AposPesquisa, 0, 0 )
			!
			On UDM_AposPesquisa
				Call SalWaitCursor( TRUE )
				Call SqlFetchNext( hSqlApp, nFetch )
				If SalIsNull( frmRestituicaoDebito.dfValorPag )
						AND SalIsNull( frmRestituicaoDebito.dfExercicio )
						AND SalIsNull( frmRestituicaoDebito.dfMoeda )
					Set nTipoEvento = EF_RegistroNovo
					If fMPSPrepareProc( hSqlRestituicao, sComandoProc2, sIntoProc2 )
						If SqlExecute( hSqlRestituicao )
							Call SqlFetchNext( hSqlRestituicao, nFetch )
				Else
					Set nTipoEvento = EF_RegistroExistente
					Set dfValorRest = dfValorPag
				Call SalWaitCursor( FALSE )
				If SalIsNull( frmRestituicaoDebito.dfValorPag )
						AND SalIsNull( frmRestituicaoDebito.dfExercicio )
					Return FALSE
				Else
					Call SalWaitCursor( TRUE )
					If fMPSPrepareProc( hSqlApp, 'Exec dbvcen..MoedaS_nome @Moeda = :dfMoeda' , ':cmbMoeda')
						If SqlExecute( hSqlApp )
							Call SqlFetchNext( hSqlApp, nFetch )
							Call SalEnableWindow( dfValorRest )
							Call SalEnableWindow( cmbMoeda )
					Call SalWaitCursor( FALSE )
				Return TRUE




