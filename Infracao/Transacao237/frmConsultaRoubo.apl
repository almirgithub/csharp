		Message Actions
			On SAM_Create
				Set _sTextoStatusBar = 'Tela de consuta de roubo de veículo'
				Set _sNroCamposIgnorados = 'NENHUM'
				!
				Set _sCriacaoConexao = 'TRUE'
				!
				Set _nEstadoInicialForm = EF_Vazio
				Set _sComandoPesquisa = 'exec RouboS_consulta @Placa = :frmConsultaRoubo.dfsPlaca,
						@UF = :frmConsultaRoubo.dfsCodUf, @Renavam = :frmConsultaRoubo.dfnRenavam,
						@Chassi = :frmConsultaRoubo.dfsChassi, @VeiculoBin = :frmConsultaRoubo.bVeiculoBin,
						@CodConsulta = :frmConsultaRoubo.nCodConsultaBin, @NumSeqTransacao = :frmConsultaRoubo.nNumSeqTransacaoBin,
						@Local = :frmConsultaRoubo.nLocal'
				Set _sListaINTOAtualizacao = ''
				Set _sListaINTOPesquisa = ':frmConsultaRoubo.dfnRoubo, :frmConsultaRoubo.dfsSituacao, :frmConsultaRoubo.dfnCor,
						 :frmConsultaRoubo.dfsCor, :frmConsultaRoubo.dfnCombustivel, :frmConsultaRoubo.dfsCombustivel,
						 :frmConsultaRoubo.nMarca, :frmConsultaRoubo.dfnMarcaModelo, :frmConsultaRoubo.dfsMarcaModelo,
						 :frmConsultaRoubo.dfnCategoria,
						 :frmConsultaRoubo.dfsCategoria, :frmConsultaRoubo.dfsChassi, :frmConsultaRoubo.nMunicipioEmpl,
						 :frmConsultaRoubo.dfnMunicipioEmpl, :frmConsultaRoubo.dfsMunicipioEmpl, :frmConsultaRoubo.dfsCodUf,
						 :frmConsultaRoubo.dfnAnoFabricacao, :frmConsultaRoubo.dfnAnoModelo, :frmConsultaRoubo.dfsNumMotor,
						 :frmConsultaRoubo.dfsFabricacao, :frmConsultaRoubo.dfnCilindrada, :frmConsultaRoubo.dfnRenavam,
						 :frmConsultaRoubo.dfsCRV, :frmConsultaRoubo.dfsCodDuplicado, :frmConsultaRoubo.dfsCodRemarcado,
						:frmConsultaRoubo.dfsPlaca'
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call HabilitacaoNome( 'mdipbConsRoubo', FALSE )
				Call SalCenterWindow( hWndForm )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				Call SalWaitCursor( FALSE )
				Set dfsPlaca = sPlaca
				Set dfsCodUf = sUFPlaca
				Set dfsChassi = sChassi
				If sPlaca = '' AND sChassi = ''
					Return TRUE
				! Verifica se a pesquisa vai ser pela BIN ou Local
				Call SalSendMsg( cmbUFPlaca, MSG_FetchRefreshDerivados, 0, 0 )
				If frmConsultaVeiculo.rbBin
					Set rbBin = TRUE
				Call SalPostMsg( hWndForm, MSG_Pesquisa, 0, 0 )
				Call SalPostMsg( hWndForm, SAM_User, 0, 0 )
			On MSG_Limpa
				! Call SWTabsBringToTop( picTabs, 0, TRUE )
				If hWndDetalheRoubo != hWndNULL
					Call SalSendMsg( hWndDetalheRoubo, MSG_Sai, 0, 0 )
				Call SalSendClassMessage( MSG_Limpa, wParam, lParam )
				Call SalSetFocus( dfsPlaca )
				Set nCodConsultaBin = NUMBER_Null
				Set nNumSeqTransacaoBin = NUMBER_Null
			On SAM_User
				Call SalDisableWindow( pbLimpar )
				! Call SalDisableWindow( pbPesquisar )
			On UDM_AntesPesquisa
				If SalIsNull( dfsCodUf ) and rbLocal
					Set dfsCodUf = sGlobalUF
					Call SalSendMsg( cmbUFPlaca, MSG_FetchRefreshDerivados, 0, 0 )
				Set sPlaca2 = dfsPlaca
				If rbBin
					Set bVeiculoBin = BINConsultaVeiculo( dfsPlaca, dfsCodUf, NUMBER_Null, dfsChassi, '', '', 'O', nCodConsultaBin,
							nNumSeqTransacaoBin )
					Set nLocal = 0
					If NOT bVeiculoBin
						Return FALSE
					If NOT fMPSPrepareProc( hSql, 'exec ' || DATABASE_RENAVAM || '..RetornoS_status @CodConsulta = :nCodConsultaBin,
							@NumSeqTransacao = :nNumSeqTransacaoBin', ':sTipoRetornoBin, :nRetornoBin, :nRetornoCom' )
						Call fMPSDisconnect( hSql )
						Return FALSE
					If NOT SqlExecute( hSql )
						Call fMPSDisconnect( hSql )
						Return FALSE
					Call SqlFetchNext( hSql, nRetorno )
				Else
					Set nLocal = 1
				If SalIsNull( cmbUFPlaca ) AND rbLocal
					Set cmbUFPlaca = 'PE'
					Call SalSendMsg( cmbUFPlaca, SAM_Click, wParam, lParam )
				Return TRUE
			On UDM_AposPesquisa
				Call SalSendMsg( tblRoubo, SAM_User, 0, 0 )
				Call SalSendMsg( pbPesquisar, MSG_Habilita1, 0, 0 )
				! Call SWTabsBringToTop( picTabs, 1, TRUE )
				Call SalDisableWindow( rbBin )
				Call SalDisableWindow( rbLocal )
				Call SalDisableWindow( pbPesquisar )
				If rbBin
					Call SalDisableWindow( pbAnterior )
					Call SalDisableWindow( pbProximo )
					! Call SalDisableWindow( pbImprimir )
				Return SalSendClassMessage( UDM_AposPesquisa, wParam, lParam )
			On MSG_Anterior
				Call SalSendClassMessage( MSG_Anterior, wParam, lParam )
				Call SalSendMsg( tblRoubo, SAM_User, 0, 0 )
			On MSG_Proximo
				Call SalSendClassMessage( MSG_Proximo, wParam, lParam )
				Call SalSendMsg( tblRoubo, SAM_User, 0, 0 )
			On SAM_Destroy
				Set hWndJanela = hWndNULL
				If hWndDetalheRoubo != hWndNULL
					Call SalSendMsg( hWndDetalheRoubo, MSG_Sai, 0, 0 )
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
				Call HabilitacaoNome( 'mdipbConsRoubo', TRUE )
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo',
						'Relatório de Movimentação de Roubos', 'Relatório de Movimentação de Roubos' )
				Return TRUE
			On SAM_ReportFetchInit
				If rbBin
					If SqlConnect( hSql2 )
						Call fMPSPrepareProc( hSql2, 'exec MovimentoRouboS_rel_BS
								@CodConsulta = :nCodConsultaBin, @NumSeqTransacao = :nNumSeqTransacaoBin',
								':frmConsultaRoubo.nTipoMovimento, :frmConsultaRoubo.sTipoMovimento,
								:frmConsultaRoubo.sPlaca1, :frmConsultaRoubo.sUFPlaca1, :frmConsultaRoubo.sChassi,
								:frmConsultaRoubo.sUFOcorrencia, :frmConsultaRoubo.nAnoBO,  :frmConsultaRoubo.nNroBO' )
						Return SqlExecute( hSql2 )
				Else
					If SqlConnect( hSql2 )
						Call fMPSPrepareProc( hSql2, 'exec MovimentoRouboS_rel_LA @Roubo = :frmConsultaRoubo.dfnRoubo, @Placa = :frmConsultaRoubo.dfsPlaca,
								@UFPlaca = :frmConsultaRoubo.cmbUFPlaca, @Renavam = :frmConsultaRoubo.dfnRenavam, @Chassi = :frmConsultaRoubo.dfsChassi',
								':frmConsultaRoubo.nRoubo, :frmConsultaRoubo.sSituacao, :frmConsultaRoubo.nTipoMovimento, :frmConsultaRoubo.sTipoMovimento,
								:frmConsultaRoubo.dMovimento, :frmConsultaRoubo.sPlaca1, :frmConsultaRoubo.sUFPlaca1, :frmConsultaRoubo.sChassi,
								:frmConsultaRoubo.nAnoBO,  :frmConsultaRoubo.dDataBO, :frmConsultaRoubo.nNroBO, :frmConsultaRoubo.sDelegacia,
								:frmConsultaRoubo.sLocal, :frmConsultaRoubo.sNomeInformante, :frmConsultaRoubo.sMunicipio,
								:frmConsultaRoubo.sUFMunicipio, :frmConsultaRoubo.sObservacao' )
						Return SqlExecute( hSql2 )
			On SAM_ReportFetchNext
				Call SqlFetchNext( hSql2, nErro )
				If nErro != FETCH_EOF
					Return TRUE
				Else
					Return FALSE
			On SAM_ReportFinish
				Call SqlPrepareAndExecute( hSql2, 'cancel' )
				Call SalPrtSetDefault(sDeviceDef,sDriverDef,sPortaDef)
				Call SqlDisconnect( hSql2 )
