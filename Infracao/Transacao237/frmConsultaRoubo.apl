							Call SWTabsEnable( Roubo, 1, FALSE )
							Call SWTabsBringToTop( Roubo, 0, TRUE )
							Call SalSetFocus( dfsPlaca )
						Else
							Call SWTabsEnable( Roubo, 1,TRUE )
							Return FALSE
					On UDM_ValidaCampo
						If NOT SalIsNull( hWndItem )
							If NOT PlacaValida( dfsPlaca )
								Return FALSE
						Return TRUE
