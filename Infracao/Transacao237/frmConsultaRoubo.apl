						Call SalWaitCursor ( TRUE )
						! Popula a tabela
						Set hSqlErro = hSqlApp
						Call fMPSPrepareProc ( hSqlApp, 'exec MovimentoRouboS_roubo @Roubo = :frmConsultaRoubo.dfnRoubo,
								 @VeiculoBin = :frmConsultaRoubo.bVeiculoBin, @CodConsulta = :frmConsultaRoubo.nCodConsultaBin,
								@NumSeqTransacao = :frmConsultaRoubo.nNumSeqTransacaoBin, @Local = :frmConsultaRoubo.nLocal',
								':frmConsultaRoubo.tblRoubo.coldData, :frmConsultaRoubo.tblRoubo.colsTipoMov,
								:frmConsultaRoubo.tblRoubo.colsLocal, :frmConsultaRoubo.tblRoubo.colsNome,
