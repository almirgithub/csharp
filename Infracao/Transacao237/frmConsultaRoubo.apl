					On SAM_Click
						If SalIsNull( dfsPlaca ) AND SalIsNull( dfnRenavam ) AND SalIsNull( dfsChassi )
							Call SalMessageBeep( MB_IconExclamation )
							Call SalMessageBox( 'É necessário fazer a pesquisa antes', 'Atenção', MB_IconExclamation | MB_Ok )
							Return FALSE
						If hWndReport != hWndNULL
							Call SalSendMsg( hWndReport, SAM_Close, 0,0)
						If rbBin
							Set hWndReport = SalCreateWindow( frmReportView, hWndForm )
							Call SalSetWindowText( hWndReport, 'Relatório de Movimentação de Roubos' )
							Call SalReportView( hWndForm, hWndReport, 'Roubo_BS.qrp', 'sGlobalSecretaria, sGlobalDetran, nGlobalSetor, sGlobalSetor, SqlUser,
