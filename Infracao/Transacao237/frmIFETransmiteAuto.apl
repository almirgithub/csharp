										tblAutos.colnInfracao,tblAutos.colnTransacao,'EV01',hSqlApp,'','','','',7,2,'3',tblAutos.colnDesdobramento)
							Else If tblAutos.colsTipoMovimento = 'M'
								Call fTransmitePendINFRAEST(tblAutos.colsSerie,tblAutos.colnOrgaoAutuante,
										tblAutos.colnAuto,
										tblAutos.colnInfracao,tblAutos.colnTransacao,'EV01',hSqlApp,'','','','',8,1,'3',tblAutos.colnDesdobramento)
							Else If tblAutos.colsTipoMovimento = 'S'
								Call fTransmitePendINFRAEST(tblAutos.colsSerie,tblAutos.colnOrgaoAutuante,
										tblAutos.colnAuto,
										tblAutos.colnInfracao,tblAutos.colnTransacao,'EV01',hSqlApp,'','','','',6,1,'3',tblAutos.colnDesdobramento)
							Else If tblAutos.colsTipoMovimento = 'T'
								Call fTransmitePendINFRAEST(tblAutos.colsSerie,tblAutos.colnOrgaoAutuante,
										tblAutos.colnAuto,
										tblAutos.colnInfracao,tblAutos.colnTransacao,'EV01',hSqlApp,'','','','',8,9,'3',tblAutos.colnDesdobramento)
							Else If tblAutos.colsTipoMovimento = 'Y'
								Call fTransmitePendINFRAEST(tblAutos.colsSerie,tblAutos.colnOrgaoAutuante,
										tblAutos.colnAuto,
										tblAutos.colnInfracao,tblAutos.colnTransacao,'EV01',hSqlApp,'','','','',8,9,'4',tblAutos.colnDesdobramento)
							Else
								Call fTransmitePendINFRAEST(tblAutos.colsSerie,tblAutos.colnOrgaoAutuante,
										tblAutos.colnAuto,
										tblAutos.colnInfracao,tblAutos.colnTransacao,'EV01',hSqlApp,'','','','',7,1,'3',tblAutos.colnDesdobramento)
							Set nRow = nRow + 1
					Call SalSendClassMessage( MSG_Limpa, wParam, lParam )
					If sOperacao	=	'R'
						Set rbInclusao = TRUE
					Else
						Set rbAlteracao = TRUE
					Call SalSendMsg( pbPesquisa, SAM_Click, 0 ,0)
					! Call SalModalDialog( dlgEnvioTransacao, hWndNULL )
					Return TRUE
