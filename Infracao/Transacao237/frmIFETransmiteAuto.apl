			Function: TransmiteTransacao
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					Number: nRow
				Actions
					! começa a verificação do Arquivo...
					Set nRow = 0
					Set siOperacao = sOperacao
					Call SalTblSetContext( tblAutos, nRow )
					If SalTblAnyRows( tblAutos, ROW_Edited, 0 )
						Loop
							If SalTblFindNextRow( tblAutos, nRow, ROW_Edited, 0)  = FALSE
								Break
							! Call SalTblSetRowFlags( tblAutos, ROW_Edited, nRow, TRUE )
							Call SalTblSetContext( tblAutos, nRow )
							If tblAutos.colsTipoMovimento = 'E' 
								Call fTransmitePendINFRAEST(tblAutos.colsSerie,tblAutos.colnOrgaoAutuante,
										tblAutos.colnAuto,
										tblAutos.colnInfracao,tblAutos.colnTransacao,'EV01',hSqlApp,'','','','',7,2,'3',tblAutos.colnDesdobramento)
							Else If tblAutos.colsTipoMovimento = 'M'
								Call fTransmitePendINFRAEST(tblAutos.colsSerie,tblAutos.colnOrgaoAutuante,
										tblAutos.colnAuto,
										tblAutos.colnInfracao,tblAutos.colnTransacao,'EV01',hSqlApp,'','','','',8,1,'3',tblAutos.colnDesdobramento)
							Else If tblAutos.colsTipoMovimento = 'S'
