				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Data de infração que indica o início do intervalo'
						Set _sNomeParaMsgs = 'Data de infração inicial'
						Set _sTipoMsgNulo = 'PADRAO'
						Set _bVisivel = TRUE
						! Variáveis para indentificar o Componente Chave
						Set _bComponenteChave = FALSE
						Set _bCompChaveGeradoPeloSistema = FALSE
						Set _bCompChaveOpcionalParaPesquisa = TRUE
						Set _bDisparaConsultaEmValidate = FALSE
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On MSG_Limpa
						Return TRUE
					On SAM_Validate
						If NOT SalIsNull( hWndItem)
							If NOT (SalIsValidDateTime( hWndItem ))
								Call SalMessageBeep( 0 )
								Call SalMessageBox( 'Data inválida', 'Atenção', MB_Ok | MB_IconExclamation )
								Call SalClearField( hWndItem )
								Return VALIDATE_Cancel
							Else If (SalDateYear( dfdInicial ) < 1753)
								Call SalMessageBeep( 0 )
								Call SalMessageBox( 'Ano inválido', 'Atenção', MB_Ok | MB_IconExclamation )
								Call SalClearField( hWndItem )
								Return VALIDATE_Cancel
							If NOT SalIsNull( dfdFinal )
								If dfdFinal < dfdInicial
									Call SalMessageBeep( 0 )
									Call SalMessageBox( 'Data inicial deve ser menor do que a data final', 'Atenção', MB_IconExclamation | MB_Ok )
								If dfdFinal - dfdInicial > 7
									Call SalMessageBeep( 0 )
									Call SalMessageBox( 'Não é aconselhável o intervalo maior do que 7 (sete) dias', 'Atenção', MB_IconExclamation | MB_Ok )
						Return VALIDATE_OkClearFlag
