				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Selecione os órgãos autuantes dos autos a serem selecionados'
						Set _bPesquisaCreate = FALSE
						Set _bNuncaHabilitado = TRUE
						Set _bSempreHabilitado = FALSE
						Set _sCriacaoConexao = 'FALSE'
						Set _sComandoPesquisa = 'exec dbinfracao..RNFEnvioPreNotifPenalidadeS'
						! coluna para o retorno
						Set _sIntoPesquisa = ':frmRNFPreNotificacaoPen.tblAutos.colnOrgaoAutuante,
								:frmRNFPreNotificacaoPen.tblAutos.colsNumAutoInfracao,
								:frmRNFPreNotificacaoPen.tblAutos.colnInfracao,
								:frmRNFPreNotificacaoPen.tblAutos.colnCodigoRENAINF,
								:frmRNFPreNotificacaoPen.tblAutos.coldDataLimiteDefesa,
								:frmRNFPreNotificacaoPen.tblAutos.colsSerie,
								:frmRNFPreNotificacaoPen.tblAutos.colnAuto,
								:frmRNFPreNotificacaoPen.tblAutos.colsTipoMovimento,:frmRNFPreNotificacaoPen.tblAutos.colnTransacao,
								:frmRNFPreNotificacaoPen.tblAutos.colsMensagem,
								:frmRNFPreNotificacaoPen.tblAutos.colsDescMovimento,
								:frmRNFPreNotificacaoPen.tblAutos.coldDataMovimento,
								:frmRNFPreNotificacaoPen.tblAutos.colsPlaca,
								:frmRNFPreNotificacaoPen.tblAutos.colnCodRetorno'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_FetchRowDone
						Set dfnQtde = dfnQtde + 1
					! On SAM_FetchRowDone
						Call SalTblSetContext( hWndForm, lParam )
						If colsTipoMovimento = 'D'
							Set colnTransacao = 412
						Else If colsTipoMovimento = 'N'
							Set colnTransacao =413
					On SAM_RowHeaderClick
						Call SalTblSetRowFlags( tblAutos, SalTblQueryContext( tblAutos ), ROW_Edited, TRUE )
					On SAM_RowHeaderDoubleClick
						Call SalTblSetRowFlags( tblAutos, SalTblQueryContext( tblAutos ), ROW_Edited, FALSE )
					On SAM_ReportNotify
						Set hWndReport = SalNumberToWindowHandle( wParam )
						If lParam = RPT_BeforePageHeader
							Call SalReportSetStringVar( hWndReport, 'Usuario', SqlUser )
							Call SalReportSetNumberVar( hWndReport, 'nSetor',nGlobalSetor)
							Call SalReportSetStringVar( hWndReport, 'sSetor',sGlobalSetor)
