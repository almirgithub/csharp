			Number: nTipoDoc
			Number: nTipoDocto
			String: sDescTipoDocto
			String: sOrgao
			String: sNumeroDocProp
		Message Actions
			On SAM_CreateComplete
				Call SqlImmediate( 'select getdate() into :dDataImp' )
				Call SalFmtKeepMask( TRUE )
				Set _sStored = 'exec ProprietarioS_consveic_compl @Veiculo = :frmRelPropVeiculo.nVeiculo'
				Set _sNomeQRP = 'propvei.QRP'
				Set _sTituloRelatório = 'Características do Proprietário'
				Set _sPrimeiroRetorno = ':frmRelPropVeiculo.dfsDoc, 
						                                       :frmRelPropVeiculo.nCNH,
						                                       :frmRelPropVeiculo.sDescricao, 
						                                       :frmRelPropVeiculo.nNumRG, 
						                                       :frmRelPropVeiculo.sOrgaoExp,
						                                       :frmRelPropVeiculo.sUF, 
						                                       :frmRelPropVeiculo.sLogradouro, 
						                                       :frmRelPropVeiculo.sComplemento,
						                                       :frmRelPropVeiculo.nCEP, 
						                                       :frmRelPropVeiculo.sUFLog, 
						                                       :frmRelPropVeiculo.nMunicipio,
						                                       :frmRelPropVeiculo.sMunicipio, 
						                                       :frmRelPropVeiculo.nBairro, 
						                                       :frmRelPropVeiculo.sBairro,
						                                       :frmRelPropVeiculo.dfsTelefone, 
						                                       :frmRelPropVeiculo.dfsFAX, 
						                                       :frmRelPropVeiculo.sEmail,
						                                       :frmRelPropVeiculo.nTipoDoc,
						                                       :frmRelPropVeiculo.nTipoDocto,
						                                       :frmRelPropVeiculo.sDescTipoDocto,
						                                       :frmRelPropVeiculo.sOrgao,
						                                       :frmRelPropVeiculo.sNumeroDocProp'
						
				Set _sVarSQLWindows = 'sGlobalSecretaria,
						                                          sGlobalDetran,
						                                          nGlobalSetor, 
						                                           sGlobalSetor, 
						                                           frmRelPropVeiculo.dDataImp, 
						                                          SqlUser,
						                                          frmConsultaVeiculo.tblResultado.colsChassi, 
						                                          frmConsultaVeiculo.tblResultado.colnRenavam,
						                                          frmConsultaVeiculo.tblResultado.colsPlaca, 
						                                          frmRelPropVeiculo.dfsDoc, 
						                                          frmRelPropVeiculo.nCNH,
						                                          frmRelPropVeiculo.sDescricao, 
						                                          frmRelPropVeiculo.nNumRG, 
						                                          frmRelPropVeiculo.sOrgaoExp,
						                                          frmRelPropVeiculo.sUF, 
						                                          frmRelPropVeiculo.sLogradouro, 
						                                          frmRelPropVeiculo.sComplemento,
						                                          frmRelPropVeiculo.nCEP, 
						                                          frmRelPropVeiculo.sUFLog, 
						                                          frmRelPropVeiculo.nMunicipio,
						                                          frmRelPropVeiculo.sMunicipio, 
						                                          frmRelPropVeiculo.nBairro, frmRelPropVeiculo.sBairro,
						                                          frmRelPropVeiculo.dfsTelefone, 
						                                          frmRelPropVeiculo.dfsFAX, 
						                                          frmRelPropVeiculo.sEmail,
						                                          frmRelPropVeiculo.nTipoDoc,
						                                          frmRelPropVeiculo.sDescTipoDocto,
						                                          frmRelPropVeiculo.sOrgao,
						                                          frmRelPropVeiculo.sNumeroDocProp'
						
				Set _sVarQRP = 'Secretaria,
						                            Detran,
						                            Setor, 
						                            NomeSetor, 
						                            DataImp, 
						                            Usuario, 
						                            Chassi, 
						                            Renavam, 
						                            Placa, 
						                            Doc, 
						                            CNH,
						                            Descricao, 
						                           NumRG, 
						                           OrgaoExp, 
						                           UF, 
						                           Logradouro, 
						                           Complemento, 
						                           CEP, 
						                           UFLog, 
						                          Municipio,
						                          MunicipioDesc, 
						                          Bairro, 
						                          BairroDesc, 
						                          Telefone, 
						                          FAX, 
						                          Email, 
						                         TipoDoc,
						                         DescTipoDocto,
						                         Orgao,
						                         NumeroDocProp'
				Call SalSendMsg( pbImprimir, SAM_Click, nParCopias, 0)
			On SAM_Destroy
				Set hWndJanela = hWndNULL
				Call SalFmtKeepMask( FALSE )
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
			On UDM_AposPesquisa
				If nTipoDoc = 2
					Call SalFmtSetInputMask ( dfsDoc, '99.999.999/9999-99' )
				Else If nTipoDoc = 1 OR nTipoDoc = 3 OR nTipoDoc = 4
					Call SalFmtSetInputMask ( dfsDoc, '999.999.999-99' )
				Else
					Call SalFmtSetInputMask ( dfsDoc, '999999999999999' )
				Call MPSFormataTelefone( dfsTelefone )
				Call MPSFormataTelefone( dfsFAX )
				If NOT hWndMDI.cQCK1MDI.MenuHabilitado( 'frmConsVeicPropEnd' )
					Set sLogradouro = ''
					Set sComplemento = ''
					Set nBairro = NUMBER_Null
					Set sBairro = ''
					Set nCEP = NUMBER_Null
					Set dfsTelefone = ''
					Set dfsFAX = ''
				Return TRUE
