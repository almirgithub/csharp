			On SAM_Create
				Set _nEstadoInicialForm = EF_Vazio
				Set _sTextoStatusBar = 'Tela de baixa de autos de infração'
				Set _sCriacaoConexao = 'Default'
				! Variáveis para as procedure's
				Set _bLimpaTelaAposAtualizacao =FALSE
				Set _bLimpaTelaAposInclusao = FALSE
				Set _bLimpaTelaAposRemocao = TRUE
				Set _sComandoAtualizacao = 'exec dbinfracao..MovimentoAutoI_restituicao @Serie = :frmRestituicao.dfsSerie, @Auto = :frmRestituicao.dfnCod,
						@Infracao = :frmRestituicao.dfnInfracao, @Favorecido = :frmRestituicao.dfsFavorecido, @Restituido = :frmRestituicao.dfnRestituido,
						@ProcessoRecurso = :frmRestituicao.dfnProcesso,@OrgaoAutuante = :frmRestituicao.dfnOrgaoAutuante'
				Set _sComandoPesquisa = 'exec dbinfracao..AutoS_restituicao @Setor = :nGlobalSetor, @Serie = :frmRestituicao.dfsSerie, @Cod = :frmRestituicao.dfnCod,
						@Infracao = :frmRestituicao.dfnInfracao, @OrgaoAutuante = :frmRestituicao.dfnOrgaoAutuante'
				! Variáveis para o retorno
				Set _sListaINTOPesquisa = ':frmRestituicao.dfsPlaca, :frmRestituicao.dfsUFPlaca, :frmRestituicao.cmbUFPlaca,
						:frmRestituicao.dfnNotificacao, :frmRestituicao.dfdVencimento, :frmRestituicao.dfnValorMoeda,
						:frmRestituicao.dfsFavorecido, :frmRestituicao.dfnRestituido, :frmRestituicao.dfnAutorizacao, :frmRestituicao.nProcesso'
				Set _sListaINTOAtualizacao = ''
				Set sSetorUsuarioN = SalStrMidX(SalNumberToStrX(nGlobalSetor,0),0,SalStrLength( SalNumberToStrX(nGlobalSetor,0) )-1)
				Set nSetorUsuario = SalStrToNumber(sSetorUsuarioN)
				Call SalCenterWindow( hWndForm )
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalDisableWindow( mdipbRestituicao )
			On UDM_AntesPesquisa
				If SalStrLength( dfsPlaca ) = 6 and SalIsNull( dfsUFPlaca)
					Call SalMessageBeep( MB_IconExclamation )
					Call SalMessageBox( 'É preciso informar a UF para a placa de 6 (seis) dígitos', 'Atenção', MB_IconExclamation | MB_Ok )
					Return FALSE
				Return TRUE
			On UDM_AposPesquisa
				If nEstadoForm = EF_RegistroNovo
					Call SalMessageBeep( MB_IconExclamation )
					Call SalMessageBox( 'Auto não localizado', 'Atenção', MB_IconExclamation | MB_Ok )
					Return FALSE
				If NOT SalIsNull( dfnAutorizacao)
					! Call SalPostMsg( dfsFavorecido, MSG_Desabilita1, 0, 0)
					! Call SalPostMsg( dfnRestituido, MSG_Desabilita1, 0, 0)
					! Call SalPostMsg( pbArquivar, MSG_Desabilita1, 0, 0)
					Call SalMessageBeep( MB_IconExclamation )
					Call SalMessageBox( 'Já foi impresso uma restituição para o auto informado', 'Atenção', MB_IconExclamation | MB_Ok )
					Call SalPostMsg( pbImprimir, MSG_Habilita1, 0, 0)
					Call SalPostMsg( pbArquivar, MSG_Habilita1, 0, 0)
				Else
					Call SalPostMsg( pbArquivar, MSG_Habilita1, 0, 0)
					If SalIsNull( dfnRestituido)
						Call SalPostMsg( pbImprimir, MSG_Desabilita1, 0, 0)
					Else
						Call SalPostMsg( pbImprimir, MSG_Habilita1, 0, 0)
				Return TRUE
			On SAM_Destroy
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
				Call SalEnableWindow( mdipbRestituicao )
			On UDM_AntesAtualizacao
				! If dfnRestituido > dfnValorMoeda
					  Call SalMessageBeep( MB_IconExclamation )
					  Call SalMessageBox( 'Valor a restituir não pode ser maior que o valor pago!', 'Atenção', MB_IconExclamation | MB_Ok )
					  Return FALSE
				Return TRUE
			On UDM_AposAtualizacao
				Call SalPostMsg( pbImprimir, MSG_Habilita1, 0, 0)
				Return TRUE
			! On MSG_MudaSemaforo
				    If nEstadoSemaforo = S_Vermelho or NOT SalIsNull( dfnAutorizacao)
					    Call SalSendMsg( pbImprimir, MSG_Habilita1, 0, 0)
				    Else
					    Call SalSendMsg( pbImprimir, MSG_Desabilita1, 0, 0)
				    Call SalSendClassMessage( MSG_MudaSemaforo, wParam, lParam)
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo',
						'Autorização de restituição', 'Autorização de restituição' )
				Return TRUE
			On SAM_ReportFetchInit
				Call fMPSPrepareProc( hSql, 'exec AutoS_restituicao_rel @Serie = :frmRestituicao.dfsSerie, @Cod = :frmRestituicao.dfnCod,
						@Infracao = :frmRestituicao.dfnInfracao, @OrgaoAut = :frmResituicao.dfnOrgaoAutuante',
						':frmRestituicao.nValor, :frmRestituicao.sExtenso, :frmRestituicao.sFavorecido, :frmRestituicao.sNome,
						:frmRestituicao.nAutorizacao, :frmRestituicao.dfdCorrente' )
				Return SqlExecute( hSql )
			On SAM_ReportFetchNext
				Call SqlFetchNext( hSql, nErro )
				If nErro != FETCH_EOF
					Return 1
				Else
					Return 0
			On SAM_ReportFinish
				Call SalPrtSetDefault(sDeviceDef,sDriverDef,sPortaDef)
