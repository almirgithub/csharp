						Set _sTextoStatusBar = 'Imprime a restuição efetuada para o auto'
						Set _sTipoComando = 'Comum'
						Set _bValidaCamposNulos = TRUE
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						If frmRestituicao.nEstadoSemaforo = S_Vermelho
							Call SalMessageBeep( MB_IconExclamation )
							If SalMessageBox( 'Os dados da tela não foram atualizados. Deseja arquivá-los agora ?', 'Atenção', MB_IconExclamation | MB_YesNo ) = IDYES
								If NOT frmRestituicao._Arquiva( )
									Return FALSE
							Else
								Return FALSE
						! Call SalReportView( hWndForm, SalCreateWindow( frmReportView, hWndForm ), 'Restitui.qrp',
								'sGlobalSecretaria, sGlobalDetran, nValor, sExtenso, sFavorecido, nAutorizacao, nProcesso, sNome, nGlobalSetor, sGlobalSetor, SqlUser, dfdCorrente',
								'Secretaria, Detran, Valor, Extenso, Favorecido, Autorizacao, Processo, Nome, Setor, NomeSetor, Usuario, DataCorrente', nErro )
						Call SalReportPrint( hWndForm, 'Restitui.qrp',
								'sGlobalSecretaria, sGlobalDetran, nValor, sExtenso, sFavorecido, nAutorizacao, nProcesso, sNome, nGlobalSetor, sGlobalSetor, SqlUser, dfdCorrente',
								'Secretaria, Detran, Valor, Extenso, Favorecido, Autorizacao, Processo, Nome, Setor, NomeSetor, Usuario, DataCorrente', 1,RPT_PrintAll,0,0, nErro )
						Call SalPostMsg( dfsFavorecido, MSG_Desabilita1, 0, 0)
						Call SalPostMsg( dfnRestituido, MSG_Desabilita1, 0, 0)
						Call SalPostMsg( dfnProcesso, MSG_Desabilita1, 0, 0)
						Call SalPostMsg( pbArquivar, MSG_Desabilita1, 0, 0)
