					On SAM_Click
						! Data Obrigatórias
						If cmbInfracao = ''
							Set sInfracao  = 'Ano:  ' || cmbAno
						Else
							If cmbMunicipio =''
								Set sInfracao = 'Ano: ' || cmbAno || ' Infracao: ' || cmbInfracao
