							Call SalReportPrint( hWndForm, 'MultasTP.qrp',
									'sGlobalSecretaria, sGlobalDetran, nGlobalSetor, sGlobalSetor, dCorrente, SqlUser,
									 :tblMultas.colDescricao, 
									:tblMultas.colQuantidade,
									:tblMultas.colValorEmUFIR, 
