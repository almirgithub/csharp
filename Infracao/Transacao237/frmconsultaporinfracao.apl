				! Call SalReportSetStringVar( hWndReport, 'Mes', frmConsultaPorInfracao.sMes)
				Return TRUE
			On SAM_ReportFetchInit
				Call SqlImmediate( 'select getdate() into :frmConsultaPorInfracao.dCorrente' )
				Set nRow= 0
				Call SalTblSetContext( tblMultas, 0 )
			On SAM_ReportFetchNext
