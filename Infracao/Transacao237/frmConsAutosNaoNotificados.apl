		Message Actions
			On SAM_Create
				Set _sTextoStatusBar = 'Tela para consulta de autos de infração'
				Set _nEstadoInicialForm = EF_Vazio
				Set _bLimpaTelaAposAtualizacao = FALSE
				Set _bLimpaTelaAposInclusao = FALSE
				Set _bLimpaTelaAposRemocao = TRUE
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalCenterWindow( hWndForm )
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				If nGlobalOrgaoAutuante != NUMBER_Null
					Set dfnOrgao = nGlobalOrgaoAutuante   
					Call SalSendMsg( dfsOrgao ,MSG_RefreshDependentes , SalWindowHandleToNumber( dfnOrgao ), 0)
					Set dfdInicial = dGlobalDataInicial
					Set dfdFinal = dGlobalDataFinal
					Call SalSendMsg( pbPesquisa, SAM_Click, 0, 0)
			On UDM_Pesquisa
				If SalIsNull( dfdInicial ) or SalIsNull( dfdFinal )
					Call SalMessageBeep( 0 )
					Call SalMessageBox( 'O período é obrigatório! ', 'Erro', MB_IconAsterisk | MB_Ok )
					If SalIsNull( dfdInicial )
						Call SalSetFocus(dfdInicial)
					Else
						Call SalSetFocus(dfdFinal)
					Return FALSE
				Call SalSendMsg( tblAutosNaoNotificados, MSG_PesquisaTabela, 0, 0)
				Return TRUE
			On UDM_AposPesquisa
				! Desabilita Campos/Botões
				If nGlobalOrgaoAutuante != NUMBER_Null
					If NOT fMPSPrepareProc( hSqlApp, 'dbinfracao..OrgaoAutuanteS_desc_sint 
							@Cod = :frmConsAutosNaoNotificados.dfnOrgao, @Setor=:frmConsAutosNaoNotificados.nSetorUsuario',
							'dfsOrgao' )
						Call SalWaitCursor( FALSE )
						Return FALSE
					Call SalSendClassMessage( UDM_AposPesquisa, wParam, lParam )
				Else
					Call SalDisableWindow(dfdInicial)
					Call SalDisableWindow(dfdFinal)
					Call SalDisableWindow(dfnOrgao)
					Call SalDisableWindow(dfsOrgao)
					! Habilita Campos/Botões
					Call SalEnableWindow(pbImprimir)
					!
					Call SalSendClassMessage( UDM_AposPesquisa, wParam, lParam )
			On SAM_Destroy
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo',
						'Relatório de Autos não Notificados', 'Relatório de Autos não Notificados' )
				Return TRUE
			On SAM_ReportFetchInit
				Set nRow= 0
			On SAM_ReportFetchNext
				If SalTblFetchRow(tblAutosNaoNotificados,nRow)!=TBL_NoMoreRows
					Set nRow = nRow + 1
					Return TRUE
				Else
					Return FALSE



