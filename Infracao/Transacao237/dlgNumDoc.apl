		Functions
		Window Parameters
			String: sNumDoc
			Receive Number: nPropCod
		Window Variables
			Number: nNumCPFCGC
			Number: nRetorno
			! ---- [faml] 31/08/2010
			String: sSohMaximo
		Message Actions
			On SAM_Create
				Set _sTextoStatusBar = ''
				Set _sNroCamposIgnorados = 'NENHUM'
				Set _sCriacaoConexao = 'FALSE'
				! Faz tratamento sobre o CPF/CGC
				Set dfsNumDoc = sNumDoc
				! If DetranHCPF( dfsNumDoc )
					Call SalFmtSetInputMask ( dfsNumDoc, '999.999.999-99' )
				! Else If DetranHCGC( dfsNumDoc )
					Call SalFmtSetInputMask ( dfsNumDoc, '99.999.999/9999-99' )
				! Else
					Call SalFmtSetInputMask ( dfsNumDoc, '99999999999999' )
				! Verifica se realmente há mais de um CPF/CGC
				Call fMPSPrepareProc( hSqlApp, 'exec dbvcen..ProprietarioS_n_numdoc @NumDoc = :dlgNumDoc.dfsNumDoc',
						':dlgNumDoc.nNumCPFCGC' )
				Call SqlExecute( hSqlApp )
				Call SqlFetchNext( hSqlApp, nRetorno )
				If nNumCPFCGC >= 1
					Call SalSendClassMessage( SAM_Create, wParam, lParam )
					Call SalCenterWindow( hWndForm )
				Else
					Call SalSendMsg( hWndForm, SAM_Close, 0, 0 )
			On SAM_CreateComplete
				! Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				If nNumCPFCGC >= 1
					Call SalSendMsg( tblNomes, MSG_PesquisaTabela, 0, 0 )
					Call SalSetFocus( tblNomes )
				!
				If not SalTblSetContext( tblNomes, 0 )
					Call SalSendMsg( hWndForm, SAM_Close, 0, 0 )

