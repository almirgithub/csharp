				Window Variables
				Message Actions
					On SAM_Create
						Set _sTextoStatusBar = 'Tabela dos nomes com o mesmo CPF/CNPJ'
						Set _sNroCamposIgnorados = 'NENHUM'
						!
						Set _sCriacaoConexao = 'Default'
						Set _bNuncaHabilitado = FALSE
						Set _bPesquisaCreate = FALSE
						Set _bSempreHabilitado = TRUE
						! ---- [faml] 31/08/2010 
						! ---- Set sSohMaximo  = 'N' para buscar todos os proprietários cadastrados com o mesmo @NumDoc 
						! ---- Set sSohMaximo != 'N' ou nulo, para buscar apenas o proprietário recém cadastrado com o @NumDoc
						Set sSohMaximo= 'S'
						Set _sComandoPesquisa = 'exec dbvcen..ProprietarioS_nomecod @NumDoc = :dlgNumDoc.dfsNumDoc
								, @SohMaximo= :sSohMaximo'
						Set _sIntoPesquisa = ':dlgNumDoc.tblNomes.colsNome, 
								:dlgNumDoc.tblNomes.colsDoc,
								:dlgNumDoc.tblNomes.colsOrgao, 
								:dlgNumDoc.tblNomes.colsUF, 
								:dlgNumDoc.tblNomes.colnCod'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
						Set nPermissao = H_SELECT
					! Seleciona com DoubleClick
					On SAM_DoubleClick
						Call SalSendMsg( pbSelecionar, SAM_Click, 0, 0 )
					On SAM_RowHeaderDoubleClick
						Call SalSendMsg( pbSelecionar, SAM_Click, 0, 0 )
