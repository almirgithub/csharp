			! Function: fCriaTabelaNoBanco
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					Number: nRow
					String: sColunas
					Boolean: bOk
					String: sTipoCaracter
				Actions
					! When SqlError
						Call SalMessageBox( "Tabela já existe" , "ERRO", MB_IconExclamation )
						Return FALSE
					! Set sInsert = "Insert Into " || dfNomeDaTabela || " ( "
					Call SalTblSetFlagsAnyRows( tblColunas, ROW_New, FALSE, 0, 0 )
					Set nRow = 0
					Set nNumeroColunas =  0
					Set nIndice = 0
					Set sColunas = ''
					!
					! If Not SqlConnect( hSqlApoio )
						Return FALSE
					Loop
						Set bOk = SalTblSetContext( tblColunas, nRow )
						If tblColunas.colNome = STRING_Null
							Break
						If sColunas != ''
							Set sColunas = sColunas || ", "
						If tblColunas.colNome = STRING_Null or tblColunas.colPosicaoInicial = NUMBER_Null
								   or tblColunas.colTamanho = NUMBER_Null or tblColunas.colTipoCaracter = STRING_Null
							Call SalMessageBox( "Existe dados incompletos na tabela", "Erro", MB_IconExclamation )
							Return FALSE
						If tblColunas.colTipoCaracter  = "Alfanumérico"
							Set sTipoCaracter = "    CHAR(" || SalNumberToStrX(tblColunas.colTamanho,0) || ")    null "
						Else
							If tblColunas.colCasasDecimais = NUMBER_Null
								Set tblColunas.colCasasDecimais = 0
							Set sTipoCaracter = "   numeric (" || SalNumberToStrX(tblColunas.colTamanho,0) || ", " ||
									      SalNumberToStrX(tblColunas.colCasasDecimais,0) || " )    null "
						Set sColunas  = sColunas || tblColunas.colNome || sTipoCaracter
						Set nIndice = nIndice + 1
						Set nRow = nRow + 1
						Set nNumeroColunas = nNumeroColunas + 1
					!
					If Not fCriaTable(sColunas)
						Return FALSE
					Set sInsert = sInsert || ") Values ( "
					Return TRUE
			!
			! Function: fObrigatorio
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
				Actions
					If Not SalTblAnyRows( tblColunas, ROW_Edited|ROW_New|ROW_Selected, ROW_MarkDeleted )
						Return FALSE
					Return TRUE
			!
			! Function: fCriaTable
				Description:
				Returns
					Boolean:
				Parameters
					String: sColunas
				Static Variables
				Local variables
					String: sSelect
					Boolean: bExists
				Actions
					! When SqlError
						Call SalMessageBox( "Erro durante a criação da tabela", "Erro", MB_IconExclamation )
						Return FALSE
					If fExistsTable()
						Set sCreate = "Drop Table dbinfracao.." || sTabela
						If Not SqlPrepareAndExecute( hSqlApoio, sCreate  )
							Return FALSE
					Set sCreate = "Create Table dbinfracao.." || sTabela || " (" || sColunas  || ")"
					If Not SqlPrepareAndExecute(  , sCreate  )
						Return FALSE
					Return TRUE
			!
			! Function: fExistsTable
				Description:
				Returns
					Boolean:
				Parameters
				Static Variables
				Local variables
					String: sSelect
					Boolean: bExists
					Boolean: bOk
				Actions
					Set sSelect = "Select name  From dbinfracao..sysobjects Where name = '" || sTabela || "'
							and type = 'U' "
					Set bOk  = SqlExists( sSelect, bExists )
					If bExists
						Return TRUE
					Return FALSE
			!
			Function: fLimpa
				Description:
				Returns
				Parameters
					Window Handle: hHandle
				Static Variables
				Local variables
					Number: nType
				Actions
					Set nType = SalGetType( hHandle )
					If nType = TYPE_ChildTable
						Call SalTblReset( hHandle )
					Else If nType =  TYPE_ComboBox
						Call SalListSetSelect( hHandle, -1 )
					Else If nType = TYPE_DataField
						Call SalClearField( hHandle )
					Else If nType = TYPE_MultilineText
						Call SalClearField( hHandle )
			!
			Function: PreencheLista
				Description: Preenche a lista e o array de codigos dos itens.
						Esta função recebe um Sql Handle e uma string com o
						comando.
				Returns
					Number:
				Parameters
					Window Handle: hCombo
					String: strComando
				Static Variables
				Local variables
					! Flags de retorno de funcoes
					Boolean: bOk
					Number: nInd
					! Contador
					Number: nCount
					! Variaveis para codigos
					String: strNome
					! Armazena temporariamente a string que sera´ jogada dentro da combo propriamente dita
					String: strItem
					! car 21/06/96 - Armazena a selecao do combo para que nao se perca no drop-down
					String: strCampoCodigo
				Actions
					Set bOk = fMPSConnect(hSqlApoio)
					Set bOk = fMPSPrepareProc(hSqlApoio,strComando ,':strNome')
					Set bOk = SqlExecute(hSqlApoio)
					Set nCount = 0
					Call SqlFetchNext(hSqlApoio,nInd)
					While nInd != FETCH_EOF
						Set nCount = nCount + 1
						If nCount = 1
							Set strCampoCodigo = strNome
						Call SalListAdd(hCombo,strNome)
						Call SqlFetchNext(hSqlApoio,nInd)
					! Call SqlDisconnect(hSqlApoio)
			!
			Function: fInseriTable
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Set strComando = 'Exec dbinfracao..RNFRegistroTransacaoS 
							@Transacao = :cmbTransacao, 
							@TipoRegistro=:cmbTipoRegistro'
					Set bOK = fMPSPrepareProc(hSqlApoio,strComando ,'
							:frmCadastroLayoutRenainf.sCampo,
							:frmCadastroLayoutRenainf.sTipoCampo, 
							:frmCadastroLayoutRenainf.nPosicaoInic,
							:frmCadastroLayoutRenainf.nTamanho,
							:frmCadastroLayoutRenainf.nCasasDecimais,
							:frmCadastroLayoutRenainf.sTabela,
							:frmCadastroLayoutRenainf.sStorageProcedure,
							:frmCadastroLayoutRenainf.sLimparInicio')
					Set bOK = SqlExecute(hSqlApoio)
					If Not bOK
						Return FALSE
					Call SqlFetchNext(hSqlApoio,frmCadastroLayoutRenainf.nInd)
					If Not bOK
						Call SalTblReset( tblColunas )
						Return FALSE
					Set nRow = 0
					Call SalTblReset( tblColunas )
					Set frmCadastroLayoutRenainf.nTamanhoRegistro = 0
					While frmCadastroLayoutRenainf.nInd != FETCH_EOF
						Call SalTblInsertRow( tblColunas, nRow )
						Set tblColunas.colNome = frmCadastroLayoutRenainf.sCampo
						Set tblColunas.colPosicaoInicial = frmCadastroLayoutRenainf.nPosicaoInic
						Set tblColunas.colTamanho = frmCadastroLayoutRenainf.nTamanho
						Set tblColunas.colTipoCaracter = frmCadastroLayoutRenainf.sTipoCampo
						Set tblColunas.colCasasDecimais = frmCadastroLayoutRenainf.nCasasDecimais
						Set tblColunas.colPosicaoFinal= frmCadastroLayoutRenainf.nPosicaoInic + frmCadastroLayoutRenainf.nTamanho - 1
						Set nRow = nRow + 1
						Set frmCadastroLayoutRenainf.nTamanhoRegistro=frmCadastroLayoutRenainf.nTamanhoRegistro+frmCadastroLayoutRenainf.nTamanho
						Call SqlFetchNext(hSqlApoio,frmCadastroLayoutRenainf.nInd)
					While frmCadastroLayoutRenainf.nRow <100
						Call SalTblInsertRow( tblColunas, nRow )
						Set nRow = nRow + 1
					!
					Call fInsereTabelaPos()
			!
			Function: fArquivaLayout
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Set nInd = 0
					!
					Set strComando = 'Exec dbinfracao..RNFArquivaLayout
							@Coluna = :frmCadastroLayoutRenainf.tblColunas.colNome, 
							@Sequencial=:frmCadastroLayoutRenainf.nSequencial,
							@DescTransacao = :cmbTransacao, 
							@TipoRegistroTransacao=:cmbTipoRegistro'
					Call SalTblSetContext( tblColunas, 0 )
					Set nSequencial =0
					Set nInd = 0
					!
					Loop
						If SalTblSetContext( tblColunas, nInd ) = FALSE
							Break
						If tblColunas.colNome = ''
							Break
						If Not SalTblQueryRowFlags( tblColunas, SalTblQueryContext( tblColunas ), ROW_MarkDeleted )
							Set nSequencial = nSequencial + 1
							Set bOK = fMPSPrepareProc(hSqlApoio,strComando ,'')
							Set bOK = SqlExecute(hSqlApoio)
							If Not bOK
								Call SalMessageBox( 'Erro durante inserção do campo: ' || tblColunas.colNome , 'Alerta', MB_IconExclamation )
								Break
						Set nInd = nInd + 1
					If Not bOK
						Call SalMessageBox( 'Ocorreu(ram) erro(s) durante inserção' , 'Alerta', MB_IconExclamation )
					Else
						Call SalMessageBox( 'Arquivamento Realizado Com Sucesso' , 'Alerta', MB_IconExclamation )
						Call fInsereTabelaPos()
			!
			Function: fInsereTabelaPos
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Set bOK = fMPSPrepareProc(hSqlApoio,"exec dbinfracao..RNFRegistroTransacaoS
										@Transacao	= :frmCadastroLayoutRenainf.cmbTransacao,
										@TipoRegistro 	= :frmCadastroLayoutRenainf.cmbTipoRegistro,
										@Indicador	= 'RETORNO'",
							":frmCadastroLayoutRenainf.sCampo,
							:frmCadastroLayoutRenainf.sTipoCampo, 
							:frmCadastroLayoutRenainf.nPosicaoInic,
							:frmCadastroLayoutRenainf.nTamanho,
							:frmCadastroLayoutRenainf.nCasasDecimais,
							:frmCadastroLayoutRenainf.sTabela,
							:frmCadastroLayoutRenainf.sStorageProcedure,
							:frmCadastroLayoutRenainf.sLimparInicio,
							:frmCadastroLayoutRenainf.naSequencial,
							:frmCadastroLayoutRenainf.naTransacao,
							:frmCadastroLayoutRenainf.saTipoTransacao,
							:frmCadastroLayoutRenainf.naPosicaoInicial,
							:frmCadastroLayoutRenainf.naPosicaoFinal,
							:frmCadastroLayoutRenainf.saDescricao,
							:frmCadastroLayoutRenainf.saRNFCampo,
							:frmCadastroLayoutRenainf.saFormatoCampo,
							:frmCadastroLayoutRenainf.naTamanho,
							:frmCadastroLayoutRenainf.naDecimais")
					Set bOK = SqlExecute(hSqlApoio)
					If Not bOK
						Return FALSE
					Call SqlFetchNext(hSqlApoio,frmCadastroLayoutRenainf.nInd)
					If Not bOK
						Call SalTblReset( tblColunas )
						Return FALSE
					Set nRow = 0
					Call SalTblReset( tblColunas )
					Set frmCadastroLayoutRenainf.nTamanhoRegistro = 0
					While frmCadastroLayoutRenainf.nInd != FETCH_EOF
						Call SalTblInsertRow( tblColunas, nRow )
						Set tblColunas.colNome = frmCadastroLayoutRenainf.sCampo
						Set tblColunas.colPosicaoInicial = frmCadastroLayoutRenainf.nPosicaoInic
						Set tblColunas.colTamanho = frmCadastroLayoutRenainf.nTamanho
						Set tblColunas.colTipoCaracter = frmCadastroLayoutRenainf.sTipoCampo
						Set tblColunas.colCasasDecimais = frmCadastroLayoutRenainf.nCasasDecimais
						Set tblColunas.colPosicaoFinal= frmCadastroLayoutRenainf.nPosicaoInic + frmCadastroLayoutRenainf.nTamanho - 1
						!
						Set tblColunas.nvSequencial = frmCadastroLayoutRenainf.naSequencial
						Set tblColunas.nvTransacao = frmCadastroLayoutRenainf.naTransacao
						Set tblColunas.svTipoTransacao =frmCadastroLayoutRenainf.saTipoTransacao
						Set tblColunas.nvPosicaoInicial = frmCadastroLayoutRenainf.naPosicaoInicial
						Set tblColunas.nvPosicaoFinal = frmCadastroLayoutRenainf.naPosicaoFinal
						Set tblColunas.svDescricao = frmCadastroLayoutRenainf.saDescricao
						Set tblColunas.svRNFCampo = frmCadastroLayoutRenainf.saRNFCampo
						Set tblColunas.svFormatoCampo = frmCadastroLayoutRenainf.saFormatoCampo
						Set tblColunas.nvTamanho = frmCadastroLayoutRenainf.naTamanho
						Set tblColunas.nvDecimais = frmCadastroLayoutRenainf.naDecimais
						Set nRow = nRow + 1
						Set frmCadastroLayoutRenainf.nTamanhoRegistro=frmCadastroLayoutRenainf.nTamanhoRegistro+frmCadastroLayoutRenainf.nTamanho
						Call SqlFetchNext(hSqlApoio,frmCadastroLayoutRenainf.nInd)
					While frmCadastroLayoutRenainf.nRow <100
						Call SalTblInsertRow( tblColunas, nRow )
						Set nRow = nRow + 1
					! Setando o retorno das partes Fixas
					! If NOT SalTblPopulate(tblColunas, hSqlApoio, '', TBL_FillNormal )
						Call SalWaitCursor( FALSE )
						Call SqlDisconnect(hSqlApoio)
						Return FALSE
					Return TRUE 
