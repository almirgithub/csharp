						Else If colsTipoMov = '2'
							Set colsTipoMov = 'Emissão CRLV'
						Else If colsTipoMov = '3'
							Set colsTipoMov = 'Entrada SURVE'
						Else If colsTipoMov = '4'
							Set colsTipoMov = 'Saída SURVE'
						Else If colsTipoMov = '5'
							Set colsTipoMov = 'Digitação'
						Else If colsTipoMov = '6'
							Set colsTipoMov = 'Entrega ECT'
						Else If colsTipoMov = '7'
							Set colsTipoMov = 'Devolvido ECT'
						Else If colsTipoMov = '8'
							Set colsTipoMov = 'Especial'
						Else If colsTipoMov = '9'
							Set colsTipoMov = 'Autoriza emissão CRV'
						Else If colsTipoMov = '10'
							Set colsTipoMov = 'Veículo Baixado'
						Else If colsTipoMov = '11'
							Set colsTipoMov = 'Veículo Transferido'
						! Descricao da situacao do movimento
						If colsSituacao = '1'
							Set colsSituacao = 'Normal'
						Else If colsSituacao = '2'
							Set colsSituacao = 'Anormal'
						Else If colsSituacao = '3'
							Set colsSituacao = 'On-line'
						Else If colsSituacao = '4'
							Set colsSituacao = 'Batch'
						Else If colsSituacao = '5'
							Set colsSituacao = 'Digitado'
						Else If colsSituacao = '6'
							Set colsSituacao = 'Acerto OK'
						Else If colsSituacao = '7'
							Set colsSituacao = 'Temporário'
						Else If colsSituacao = '8'
							Set colsSituacao = 'Consulta'
						Else If colsSituacao = '9'
							Set colsSituacao = 'Emissão CRV Autorizado'
					On SAM_DoubleClick
						Call SalSendMsg( frmControle.tblCaixa, MSG_PesquisaTabela, 0, 0 )
						! Call SWTabsBringToTop( picTabs, 1, TRUE )
					On SAM_RowHeaderDoubleClick
						Call SalSendMsg( frmControle.tblCaixa, MSG_PesquisaTabela, 0, 0 )
