								:frmControle.tblControle.colsSituacao, :frmControle.tblControle.colnMatricula,
								:frmControle.tblControle.colnCaixa'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
						Set nPermissao = nPermissao & ( H_SELECT | H_UPDATE )
					On UDM_AposPesquisa
						Call SalSendClassMessage( UDM_AposPesquisa, wParam, lParam )
						! Call picTabs.Enable( 1, FALSE )
						! Call SalSendMsg( pbPesquisar, SAM_Click, 0, 0 )
						! Botão Limpar e Pesquisar devem ficar desabilitados
						Call SalDisableWindow( dfsPlaca )
						Call SalDisableWindow( dfnRenavam )
						Call SalDisableWindow( dfsChassi )
						Call SalDisableWindow( pbPesquisar )
						Call SalEnableWindow( picTabs )
						! Call SalDisableWindow( pbLimpar )
						Return TRUE
					On SAM_FetchRowDone
						! Descricao do tipo de movimento
						If colsTipoMov = '1'
