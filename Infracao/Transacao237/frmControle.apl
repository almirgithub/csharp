						Set _sCriacaoConexao = 'Default'
						Set _sComandoPesquisa =  'exec ControleAntigoS @Placa = :frmControle.dfsPlaca,
								@Chassi = :frmControle.dfsChassi'
						Set _sIntoPesquisa = ':frmControle.dfsPlaca, :frmControle.dfsChassi,
