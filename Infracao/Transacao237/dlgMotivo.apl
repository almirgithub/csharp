				Message Actions
					On SAM_Click
						! - - Seta o retorno do motivo
						Set psMotivo = SalStrTrimX( mlsMotivo )
						! - - Se o tamanho do texto for menor que 7 é mostrado o erro. 
						If SalStrLength( psMotivo ) < 7
							Call SalMessageBeep( -1 )
							Call SalMessageBox( 'Por favor, informe um motivo válido!', 'Error', MB_IconStop )
							Call SalSetFocus(mlsMotivo)
						! - - Se não, é fechado o modal passando TRUE para tela que chamou
						Else
							Call SalEndDialog( hWndForm, TRUE )
