			On SAM_Create
				Set _sTextoStatusBar = 'Tela de Cadastro Campos'
				!
				Set _nEstadoInicialForm = 1
				Set _sCriacaoConexao = 'Default'
				!
				! -------------------------Pesquisa----------------------------
				!
				Set _sComandoPesquisa = 'exec dbinfracao..RNFCamposS
									@Nome 		= :frmCadastroCampos.dfsNome'
				Set _sListaINTOPesquisa =
						':frmCadastroCampos.dfsNome,
						:frmCadastroCampos.dfsTipoDado,
						:frmCadastroCampos.dfnTamanho,
						:frmCadastroCampos.dfnDecimais,
						:frmCadastroCampos.dfsDescricao,
						:frmCadastroCampos.dfsColuna'
				!
				! -------------------------Inclusão----------------------------
				!
				Set _sComandoInclusao = 'exec dbinfracao..RNFCamposI
									@Nome 		= :frmCadastroCampos.dfsNome,
									@TipoCampo 	= :frmCadastroCampos.dfsTipoDado,
									@Tamanho 	= :frmCadastroCampos.dfnTamanho,
									@Decimais 	= :frmCadastroCampos.dfnDecimais,
									@Descricao 	= :frmCadastroCampos.dfsDescricao,
									@NomeFisico 	= :frmCadastroCampos.dfsColuna'
				Set _sListaINTOInclusao = ''
				Set _bLimpaTelaAposInclusao = FALSE
				!
				! -------------------------Atualização----------------------------
				!
				Set _sComandoAtualizacao = 'exec dbinfracao..RNFCamposU
									@Nome 		= :frmCadastroCampos.dfsNome,
									@TipoCampo 	= :frmCadastroCampos.dfsTipoDado,
									@Tamanho 	= :frmCadastroCampos.dfnTamanho,
									@Decimais 	= :frmCadastroCampos.dfnDecimais,
									@Descricao 	= :frmCadastroCampos.dfsDescricao,
									@NomeFisico	= :frmCadastroCampos.dfsColuna'
				Set _sListaINTOAtualizacao = ''
				Set _bLimpaTelaAposAtualizacao = FALSE
				!
				! -------------------------Remoção----------------------------
				!
				Set _sComandoRemocao = 'exec dbinfracao..RNFCamposD
									@Nome 		= :frmCadastroCampos.dfsNome'
				Set _sListaINTORemocao = ''
				Set _bLimpaTelaAposRemocao = TRUE
				!
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				!
				Call SalCenterWindow ( hWndForm )
			On UDM_AposAtualizacao
				Call SalDisableWindow( pbAnterior )
				Call SalDisableWindow( pbProximo )
				!
				Call SalSendClassMessage( UDM_AposAtualizacao, wParam, lParam )
			On UDM_AposInclusao
				Call SalSendClassMessage( UDM_AposInclusao, wParam, lParam )
				!
				Return SalSendMsg( pbNovo, SAM_Click, wParam, lParam )
