		Functions 
			Function: fAlteraCpfUsuario 
				Description: 
				Returns 
				Parameters 
				Static Variables 
				Local variables 
					Sql Handle: hSqlAltera
					Number: nLinha
				Actions 
					If SqlConnect(hSqlAltera)
						If fMPSPrepareProc(hSqlAltera, 'exec dbvcen..UsuarioCpfA 
												@Cpf 	= :dlgInformaCpfUsuario.dfsCpf,
												@Login 	= :dfsLogin', '')
							If SqlExecute(hSqlAltera)
								Call SqlDisconnect(hSqlAltera)
								Return TRUE
						Call SqlDisconnect(hSqlAltera)
					Return FALSE
