		Functions
			Function: GeraArquivo
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					String: sNomeArq
					Number: nRow
				Actions
					! começa a verificação do Arquivo...
					If sArquivo = STRING_Null
						Call SalMessageBox("Selecione um arquivo!", "Mensagem", MB_IconExclamation)
						Return FALSE
					Set nTipoArquivoAnt = 0
					Call SalSendMsg( dfmContador, SAM_Validate, 0, 0 )
					! Seleciona dados para o arquivo
					Call SalWaitCursor( TRUE )
					Set sArquivo = NomeArquivo( sArquivo)
					Set sCaminho = NomeDiretorio( sArquivo)
					Set nRegistros = nRegistros + 1
					Call SalSendMsg( dfmContador, SAM_Validate, nRegistros, 0)
					Call SybPrepareProc( hSqlApp, 'exec dbinfracao..ReemitirNotificacao  @Arquivo =:frmReNotificacaoAutuacaoDetran.sArquivo, @DataCriacao = :frmReNotificacaoAutuacaoDetran.dDataCriacao',
							':frmReNotificacaoAutuacaoDetran.nTipoArquivo,:frmReNotificacaoAutuacaoDetran.sArquivo,:frmReNotificacaoAutuacaoDetran.nQtdeRegistros,:frmReNotificacaoAutuacaoDetran.sRegistro1, :frmReNotificacaoAutuacaoDetran.sRegistro2,
							:frmReNotificacaoAutuacaoDetran.sRegistro3,
							:frmReNotificacaoAutuacaoDetran.sRegistro4,
							:frmReNotificacaoAutuacaoDetran.sRegistro5, :frmReNotificacaoAutuacaoDetran.sRegistro6,
							:frmReNotificacaoAutuacaoDetran.sRegistro7, :frmReNotificacaoAutuacaoDetran.sRegistro8,:frmReNotificacaoAutuacaoDetran.sRegistro9')
					If NOT SqlExecute( hSqlApp )
						Call SalWaitCursor( FALSE )
						Return FALSE
					! 26/11/2004
					Call SqlGetResultSetCount ( hSqlApp, nTotalRegistros)
					Set nTotalRegistros = nTotalRegistros + 2
					Call SqlFetchNext( hSqlApp, nProximo )
					! If nTotalRegistros <= 2
						! Call SalWaitCursor( FALSE )
						! Call SalMessageBeep( 0 )
						! Call SalMessageBox( 'Não há notificações de multa a serem enviadas', 'Atenção', MB_IconExclamation | MB_Ok )
						Return FALSE
					! TERMINA 26/11/2004
					If NOT  MontaArquivo( )
						Call SalWaitCursor( FALSE )
						! Return TRUE
					! Fecha arquivo
					Call SalWaitCursor( FALSE )
					Call SalSendMsg( dfmContador, SAM_Validate, 65000, 0 )
					Call SalWaitCursor( FALSE )
					Set nRow = nRow+1
			Function: MontaArquivo 
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					Boolean: bFimLeitura
				Actions
					Set nRegistros = 0
					Set nRow = 0
					Set bFimLeitura = FALSE
					! Call SqlFetchNext( hSqlApp, nProximo )
					If nProximo = FETCH_EOF
						! Call SalMessageBox( 'Não há notificações de multa a serem enviadas', 'Atenção', MB_IconExclamation | MB_Ok )
						! Call SalWaitCursor( FALSE )
						Return TRUE
					Loop
						Call SalSendMsg( dfmContador, SAM_Validate, nRegistros, 0)
						Set nRegistros = nRegistros + 1
						If NOT bFimLeitura
							If nTipoArquivo != nTipoArquivoAnt
								If nTipoArquivoAnt != 0
									Call SalFileClose( hArquivo )
								! Call SalTblInsertRow( tblArquivos, nRow )
								! Set tblArquivos.colsArquivo = sArquivo
								! Set tblArquivos.colnQtdeRegistros = nQtdeRegistros
								Set nRow = nRow + 1
								Set nTipoArquivoAnt = nTipoArquivo
								If SalFileOpen( hArquivo, sArquivo, OF_Read | OF_Share_Exclusive)
									Call SalFileClose( hArquivo )
									Call SalFileCopy( sArquivo, SalStrLeftX( sArquivo,  SalStrLength( sArquivo ) - 3 ) || 'BCK', FALSE)
								If NOT SalFileOpen( hArquivo, sArquivo, OF_Write | OF_Create  )
									Call SalMessageBox( 'Não foi possível abrir Arquivo: '||sArquivo||' verifique se caminho está correto, ou se há espaço na unidade de disco ou ainda se existe permissão para a gravação do arquivo',
											                                     'Atenção', MB_Ok|MB_IconStop )
									Return FALSE
							Call fGravaArquivoNotificacao(hArquivo,sRegistro1,sRegistro2,sRegistro3,sRegistro4,sRegistro5,sRegistro6,sRegistro7,sRegistro8,sRegistro9)
							If NOT  SqlFetchNext( hSqlApp, nProximo )
								Set bFimLeitura = TRUE
								Break
						Else
							Call SalFilePutStr( hArquivo, SalStrRepeatX( ' ', 790 ) )
							If SalNumberMod( nRegistros, 30 ) = 0
								Break
					Call SalWaitCursor( FALSE )
					! adilson 25/07/2005
					Call SalFileClose( hArquivo )
					Return TRUE
			! Function: MontaArquivo 11/06/2008
				Description: 
				Returns 
				Parameters 
				Static Variables 
				Local variables 
					Boolean: bFimLeitura
				Actions 
					Set nRegistros = 0
					Set nRow = 0
					Set bFimLeitura = FALSE
					! Call SqlFetchNext( hSqlApp, nProximo )
					If nProximo = FETCH_EOF
						Call SalMessageBox( 'Não há notificações de multa a serem enviadas', 'Atenção', MB_IconExclamation | MB_Ok )
						Call SalWaitCursor( FALSE )
						Return FALSE
					Loop 
						Call SalSendMsg( dfmContador, SAM_Validate, nRegistros, 0)
						Set nRegistros = nRegistros + 1
						If NOT bFimLeitura
							If nTipoArquivo != nTipoArquivoAnt
								If nTipoArquivoAnt != 0
									Call SalFileClose( hArquivo )
								Call SalTblInsertRow( tblArquivoss, nRow )
								Set tblArquivoss.colsArquivo = sArquivo
								Set tblArquivoss.colnQtdeRegistros = nQtdeRegistros
								Set nRow = nRow + 1
								Set nTipoArquivoAnt = nTipoArquivo
								If SalFileOpen( hArquivo, sArquivo, OF_Read | OF_Share_Exclusive)
									Call SalFileClose( hArquivo )
									Call SalFileCopy( sArquivo, SalStrLeftX( sArquivo,  SalStrLength( sArquivo ) - 3 ) || 'BCK', FALSE)
								If NOT SalFileOpen( hArquivo, sArquivo, OF_Write | OF_Create  )
									Call SalMessageBox( 'Não foi possível abrir Arquivo: '||sArquivo||' verifique se caminho está correto, ou se há espaço na unidade de disco ou ainda se existe permissão para a gravação do arquivo',
											                                     'Atenção', MB_Ok|MB_IconStop )
									Return FALSE
							Call SalFilePutStr( hArquivo,
									sRegistro1||SalStrRepeatX(' ',255-SalStrLength(sRegistro1)) || sRegistro2 ||SalStrRepeatX(' ',256-SalStrLength(sRegistro2))   || sRegistro3 ||SalStrRepeatX(' ',249-SalStrLength(sRegistro3))
									|| sRegistro4 || SalStrRepeatX(' ',249-SalStrLength(sRegistro4)) || sRegistro5 || SalStrRepeatX(' ',249-SalStrLength(sRegistro5)) || sRegistro6 ||SalStrRepeatX(' ',249-SalStrLength(sRegistro6)) || sRegistro7 ||SalStrRepeatX(' ',249-SalStrLength(sRegistro7)) ||
									sRegistro8 || SalStrRepeatX(' ',57-SalStrLength(sRegistro8)) || sRegistro9 || SalStrRepeatX(' ',22-SalStrLength(sRegistro9)))
							If NOT  SqlFetchNext( hSqlApp, nProximo )
								Set bFimLeitura = TRUE
								Break 
						Else 
							Call SalFilePutStr( hArquivo, SalStrRepeatX( ' ', 790 ) )
							If SalNumberMod( nRegistros, 30 ) = 0
								Break 
					Call SalWaitCursor( FALSE )
					! adilson 25/07/2005
					Call SalFileClose( hArquivo )
					Return TRUE
		Window Parameters
		Window Variables
			Number: nRegistros
			Number: nTotalRegistros
			Number: nTipoArquivoAnt
			Number: nTipoArquivo
			! Variaveis que conterao os dados do Arquivo de Remessa
			String: sHeader
