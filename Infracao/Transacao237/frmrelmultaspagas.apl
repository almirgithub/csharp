		Message Actions
			! Observações
				  Autor: Roberto T. - 06/04/2000
			On SAM_Create
				Set _sTextoStatusBar = 'Tela de relatório de multas pagas'
				Set _nEstadoInicialForm = EF_Vazio
				Call SalCenterWindow( hWndForm )
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
				Call SalHideWindow( picSemaforo )
				Set nSetorReduzido = nGlobalSetor
				Set sSetorUsuarioN = SalStrMidX(SalNumberToStrX(nGlobalSetor,0),0,SalStrLength( SalNumberToStrX(nGlobalSetor,0) )-1)
				Set nSetorUsuario = SalStrToNumber(sSetorUsuarioN)
			On SAM_Destroy
				If hWndReport != hWndNULL
					Call SalSendMsg( hWndReport, SAM_Close, 0,0)
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo',
						'Relatório de Multas Pagas', 'Relatório de Multas Pagas' )
				Return TRUE
			On SAM_ReportFetchInit
				Call SqlImmediate( 'select getdate() into :frmRelMultasPagas.dCorrente' )
				If rbAnalitica
					Call fMPSPrepareProc( hSql, 'exec dbinfracao..RelMultasPagas
							@OrgaoAutuante = :frmRelMultasPagas.dfnOrgao,		@DataI = :frmRelMultasPagas.dfdInicial,
							@DataF = :frmRelMultasPagas.dfdFinal,			@Saida=:frmRelMultasPagas.sSaida,
							@AgenteEquip=:frmRelMultasPagas.dfnAgenteEquip,	@Opcao=:frmRelMultasPagas.dfsOpcao,
							@Municipio = :frmRelMultasPagas.dfnMunicipio,                    @Competencia = :frmRelMultasPagas.cmbCompetencia',
							
							':frmRelMultasPagas.dDataPagamento, 			:frmRelMultasPagas.sSerie,
							:frmRelMultasPagas.sAuto, 				:frmRelMultasPagas.sInfracao,
							:frmRelMultasPagas.nValor,				:frmRelMultasPagas.sPlaca,
							:frmRelMultasPagas.dDataCredito, 			:frmRelMultasPagas.nExercicio,
							:frmRelMultasPagas.dDataVencimento,			:frmRelMultasPagas.dDataImplantacao' )
							
				Else If rbSintetica
					Call fMPSPrepareProc( hSql, 'exec dbinfracao..RelMultasPagas
							@OrgaoAutuante = :frmRelMultasPagas.dfnOrgao,		@DataI = :frmRelMultasPagas.dfdInicial,
							@DataF = :frmRelMultasPagas.dfdFinal,			@Saida=:frmRelMultasPagas.sSaida,
							@AgenteEquip=:frmRelMultasPagas.dfnAgenteEquip,	@Opcao=:frmRelMultasPagas.dfsOpcao,
							@Municipio = :frmRelMultasPagas.dfnMunicipio,                    @Competencia = :frmRelMultasPagas.cmbCompetencia',
							
							':frmRelMultasPagas.nExercicio, 			:frmRelMultasPagas.nPagasnoVencimento,
							:frmRelMultasPagas.nValorPagasnoVencimento, 		:frmRelMultasPagas.nPagasAposVencimento,
							:frmRelMultasPagas.nValorPagasAposVencimento' )
							
				Else If rbAnaliticaRec
					Call fMPSPrepareProc( hSql, 'exec dbinfracao..RelMultasPagas
							@OrgaoAutuante = :frmRelMultasPagas.dfnOrgao,		@DataI = :frmRelMultasPagas.dfdInicial,
							@DataF = :frmRelMultasPagas.dfdFinal,			@Saida=:frmRelMultasPagas.sSaida,
							@AgenteEquip=:frmRelMultasPagas.dfnAgenteEquip,	@Opcao=:frmRelMultasPagas.dfsOpcao,
							@Municipio = :frmRelMultasPagas.dfnMunicipio,                    @Competencia = :frmRelMultasPagas.cmbCompetencia',
							
							':frmRelMultasPagas.dDataPagamento, 			:frmRelMultasPagas.sSerie,
							:frmRelMultasPagas.sAuto, 				:frmRelMultasPagas.sInfracao,
							:frmRelMultasPagas.nValor,				:frmRelMultasPagas.sPlaca,
							:frmRelMultasPagas.dDataCredito, 			:frmRelMultasPagas.nExercicio,
							:frmRelMultasPagas.dDataVencimento,			:frmRelMultasPagas.dDataImplantacao,
							:frmRelMultasPagas.nProtDef,	                 		:frmRelMultasPagas.nProtJARI,
							:frmRelMultasPagas.nProtCETRAN,			:frmRelMultasPagas.nProtDecAdm,
							:frmRelMultasPagas.nProtDecJur' )
							
				If NOT SqlExecute( hSql )
					Return FALSE
				Return SqlGetResultSetCount( hSql, nErro )
			On SAM_ReportFetchNext
				Call SqlFetchNext( hSql, nErro )
				If nErro != FETCH_EOF
					Return TRUE
				Else
					Return FALSE
			On SAM_ReportFinish
				Call SalPrtSetDefault(sDeviceDef,sDriverDef,sPortaDef)
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				! Call SalHideWindow( dfArquivo )
				Call SalHideWindow( dfTexto )

