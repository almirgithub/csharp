		Functions
			Function: fMontaArquivo
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
					String: sTexto
					String: sArquivo
					Number: nRegistros
					File Handle: hArquivo
					Number: nProximo
					Boolean: bok
					String: sSequencial
				Actions
					Call SalHideWindow( dfArquivo )
					Call SalHideWindow( dfTexto )
					! Call SalShowWindow( dfMensagem )
					Set sArquivo = 'C:\\'||dfArquivo||"."||SalStrMidX( SalNumberToStrX(dfnOrgao,0), 0, 3 )
					Call SalWaitCursor( TRUE )
					! Set dfMensagem = 'GERANDO O ARQUIVO: '||sArquivo
					! Call SalModalDialog( dlgEsperaGeracao, frmRelMultasPagas )
					! Set dlgEsperaGeracao.dfArquivo = sArquivo
					Call fMPSPrepareProc( hSql, "exec dbinfracao..RelMultasPagas
							@OrgaoAutuante = :frmRelMultasPagas.dfnOrgao,		@DataI = :frmRelMultasPagas.dfdInicial,
							@DataF = :frmRelMultasPagas.dfdFinal,
							@AgenteEquip=:frmRelMultasPagas.dfnAgenteEquip,	@Opcao=:frmRelMultasPagas.dfsOpcao,
							@Saida='A' ,                                                                            @Municipio = :frmRelMultasPagas.dfnMunicipio,
							@Competencia = :frmRelMultasPagas.cmbCompetencia",
							':sTexto' )
					If NOT SqlExecute( hSql )
						Call SalMessageBeep( 0 )
						Call SalMessageBox( 'Ocorreu um erro durante a execução. Chame o Analista Responsável.', 'Atenção', MB_Ok | MB_IconExclamation )
						Return FALSE
					!
					Set bok =  SalFileOpen( hArquivo, sArquivo,OF_Write|OF_Create)
					If not bok
						Call SalMessageBeep( 0 )
						Call SalMessageBox( 'Ocorreu um erro durante a abertura do arquivo txt. Chame o Analista Responsável.', 'Atenção', MB_Ok | MB_IconExclamation )
					Set nRegistros = 0
					Call SqlFetchNext( hSql, nProximo )
					!
					Loop
						If nProximo = FETCH_EOF
							Break
						Set nRegistros = nRegistros + 1
						Set sSequencial = SalStrRepeatX( '0', 8-SalStrLength(SalNumberToStrX( nRegistros, 0 )))||SalNumberToStrX( nRegistros, 0 )
						Call SalFilePutStr( hArquivo, sSequencial||sTexto )
						Call SqlFetchNext( hSql, nProximo )
					Call SalFileClose( hArquivo )
					Call SalMessageBox(  'O ARQUIVO: '||sArquivo ||' FOI GERADO COM SUCESSO', 'ATENÇÃO', MB_IconExclamation )
					! Set dfMensagem = ''
					Call SalWaitCursor( FALSE )
					If SalMessageBox( 'Deseja visualizar o layout do arquivo gerado?', 'Atenção',MB_YesNo )=IDYES
						Set hWndReport = SalCreateWindow( frmReportView, hWndForm )
						If rbAnaliticaRec
							Call SalReportView( frmRelMultasPagas, hWndReport, 'MultasPR.qrp',
									'sGlobalSecretaria, sGlobalDetran, nGlobalSetor, sGlobalSetor, dCorrente, SqlUser,dfdInicial, dfdFinal, dfsOrgao',
									'Secretaria, Detran, Setor, DesSetor, DataCorrente, Usuario, DataI, DataF, OrgaoAutuante', nErro)
						Else
							Call SalReportView( frmRelMultasPagas, hWndReport, 'MultasP.qrp',
									'sGlobalSecretaria, sGlobalDetran, nGlobalSetor, sGlobalSetor, dCorrente, SqlUser,dfdInicial, dfdFinal, dfsOrgao',
									'Secretaria, Detran, Setor, DesSetor, DataCorrente, Usuario, DataI, DataF, OrgaoAutuante', nErro)
					Call SalSendMsgToChildren( frmRelMultasPagas, MSG_Limpa,wParam,lParam )
					Return TRUE
