					On SAM_Create
						Set _sTextoStatusBar = 'Imprime o relatório'
						Set _nPermissao = 15
						Set _sTipoComando = 'Comum'
						Call SalSendClassMessage( SAM_Create, wParam, lParam )
					On SAM_Click
						! Validações
						If NOT SalIsNull( dfdInicial) and NOT SalIsNull( dfdFinal)
							If dfdInicial > dfdFinal
								Call SalMessageBeep( MB_IconExclamation )
								Call SalMessageBox( 'A data de entrada inicial deve ser menor do que a data de entrada final', 'Atenção', MB_IconExclamation | MB_Ok )
								Call SalSetFocus( dfdInicial )
								Return FALSE
						If Not cbPagosemPE and Not cbPagosOutraUF
							Call SalMessageBeep( -1 )
							Call SalMessageBox( 'E necessário  escolhe uma opção de pesquisa', 'Atenção', MB_IconExclamation | MB_Ok )
							Call SalSetFocus( dfdInicial )
							Return FALSE
						Set sTituloRelatorio = 'Relatório de Autos Pagos Registrados por Outra UF'
						!
						! Insere Dados na Tabela Temporária
						!
						If cbPagosemPE
							Set dfsOpcao = 'P'
						If cbPagosOutraUF
							Set dfsOpcao = 'O'
						If cbPagosemPE and cbPagosOutraUF
							Set dfsOpcao = 'A'
						!
						If hWndReport != hWndNULL
							Call SalSendMsg( hWndReport, SAM_Close, 0,0)
						If dfsSaida = 'I'
							Call SalReportPrint( hWndForm, 'PAGOSRNF.QRP',
									'sGlobalSecretaria, 			sGlobalDetran, 		nGlobalSetor, 		sGlobalSetor,
									SqlUser, 				dfdInicial,
									dfdFinal, 				sOpcao,
									
									sDescOrgaoCompetencia,		sSerie,			sCod,			nDigitoVerificador,
									nInfracao,			sAutoRENAINF,		nOrgaoCompetencia,	sPlaca,
									sUFPlaca,			dDataPagamento,		nValorPago,		sUFPagamento,
									sTituloRelatorio',
									
									'Secretaria, 			Detran, 			Setor, 			NomeSetor,
									Usuario, 				EntradaI,
									EntradaF, 			Opcao,
											 
									DescOrgaoCompetencia,		Serie,			Cod,			DigitoVerificador,
									Infracao,				AutoRENAINF,		OrgaoCompetencia,		Placa,
									UFPlaca,				DataPagamento,		ValorPago,		UFPagamentos,
									TituloRelatorio', 1,RPT_PrintAll,0,0, wParam )
						Else
							Set hWndReport = SalCreateWindow( frmReportView, hWndForm )
							Call SalSetWindowText( hWndReport, 'Relatório de Autos Pagos Registrados por outras UF´s' )
							Call SalReportView( hWndForm, hWndReport, 'PAGOSRNF.QRP',
									'sGlobalSecretaria, 			sGlobalDetran, 		nGlobalSetor, 		sGlobalSetor,
									SqlUser, 				dfdInicial,
									dfdFinal, 				sOpcao,
									
									sDescOrgaoCompetencia,		sSerie,			sCod,			nDigitoVerificador,
									nInfracao,			sAutoRENAINF,		nOrgaoCompetencia,	sPlaca,
									sUFPlaca,			dDataPagamento,		nValorPago,		sUFPagamento,
									sTituloRelatorio',
									
									'Secretaria, 			Detran, 			Setor, 			NomeSetor,
									Usuario, 				EntradaI,
									EntradaF, 			Opcao,
											 
