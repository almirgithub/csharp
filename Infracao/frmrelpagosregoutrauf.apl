		Message Actions
			! Observações
				    Autor: Celso - 09/10/1997
			On SAM_Create
				Set _sTextoStatusBar = 'Tela de relatório de autos inconsistentes'
				Set _nEstadoInicialForm = EF_RegistroExistente
				Call SalCenterWindow( hWndForm )
				Call SalSendClassMessage( SAM_Create, wParam, lParam )
			!
			On SAM_CreateComplete
				Call SalSendClassMessage( SAM_CreateComplete, wParam, lParam )
				Set sSetorUsuarioN = SalStrMidX(SalNumberToStrX(nGlobalSetor,0),0,SalStrLength( SalNumberToStrX(nGlobalSetor,0) )-1)
				Set nSetorUsuario = SalStrToNumber(sSetorUsuarioN)
				Set dfsOpcao = 'C'
			!
			On SAM_Destroy
				If hWndReport != hWndNULL
					Call SalSendMsg( hWndReport, SAM_Close, 0,0)
				Call SalSendClassMessage( SAM_Destroy, wParam, lParam )
			!
			On SAM_ReportStart
				Call SalReportDlgOptions( SalNumberToWindowHandle( wParam ), 'Impressão', 'Imprimindo',
						'Relatório de autos inconsistentes', 'Relatório de autos inconsistentes' )
				Return TRUE
			On SAM_ReportFetchInit
				If fMPSConnect( hSql )
					If fMPSPrepareProc( hSql, "exec dbinfracao..RNFAutosPagosR
							@DataInicial 	= :frmRelPagosRegOutraUF.dfdInicial,
							@DataFinal 	= :frmRelPagosRegOutraUF.dfdFinal,
							@TipoRelatorio	= 'O',
							@Opcao	 	= :frmRelPagosRegOutraUF.dfsOpcao",
							
							':frmRelPagosRegOutraUF.sDescOrgaoCompetencia, 	:frmRelPagosRegOutraUF.sSerie, 		:frmRelPagosRegOutraUF.sCod,
							:frmRelPagosRegOutraUF.nDigitoVerificador, 		:frmRelPagosRegOutraUF.nInfracao, 		:frmRelPagosRegOutraUF.sAutoRENAINF,
							:frmRelPagosRegOutraUF.nOrgaoCompetencia, 		:frmRelPagosRegOutraUF.sPlaca,		:frmRelPagosRegOutraUF.sUFPlaca,
							:frmRelPagosRegOutraUF.dDataPagamento,		:frmRelPagosRegOutraUF.nValorPago,		:frmRelPagosRegOutraUF.sUFPagamento' )
						Return SqlExecute( hSql )
					!
			On SAM_ReportFetchNext
				!
				Call SqlFetchNext( hSql, nErro )
				!
				If nErro != FETCH_EOF
					If dfsOpcao = 'P'
						Set sOpcao =  'Consultar registrados em PE'
					Else If dfsOpcao = 'O'
						Set sOpcao = 'Consultar registrados em outra UF'
					Else If dfsOpcao = 'A'
						Set sOpcao = 'Consultar registrados em Pernambuco e em outra UF'
					Return TRUE
				Else
					Return FALSE
			On SAM_ReportFinish
				Call SalPrtSetDefault(sDeviceDef,sDriverDef,sPortaDef)
				Return TRUE
